{
  if (cls.equals(EntityPlain.class))   return new EntityPlain(val);
 else   if (cls.equals(EntitySerializable.class))   return new EntitySerializable(val);
 else   if (cls.equals(EntityExternalizable.class))   return new EntityExternalizable(val);
 else   if (cls.equals(EntityBinarylizable.class))   return new EntityBinarylizable(val);
 else   return new EntityWriteReadObject(val);
}
