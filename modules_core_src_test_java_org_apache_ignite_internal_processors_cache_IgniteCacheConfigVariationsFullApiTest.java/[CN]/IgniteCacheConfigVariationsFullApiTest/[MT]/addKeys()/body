{
  Map<String,List<String>> keys=new HashMap<>();
  for (int i=0; i < gridCount(); ++i)   keys.put(grid(i).name(),new ArrayList<String>());
  for (int i=0; i < 500; ++i) {
    String key="key" + i;
    Ignite g=primaryIgnite(key);
    g.cache(cacheName()).put(key,"value" + i);
    keys.get(g.name()).add(key);
  }
  return keys;
}
