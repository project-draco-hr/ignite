{
  if (parent != null)   obj=parent.value(obj);
  if (obj == null)   return null;
  if (!(obj instanceof PortableObject))   throw new IgniteSpiException("Non-portable object received as a result of property extraction " + "[parent=" + parent + ", propName="+ propName+ ", obj="+ obj+ ']');
  try {
    return ((PortableObject)obj).field(propName);
  }
 catch (  PortableException e) {
    throw new IgniteSpiException(e);
  }
}
