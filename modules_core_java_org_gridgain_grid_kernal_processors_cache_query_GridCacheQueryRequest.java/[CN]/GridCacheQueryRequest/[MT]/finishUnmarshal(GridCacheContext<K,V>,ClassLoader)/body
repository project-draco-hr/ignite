{
  super.finishUnmarshal(ctx,ldr);
  GridMarshaller mrsh=ctx.marshaller();
  if (keyFilterBytes != null)   keyFilter=mrsh.unmarshal(keyFilterBytes,ldr);
  if (valFilterBytes != null)   valFilter=mrsh.unmarshal(valFilterBytes,ldr);
  if (beforeCbBytes != null)   beforeCb=mrsh.unmarshal(beforeCbBytes,ldr);
  if (afterCbBytes != null)   afterCb=mrsh.unmarshal(afterCbBytes,ldr);
  if (prjFilterBytes != null)   prjFilter=mrsh.unmarshal(prjFilterBytes,ldr);
  if (rdcBytes != null)   rdc=mrsh.unmarshal(rdcBytes,ldr);
  if (fieldsRdcBytes != null)   fieldsRdc=mrsh.unmarshal(fieldsRdcBytes,ldr);
  if (transBytes != null)   trans=mrsh.unmarshal(transBytes,ldr);
  if (visBytes != null)   vis=mrsh.unmarshal(visBytes,ldr);
  if (argsBytes != null)   args=mrsh.unmarshal(argsBytes,ldr);
  if (cArgsBytes != null)   cArgs=mrsh.unmarshal(cArgsBytes,ldr);
}
