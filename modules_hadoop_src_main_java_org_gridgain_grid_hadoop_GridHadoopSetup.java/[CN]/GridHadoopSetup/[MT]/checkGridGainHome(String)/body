{
  URL jarUrl=U.class.getProtectionDomain().getCodeSource().getLocation();
  try {
    Path jar=Paths.get(jarUrl.toURI());
    Path gg=Paths.get(ggHome);
    if (!jar.startsWith(gg))     warn("GridGain JAR files are not under GRIDGAIN_HOME. Make sure GRIDGAIN_HOME is correct!");
  }
 catch (  Exception e) {
    exit(e.getMessage());
  }
}
