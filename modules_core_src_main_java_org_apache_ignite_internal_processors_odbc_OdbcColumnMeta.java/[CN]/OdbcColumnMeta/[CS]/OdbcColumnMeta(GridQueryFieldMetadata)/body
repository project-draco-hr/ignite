{
  this.schemaName=OdbcUtils.addQuotationMarksIfNeeded(info.schemaName());
  this.tableName=info.typeName();
  this.columnName=info.fieldName();
  Class<?> type;
  try {
    type=Class.forName(info.fieldTypeName());
  }
 catch (  Exception ignored) {
    type=Object.class;
  }
  this.dataType=type;
}
