{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeMap("deltas",deltas,UUID.class,long.class))     return false;
  state++;
case 1:
if (!writer.writeMessage("snapVer",snapVer)) return false;
state++;
}
return true;
}
