{
  writer.setBuffer(buf);
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeMapField("deltas",deltas,MessageFieldType.UUID,MessageFieldType.LONG))     return false;
  writer.incrementState();
case 1:
if (!writer.writeField("snapVer",snapVer,MessageFieldType.MSG)) return false;
writer.incrementState();
}
return true;
}
