{
  writer.setBuffer(buf);
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeField("dataBytes",dataBytes,MessageFieldType.BYTE_ARR))     return false;
  writer.incrementState();
case 1:
if (!writer.writeField("futId",futId,MessageFieldType.IGNITE_UUID)) return false;
writer.incrementState();
case 2:
if (!writer.writeField("routineId",routineId,MessageFieldType.UUID)) return false;
writer.incrementState();
case 3:
if (!writer.writeField("type",type != null ? (byte)type.ordinal() : -1,MessageFieldType.BYTE)) return false;
writer.incrementState();
}
return true;
}
