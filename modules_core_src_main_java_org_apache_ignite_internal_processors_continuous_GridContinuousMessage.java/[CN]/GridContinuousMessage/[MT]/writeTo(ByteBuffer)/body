{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeByteArray("dataBytes",dataBytes))     return false;
  state++;
case 1:
if (!writer.writeIgniteUuid("futId",futId)) return false;
state++;
case 2:
if (!writer.writeUuid("routineId",routineId)) return false;
state++;
case 3:
if (!writer.writeEnum("type",type)) return false;
state++;
}
return true;
}
