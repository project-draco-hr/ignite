{
synchronized (this) {
    if (isDone())     return true;
    U.log(log,"Cancelled rebalancing from all nodes [cache=" + cctx.name() + ", topology="+ topologyVersion());
    for (    UUID nodeId : remaining.keySet()) {
      cleanupRemoteContexts(nodeId);
    }
    remaining.clear();
    checkIsDone(true);
  }
  return true;
}
