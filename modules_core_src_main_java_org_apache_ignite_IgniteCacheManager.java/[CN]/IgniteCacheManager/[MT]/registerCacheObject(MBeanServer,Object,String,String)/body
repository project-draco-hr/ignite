{
  ObjectName registeredObjectName=getObjectName(name,objectName);
  try {
    if (!isRegistered(mBeanServer,registeredObjectName))     mBeanServer.registerMBean(mxbean,registeredObjectName);
  }
 catch (  Exception e) {
    throw new CacheException("Error registering cache MXBeans for CacheManager " + registeredObjectName.toString() + " . Error was "+ e.getMessage(),e);
  }
}
