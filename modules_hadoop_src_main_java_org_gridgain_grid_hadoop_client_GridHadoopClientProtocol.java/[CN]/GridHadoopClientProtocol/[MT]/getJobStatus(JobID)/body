{
  try {
    GridHadoopJobStatus jobStatus=cli.compute().execute(GridHadoopProtocolJobStatusTask.class.getName(),new GridHadoopProtocolTaskArguments(jobId.getJtIdentifier(),jobId.getId()));
    return new JobStatus();
  }
 catch (  GridClientException e) {
    throw new IOException("Failed to get job status: " + jobId,e);
  }
}
