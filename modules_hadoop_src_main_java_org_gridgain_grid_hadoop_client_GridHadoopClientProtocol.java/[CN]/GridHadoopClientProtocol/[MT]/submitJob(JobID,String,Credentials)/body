{
  try {
    GridHadoopJobStatus status=cli.compute().execute(GridHadoopProtocolSubmitJobTask.class.getName(),new GridHadoopProtocolTaskArguments(jobId.getJtIdentifier(),jobId.getId(),conf));
    return new JobStatus();
  }
 catch (  GridClientException e) {
    throw new IOException("Failed to submit job.",e);
  }
}
