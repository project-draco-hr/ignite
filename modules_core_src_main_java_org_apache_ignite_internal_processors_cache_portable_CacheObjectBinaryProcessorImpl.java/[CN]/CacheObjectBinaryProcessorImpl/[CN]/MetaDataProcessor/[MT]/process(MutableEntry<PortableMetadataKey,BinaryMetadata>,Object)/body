{
  try {
    BinaryMetadata oldMeta=entry.getValue();
    Map<String,Integer> fields=new HashMap<>();
    if (checkMeta(typeId,oldMeta,newMeta,fields)) {
      BinaryMetadata res=new BinaryMetadata(typeId,newMeta.typeName(),fields,newMeta.affinityKeyFieldName());
      entry.setValue(res);
      return null;
    }
 else     return null;
  }
 catch (  BinaryObjectException e) {
    return e;
  }
}
