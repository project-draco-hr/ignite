{
  try {
    BinaryType oldMeta=entry.getValue();
    Map<String,String> fields=new HashMap<>();
    if (checkMeta(typeId,oldMeta,newMeta,fields)) {
      BinaryType res=new BinaryMetaDataImpl(newMeta.typeName(),fields,newMeta.affinityKeyFieldName());
      entry.setValue(res);
      return null;
    }
 else     return null;
  }
 catch (  BinaryObjectException e) {
    return e;
  }
}
