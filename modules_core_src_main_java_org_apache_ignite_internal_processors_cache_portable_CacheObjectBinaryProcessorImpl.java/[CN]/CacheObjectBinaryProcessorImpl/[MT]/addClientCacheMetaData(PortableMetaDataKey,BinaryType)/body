{
  clientMetaDataCache.compute(key,new ConcurrentHashMap8.BiFun<PortableMetaDataKey,BinaryType,BinaryType>(){
    @Override public BinaryType apply(    PortableMetaDataKey key,    BinaryType oldMeta){
      BinaryType res;
      try {
        res=checkMeta(key.typeId(),oldMeta,newMeta,null) ? newMeta : oldMeta;
      }
 catch (      BinaryObjectException e) {
        res=oldMeta;
      }
      return res;
    }
  }
);
}
