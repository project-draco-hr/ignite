{
  if (clientNode)   return new ArrayList<>(clientMetaDataCache.values());
  return F.viewReadOnly(metaDataCache.entrySetx(metaPred),new C1<Cache.Entry<PortableMetaDataKey,BinaryType>,BinaryType>(){
    private static final long serialVersionUID=0L;
    @Override public BinaryType apply(    Cache.Entry<PortableMetaDataKey,BinaryType> e){
      return e.getValue();
    }
  }
);
}
