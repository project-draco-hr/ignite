{
  GridProtocolHandler.deregisterDeploymentManager();
  if (verStore != null)   verStore.stop();
  if (ldrStore != null)   ldrStore.stop();
  if (locStore != null)   locStore.stop();
  if (comm != null)   comm.stop();
  getSpi().setListener(null);
  stopSpi();
  if (log.isDebugEnabled())   log.debug(stopInfo());
}
