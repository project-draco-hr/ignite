{
  for (  GridCacheDistributionMode mode : new GridCacheDistributionMode[]{PARTITIONED_ONLY,NEAR_PARTITIONED}) {
    distroMode=mode;
    writeBehind=false;
    try {
      super.testLifecycleAware();
    }
 catch (    AssertionError e) {
      throw new AssertionError("Failed for [distroMode=" + distroMode + ", writeBehind="+ writeBehind+ ']',e);
    }
    writeBehind=true;
    try {
      super.testLifecycleAware();
    }
 catch (    AssertionError e) {
      throw new AssertionError("Failed for [distroMode=" + distroMode + ", writeBehind="+ writeBehind+ ']',e);
    }
  }
}
