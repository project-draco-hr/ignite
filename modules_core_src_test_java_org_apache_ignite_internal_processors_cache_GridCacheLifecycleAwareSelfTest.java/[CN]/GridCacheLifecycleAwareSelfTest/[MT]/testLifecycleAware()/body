{
  for (  boolean nearEnabled : new boolean[]{true,false}) {
    near=nearEnabled;
    writeBehind=false;
    try {
      super.testLifecycleAware();
    }
 catch (    AssertionError e) {
      throw new AssertionError("Failed for [near=" + near + ", writeBehind="+ writeBehind+ ']',e);
    }
    writeBehind=true;
    try {
      super.testLifecycleAware();
    }
 catch (    AssertionError e) {
      throw new AssertionError("Failed for [near=" + near + ", writeBehind="+ writeBehind+ ']',e);
    }
  }
}
