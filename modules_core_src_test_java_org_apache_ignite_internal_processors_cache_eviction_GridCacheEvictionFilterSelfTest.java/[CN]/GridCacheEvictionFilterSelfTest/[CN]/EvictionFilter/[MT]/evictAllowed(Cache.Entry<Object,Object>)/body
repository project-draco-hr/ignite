{
  AtomicInteger i=cnts.get(entry.getKey());
  if (i == null) {
    AtomicInteger old=cnts.putIfAbsent(entry.getKey(),i=new AtomicInteger());
    if (old != null)     i=old;
  }
  i.incrementAndGet();
  boolean ret=!(entry.getValue() instanceof Integer);
  if (!ret)   info(">>> Not evicting key [key=" + entry.getKey() + ", cnt="+ i.get()+ ']');
 else   info(">>> Evicting key [key=" + entry.getKey() + ", cnt="+ i.get()+ ']');
  return ret;
}
