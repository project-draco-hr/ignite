{
  if (rootPane.getCenter() == connLayerPnl)   if (checkInput(jdbcDrvJarTf,true,"Path to JDBC driver is not specified!") && checkInput(jdbcDrvClsTf,true,"JDBC driver class name is not specified!") && checkInput(jdbcUrlTf,true,"JDBC URL connection string is not specified!")&& checkInput(userTf,true,"User name is not specified!"))   fill();
 else   generate();
}
