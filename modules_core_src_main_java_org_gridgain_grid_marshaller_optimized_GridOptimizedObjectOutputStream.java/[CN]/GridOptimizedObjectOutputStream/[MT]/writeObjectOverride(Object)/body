{
  try {
    writeObject0(obj);
  }
 catch (  IOException e) {
    Throwable t=e;
    do {
      if (CONVERTED_ERR.contains(t.getMessage()))       throw new IOException("You are trying to serialize internal classes that are not supposed " + "to be serialized. Check that all non-serializable fields are transient. Consider using " + "static inner classes instead of non-static inner classes and anonymous classes.",e);
    }
 while ((t=t.getCause()) != null);
    throw e;
  }
}
