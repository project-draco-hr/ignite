{
  super.prepareMarshal(ctx);
  if (nearEvictedBytes == null && nearEvicted != null)   nearEvictedBytes=marshalCollection(nearEvicted,ctx);
  if (preloadEntriesBytes == null && preloadEntries != null)   preloadEntriesBytes=marshalCollection(preloadEntries,ctx);
  GridCacheContext cctx=ctx.cacheContext(cacheId);
  if (preloadEntriesBytes == null && preloadEntries != null) {
    marshalInfos(preloadEntries,cctx);
    preloadEntriesBytes=marshalCollection(preloadEntries,ctx);
  }
}
