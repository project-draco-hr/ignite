{
  msgC.run();
  if (msg instanceof GridTestMessage) {
    GridTestMessage testMsg=(GridTestMessage)msg;
    if (!testMsg.getSourceNodeId().equals(nodeId))     fail("Listener nodeId is not equal to message nodeId.");
    if (!reject)     rcvdMsgs.offer(testMsg);
    if (!locNodeId.equals(nodeId))     rmtMsgCnt.incrementAndGet();
  }
}
