{
  super.prepareMarshal(ctx);
  if (candsByIdx != null)   candsByIdxBytes=ctx.marshaller().marshal(candsByIdx);
  if (candsByKey != null) {
    if (ctx.deploymentEnabled()) {
      for (      KeyCacheObject key : candsByKey.keySet())       prepareObject(key,ctx);
    }
    candsByKeyBytes=CU.marshal(ctx,candsByKey);
  }
}
