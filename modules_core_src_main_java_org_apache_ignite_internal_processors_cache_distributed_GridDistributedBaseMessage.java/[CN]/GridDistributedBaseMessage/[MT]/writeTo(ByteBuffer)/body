{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 3:
    if (!writer.writeByteArray("candsByIdxBytes",candsByIdxBytes))     return false;
  state++;
case 4:
if (!writer.writeByteArray("candsByKeyBytes",candsByKeyBytes)) return false;
state++;
case 5:
if (!writer.writeCollection("committedVers",committedVers,GridCacheVersion.class)) return false;
state++;
case 6:
if (!writer.writeCollection("rolledbackVers",rolledbackVers,GridCacheVersion.class)) return false;
state++;
case 7:
if (!writer.writeMessage("ver",ver != null ? ver.clone() : null)) return false;
state++;
}
return true;
}
