{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    byte[] dataBytes0=commState.getByteArray("dataBytes");
  if (dataBytes0 == BYTE_ARR_NOT_READ)   return false;
dataBytes=dataBytes0;
commState.idx++;
case 1:
