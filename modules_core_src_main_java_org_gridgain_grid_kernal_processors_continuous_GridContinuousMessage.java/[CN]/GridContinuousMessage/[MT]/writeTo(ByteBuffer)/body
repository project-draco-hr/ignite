{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(null,directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putByteArray(null,dataBytes))     return false;
  commState.idx++;
case 1:
if (!commState.putUuid(null,routineId)) return false;
commState.idx++;
case 2:
if (!commState.putEnum(null,type)) return false;
commState.idx++;
}
return true;
}
