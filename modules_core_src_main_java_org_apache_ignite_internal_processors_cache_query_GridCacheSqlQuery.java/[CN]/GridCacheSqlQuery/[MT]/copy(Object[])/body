{
  GridCacheSqlQuery cp=new GridCacheSqlQuery();
  cp.qry=qry;
  cp.cols=cols;
  cp.paramIdxs=paramIdxs;
  cp.paramsSize=paramsSize;
  if (F.isEmpty(args))   cp.params=EMPTY_PARAMS;
 else {
    cp.params=new Object[paramsSize];
    for (    int paramIdx : paramIdxs)     cp.params[paramIdx]=args[paramIdx];
  }
  return cp;
}
