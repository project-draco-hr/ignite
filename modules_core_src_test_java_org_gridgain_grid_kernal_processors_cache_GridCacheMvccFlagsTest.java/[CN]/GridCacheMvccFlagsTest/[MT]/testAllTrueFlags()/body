{
  GridCacheAdapter<String,String> cache=grid.internalCache();
  GridCacheTestEntryEx<String,String> entry=new GridCacheTestEntryEx<>(cache.context(),"1");
  UUID id=UUID.randomUUID();
  GridCacheVersion ver=new GridCacheVersion(1,0,0,0,0);
  GridCacheMvccCandidate<String> c=new GridCacheMvccCandidate<>(entry,id,id,ver,1,ver,0,true,true,true,true,true,true);
  c.setOwner();
  c.setReady();
  c.setUsed();
  short flags=c.flags();
  info("Candidate: " + c);
  for (  GridCacheMvccCandidate.Mask mask : GridCacheMvccCandidate.Mask.values())   assertTrue("Candidate: " + c,mask.get(flags));
}
