{
  if (log.isDebugEnabled())   log.debug("Transaction node left grid (will ignore) [fut=" + this + ']');
  if (nearCheck) {
    onDone(new ClusterTopologyException("Failed to check near transaction state (near node left grid): " + nodeId));
    return;
  }
  onDone((GridCacheCommittedTxInfo<K,V>)null);
}
