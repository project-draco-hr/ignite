{
  reader.setBuffer(buf);
switch (readState) {
case 0:
    bytes=reader.readField("bytes",MessageFieldType.BYTE_ARR);
  if (!reader.isLastRead())   return false;
readState++;
case 1:
plain=reader.readField("plain",MessageFieldType.BOOLEAN);
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
