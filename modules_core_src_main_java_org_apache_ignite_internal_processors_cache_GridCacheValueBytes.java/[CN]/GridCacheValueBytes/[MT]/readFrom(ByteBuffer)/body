{
  reader.setBuffer(buf);
  if (!reader.beforeMessageRead())   return false;
switch (readState) {
case 0:
    bytes=reader.readByteArray("bytes");
  if (!reader.isLastRead())   return false;
readState++;
case 1:
plain=reader.readBoolean("plain");
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
