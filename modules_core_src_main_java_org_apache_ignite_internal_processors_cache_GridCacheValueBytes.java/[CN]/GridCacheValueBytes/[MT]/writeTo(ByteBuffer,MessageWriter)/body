{
  writer.setBuffer(buf);
  if (!writer.isHeaderWritten()) {
    if (!writer.writeHeader(directType(),(byte)2))     return false;
    writer.onHeaderWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeByteArray("bytes",bytes))     return false;
  writer.incrementState();
case 1:
if (!writer.writeBoolean("plain",plain)) return false;
writer.incrementState();
}
return true;
}
