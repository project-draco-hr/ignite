{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeMessage("msg",msg != null ? msg.clone() : null))     return false;
  state++;
case 1:
if (!writer.writeBoolean("ordered",ordered)) return false;
state++;
case 2:
if (!writer.writeEnum("plc",plc)) return false;
state++;
case 3:
if (!writer.writeBoolean("skipOnTimeout",skipOnTimeout)) return false;
state++;
case 4:
if (!writer.writeLong("timeout",timeout)) return false;
state++;
case 5:
if (!writer.writeByteArray("topicBytes",topicBytes)) return false;
state++;
case 6:
if (!writer.writeInt("topicOrd",topicOrd)) return false;
state++;
}
return true;
}
