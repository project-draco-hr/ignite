{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(null,directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putMessage("msg",msg))     return false;
  commState.idx++;
case 1:
if (!commState.putBoolean("ordered",ordered)) return false;
commState.idx++;
case 2:
if (!commState.putEnum("plc",plc)) return false;
commState.idx++;
case 3:
if (!commState.putBoolean("skipOnTimeout",skipOnTimeout)) return false;
commState.idx++;
case 4:
if (!commState.putLong("timeout",timeout)) return false;
commState.idx++;
case 5:
if (!commState.putByteArray("topicBytes",topicBytes)) return false;
commState.idx++;
case 6:
if (!commState.putInt("topicOrd",topicOrd)) return false;
commState.idx++;
}
return true;
}
