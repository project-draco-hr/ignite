{
  String[] arr={"1","2"};
  String[] newArr=F.concat(arr,"3");
  assertEquals("1",newArr[0]);
  assertEquals("2",newArr[1]);
  assertEquals("3",newArr[2]);
  assertEquals(3,newArr.length);
  arr=null;
  newArr=F.concat(arr,"1");
  assertEquals("1",newArr[0]);
  assertEquals(1,newArr.length);
  arr=new String[0];
  newArr=F.concat(arr,"1");
  assertEquals("1",newArr[0]);
  assertEquals(1,newArr.length);
}
