{
  GridCacheAdapter<String,Integer> cache=((IgniteKernal)grid(0)).internalCache();
  for (int i=0; i < 100; i++) {
    String key=String.valueOf(i);
    cache.put(key,i);
  }
  Affinity aff=grid(0).affinity(cache.name());
  for (int i=0; i < aff.partitions(); i++)   String.valueOf(cache.entrySet(i));
}
