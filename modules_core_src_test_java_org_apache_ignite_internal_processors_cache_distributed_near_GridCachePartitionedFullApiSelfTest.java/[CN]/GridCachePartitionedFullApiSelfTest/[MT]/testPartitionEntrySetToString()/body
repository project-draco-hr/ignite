{
  GridCacheAdapter<String,Integer> cache=((GridKernal)grid(0)).internalCache();
  for (int i=0; i < 100; i++) {
    String key=String.valueOf(i);
    cache.put(key,i);
  }
  CacheConsistentHashAffinityFunction aff=(CacheConsistentHashAffinityFunction)cache.configuration().getAffinity();
  for (int i=0; i < aff.getPartitions(); i++)   String.valueOf(cache.entrySet(i));
}
