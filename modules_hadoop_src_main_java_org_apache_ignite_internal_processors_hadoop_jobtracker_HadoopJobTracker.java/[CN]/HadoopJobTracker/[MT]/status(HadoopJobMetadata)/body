{
  HadoopJobInfo jobInfo=meta.jobInfo();
  return new HadoopJobStatus(meta.jobId(),jobInfo.jobName(),jobInfo.user(),meta.pendingSplits() != null ? meta.pendingSplits().size() : 0,meta.pendingReducers() != null ? meta.pendingReducers().size() : 0,meta.mapReducePlan().mappers(),meta.mapReducePlan().reducers(),meta.phase(),meta.failCause() != null,meta.version());
}
