{
  startServerNodes(2);
  startClientNodes(2);
  long startTime=-1;
  for (  Ignite g : G.allGrids()) {
    IgniteEx kernal=(IgniteKernal)g;
    assertTrue(kernal.context().discovery().gridStartTime() > 0);
    if (startTime == -1)     startTime=kernal.context().discovery().gridStartTime();
 else     assertEquals(startTime,kernal.context().discovery().gridStartTime());
  }
}
