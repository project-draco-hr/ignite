{
  U.closeQuiet(outT1.get());
  U.error(log,"File create in DUAL mode failed [path=" + path + ", simpleCreate="+ simpleCreate+ ", props="+ props+ ", overwrite="+ overwrite+ ", bufferSize="+ bufSize+ ", replication="+ replication+ ", blockSize="+ blockSize+ ']',err);
  throw new IgniteCheckedException("Failed to create the file due to secondary file system " + "exception: " + path,err);
}
