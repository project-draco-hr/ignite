{
  int max=this.max;
  int startSize=queue.sizex();
  for (int i=0; i < startSize && queue.sizex() > max; i++) {
    EvictableEntry<K,V> entry=queue.poll();
    if (entry == null)     break;
    Node<EvictableEntry<K,V>> meta=entry.removeMeta();
    if (meta != null && !entry.evict())     touch(entry);
  }
}
