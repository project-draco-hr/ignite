{
  if (left < 0)   throw new IllegalStateException();
  lock.readLock();
  try {
    E evt=iter.removex();
    if (evt != null) {
      try {
        updateIndexes(evt,true);
      }
 catch (      IgniteException e) {
        throw new IgniteException("Failed to remove event: " + evt,e);
      }
    }
  }
  finally {
    lock.readUnlock();
  }
}
