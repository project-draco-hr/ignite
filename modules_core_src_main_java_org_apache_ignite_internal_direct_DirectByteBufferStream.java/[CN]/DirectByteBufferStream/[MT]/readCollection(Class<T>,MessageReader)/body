{
  if (readSize == -1) {
    int size=readInt();
    if (!lastFinished)     return null;
    readSize=size;
  }
  if (readSize >= 0) {
    if (col == null)     col=new ArrayList<>(readSize);
    MessageAdapter.Type itemType=null;
    for (int i=readItems; i < readSize; i++) {
      Object item=read(itemType,reader);
      if (!lastFinished)       return null;
      col.add(item);
      readItems++;
    }
  }
  readSize=-1;
  readItems=0;
  cur=null;
  Collection<T> col0=(Collection<T>)col;
  col=null;
  return (C)col0;
}
