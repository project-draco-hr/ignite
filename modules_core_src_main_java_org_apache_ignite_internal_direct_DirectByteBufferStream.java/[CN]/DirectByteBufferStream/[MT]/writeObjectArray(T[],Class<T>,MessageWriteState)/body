{
  if (arr != null) {
    if (it == null) {
      writeInt(arr.length);
      if (!lastFinished)       return;
      it=arrayIterator(arr);
    }
    MessageAdapter.Type itemType=null;
    while (it.hasNext() || cur != NULL) {
      if (cur == NULL) {
        cur=it.next();
      }
      write(itemType,cur,state);
      if (!lastFinished)       return;
      cur=NULL;
    }
    it=null;
  }
 else   writeInt(-1);
}
