{
  if (col != null) {
    if (it == null) {
      writeInt(col.size());
      if (!lastFinished)       return;
      it=col.iterator();
    }
    Type itemType=type(itemCls);
    while (it.hasNext() || cur != NULL) {
      if (cur == NULL) {
        cur=it.next();
      }
      write(itemType,cur,state);
      if (!lastFinished)       return;
      cur=NULL;
    }
    it=null;
  }
 else   writeInt(-1);
}
