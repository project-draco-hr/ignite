{
  if (readSize == -1) {
    int size=readInt();
    if (!lastFinished)     return null;
    readSize=size;
  }
  if (readSize >= 0) {
    if (col == null)     col=new ArrayList<>(readSize);
    for (int i=readItems; i < readSize; i++) {
      Object item=read(itemType);
      if (!lastFinished)       return null;
      col.add(item);
      readItems++;
    }
  }
  readSize=-1;
  readItems=0;
  cur=null;
  C col0=(C)col;
  col=null;
  return col0;
}
