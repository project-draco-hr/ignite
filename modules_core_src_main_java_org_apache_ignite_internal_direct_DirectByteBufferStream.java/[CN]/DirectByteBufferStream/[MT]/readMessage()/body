{
  if (!msgTypeDone) {
    if (!buf.hasRemaining()) {
      lastFinished=false;
      return null;
    }
    byte type=readByte();
    msg=type == Byte.MIN_VALUE ? null : msgFactory.create(type);
    if (msg != null)     reader=msgFormatter.reader(msgFactory,msg.getClass());
    msgTypeDone=true;
  }
  lastFinished=msg == null || msg.readFrom(buf,reader);
  if (lastFinished) {
    Message msg0=msg;
    msgTypeDone=false;
    msg=null;
    return (T)msg0;
  }
 else   return null;
}
