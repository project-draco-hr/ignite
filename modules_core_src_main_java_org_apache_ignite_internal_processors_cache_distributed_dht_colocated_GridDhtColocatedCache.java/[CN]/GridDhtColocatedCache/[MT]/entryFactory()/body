{
  return new GridCacheMapEntryFactory(){
    @Override public GridCacheMapEntry create(    GridCacheContext ctx,    AffinityTopologyVersion topVer,    KeyCacheObject key,    int hash,    CacheObject val){
      if (ctx.useOffheapEntry())       return new GridDhtColocatedOffHeapCacheEntry(ctx,topVer,key,hash,val);
      return new GridDhtColocatedCacheEntry(ctx,topVer,key,hash,val);
    }
  }
;
}
