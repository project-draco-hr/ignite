{
  reader.setBuffer(buf);
switch (reader.state()) {
case 0:
    srcNodeId=reader.readUuid(null);
  if (!reader.isLastRead())   return false;
reader.incrementState();
case 1:
msgId=reader.readLong(null);
if (!reader.isLastRead()) return false;
reader.incrementState();
case 2:
resId=reader.readLong(null);
if (!reader.isLastRead()) return false;
reader.incrementState();
case 3:
payload=reader.readByteArray(null);
if (!reader.isLastRead()) return false;
reader.incrementState();
}
return true;
}
