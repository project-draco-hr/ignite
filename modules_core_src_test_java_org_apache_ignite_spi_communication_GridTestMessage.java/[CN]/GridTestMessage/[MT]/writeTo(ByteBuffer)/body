{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(null,directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putUuid(null,srcNodeId))     return false;
  commState.idx++;
case 1:
if (!commState.putLong(null,msgId)) return false;
commState.idx++;
case 2:
if (!commState.putLong(null,resId)) return false;
commState.idx++;
case 3:
if (!commState.putByteArray(null,payload)) return false;
commState.idx++;
}
return true;
}
