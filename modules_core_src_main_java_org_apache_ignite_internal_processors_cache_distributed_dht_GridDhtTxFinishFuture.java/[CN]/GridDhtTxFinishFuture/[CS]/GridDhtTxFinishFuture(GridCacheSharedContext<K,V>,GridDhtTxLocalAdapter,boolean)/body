{
  super(F.<IgniteInternalTx>identityReducer(tx));
  this.cctx=cctx;
  this.tx=tx;
  this.commit=commit;
  dhtMap=tx.dhtMap();
  nearMap=tx.nearMap();
  futId=IgniteUuid.randomUuid();
  if (log == null) {
    msgLog=cctx.txFinishMessageLogger();
    log=U.logger(cctx.kernalContext(),logRef,GridDhtTxFinishFuture.class);
  }
}
