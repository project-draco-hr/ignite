{
  if (comp == null) {
    if (!(evt instanceof Comparable))     throw new IllegalArgumentException("Failed to add object to window (object is not comparable and no " + "comparator is specified: " + evt);
  }
  if (set != null) {
    if (set.add(evt)) {
      col.add(new Holder<>(evt,orderCnt.getAndIncrement()));
      return true;
    }
    return false;
  }
 else {
    col.add(new Holder<>(evt,orderCnt.getAndIncrement()));
    return true;
  }
}
