{
  GridCacheAdapter<String,String> cache=grid.internalCache();
  GridCacheTestEntryEx entry=new GridCacheTestEntryEx(cache.context(),"1");
  UUID id=UUID.randomUUID();
  GridCacheVersion ver=new GridCacheVersion(1,0,0,0,0);
  GridCacheMvccCandidate c=new GridCacheMvccCandidate(entry,id,id,ver,1,ver,0,false,false,false,false,false,false,null);
  short flags=c.flags();
  info("Candidate: " + c);
  for (  GridCacheMvccCandidate.Mask mask : GridCacheMvccCandidate.Mask.values())   assertFalse("Mask check failed [mask=" + mask + ", c="+ c+ ']',mask.get(flags));
}
