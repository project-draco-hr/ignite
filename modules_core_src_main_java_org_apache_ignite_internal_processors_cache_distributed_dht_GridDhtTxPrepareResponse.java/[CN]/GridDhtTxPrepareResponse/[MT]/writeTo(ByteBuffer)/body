{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 10:
    if (!writer.writeIgniteUuid("futId",futId))     return false;
  state++;
case 11:
if (!writer.writeCollection("invalidParts",invalidParts,int.class)) return false;
state++;
case 12:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
state++;
case 13:
if (!writer.writeCollection("nearEvictedBytes",nearEvictedBytes,byte[].class)) return false;
state++;
case 14:
if (!writer.writeCollection("preloadEntriesBytes",preloadEntriesBytes,byte[].class)) return false;
state++;
}
return true;
}
