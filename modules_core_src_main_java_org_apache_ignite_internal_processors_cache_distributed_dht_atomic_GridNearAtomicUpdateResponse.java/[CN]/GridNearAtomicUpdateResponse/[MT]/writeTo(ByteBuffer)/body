{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 3:
    if (!writer.writeByteArray("errBytes",errBytes))     return false;
  state++;
case 4:
if (!writer.writeByteArray("failedKeysBytes",failedKeysBytes)) return false;
state++;
case 5:
if (!writer.writeMessage("futVer",futVer)) return false;
state++;
case 6:
if (!writer.writeMessage("nearExpireTimes",nearExpireTimes)) return false;
state++;
case 7:
if (!writer.writeCollection("nearSkipIdxs",nearSkipIdxs,int.class)) return false;
state++;
case 8:
if (!writer.writeMessage("nearTtls",nearTtls)) return false;
state++;
case 9:
if (!writer.writeCollection("nearValBytes",nearValBytes,GridCacheValueBytes.class)) return false;
state++;
case 10:
if (!writer.writeCollection("nearValsIdxs",nearValsIdxs,int.class)) return false;
state++;
case 11:
if (!writer.writeMessage("nearVer",nearVer)) return false;
state++;
case 12:
if (!writer.writeByteArray("remapKeysBytes",remapKeysBytes)) return false;
state++;
case 13:
if (!writer.writeByteArray("retValBytes",retValBytes)) return false;
state++;
}
return true;
}
