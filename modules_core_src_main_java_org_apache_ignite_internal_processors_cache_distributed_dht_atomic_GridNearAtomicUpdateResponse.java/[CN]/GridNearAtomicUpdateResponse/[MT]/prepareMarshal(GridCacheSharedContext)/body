{
  super.prepareMarshal(ctx);
  if (err != null)   errBytes=ctx.marshaller().marshal(err);
  GridCacheContext cctx=ctx.cacheContext(cacheId);
  prepareMarshalCacheObjects(failedKeys,cctx);
  prepareMarshalCacheObjects(remapKeys,cctx);
  prepareMarshalCacheObjects(nearVals,cctx);
  if (retVal != null)   retVal.prepareMarshal(cctx.cacheObjectContext());
  if (invokeRes != null) {
    for (    CacheInvokeDirectResult res : invokeRes)     res.prepareMarshal(cctx);
  }
}
