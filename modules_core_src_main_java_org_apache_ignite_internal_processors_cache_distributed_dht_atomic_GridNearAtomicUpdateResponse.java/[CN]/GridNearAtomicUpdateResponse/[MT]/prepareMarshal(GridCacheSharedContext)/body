{
  super.prepareMarshal(ctx);
  if (err != null)   errBytes=ctx.marshaller().marshal(err);
  prepareMarshalCacheObjects(failedKeys,ctx);
  prepareMarshalCacheObjects(remapKeys,ctx);
  prepareMarshalCacheObjects(nearVals,ctx);
  if (retVal != null)   retVal.prepareMarshal(ctx);
  if (invokeRes != null) {
    for (    CacheInvokeDirectResult res : invokeRes)     res.prepareMarshal(ctx);
  }
}
