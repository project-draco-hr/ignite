{
  super.prepareMarshal(ctx);
  if (err != null)   errBytes=ctx.marshaller().marshal(err);
  if (retVal != null)   retValBytes=ctx.marshaller().marshal(retVal);
  if (failedKeys != null)   failedKeysBytes=ctx.marshaller().marshal(failedKeys);
  if (remapKeys != null)   remapKeysBytes=ctx.marshaller().marshal(remapKeys);
  nearValBytes=marshalValuesCollection(nearVals,ctx);
}
