{
  try {
    if (tx == null) {
      if (sesLsnrs != null) {
        for (        CacheStoreSessionListener lsnr : sesLsnrs)         lsnr.onSessionEnd(locSes,!threwEx);
      }
      if (!sesHolder.get().storeEnded(store))       store.sessionEnd(!threwEx);
    }
  }
 catch (  Exception e) {
    if (!threwEx)     throw U.cast(e);
  }
 finally {
    if (sesHolder != null)     sesHolder.set(null);
  }
}
