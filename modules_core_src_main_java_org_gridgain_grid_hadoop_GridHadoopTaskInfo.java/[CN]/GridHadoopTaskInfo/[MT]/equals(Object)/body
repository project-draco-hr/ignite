{
  if (this == o)   return true;
  if (!(o instanceof GridHadoopTaskInfo))   return false;
  GridHadoopTaskInfo that=(GridHadoopTaskInfo)o;
  return attempt == that.attempt && taskNum == that.taskNum && F.eq(inputSplit,that.inputSplit) && jobId.equals(that.jobId) && nodeId.equals(that.nodeId) && type == that.type;
}
