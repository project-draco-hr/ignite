{
  Map<UUID,IgniteBiTuple<Long,List<IgniteExceptionRegistry.ExceptionInfo>>> taskRes=new HashMap<>(results.size());
  for (  ComputeJobResult res : results) {
    IgniteBiTuple<Long,List<IgniteExceptionRegistry.ExceptionInfo>> jobRes=res.getData();
    taskRes.put(res.getNode().id(),jobRes);
  }
  return taskRes;
}
