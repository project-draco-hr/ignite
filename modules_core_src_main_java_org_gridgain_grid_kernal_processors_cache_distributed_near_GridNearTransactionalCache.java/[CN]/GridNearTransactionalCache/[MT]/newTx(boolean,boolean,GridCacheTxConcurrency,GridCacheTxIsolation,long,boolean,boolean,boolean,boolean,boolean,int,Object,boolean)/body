{
  GridCacheProjectionImpl<K,V> prj=ctx.projectionPerCall();
  UUID subjId=prj == null ? null : prj.subjectId();
  return new GridNearTxLocal<>(ctx,implicit,implicitSingle,concurrency,isolation,timeout,invalidate,syncCommit,syncRollback,swapEnabled,storeEnabled,txSize,grpLockKey,partLock,subjId);
}
