{
  cache1.put(1,"val1");
  cache2.put(2,"val2");
  cache3.put(3,"val3");
  assertEquals(3,cache1.localSize());
  assertEquals(3,cache2.localSize());
  assertEquals(3,cache3.localSize());
  cache1.clear();
  assertEquals(0,cache1.localSize());
  assertEquals(0,cache2.localSize());
  assertEquals(0,cache3.localSize());
}
