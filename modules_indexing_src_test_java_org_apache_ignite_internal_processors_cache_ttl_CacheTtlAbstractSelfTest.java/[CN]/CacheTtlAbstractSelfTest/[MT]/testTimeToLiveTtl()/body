{
  long time=DEFAULT_TIME_TO_LIVE + 2000;
  IgniteCache<Integer,Integer> cache=this.<Integer,Integer>jcache(0).withExpiryPolicy(new TouchedExpiryPolicy(new Duration(MILLISECONDS,time)));
  for (int i=0; i < SIZE; i++)   cache.put(i,i);
  checkSizeBeforeLive(SIZE);
  Thread.sleep(DEFAULT_TIME_TO_LIVE + 500);
  checkSizeBeforeLive(SIZE);
  Thread.sleep(time - DEFAULT_TIME_TO_LIVE + 500);
  checkSizeAfterLive();
}
