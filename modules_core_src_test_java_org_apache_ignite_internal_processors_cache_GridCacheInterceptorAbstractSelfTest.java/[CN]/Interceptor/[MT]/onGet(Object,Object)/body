{
  if (disabled)   return val;
  assertNotNull(retInterceptor);
  Object ret=retInterceptor.onGet(key,val);
  log.info("Get [key=" + key + ", val="+ val+ ", ret="+ ret+ ']');
  if (val != null) {
    Object old=getMap.put(key,val);
    assertNull(old);
  }
  invokeCnt.incrementAndGet();
  return ret;
}
