{
  interceptor.disabled=true;
  if (storeEnabled())   assertEquals("Unexpected store value",expVal,map.get(key));
  try {
    for (int i=0; i < gridCount(); i++)     assertEquals("Unexpected value for grid " + i,expVal,grid(i).jcache(null).get(key));
  }
  finally {
    interceptor.disabled=false;
  }
}
