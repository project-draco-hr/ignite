{
  readLock().lock();
  try {
    if (evts == null)     return Collections.emptyList();
    Collection<E> res=new ArrayList<>(cnt);
    for (int i=0; i < cnt; i++) {
      E evt=evts.poll();
      if (evt != null)       res.add(evt);
 else       return res;
    }
    return res;
  }
  finally {
    readLock().unlock();
  }
}
