{
  if (col != null) {
    for (Iterator<GridCacheMvccCandidate> it=col.iterator(); it.hasNext(); ) {
      GridCacheMvccCandidate cand=it.next();
      if (cand.version().equals(ver)) {
        cand.setUsed();
        cand.setRemoved();
        it.remove();
        reassign();
        if (cand.local())         cctx.mvcc().removeLocal(cand);
        return true;
      }
    }
  }
  return false;
}
