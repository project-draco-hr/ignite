{
  Ignite ignite=cache.unwrap(Ignite.class);
synchronized (cacheManagers) {
    for (    Map<URI,CacheManager> map : cacheManagers.values()) {
      for (      CacheManager manager : map.values()) {
        if (manager.isManagedIgnite(ignite))         return manager;
      }
    }
  }
  return null;
}
