{
  int len=doReadInt(raw);
  UUID[] arr=new UUID[len];
  for (int i=0; i < len; i++) {
    byte flag=doReadByte(raw);
    if (flag == NULL)     arr[i]=null;
 else {
      if (flag != UUID)       throw new PortableException("Invalid flag value: " + flag);
      arr[i]=doReadUuid(raw);
    }
  }
  return arr;
}
