{
  int hPos=in.position() - 1;
  int len=in.readInt();
  UUID[] arr=new UUID[len];
  setHandler(arr,hPos);
  for (int i=0; i < len; i++) {
    byte flag=in.readByte();
    if (flag == NULL)     arr[i]=null;
 else {
      if (flag != UUID)       throw new PortableException("Invalid flag value: " + flag);
      arr[i]=doReadUuid();
    }
  }
  return arr;
}
