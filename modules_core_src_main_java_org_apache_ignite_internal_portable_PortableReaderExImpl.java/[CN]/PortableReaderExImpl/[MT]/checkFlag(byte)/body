{
  byte flag=in.readByte();
  if (flag == NULL)   return Flag.NULL;
 else   if (flag == HANDLE)   return Flag.HANDLE;
 else   if (flag != expFlag) {
    int pos=in.position() - 1;
    throw new PortableException("Unexpected flag value [pos=" + pos + ", expected="+ expFlag+ ", actual="+ flag+ ']');
  }
  return Flag.NORMAL;
}
