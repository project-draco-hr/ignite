{
  off=fieldOffset(fieldId);
  if (off >= 0) {
    byte flag=doReadByte(false);
    if (flag == NULL)     return null;
    if (flag != ENUM)     throw new PortableException("Invalid flag value: " + flag);
    if (cls == null)     cls=doReadClass(false);
 else     doReadClass(false);
    Object[] vals=GridEnumCache.get(cls);
    return (Enum<?>)vals[doReadInt(false)];
  }
 else   return null;
}
