{
  int hPos=(raw ? rawOff : off) - 1;
  int len=doReadInt(raw);
  Date[] arr=new Date[len];
  setHandler(arr,hPos);
  for (int i=0; i < len; i++) {
    byte flag=doReadByte(raw);
    if (flag == NULL)     arr[i]=null;
 else {
      if (flag != DATE)       throw new PortableException("Invalid flag value: " + flag);
      arr[i]=doReadDate(raw);
    }
  }
  return arr;
}
