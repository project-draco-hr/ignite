{
  if (!(o instanceof GridCacheEntry))   return false;
  GridCacheEntry<K,V> entry=(GridCacheEntry<K,V>)o;
  K key=entry.getKey();
  V val=entry.peek();
  if (val == null)   return false;
  try {
    return entry(key).remove(val);
  }
 catch (  IgniteCheckedException e) {
    throw new IgniteException(e);
  }
}
