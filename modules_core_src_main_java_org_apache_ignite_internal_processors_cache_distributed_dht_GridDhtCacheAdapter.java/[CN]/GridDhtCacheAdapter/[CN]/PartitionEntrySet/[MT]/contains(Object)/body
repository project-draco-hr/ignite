{
  if (!(o instanceof Cache.Entry))   return false;
  Cache.Entry<K,V> entry=(Cache.Entry<K,V>)o;
  try {
    return partId == ctx.affinity().partition(entry.getKey()) && F.eq(entry.getValue(),localPeek(entry.getKey(),new CachePeekMode[]{CachePeekMode.ONHEAP},null));
  }
 catch (  IgniteCheckedException e) {
    throw new IgniteException(e);
  }
}
