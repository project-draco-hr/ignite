{
  System.out.println();
  System.out.println(">>> Cache put-get example started.");
  Ignite g=Ignition.ignite();
  final IgniteCache<Integer,String> cache=g.jcache(CACHE_NAME);
  final int keyCnt=20;
  for (int i=0; i < keyCnt; i++)   cache.put(i,Integer.toString(i));
  System.out.println(">>> Stored values in cache.");
  for (int i=0; i < keyCnt; i++)   System.out.println("Got [key=" + i + ", val="+ cache.get(i)+ ']');
}
