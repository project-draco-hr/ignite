{
  IgniteCache<Object,Object> cache=grid(0).cache(null);
  Map map=new HashMap();
  try (Transaction tx=grid(0).transactions().txStart(TransactionConcurrency.PESSIMISTIC,TransactionIsolation.REPEATABLE_READ,100000,1000)){
    for (int i=4; i < 400; i++) {
      map.put("key" + i,new TestEntity("value"));
      map.put(i,"value");
    }
    cache.putAll(map);
    tx.commit();
  }
   for (int i=0; i < 100; i++) {
    cache.get("key" + i);
    cache.get(i);
  }
}
