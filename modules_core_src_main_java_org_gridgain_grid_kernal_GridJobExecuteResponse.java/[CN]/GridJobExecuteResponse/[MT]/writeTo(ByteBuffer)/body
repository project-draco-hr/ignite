{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putByteArray(gridExBytes))     return false;
  commState.idx++;
case 1:
if (!commState.putBoolean(isCancelled)) return false;
commState.idx++;
case 2:
if (!commState.putByteArray(jobAttrsBytes)) return false;
commState.idx++;
case 3:
if (!commState.putGridUuid(jobId)) return false;
commState.idx++;
case 4:
if (!commState.putUuid(nodeId)) return false;
commState.idx++;
case 5:
if (!commState.putByteArray(resBytes)) return false;
commState.idx++;
case 6:
if (!commState.putGridUuid(sesId)) return false;
commState.idx++;
}
return true;
}
