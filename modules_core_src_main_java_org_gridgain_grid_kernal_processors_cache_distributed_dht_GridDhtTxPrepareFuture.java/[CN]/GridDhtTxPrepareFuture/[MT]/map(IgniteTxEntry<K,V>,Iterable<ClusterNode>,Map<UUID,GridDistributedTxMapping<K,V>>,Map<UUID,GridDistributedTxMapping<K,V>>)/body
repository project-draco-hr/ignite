{
  boolean ret=false;
  if (nodes != null) {
    for (    ClusterNode n : nodes) {
      GridDistributedTxMapping<K,V> global=globalMap.get(n.id());
      if (global == null)       globalMap.put(n.id(),global=new GridDistributedTxMapping<>(n));
      global.add(entry);
      GridDistributedTxMapping<K,V> loc=locMap.get(n.id());
      if (loc == null)       locMap.put(n.id(),loc=new GridDistributedTxMapping<>(n));
      loc.add(entry);
      ret=true;
    }
  }
  return ret;
}
