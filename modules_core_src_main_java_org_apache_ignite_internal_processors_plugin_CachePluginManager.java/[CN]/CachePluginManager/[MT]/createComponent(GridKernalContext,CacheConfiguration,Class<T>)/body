{
  for (  CachePluginProvider provider : providers) {
    T res=(T)provider.createComponent(cls);
    if (res != null)     return res;
  }
  if (cls.equals(GridCacheDrManager.class))   return (T)new GridOsCacheDrManager();
 else   if (cls.equals(CacheConflictResolutionManager.class))   return (T)new CacheOsConflictResolutionManager<>();
 else   if (cls.equals(CacheStoreManager.class))   return (T)new CacheOsStoreManager(ctx,cfg);
  throw new IgniteException("Unsupported component type: " + cls);
}
