{
  if (AgentMetadataDemo.isTestDriveUrl(jdbcUrl))   return AgentMetadataDemo.testDrive();
  if (!new File(jdbcDriverJarPath).isAbsolute() && driversFolder != null)   jdbcDriverJarPath=new File(driversFolder,jdbcDriverJarPath).getPath();
  return DbMetadataReader.getInstance().connect(jdbcDriverJarPath,jdbcDriverCls,jdbcUrl,jdbcInfo);
}
