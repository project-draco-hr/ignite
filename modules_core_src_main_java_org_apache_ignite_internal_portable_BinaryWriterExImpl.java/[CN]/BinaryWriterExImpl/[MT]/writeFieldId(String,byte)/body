{
  A.notNull(fieldName,"fieldName");
  if (rawOffPos != 0)   throw new BinaryObjectException("Individual field can't be written after raw writer is acquired " + "via rawWriter() method. Consider fixing serialization logic for class: " + cls.getName());
  if (idMapper == null)   idMapper=ctx.userTypeIdMapper(typeId);
  int id=idMapper.fieldId(typeId,fieldName);
  writeFieldId(id);
  if (metaEnabled)   metaHashSum=31 * metaHashSum + (id + fieldType);
}
