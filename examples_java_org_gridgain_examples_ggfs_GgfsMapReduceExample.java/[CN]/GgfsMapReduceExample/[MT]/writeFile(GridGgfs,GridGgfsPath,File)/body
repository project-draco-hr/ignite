{
  System.out.println();
  System.out.println("Copying file to GGFS: " + file);
  GridGgfsOutputStream os=null;
  FileInputStream fis=null;
  try {
    os=fs.create(fsPath,true);
    fis=new FileInputStream(file);
    byte[] buf=new byte[2048];
    int read=fis.read(buf);
    while (read != -1) {
      os.write(buf,0,read);
      read=fis.read(buf);
    }
  }
  finally {
    U.closeQuiet(os);
    U.closeQuiet(fis);
  }
}
