{
  try {
    sendAllPartitions(rmtNodes,exchId);
    return true;
  }
 catch (  IgniteCheckedException e) {
    scheduleRecheck();
    if (!X.hasCause(e,InterruptedException.class))     U.error(log,"Failed to send full partition map to nodes (will retry after timeout) [nodes=" + F.nodeId8s(rmtNodes) + ", exchangeId="+ exchId+ ']',e);
    return false;
  }
}
