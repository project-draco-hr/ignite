{
  try {
    sendAllPartitions(rmtNodes,exchId,partMap);
    return true;
  }
 catch (  GridException e) {
    scheduleRecheck();
    U.error(log,"Failed to send full partition map to nodes (will retry after timeout) [nodes=" + F.nodeId8s(rmtNodes) + ", exchangeId="+ exchId+ ']',e);
    return false;
  }
}
