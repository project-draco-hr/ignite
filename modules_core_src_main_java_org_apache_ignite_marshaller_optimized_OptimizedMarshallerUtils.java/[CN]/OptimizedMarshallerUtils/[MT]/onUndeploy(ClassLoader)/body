{
  for (  Class<?> cls : DESC_BY_CLS.keySet()) {
    if (ldr.equals(cls.getClassLoader()))     DESC_BY_CLS.remove(cls);
  }
  for (  Map.Entry<Integer,IgniteBiTuple<Class,Boolean>> e : CLS_BY_ID.entrySet()) {
    if (!e.getValue().get2() && ldr.equals(e.getValue().get1().getClassLoader()))     CLS_BY_ID.remove(e.getKey());
  }
}
