{
  try (Connection conn=DriverManager.getConnection(BASE_URL);ResultSet tbls=conn.getMetaData().getTables(null,null,"%",null)){
    int colCnt=tbls.getMetaData().getColumnCount();
    while (tbls.next()) {
      for (int i=0; i < colCnt; i++)       tbls.getObject(i + 1);
    }
  }
 catch (  Exception e) {
    fail();
  }
}
