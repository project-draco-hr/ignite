{
  if (e.explicitVersion() != null) {
    if (explicitVers == null)     explicitVers=new LinkedList<>();
    if (!explicitVers.contains(e.explicitVersion())) {
      explicitVers.add(e.explicitVersion());
      if (log.isDebugEnabled())       log.debug("Added explicit version to transaction [explicitVer=" + e.explicitVersion() + ", tx="+ this+ ']');
      cctx.tm().addAlternateVersion(e.explicitVersion(),this);
    }
  }
}
