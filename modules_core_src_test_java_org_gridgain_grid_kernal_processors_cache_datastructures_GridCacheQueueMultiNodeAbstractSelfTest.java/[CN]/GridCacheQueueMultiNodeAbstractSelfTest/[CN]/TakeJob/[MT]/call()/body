{
  assertNotNull(grid);
  grid.log().info("Running job [node=" + grid.localNode().id() + ", job="+ this+ ']');
  GridCacheQueue<Integer> queue=grid.cache(null).dataStructures().queue(queueName,QUEUE_CAPACITY,false,true);
  assertNotNull(queue);
  try {
    lthTake.countDown();
    queue.take();
  }
 catch (  GridRuntimeException e) {
    grid.log().info("Caught expected exception: " + e.getMessage());
  }
  return queue.removed();
}
