{
  assertNotNull(ignite);
  ignite.log().info("Running job [node=" + ignite.cluster().localNode().id() + ", job="+ this+ ']');
  GridCacheQueue<Integer> queue=ignite.cache(null).dataStructures().queue(queueName,QUEUE_CAPACITY,false,true);
  assertNotNull(queue);
  for (int i=0; i < retries; i++) {
    queue.put(i);
    assertNotNull(queue.take());
  }
  return queue.size();
}
