{
  assertNotNull(grid);
  grid.log().info("Running job [node=" + grid.cluster().localNode().id() + ", job="+ this+ "]");
  GridCacheQueue<String> queue=grid.cache(null).dataStructures().queue(queueName,QUEUE_CAPACITY,false,true);
  assertNotNull(queue);
  assertEquals(1,queue.size());
  for (int i=0; i < retries; i++) {
    assertEquals(expVal,queue.peek());
    assertEquals(expVal,queue.element());
  }
  return queue.size();
}
