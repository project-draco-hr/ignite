{
  assertNotNull(grid);
  grid.log().info("Running job [node=" + grid.cluster().localNode().id() + ", job="+ this+ "]");
  GridCacheQueue<Integer> queue=grid.cache(null).dataStructures().queue(queueName,QUEUE_CAPACITY,false,true);
  assertNotNull(queue);
  Collection<Integer> items=new ArrayList<>();
  for (int i=0; i < size; i++)   items.add(cntr.getAndIncrement());
  queue.addAll(items);
  return queue.size();
}
