{
  if (commit.compareAndSet(false,true)) {
    try {
      tx.commit0();
      onComplete();
    }
 catch (    IgniteTxTimeoutCheckedException e) {
      onError(e);
    }
catch (    IgniteCheckedException e) {
      if (tx.state() == UNKNOWN) {
        onError(new IgniteTxHeuristicCheckedException("Commit only partially succeeded " + "(entries will be invalidated on remote nodes once transaction timeout passes): " + tx,e));
      }
 else {
        onError(new IgniteTxRollbackCheckedException("Failed to commit transaction (will attempt rollback): " + tx,e));
      }
    }
  }
}
