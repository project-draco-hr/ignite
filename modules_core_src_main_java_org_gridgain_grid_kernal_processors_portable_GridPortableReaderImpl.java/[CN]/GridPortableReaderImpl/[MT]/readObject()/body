{
  byte flag=readByte();
switch (flag) {
case NULL:
    return null;
case HANDLE:
  return null;
case OBJ:
int typeId=readInt();
GridPortableClassDescriptor desc=ctx.descriptorForTypeId(typeId);
if (desc == null) throw new GridPortableInvalidClassException("Unknown type ID: " + typeId);
rawOff+=8;
int rawOff0=readInt();
return desc.read(new GridPortableReaderImpl(ctx,arr,rawOff,rawOff0));
default :
throw new GridPortableException("Invalid flag value: " + flag);
}
}
