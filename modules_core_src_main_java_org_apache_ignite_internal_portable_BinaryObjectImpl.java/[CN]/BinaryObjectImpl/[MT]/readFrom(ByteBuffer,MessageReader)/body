{
  reader.setBuffer(buf);
  if (!reader.beforeMessageRead())   return false;
switch (reader.state()) {
case 0:
    arr=reader.readByteArray("arr");
  if (!reader.isLastRead())   return false;
reader.incrementState();
case 1:
start=reader.readInt("start");
if (!reader.isLastRead()) return false;
reader.incrementState();
}
return true;
}
