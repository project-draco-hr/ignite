{
  if (col != null) {
    int handle=handles.lookup(col);
    if (handle >= 0) {
      out.writeByte(OBJECT_TYPE_REF);
      out.writeInt(handle);
    }
 else {
      out.writeByte(OBJECT_TYPE_OBJECT);
      out.writeInt(col.size());
      for (      Object obj : col)       writeObject(obj);
    }
  }
 else   out.writeByte(OBJECT_TYPE_NULL);
}
