{
  Integer key=100000;
  Ignite primary;
  Ignite backup;
  if (grid(0) == primaryGrid(key)) {
    primary=grid(0);
    backup=grid(1);
  }
 else {
    primary=grid(1);
    backup=grid(0);
  }
  assertEquals(String.valueOf(key),backup.jcache(null).get(key));
  primary.jcache(null).put(key,"a");
  assertEquals("a",backup.jcache(null).get(key));
}
