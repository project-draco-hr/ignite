{
  int keyCnt=4;
  for (int i=1; i <= keyCnt; i++)   cache1.put(new CacheKey(i),new CacheValue(String.valueOf(i)));
  QueryCursor<Cache.Entry<CacheKey,CacheValue>> qry=cache1.query(new QuerySqlPredicate<CacheKey,CacheValue>("val > 0"));
  assertEquals(keyCnt * 3,qry.getAll().size());
  info("Query result: " + qry.getAll());
}
