{
  A.notNull(c,"job");
  if (c instanceof GridComputeJob) {
    return (GridComputeJob)c;
  }
  return new GridComputeJobAdapter(){
{
      setPeerDeployAware(U.peerDeployAware(c));
    }
    @Override public Object execute(){
      try {
        return c.call();
      }
 catch (      Exception e) {
        throw new GridRuntimeException(e);
      }
    }
  }
;
}
