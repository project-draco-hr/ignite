{
  return new R1<T,Double>(){
    private double sum;
    private int i;
    private final Object lock=new Object();
    @Override public boolean collect(    T e){
      if (e != null) {
        double d=e.doubleValue();
synchronized (lock) {
          sum+=d * d;
          i++;
        }
      }
      return true;
    }
    @Override public Double apply(){
synchronized (lock) {
        return Math.sqrt(sum / i);
      }
    }
  }
;
}
