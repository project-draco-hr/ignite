{
  UUID subjId=null;
  int taskNameHash=cctx.kernalContext().job().currentTaskNameHash();
  GridNearTxLocal<K,V> tx=new GridNearTxLocal<>(cctx,implicit,implicitSingle,sys,concurrency,isolation,timeout,invalidate,storeEnabled,txSize,grpLockKey,partLock,subjId,taskNameHash);
  return onCreated(tx);
}
