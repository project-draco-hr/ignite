{
  Boolean committed=completedVers.putIfAbsent(xidVer,false);
  if (committed == null || !committed) {
    if (log.isDebugEnabled())     log.debug("Added transaction to rolled back version set: " + xidVer);
    return true;
  }
 else {
    if (log.isDebugEnabled())     log.debug("Transaction is already present in committed version set: " + xidVer);
    return false;
  }
}
