{
  if (!(msg instanceof GridTaskMessage)) {
    U.warn(log,"Received message of unknown type: " + msg);
    return;
  }
  if (msg instanceof GridJobExecuteResponse)   processJobExecuteResponse(nodeId,(GridJobExecuteResponse)msg);
 else   if (jobResOnly)   U.warn(log,"Received message of type other than job response: " + msg);
 else   if (msg instanceof GridTaskSessionRequest)   processTaskSessionRequest(nodeId,(GridTaskSessionRequest)msg);
 else   U.warn(log,"Received message of unknown type: " + msg);
}
