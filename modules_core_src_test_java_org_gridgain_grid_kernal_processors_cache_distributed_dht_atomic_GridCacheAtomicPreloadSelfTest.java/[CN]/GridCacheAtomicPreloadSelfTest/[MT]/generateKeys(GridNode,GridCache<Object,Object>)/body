{
  List<Integer> keys=new ArrayList<>(3);
  GridCacheAffinity<Object> aff=cache.affinity();
  int base=0;
  while (aff.isPrimary(node,base) || aff.isBackup(node,base))   base++;
  keys.add(base);
  if (true)   return keys;
  while (!aff.isPrimary(node,base))   base++;
  keys.add(base);
  while (!aff.isBackup(node,base))   base++;
  keys.add(base);
  return keys;
}
