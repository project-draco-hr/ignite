{
  A.notNull(c,"c");
  List<T> res;
  if (!cp) {
    res=c;
    if (p != null)     for (Iterator<T> it=c.iterator(); it.hasNext(); )     if (isAny(it.next(),p))     it.remove();
  }
 else {
    res=new ArrayList<>(c.size());
    for (    T t : c)     if (!isAny(t,p))     res.add(t);
  }
  return res;
}
