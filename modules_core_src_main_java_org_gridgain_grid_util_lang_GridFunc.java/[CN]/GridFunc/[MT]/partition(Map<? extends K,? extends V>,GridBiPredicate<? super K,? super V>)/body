{
  A.notNull(m,"m",p,"p");
  Map<K,V> m1=new HashMap<>();
  Map<K,V> m2=new HashMap<>();
  for (  Map.Entry<? extends K,? extends V> e : m.entrySet()) {
    if (p.apply(e.getKey(),e.getValue()))     m1.put(e.getKey(),e.getValue());
 else     m2.put(e.getKey(),e.getValue());
  }
  return t(m1,m2);
}
