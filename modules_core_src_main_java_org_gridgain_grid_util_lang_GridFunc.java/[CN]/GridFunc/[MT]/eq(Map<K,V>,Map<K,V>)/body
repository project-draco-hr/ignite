{
  if (m1 == m2)   return true;
  if (m1 == null || m2 == null)   return false;
  if (m1.size() != m2.size())   return false;
  for (  Entry<K,V> e : m1.entrySet()) {
    V v1=e.getValue();
    V v2=m2.get(e.getKey());
    if (v2 == null)     return false;
    if (!eq(v1,v2))     return false;
  }
  return true;
}
