{
  BufferedReader reader=null;
  try {
    reader=new BufferedReader(new InputStreamReader(url.openStream()));
    List<String> lines=new LinkedList<>();
    String line;
    int i=0;
    while ((line=reader.readLine()) != null) {
      i++;
      if (from != -1 && i - 1 < from)       continue;
      if (to != -1 && i - 1 > to)       break;
      lines.add(line);
    }
    return lines;
  }
  finally {
    U.close(reader,log);
  }
}
