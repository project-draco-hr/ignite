{
  multithreaded(new Callable<Object>(){
    private final Random rnd=new Random();
    @Nullable @Override public Object call() throws Exception {
      for (int i=0; i < TX_CNT; i++) {
        int cnt=rnd.nextInt(BATCH_CNT);
        List<Integer> ids=new ArrayList<>(cnt);
        for (int j=0; j < cnt; j++)         ids.add(rnd.nextInt());
        Collections.sort(ids);
        Map<Object,Object> map=U.newLinkedHashMap(cnt);
        for (        Integer id : ids) {
          if (rnd.nextBoolean())           map.put(new OrganizationKey(id),new Organization(id,"Name" + id,"City" + id));
 else           map.put(new PersonKey(id),new Person(id,rnd.nextInt(),"Name" + id));
        }
        IgniteCache<Object,Object> cache=jcache();
        cache.putAll(map);
      }
      return null;
    }
  }
,8,"putAll");
}
