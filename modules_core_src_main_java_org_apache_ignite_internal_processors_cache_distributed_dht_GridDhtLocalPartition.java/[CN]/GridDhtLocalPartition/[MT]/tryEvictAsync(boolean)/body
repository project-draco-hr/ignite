{
  long reservations=state.get();
  int ord=(int)(reservations >> 32);
  if (isEmpty() && !GridQueryProcessor.isEnabled(cctx.config()) && ord == RENTING.ordinal() && (reservations & 0xFFFF) == 0 && casState(reservations,EVICTED)) {
    if (log.isDebugEnabled())     log.debug("Evicted partition: " + this);
    clearSwap();
    if (cctx.isDrEnabled())     cctx.dr().partitionEvicted(id);
    cctx.dataStructures().onPartitionEvicted(id);
    rent.onDone();
    ((GridDhtPreloader)cctx.preloader()).onPartitionEvicted(this,updateSeq);
    clearDeferredDeletes();
  }
 else   cctx.preloader().evictPartitionAsync(this);
}
