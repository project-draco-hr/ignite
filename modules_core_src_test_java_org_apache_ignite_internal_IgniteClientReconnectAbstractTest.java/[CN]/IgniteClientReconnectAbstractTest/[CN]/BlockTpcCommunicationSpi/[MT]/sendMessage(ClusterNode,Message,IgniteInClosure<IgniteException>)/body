{
  Class msgCls0=msgCls;
  if (collectStart.get() && msg instanceof GridIoMessage)   classes.put(((GridIoMessage)msg).message().getClass().getName(),node);
  if (msgCls0 != null && msg instanceof GridIoMessage && ((GridIoMessage)msg).message().getClass().equals(msgCls)) {
    log.info("Block message: " + msg);
    return;
  }
  super.sendMessage(node,msg,ackC);
}
