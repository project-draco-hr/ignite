{
  List<List<GridNode>> res=new ArrayList<>(parts);
  Collection<GridNode> topSnapshot=ctx.currentTopologySnapshot();
  for (int part=0; part < parts; part++) {
    res.add(F.isEmpty(topSnapshot) ? Collections.<GridNode>emptyList() : U.sealList(nodes(part,topSnapshot,ctx.backups())));
  }
  return res;
}
