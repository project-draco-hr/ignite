{
  gate.enter();
  try {
    if (cctx.transactional())     return CU.outTx(new Callable<T>(){
      @Override public T call() throws Exception {
        return delegate.poll(timeout,unit);
      }
    }
,cctx);
    return delegate.poll(timeout,unit);
  }
 catch (  IgniteCheckedException e) {
    throw U.convertException(e);
  }
 finally {
    gate.leave();
  }
}
