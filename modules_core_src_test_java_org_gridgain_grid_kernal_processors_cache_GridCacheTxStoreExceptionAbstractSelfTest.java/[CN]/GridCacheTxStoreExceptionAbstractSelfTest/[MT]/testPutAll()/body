{
  checkPutAll(true,keyForNode(grid(0).localNode(),PRIMARY),keyForNode(grid(0).localNode(),PRIMARY),keyForNode(grid(0).localNode(),PRIMARY));
  checkPutAll(false,keyForNode(grid(0).localNode(),PRIMARY),keyForNode(grid(0).localNode(),PRIMARY),keyForNode(grid(0).localNode(),PRIMARY));
  if (gridCount() > 1) {
    checkPutAll(true,keyForNode(grid(1).localNode(),PRIMARY),keyForNode(grid(1).localNode(),PRIMARY),keyForNode(grid(1).localNode(),PRIMARY));
    checkPutAll(false,keyForNode(grid(1).localNode(),PRIMARY),keyForNode(grid(1).localNode(),PRIMARY),keyForNode(grid(1).localNode(),PRIMARY));
  }
}
