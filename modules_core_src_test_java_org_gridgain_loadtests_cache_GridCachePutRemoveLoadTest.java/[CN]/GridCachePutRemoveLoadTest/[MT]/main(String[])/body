{
  Arguments args=new Arguments();
  JCommander jCommander=new JCommander();
  jCommander.setAcceptUnknownOptions(true);
  jCommander.addObject(args);
  jCommander.parse(a);
  if (args.help()) {
    jCommander.usage();
