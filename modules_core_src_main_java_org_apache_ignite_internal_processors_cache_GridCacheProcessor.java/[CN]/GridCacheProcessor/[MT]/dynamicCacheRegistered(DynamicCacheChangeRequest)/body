{
  DynamicCacheDescriptor desc=registeredCaches.get(maskNull(req.cacheName()));
  if (desc != null) {
    if (desc.deploymentId().equals(req.deploymentId())) {
      if (req.start())       return !desc.cancelled();
 else       return desc.cancelled();
    }
    if (req.initiatingNodeId() != null)     return true;
  }
  return false;
}
