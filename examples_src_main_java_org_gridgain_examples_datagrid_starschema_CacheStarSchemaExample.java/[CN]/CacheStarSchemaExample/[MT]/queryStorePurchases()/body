{
  GridCache<Integer,FactPurchase> factCache=Ignition.grid().cache(PARTITIONED_CACHE_NAME);
  GridCacheQuery<Map.Entry<Integer,FactPurchase>> storePurchases=factCache.queries().createSqlQuery(FactPurchase.class,"from \"replicated\".DimStore, \"partitioned\".FactPurchase " + "where DimStore.id=FactPurchase.storeId and DimStore.name=?");
  printQueryResults("All purchases made at store1:",storePurchases.execute("Store1").get());
}
