{
  atLeastOnce=c.isAtLeastOnce();
  execSvc=c.getExecutorService();
  execSvcShutdown=c.isExecutorServiceShutdown();
  maxConcurrentSessions=c.getMaximumConcurrentSessions();
  maxFailoverAttempts=c.getMaximumFailoverAttempts();
  name=c.getName();
  router=c.getRouter();
  stages=c.getStages();
  win=c.getWindows();
}
