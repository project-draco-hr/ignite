{
  Integer res=taskNumMap.get(src);
  U.debug("TASK NUM FOR SPLIT [src=" + src + ", res="+ res+ ']');
  for (  Map.Entry<Object,Integer> entry : taskNumMap.entrySet())   U.debug("    " + entry);
  System.out.println("");
  if (res == null)   throw new IllegalArgumentException("Failed to find task number for source [src=" + src + ", map="+ taskNumMap+ ']');
  return res;
}
