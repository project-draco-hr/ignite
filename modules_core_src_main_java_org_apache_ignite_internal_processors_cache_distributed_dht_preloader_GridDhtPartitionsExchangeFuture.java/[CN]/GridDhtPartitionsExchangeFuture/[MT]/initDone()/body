{
  while (!isDone()) {
    List<IgniteRunnable> evts;
synchronized (discoEvts) {
      if (discoEvts.isEmpty()) {
        init=true;
        break;
      }
      evts=new ArrayList<>(discoEvts);
      discoEvts.clear();
    }
    for (    IgniteRunnable c : evts)     c.run();
  }
  initFut.onDone(true);
}
