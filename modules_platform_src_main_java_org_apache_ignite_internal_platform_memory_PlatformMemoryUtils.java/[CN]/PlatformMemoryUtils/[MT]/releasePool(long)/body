{
  long mem=UNSAFE.getLong(poolPtr + POOL_HDR_OFF_MEM_1);
  if (mem != 0)   UNSAFE.freeMemory(mem);
  mem=UNSAFE.getLong(poolPtr + POOL_HDR_OFF_MEM_2);
  if (mem != 0)   UNSAFE.freeMemory(mem);
  mem=UNSAFE.getLong(poolPtr + POOL_HDR_OFF_MEM_3);
  if (mem != 0)   UNSAFE.freeMemory(mem);
  UNSAFE.freeMemory(poolPtr);
}
