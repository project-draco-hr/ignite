{
  if (hasPendingAcks(nodeId)) {
    ackMap.clear();
    signalNoAcks();
    if (e.hasCause(IgfsOutOfSpaceException.class))     onDone(new IgniteCheckedException("Failed to write data (not enough space on node): " + nodeId,e));
 else     onDone(new IgniteCheckedException("Failed to wait for write completion (write failed on node): " + nodeId,e));
  }
}
