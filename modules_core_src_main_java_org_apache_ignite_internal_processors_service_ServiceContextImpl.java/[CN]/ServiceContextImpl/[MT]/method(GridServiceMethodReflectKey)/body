{
  Method mtd=mtds.get(key);
  if (mtd == null) {
    try {
      mtd=svc.getClass().getMethod(key.methodName(),key.argTypes());
      mtd.setAccessible(true);
    }
 catch (    NoSuchMethodException e) {
      mtd=NULL_METHOD;
    }
    mtds.put(key,mtd);
  }
  return mtd == NULL_METHOD ? null : mtd;
}
