{
  GridGgfsEx ggfs=null;
  if (endpoint.grid() == null) {
    try {
      Grid grid=G.grid();
      ggfs=(GridGgfsEx)grid.ggfs(endpoint.ggfs());
    }
 catch (    Exception ignore) {
    }
  }
 else {
    for (    Grid grid : G.allGrids()) {
      try {
        ggfs=(GridGgfsEx)grid.ggfs(endpoint.ggfs());
        break;
      }
 catch (      Exception ignore) {
      }
    }
  }
  return ggfs;
}
