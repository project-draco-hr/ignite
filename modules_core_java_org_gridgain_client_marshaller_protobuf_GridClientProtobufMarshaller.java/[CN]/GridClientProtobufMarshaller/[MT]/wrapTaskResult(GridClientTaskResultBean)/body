{
  ProtoTaskBean.Builder builder=ProtoTaskBean.newBuilder();
  builder.setTaskId(bean.getId());
  builder.setFinished(bean.isFinished());
  if (bean.getError() != null)   builder.setError(bean.getError());
  if (bean.getResult() != null)   builder.setResultBean(wrapObject(bean.getResult()));
  return builder.build();
}
