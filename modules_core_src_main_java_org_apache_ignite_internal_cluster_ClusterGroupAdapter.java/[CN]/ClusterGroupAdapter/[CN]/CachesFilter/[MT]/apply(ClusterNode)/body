{
  GridCacheAttributes[] caches=n.attribute(ATTR_CACHE);
  if (caches != null) {
    for (    GridCacheAttributes attrs : caches) {
      if (Objects.equals(cacheName,attrs.cacheName()) && (distributionMode == null || distributionMode.contains(attrs.partitionedTaxonomy())))       return true;
    }
  }
  return false;
}
