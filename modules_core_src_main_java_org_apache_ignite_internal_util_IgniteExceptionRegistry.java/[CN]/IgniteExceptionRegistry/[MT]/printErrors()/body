{
  int size=queue.size();
  int cnt=0;
  Iterator<ExceptionInfo> descIter=queue.descendingIterator();
  while (descIter.hasNext() && cnt < size) {
    ExceptionInfo error=descIter.next();
    log.error("Time of occurrence: " + new Date(error.time()) + "\n"+ "Error message: "+ error.message()+ "\n"+ "Thread id: "+ error.threadId()+ "\n"+ "Thread name: "+ error.threadName(),error.error());
    ++cnt;
  }
}
