{
  IgniteCache<Object,Object> cache=jcache();
  cache.put(1,"val1");
  assertEquals("val1",dht().peek(1));
  assertNull(near().peekNearOnly(1));
  IgniteTx tx=grid().transactions().txStart(PESSIMISTIC,REPEATABLE_READ);
  assertEquals("val1",cache.get(1));
  tx.commit();
  assertEquals("val1",dht().peek(1));
  assertNull(near().peekNearOnly(1));
}
