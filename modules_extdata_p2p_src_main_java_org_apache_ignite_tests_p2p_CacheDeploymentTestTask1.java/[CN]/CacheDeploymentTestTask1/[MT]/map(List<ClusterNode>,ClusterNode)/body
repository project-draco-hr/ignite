{
  return F.asMap(new ComputeJobAdapter(){
    @IgniteInstanceResource private Ignite ignite;
    @Override public Object execute(){
      X.println("Executing CacheDeploymentTestTask1 job on node " + ignite.cluster().localNode().id());
      IgniteCache<String,CacheDeploymentTestValue> cache=ignite.jcache(null);
      for (int i=0; i < PUT_CNT; i++)       cache.put("1" + i,new CacheDeploymentTestValue());
      return null;
    }
  }
,node);
}
