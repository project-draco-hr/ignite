{
  if (log.isDebugEnabled())   log.debug("Processing dht atomic update response [nodeId=" + nodeId + ", res="+ res+ ']');
  GridDhtAtomicUpdateFuture updateFut=(GridDhtAtomicUpdateFuture)ctx.mvcc().atomicFuture(res.futureVersion());
  if (updateFut != null)   updateFut.onResult(nodeId,res);
 else   U.warn(log,"Failed to find DHT update future for update response [nodeId=" + nodeId + ", res="+ res+ ']');
}
