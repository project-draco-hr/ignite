{
  if (log.isDebugEnabled())   log.debug("Processing deferred dht atomic update response [nodeId=" + nodeId + ", res="+ res+ ']');
  for (  GridCacheVersion ver : res.futureVersions()) {
    GridDhtAtomicUpdateFuture updateFut=(GridDhtAtomicUpdateFuture)ctx.mvcc().atomicFuture(ver);
    if (updateFut != null)     updateFut.onResult(nodeId);
 else     U.warn(log,"Failed to find DHT update future for deferred update response [nodeId=" + nodeId + ", res="+ res+ ']');
  }
}
