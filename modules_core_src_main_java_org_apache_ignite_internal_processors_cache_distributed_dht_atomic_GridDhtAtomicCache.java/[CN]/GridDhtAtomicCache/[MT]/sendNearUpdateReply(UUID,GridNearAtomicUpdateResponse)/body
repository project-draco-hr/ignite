{
  try {
    ctx.io().send(nodeId,res,ctx.ioPolicy());
    if (msgLog.isDebugEnabled())     msgLog.debug("Sent near update response [futId=" + res.futureVersion() + ", node="+ nodeId+ ']');
  }
 catch (  ClusterTopologyCheckedException ignored) {
    if (msgLog.isDebugEnabled()) {
      msgLog.debug("Failed to send near update response [futId=" + res.futureVersion() + ", node="+ nodeId+ ']');
    }
  }
catch (  IgniteCheckedException e) {
    U.error(msgLog,"Failed to send near update response [futId=" + res.futureVersion() + ", node="+ nodeId+ ", res="+ res+ ']',e);
  }
}
