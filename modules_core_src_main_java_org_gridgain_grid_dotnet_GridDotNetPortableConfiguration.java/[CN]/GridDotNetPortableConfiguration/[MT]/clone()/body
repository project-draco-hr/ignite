{
  try {
    GridDotNetPortableConfiguration res=(GridDotNetPortableConfiguration)super.clone();
    if (typesCfg != null) {
      res.typesCfg=new ArrayList<>(typesCfg.size());
      for (      GridDotNetPortableTypeConfiguration typeCfg : typesCfg)       res.typesCfg.add(typeCfg.clone());
    }
    if (types != null)     res.types=new ArrayList<>(types);
    return res;
  }
 catch (  CloneNotSupportedException e) {
    throw new RuntimeException(e);
  }
}
