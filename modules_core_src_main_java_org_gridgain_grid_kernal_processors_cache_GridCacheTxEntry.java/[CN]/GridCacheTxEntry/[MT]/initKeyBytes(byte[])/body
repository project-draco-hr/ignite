{
  if (bytes != null) {
    keyBytes=bytes;
    while (true) {
      try {
        if (entry != null)         entry.keyBytes(bytes);
        break;
      }
 catch (      GridCacheEntryRemovedException ignore) {
        entry=ctx.cache().entryEx(key);
      }
    }
  }
 else   if (entry != null) {
    bytes=entry.keyBytes();
    if (bytes != null)     keyBytes=bytes;
  }
}
