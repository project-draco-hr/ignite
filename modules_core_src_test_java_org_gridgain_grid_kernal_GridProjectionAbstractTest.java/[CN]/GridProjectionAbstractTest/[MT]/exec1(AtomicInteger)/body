{
  cnt.set(0);
  prj.compute().execute(TestTask.class.getName(),null);
  waitForValue(cnt,projectionSize());
  cnt.set(0);
  prj.compute().execute(new TestTask(),null);
  waitForValue(cnt,projectionSize());
  cnt.set(0);
  prj.compute().execute(TestTask.class,null);
  waitForValue(cnt,projectionSize());
}
