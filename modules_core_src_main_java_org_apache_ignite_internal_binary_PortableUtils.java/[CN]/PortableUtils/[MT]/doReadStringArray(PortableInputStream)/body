{
  int len=in.readInt();
  String[] arr=new String[len];
  for (int i=0; i < len; i++) {
    byte flag=in.readByte();
    if (flag == GridPortableMarshaller.NULL)     arr[i]=null;
 else {
      if (flag != GridPortableMarshaller.STRING)       throw new BinaryObjectException("Invalid flag value: " + flag);
      arr[i]=doReadString(in);
    }
  }
  return arr;
}
