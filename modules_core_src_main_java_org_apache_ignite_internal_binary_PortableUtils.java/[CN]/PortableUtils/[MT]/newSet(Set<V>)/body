{
  if (set instanceof LinkedHashSet)   return U.newLinkedHashSet(set.size());
 else   if (set instanceof TreeSet)   return new TreeSet<>(((TreeSet<Object>)set).comparator());
 else   if (set instanceof ConcurrentSkipListSet)   return new ConcurrentSkipListSet<>(((ConcurrentSkipListSet<Object>)set).comparator());
  return U.newHashSet(set.size());
}
