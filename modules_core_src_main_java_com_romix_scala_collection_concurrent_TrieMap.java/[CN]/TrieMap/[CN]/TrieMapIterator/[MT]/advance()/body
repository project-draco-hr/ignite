{
  if (depth >= 0) {
    int npos=stackpos[depth] + 1;
    if (npos < stack[depth].length) {
      stackpos[depth]=npos;
      BasicNode elem=stack[depth][npos];
      if (elem instanceof SNode) {
        current=(SNode<K,V>)elem;
      }
 else       if (elem instanceof INode) {
        readin((INode<K,V>)elem);
      }
    }
 else {
      depth-=1;
      advance();
    }
  }
 else   current=null;
}
