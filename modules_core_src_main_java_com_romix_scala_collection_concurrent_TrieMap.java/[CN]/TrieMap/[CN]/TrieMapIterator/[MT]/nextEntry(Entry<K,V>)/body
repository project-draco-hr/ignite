{
  return new Entry<K,V>(){
    private V updated=null;
    @Override public K getKey(){
      return rr.getKey();
    }
    @Override public V getValue(){
      return (updated == null) ? rr.getValue() : updated;
    }
    @Override public V setValue(    V value){
      updated=value;
      return ct.replace(getKey(),value);
    }
  }
;
}
