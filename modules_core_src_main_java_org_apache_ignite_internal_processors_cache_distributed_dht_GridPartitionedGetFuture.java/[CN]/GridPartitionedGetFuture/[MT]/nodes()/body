{
  return F.viewReadOnly(futures(),new IgniteClosure<IgniteInternalFuture<Map<K,V>>,ClusterNode>(){
    @Nullable @Override public ClusterNode apply(    IgniteInternalFuture<Map<K,V>> f){
      if (isMini(f))       return ((MiniFuture)f).node();
      return cctx.discovery().localNode();
    }
  }
);
}
