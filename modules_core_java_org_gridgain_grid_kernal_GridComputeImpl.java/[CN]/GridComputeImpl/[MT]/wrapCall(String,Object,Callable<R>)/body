{
  final Object affKey0=ctx.affinity().affinityKey(cacheName,affKey);
  return new CO<R>(){
    @GridCacheName private final String cn=cacheName;
    @GridCacheAffinityMapped private final Object ak=affKey0;
    @Override public R apply(){
      try {
        return call.call();
      }
 catch (      Exception e) {
        throw F.wrap(e);
      }
    }
  }
;
}
