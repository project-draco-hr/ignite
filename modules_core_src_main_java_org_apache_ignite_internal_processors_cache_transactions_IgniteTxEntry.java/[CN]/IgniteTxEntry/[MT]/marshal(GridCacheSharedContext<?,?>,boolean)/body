{
  if (transformClosBytes == null && entryProcessorsCol != null)   transformClosBytes=CU.marshal(ctx,entryProcessorsCol);
  if (F.isEmptyOrNulls(filters))   filterBytes=null;
 else   if (filterBytes == null)   filterBytes=CU.marshal(ctx,filters);
  if (transferExpiry)   transferExpiryPlc=expiryPlc != null && expiryPlc != this.ctx.expiry();
  key.prepareMarshal(context().cacheObjectContext());
  val.marshal(ctx,context());
  expiryPlcBytes=transferExpiryPlc ? CU.marshal(ctx,new IgniteExternalizableExpiryPolicy(expiryPlc)) : null;
}
