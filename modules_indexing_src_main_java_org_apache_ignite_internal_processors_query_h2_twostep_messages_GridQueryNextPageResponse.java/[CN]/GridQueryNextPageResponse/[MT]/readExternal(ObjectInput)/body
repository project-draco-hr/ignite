{
  qryReqId=in.readLong();
  qry=in.readInt();
  page=in.readInt();
  last=in.readBoolean();
  allRows=in.readInt();
  int rowCnt=in.readInt();
  if (rowCnt == 0)   rows=Collections.emptyList();
 else {
    rows=new ArrayList<>(rowCnt);
    int cols=in.readInt();
    int dataSize=in.readInt();
    byte[] dataBytes=new byte[dataSize];
    in.readFully(dataBytes);
    Data data=Data.create(null,dataBytes);
    for (int r=0; r < rowCnt; r++) {
      Value[] row=new Value[cols];
      for (int c=0; c < cols; c++)       row[c]=data.readValue();
      rows.add(row);
    }
  }
}
