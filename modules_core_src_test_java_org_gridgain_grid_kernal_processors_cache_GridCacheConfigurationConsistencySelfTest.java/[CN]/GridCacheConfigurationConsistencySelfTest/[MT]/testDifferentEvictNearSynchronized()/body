{
  cacheMode=PARTITIONED;
  checkSecondGridStartFails(new C1<GridCacheConfiguration,Void>(){
    /** 
 * {@inheritDoc} 
 */
    @SuppressWarnings("deprecation") @Override public Void apply(    GridCacheConfiguration cfg){
