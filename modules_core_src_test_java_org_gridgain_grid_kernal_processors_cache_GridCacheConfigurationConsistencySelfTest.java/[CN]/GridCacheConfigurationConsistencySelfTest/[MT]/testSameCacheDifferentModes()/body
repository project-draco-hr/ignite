{
  cacheEnabled=true;
  cacheName="myCache";
  cacheMode=REPLICATED;
  depMode=SHARED;
  startGrid(1);
  cacheEnabled=true;
  cacheName="myCache";
  cacheMode=PARTITIONED;
  depMode=SHARED;
  try {
    startGrid(2);
    fail();
  }
 catch (  GridException e) {
    info("Caught expected exception: " + e);
  }
}
