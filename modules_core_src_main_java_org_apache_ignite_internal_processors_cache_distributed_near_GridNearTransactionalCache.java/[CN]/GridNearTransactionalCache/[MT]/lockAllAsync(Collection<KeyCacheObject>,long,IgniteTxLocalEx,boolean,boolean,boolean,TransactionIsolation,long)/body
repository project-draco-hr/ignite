{
  CacheOperationContext opCtx=ctx.operationContextPerCall();
  GridNearLockFuture fut=new GridNearLockFuture(ctx,keys,(GridNearTxLocal)tx,isRead,retval,timeout,accessTtl,CU.empty0(),opCtx != null && opCtx.skipStore());
  if (!ctx.mvcc().addFuture(fut))   throw new IllegalStateException("Duplicate future ID: " + fut);
  fut.map();
  return fut;
}
