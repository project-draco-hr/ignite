{
  if (file.isDirectory())   for (  File childFile : file.listFiles())   processFile(childFile);
 else   if (file.getName().toLowerCase().endsWith(".jar"))   processJar(file);
 else   if (file.getName().toLowerCase().endsWith(".class"))   processClass(trimClassExtension(file.getPath().substring(urlPrefixLen).replace(File.separatorChar,'.')));
}
