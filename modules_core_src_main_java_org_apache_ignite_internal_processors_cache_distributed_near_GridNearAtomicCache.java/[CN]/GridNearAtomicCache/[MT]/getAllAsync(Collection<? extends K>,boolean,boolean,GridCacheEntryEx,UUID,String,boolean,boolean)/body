{
  ctx.checkSecurity(SecurityPermission.CACHE_READ);
  if (F.isEmpty(keys))   return new GridFinishedFuture<>(Collections.<K,V>emptyMap());
  if (keyCheck)   validateCacheKeys(keys);
  CacheOperationContext opCtx=ctx.operationContextPerCall();
  subjId=ctx.subjectIdPerCall(subjId,opCtx);
  return loadAsync(null,ctx.cacheKeysView(keys),false,forcePrimary,subjId,taskName,deserializePortable,skipVals ? null : opCtx != null ? opCtx.expiry() : null,skipVals);
}
