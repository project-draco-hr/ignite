{
  ctx.checkSecurity(SecurityPermission.CACHE_READ);
  if (F.isEmpty(keys))   return new GridFinishedFuture<>(Collections.<K,V>emptyMap());
  if (keyCheck)   validateCacheKeys(keys);
  CacheProjectionContext<K,V> prj=ctx.projectionPerCall();
  subjId=ctx.subjectIdPerCall(subjId,prj);
  return loadAsync(null,ctx.cacheKeysView(keys),false,forcePrimary,subjId,taskName,deserializePortable,skipVals ? null : prj != null ? prj.expiry() : null,skipVals);
}
