{
  ctx.denyOnFlag(LOCAL);
  ctx.checkSecurity(GridSecurityPermission.CACHE_READ);
  if (F.isEmpty(keys))   return new GridFinishedFuture<>(Collections.<K,V>emptyMap());
  GridCacheProjectionImpl<K,V> prj=ctx.projectionPerCall();
  subjId=ctx.subjectIdPerCall(subjId,prj);
  return loadAsync(null,keys,false,forcePrimary,subjId,taskName,deserializePortable,skipVals ? null : prj != null ? prj.expiry() : null,skipVals);
}
