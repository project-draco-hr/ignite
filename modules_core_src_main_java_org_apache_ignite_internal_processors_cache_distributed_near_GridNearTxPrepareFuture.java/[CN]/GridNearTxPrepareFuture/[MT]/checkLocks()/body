{
  boolean locked=lockKeys.isEmpty();
  if (locked) {
    if (log.isDebugEnabled())     log.debug("All locks are acquired for near prepare future: " + this);
  }
 else {
    if (log.isDebugEnabled())     log.debug("Still waiting for locks [fut=" + this + ", keys="+ lockKeys+ ']');
  }
  return locked;
}
