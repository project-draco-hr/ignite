{
  if (fields != null) {
    if (fields.containsKey(fieldName))     return (F)fields.get(fieldName);
  }
 else   fields=new HashMap<>();
  if (reader == null)   reader=new GridPortableReaderImpl(ctx,arr,start);
  Object field=reader.unmarshal(fieldName);
  fields.put(fieldName,field);
  return (F)field;
}
