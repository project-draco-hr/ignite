{
  A.notNull(sesId,"sesId");
  guard();
  try {
    GridComputeTaskFuture<Object> task=ctx.task().taskFuture(sesId);
    if (task != null)     task.cancel();
 else     ctx.io().send(sesId.globalId(),TOPIC_TASK_CANCEL,new GridTaskCancelRequest(sesId),SYSTEM_POOL);
  }
  finally {
    unguard();
  }
}
