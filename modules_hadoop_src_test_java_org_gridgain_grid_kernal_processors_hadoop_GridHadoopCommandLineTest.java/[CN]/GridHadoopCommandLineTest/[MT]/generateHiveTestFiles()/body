{
  Random random=new Random();
  try (PrintWriter writerA=new PrintWriter(new File(testWorkDir,"data-a"));PrintWriter writerB=new PrintWriter(new File(testWorkDir,"data-b"))){
    char sep='\t';
    int idB=0;
    int idA=0;
    for (int i=0; i < 1000; i++) {
      writerA.print(idA++);
      writerA.print(sep);
      writerA.println(idB);
      writerB.print(idB++);
      writerB.print(sep);
      writerB.println(random.nextInt(1000));
      writerB.print(idB++);
      writerB.print(sep);
      writerB.println(random.nextInt(1000));
    }
    writerA.flush();
    writerB.flush();
  }
 }
