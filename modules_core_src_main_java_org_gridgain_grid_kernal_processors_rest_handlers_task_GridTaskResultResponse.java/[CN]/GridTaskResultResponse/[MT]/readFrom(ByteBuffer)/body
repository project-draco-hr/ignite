{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    String err0=commState.getString(null);
  if (err0 == STR_NOT_READ)   return false;
err=err0;
commState.idx++;
case 1:
if (buf.remaining() < 1) return false;
