{
  if (e instanceof CachePartialUpdateCheckedException)   return new CachePartialUpdateException((CachePartialUpdateCheckedException)e);
 else   if (e instanceof CacheAtomicUpdateTimeoutCheckedException)   return new CacheAtomicUpdateTimeoutException(e.getMessage(),e);
  if (e.getCause() instanceof CacheException)   return (CacheException)e.getCause();
  C1<IgniteCheckedException,IgniteException> converter=exceptionConverters.get(e.getClass());
  return converter != null ? new CacheException(converter.apply(e)) : new CacheException(e);
}
