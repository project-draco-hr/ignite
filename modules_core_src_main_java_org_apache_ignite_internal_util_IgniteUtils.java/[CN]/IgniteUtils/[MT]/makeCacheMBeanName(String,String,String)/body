{
  SB sb=new SB(JMX_DOMAIN + ':');
  appendClassLoaderHash(sb);
  appendJvmId(sb);
  if (gridName != null && !gridName.isEmpty())   sb.a("grid=").a(gridName).a(',');
  cacheName=maskName(cacheName);
  sb.a("group=").a(cacheName).a(',');
  sb.a("name=").a(name);
  return new ObjectName(sb.toString());
}
