{
  ChannelShell ch=null;
  try {
    ch=(ChannelShell)ses.openChannel("shell");
    ch.connect();
    try (PrintStream out=new PrintStream(ch.getOutputStream(),true)){
      out.println(cmd);
      U.sleep(1000);
    }
   }
  finally {
    if (ch != null && ch.isConnected())     ch.disconnect();
  }
}
