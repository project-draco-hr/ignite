{
  if (cacheCtx.system() ^ tx.system())   return false;
  for (  Integer cacheId : activeCacheIds) {
    GridCacheContext<K,V> activeCacheCtx=cacheContext(cacheId);
    if (cacheCtx.system()) {
      if (activeCacheCtx.cacheId() != cacheCtx.cacheId())       return false;
    }
    if (activeCacheCtx.store().store() != cacheCtx.store().store())     return false;
  }
  return true;
}
