{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeLong("order",order))     return false;
  state++;
case 1:
if (!writer.writeBoolean("res",res)) return false;
state++;
}
return true;
}
