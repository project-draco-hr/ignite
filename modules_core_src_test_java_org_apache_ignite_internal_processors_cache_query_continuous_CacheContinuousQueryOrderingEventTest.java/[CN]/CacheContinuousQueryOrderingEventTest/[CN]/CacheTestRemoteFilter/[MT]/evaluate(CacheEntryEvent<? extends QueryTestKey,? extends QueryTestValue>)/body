{
  if (affinity(ignite.cache(cacheName)).isPrimary(ignite.cluster().localNode(),e.getKey())) {
    QueryTestValue prevVal=prevVals.put(e.getKey(),e.getValue());
    if (prevVal != null) {
      if (!new QueryTestValue(prevVal.val1 + 1).equals(e.getValue()))       fail=true;
    }
  }
  return true;
}
