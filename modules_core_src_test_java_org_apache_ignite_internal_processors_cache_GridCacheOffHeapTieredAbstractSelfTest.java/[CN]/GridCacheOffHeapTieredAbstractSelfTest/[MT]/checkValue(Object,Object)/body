{
  for (int i=0; i < gridCount(); i++) {
    if (val != null && val.getClass() == byte[].class) {
      Assert.assertArrayEquals("Unexpected value for grid: " + i,(byte[])val,(byte[])grid(i).jcache(null).get(key));
    }
 else     assertEquals("Unexpected value for grid: " + i,val,grid(i).jcache(null).get(key));
  }
}
