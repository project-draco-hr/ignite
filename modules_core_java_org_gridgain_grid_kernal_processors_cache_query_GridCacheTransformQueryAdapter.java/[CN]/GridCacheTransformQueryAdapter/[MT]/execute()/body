{
  if (trans == null)   return new GridCacheErrorQueryFuture<>(cctx.kernalContext(),new GridException("Transformer must be set for transform query."));
  Collection<GridNode> nodes=nodes();
  if (qryLog.isDebugEnabled())   qryLog.debug(U.compact("Executing transform query " + toShortString(nodes)));
  return execute(nodes,false,false,null,null);
}
