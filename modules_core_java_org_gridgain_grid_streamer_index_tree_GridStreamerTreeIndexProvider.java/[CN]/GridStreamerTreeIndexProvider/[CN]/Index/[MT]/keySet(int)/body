{
  Set<K> col=new AbstractSet<K>(){
    private Collection<K> entries=F.viewReadOnly(cnt >= 0 ? idx0.values() : idx0.descendingMap().values(),entryToKey);
    @NotNull @Override public Iterator<K> iterator(){
      return entries.iterator();
    }
    @Override public int size(){
      return entries.size();
    }
  }
;
  return cnt == 0 ? col : F.limit(col,U.safeAbs(cnt));
}
