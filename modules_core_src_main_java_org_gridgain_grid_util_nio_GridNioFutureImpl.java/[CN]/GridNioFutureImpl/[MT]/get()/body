{
  try {
    if (endTime == 0)     acquireSharedInterruptibly(0);
    if (getState() == CANCELLED)     throw new IgniteFutureCancelledException("Future was cancelled: " + this);
    if (err != null)     throw U.cast(err);
    return res;
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    throw new GridInterruptedException(e);
  }
}
