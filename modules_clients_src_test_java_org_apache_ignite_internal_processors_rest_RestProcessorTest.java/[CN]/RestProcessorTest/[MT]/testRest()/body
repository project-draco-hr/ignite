{
  IgniteConfiguration cfg=getConfiguration((String)null);
  cfg=cacheTestConfiguration(cfg);
  G.start(cfg);
  populateCache();
  deployTasks();
  JOptionPane.showMessageDialog(null,new JComponent[]{new JLabel("GridGain started."),new JLabel("<html>" + "You can use JMX console at <u>http://localhost:1234</u>" + "</html>"),new JLabel("Press OK to stop GridGain.")},"GridGain Startup JUnit",JOptionPane.INFORMATION_MESSAGE);
  G.stop(true);
}
