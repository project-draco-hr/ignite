{
  if (log.isDebugEnabled())   log.debug("Processing keys in accept worker: " + keys.size());
  for (Iterator<SelectionKey> iter=keys.iterator(); iter.hasNext(); ) {
    SelectionKey key=iter.next();
    iter.remove();
    if (!key.isValid())     continue;
    if (key.isAcceptable()) {
      ServerSocketChannel srvrCh=(ServerSocketChannel)key.channel();
      SocketChannel sockCh=srvrCh.accept();
      sockCh.configureBlocking(false);
      sockCh.socket().setTcpNoDelay(tcpNoDelay);
      sockCh.socket().setKeepAlive(true);
      if (sockSndBuf > 0)       sockCh.socket().setSendBufferSize(sockSndBuf);
      if (sockRcvBuf > 0)       sockCh.socket().setReceiveBufferSize(sockRcvBuf);
      if (log.isDebugEnabled())       log.debug("Accepted new client connection: " + sockCh.socket().getRemoteSocketAddress());
      addRegistrationReq(sockCh);
    }
  }
}
