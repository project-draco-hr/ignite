{
  ConcurrentLinkedDeque8<ByteBuffer> queue=ses.meta(BUF_SSL_SYSTEM_META_KEY);
  ByteBuffer buf;
  while ((buf=queue.peek()) != null) {
    int cnt=sockCh.write(buf);
    if (metricsLsnr != null)     metricsLsnr.onBytesSent(cnt);
    ses.bytesSent(cnt);
    if (!buf.hasRemaining())     queue.remove(buf);
 else     break;
  }
}
