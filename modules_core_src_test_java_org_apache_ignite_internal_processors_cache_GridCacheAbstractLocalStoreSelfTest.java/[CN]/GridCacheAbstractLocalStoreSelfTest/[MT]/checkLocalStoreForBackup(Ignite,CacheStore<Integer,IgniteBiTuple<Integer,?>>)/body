{
  for (int i=0; i < KEYS; i++) {
    if (ignite.affinity(BACKUP_CACHE).isBackup(ignite.cluster().localNode(),i))     assertEquals(store.load(i).get1().intValue(),i);
 else     if (!ignite.affinity(BACKUP_CACHE).isPrimaryOrBackup(ignite.cluster().localNode(),i))     assertNull(store.load(i).get1());
  }
}
