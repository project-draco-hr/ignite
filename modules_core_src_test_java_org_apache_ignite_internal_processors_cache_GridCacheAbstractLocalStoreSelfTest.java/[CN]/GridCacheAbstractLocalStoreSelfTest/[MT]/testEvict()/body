{
  Ignite ignite1=startGrid(1);
  IgniteCache<Object,Object> cache=ignite1.jcache(null).withExpiryPolicy(new CreatedExpiryPolicy(new Duration(TimeUnit.MILLISECONDS,100L)));
  for (int i=0; i < KEYS; i++)   cache.put(i,i);
  U.sleep(200);
  for (int i=0; i < KEYS; i++) {
    cache.localEvict(Arrays.asList(i));
    assertNull(cache.localPeek(i));
    assertEquals(i,(int)LOCAL_STORE_1.load(i).get1());
    assertEquals(i,cache.get(i));
  }
}
