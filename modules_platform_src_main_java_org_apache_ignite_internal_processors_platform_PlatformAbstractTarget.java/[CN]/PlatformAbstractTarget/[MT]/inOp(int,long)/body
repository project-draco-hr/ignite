{
  try (PlatformMemory mem=interopCtx.memory().get(memPtr)){
    PortableRawReaderEx reader=interopCtx.reader(mem);
    if (type == OP_META) {
      interopCtx.processMetadata(reader);
      return TRUE;
    }
 else     return processInOp(type,reader);
  }
 catch (  Exception e) {
    throw convertException(e);
  }
}
