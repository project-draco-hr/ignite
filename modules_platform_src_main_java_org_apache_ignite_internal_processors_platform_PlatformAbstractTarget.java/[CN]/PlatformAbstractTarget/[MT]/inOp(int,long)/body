{
  try (PlatformMemory mem=platformCtx.memory().get(memPtr)){
    PortableRawReaderEx reader=platformCtx.reader(mem);
    if (type == OP_META) {
      platformCtx.processMetadata(reader);
      return TRUE;
    }
 else     return processInOp(type,reader);
  }
 catch (  Exception e) {
    throw convertException(e);
  }
}
