{
  try {
    IgniteConfiguration cfg=g.configuration();
    for (    GridCache cache : g.cachesx()) {
      String cacheName=cache.name();
      if (arg.systemCaches() || !(isSystemCache(cacheName) || isGgfsCache(cfg,cacheName)))       res.caches().add(VisorCache.from(g,cache,arg.sample()));
    }
  }
 catch (  Throwable cachesEx) {
    res.cachesEx(cachesEx);
  }
}
