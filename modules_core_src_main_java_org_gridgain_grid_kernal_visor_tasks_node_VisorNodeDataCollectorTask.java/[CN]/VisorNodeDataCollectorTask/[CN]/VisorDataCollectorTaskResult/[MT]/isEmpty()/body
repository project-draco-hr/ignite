{
  return gridNames.isEmpty() && topologyVersions.isEmpty() && unhandledEx.isEmpty()&& taskMonitoringEnabled.isEmpty()&& events.isEmpty()&& eventsEx.isEmpty()&& licensesEx.isEmpty()&& caches.isEmpty()&& cachesEx.isEmpty()&& ggfss.isEmpty()&& ggfsEndpoints.isEmpty()&& ggfssEx.isEmpty()&& streamers.isEmpty()&& streamersEx.isEmpty()&& drsEx.isEmpty();
}
