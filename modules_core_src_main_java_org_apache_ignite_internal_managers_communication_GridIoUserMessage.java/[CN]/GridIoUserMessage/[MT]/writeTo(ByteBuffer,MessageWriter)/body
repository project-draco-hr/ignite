{
  writer.setBuffer(buf);
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeField("bodyBytes",bodyBytes,MessageFieldType.BYTE_ARR))     return false;
  writer.incrementState();
case 1:
if (!writer.writeField("clsLdrId",clsLdrId,MessageFieldType.IGNITE_UUID)) return false;
writer.incrementState();
case 2:
if (!writer.writeField("depClsName",depClsName,MessageFieldType.STRING)) return false;
writer.incrementState();
case 3:
if (!writer.writeField("depMode",depMode != null ? (byte)depMode.ordinal() : -1,MessageFieldType.BYTE)) return false;
writer.incrementState();
case 4:
if (!writer.writeMapField("ldrParties",ldrParties,MessageFieldType.UUID,MessageFieldType.IGNITE_UUID)) return false;
writer.incrementState();
case 5:
if (!writer.writeField("topicBytes",topicBytes,MessageFieldType.BYTE_ARR)) return false;
writer.incrementState();
case 6:
if (!writer.writeField("userVer",userVer,MessageFieldType.STRING)) return false;
writer.incrementState();
}
return true;
}
