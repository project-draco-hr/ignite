{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeByteArray("bodyBytes",bodyBytes))     return false;
  state++;
case 1:
if (!writer.writeIgniteUuid("clsLdrId",clsLdrId)) return false;
state++;
case 2:
if (!writer.writeString("depClsName",depClsName)) return false;
state++;
case 3:
if (!writer.writeEnum("depMode",depMode)) return false;
state++;
case 4:
if (!writer.writeMap("ldrParties",ldrParties,UUID.class,IgniteUuid.class)) return false;
state++;
case 5:
if (!writer.writeByteArray("topicBytes",topicBytes)) return false;
state++;
case 6:
if (!writer.writeString("userVer",userVer)) return false;
state++;
}
return true;
}
