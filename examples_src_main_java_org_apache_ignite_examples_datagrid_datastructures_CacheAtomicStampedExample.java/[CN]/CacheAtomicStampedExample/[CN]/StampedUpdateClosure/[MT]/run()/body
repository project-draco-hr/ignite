{
  try {
    CacheAtomicStamped<String,String> stamped=Ignition.ignite().cache(cacheName).dataStructures().atomicStamped(stampedName,null,null,true);
    System.out.println("Atomic stamped [value=" + stamped.value() + ", stamp="+ stamped.stamp()+ ']');
  }
 catch (  IgniteCheckedException e) {
    throw new IgniteException(e);
  }
}
