{
  Ignite ignite=ignite(NODE_CNT - 1);
  assertTrue(ignite.configuration().isClientMode());
  if (clientDiscovery())   assertTrue(ignite.configuration().getDiscoverySpi() instanceof TcpClientDiscoverySpi);
 else   assertTrue(ignite.configuration().getDiscoverySpi() instanceof TcpDiscoverySpi);
  return ignite;
}
