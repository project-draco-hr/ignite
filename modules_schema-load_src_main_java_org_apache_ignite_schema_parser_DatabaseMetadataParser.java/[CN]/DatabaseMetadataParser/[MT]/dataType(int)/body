{
switch (type) {
case BIT:
case BOOLEAN:
    return Boolean.class;
case TINYINT:
  return Byte.class;
case SMALLINT:
return Short.class;
case INTEGER:
return Integer.class;
case BIGINT:
return Long.class;
case REAL:
return Float.class;
case FLOAT:
case DOUBLE:
return Double.class;
case NUMERIC:
case DECIMAL:
return BigDecimal.class;
case CHAR:
case VARCHAR:
case LONGVARCHAR:
case NCHAR:
case NVARCHAR:
case LONGNVARCHAR:
return String.class;
case DATE:
return java.sql.Date.class;
case TIME:
return java.sql.Time.class;
case TIMESTAMP:
return java.sql.Timestamp.class;
case BINARY:
case VARBINARY:
case LONGVARBINARY:
case ARRAY:
case BLOB:
case CLOB:
case NCLOB:
return java.lang.reflect.Array.class;
case NULL:
return Void.class;
case DATALINK:
return URL.class;
default :
return Object.class;
}
}
