{
  if (keepPortable || !portableEnabled())   return col;
  if (col instanceof ArrayList)   return unwrapPortables((ArrayList<Object>)col);
  if (col instanceof Set)   return unwrapPortables((Set<Object>)col);
  Collection<Object> col0=new ArrayList<>(col.size());
  for (  Object obj : col)   col0.add(unwrapPortable(obj));
  return col0;
}
