{
  boolean removed=map.remove(entry.key(),entry);
  if (removed) {
    if (ctx.events().isRecordable(EVT_CACHE_ENTRY_DESTROYED))     ctx.events().addEvent(entry.partition(),entry.key(),ctx.localNodeId(),(IgniteUuid)null,null,EVT_CACHE_ENTRY_DESTROYED,null,false,null,false,null,null,null,false);
synchronized (entry) {
      if (!entry.deleted())       decrementPublicSize(entry);
    }
  }
  return removed;
}
