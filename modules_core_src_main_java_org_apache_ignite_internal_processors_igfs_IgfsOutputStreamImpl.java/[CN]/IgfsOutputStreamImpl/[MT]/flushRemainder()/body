{
  try {
    if (remainder != null) {
      remainder=igfsCtx.data().storeDataBlocks(fileInfo,length() + space,null,0,ByteBuffer.wrap(remainder,0,remainderDataLen),true,streamRange,batch);
      remainder=null;
      remainderDataLen=0;
    }
  }
 catch (  IgniteCheckedException e) {
    throw new IOException("Failed to flush data (remainder) [path=" + path + ", space="+ space+ ']',e);
  }
}
