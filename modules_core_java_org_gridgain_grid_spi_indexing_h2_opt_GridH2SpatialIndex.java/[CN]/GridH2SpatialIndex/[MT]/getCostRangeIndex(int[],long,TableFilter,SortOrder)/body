{
  rowCnt+=Constants.COST_ROW_OFFSET;
  long cost=rowCnt;
  long rows=rowCnt;
  if (masks == null)   return cost;
  for (  Column column : columns) {
    int idx=column.getColumnId();
    int mask=masks[idx];
    if ((mask & IndexCondition.SPATIAL_INTERSECTS) != 0) {
      cost=3 + rows / 4;
      break;
    }
  }
  return cost;
}
