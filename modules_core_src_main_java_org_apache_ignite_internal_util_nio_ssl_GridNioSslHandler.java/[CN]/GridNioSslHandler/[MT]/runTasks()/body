{
  Runnable runnable;
  while ((runnable=sslEngine.getDelegatedTask()) != null) {
    if (log.isDebugEnabled())     log.debug("Running SSL engine task [task=" + runnable + ", ses="+ ses+ ']');
    runnable.run();
  }
  if (log.isDebugEnabled())   log.debug("Finished running SSL engine tasks [handshakeStatus=" + sslEngine.getHandshakeStatus() + ", ses="+ ses+ ']');
  return sslEngine.getHandshakeStatus();
}
