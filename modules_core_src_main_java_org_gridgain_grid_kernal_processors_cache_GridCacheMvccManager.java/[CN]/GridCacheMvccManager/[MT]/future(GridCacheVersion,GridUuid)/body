{
  Collection<? extends GridCacheFuture> futs=this.futs.get(ver);
  if (futs != null)   for (  GridCacheFuture<?> fut : futs)   if (fut.futureId().equals(futId)) {
    if (log.isDebugEnabled())     log.debug("Found future in futures map: " + fut);
    return (GridCacheFuture<T>)fut;
  }
  if (log.isDebugEnabled())   log.debug("Failed to find future in futures map [ver=" + ver + ", futId="+ futId+ ']');
  return null;
}
