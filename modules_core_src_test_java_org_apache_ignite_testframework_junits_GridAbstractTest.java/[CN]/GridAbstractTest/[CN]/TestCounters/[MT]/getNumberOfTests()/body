{
  if (numOfTests == -1) {
    GridAbstractTest this0=GridAbstractTest.this;
    int cnt;
    if (this0.forceTestCnt)     cnt=this0.testCnt;
 else {
      cnt=0;
      for (      Method m : this0.getClass().getMethods())       if (m.getName().startsWith("test") && Modifier.isPublic(m.getModifiers()))       cnt++;
    }
    numOfTests=cnt;
  }
  countTestCases();
  return numOfTests;
}
