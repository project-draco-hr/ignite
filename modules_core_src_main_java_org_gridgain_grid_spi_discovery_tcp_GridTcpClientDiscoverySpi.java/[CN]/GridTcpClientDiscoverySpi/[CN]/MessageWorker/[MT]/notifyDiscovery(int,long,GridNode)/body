{
  GridDiscoverySpiListener lsnr=GridTcpClientDiscoverySpi.this.lsnr;
  if (lsnr != null)   lsnr.onDiscovery(type,topVer,node,top.values(),null);
 else   if (log.isDebugEnabled())   log.debug("Skipped discovery notification [node=" + node + ", spiState="+ spiState()+ ", type="+ U.gridEventName(type)+ ", topVer="+ topVer+ ']');
}
