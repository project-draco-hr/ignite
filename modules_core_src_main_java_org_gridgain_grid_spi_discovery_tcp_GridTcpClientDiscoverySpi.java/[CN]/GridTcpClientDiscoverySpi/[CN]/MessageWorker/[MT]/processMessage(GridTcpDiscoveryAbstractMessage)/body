{
  stats.onMessageProcessingStarted(msg);
  if (msg instanceof GridTcpDiscoveryNodeAddedMessage)   processNodeAddedMessage((GridTcpDiscoveryNodeAddedMessage)msg);
 else   if (msg instanceof GridTcpDiscoveryNodeAddFinishedMessage)   processNodeAddFinishedMessage((GridTcpDiscoveryNodeAddFinishedMessage)msg);
  stats.onMessageProcessingFinished(msg);
}
