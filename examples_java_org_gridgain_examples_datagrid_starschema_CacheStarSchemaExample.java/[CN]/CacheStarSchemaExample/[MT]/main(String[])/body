{
  GridGain.start("examples/config/example-cache.xml");
  System.out.println();
  System.out.println(">>> Cache star schema example started.");
  try {
    populateDimensions();
    populateFacts();
    queryStorePurchases();
    queryProductPurchases();
  }
  finally {
    GridGain.stop(false);
  }
}
