  uriList.add(GridTestProperties.getProperty("deploy.uri.http"));
  uriList.add(GridTestProperties.getProperty("ant.urideployment.gar.uri").replace("EXTDATA",U.resolveIgnitePath("modules/extdata").getAbsolutePath()));
