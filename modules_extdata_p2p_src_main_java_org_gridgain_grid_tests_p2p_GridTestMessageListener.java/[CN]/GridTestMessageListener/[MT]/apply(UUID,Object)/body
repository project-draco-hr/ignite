{
  grid.log().info("Received message [nodeId=" + nodeId + ", locNodeId="+ grid.cluster().localNode().id()+ ", msg="+ msg+ ']');
  GridNodeLocalMap<String,AtomicInteger> map=grid.cluster().nodeLocalMap();
  AtomicInteger cnt=map.get("msgCnt");
  if (cnt == null) {
    AtomicInteger old=map.putIfAbsent("msgCnt",cnt=new AtomicInteger(0));
    if (old != null)     cnt=old;
  }
  cnt.incrementAndGet();
  return true;
}
