{
  try {
    IgniteStreamer streamer=ignite.streamer(streamerName);
    streamer.resetMetrics();
    return null;
  }
 catch (  IllegalArgumentException iae) {
    throw new IgniteException("Failed to reset metrics for streamer: " + escapeName(streamerName) + " on node: "+ ignite.localNode().id(),iae);
  }
}
