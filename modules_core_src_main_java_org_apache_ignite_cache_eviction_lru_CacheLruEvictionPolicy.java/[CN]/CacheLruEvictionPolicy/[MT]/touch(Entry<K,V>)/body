{
  Node<Entry<K,V>> node=entry.meta(meta);
  if (node == null) {
    while (true) {
      node=queue.offerLastx(entry);
      if (entry.putMetaIfAbsent(meta,node) != null) {
        queue.unlinkx(node);
        return false;
      }
 else       if (node.item() != null) {
        if (!entry.isCached()) {
          queue.unlinkx(node);
          return false;
        }
        return true;
      }
 else       if (!entry.removeMeta(meta,node))       return false;
    }
  }
 else   if (queue.unlinkx(node)) {
    Node<Entry<K,V>> newNode=queue.offerLastx(entry);
    if (!entry.replaceMeta(meta,node,newNode))     queue.unlinkx(newNode);
  }
  return false;
}
