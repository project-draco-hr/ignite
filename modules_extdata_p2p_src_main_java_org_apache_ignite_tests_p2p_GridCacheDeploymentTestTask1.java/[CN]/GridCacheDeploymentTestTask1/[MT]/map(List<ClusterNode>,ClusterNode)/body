{
  return F.asMap(new ComputeJobAdapter(){
    @IgniteInstanceResource private Ignite ignite;
    @Override public Object execute() throws IgniteCheckedException {
      X.println("Executing GridCacheDeploymentTestTask1 job on node " + ignite.cluster().localNode().id());
      Cache<String,GridCacheDeploymentTestValue> cache=ignite.cache(null);
      for (int i=0; i < PUT_CNT; i++)       cache.putx("1" + i,new GridCacheDeploymentTestValue());
      return null;
    }
  }
,node);
}
