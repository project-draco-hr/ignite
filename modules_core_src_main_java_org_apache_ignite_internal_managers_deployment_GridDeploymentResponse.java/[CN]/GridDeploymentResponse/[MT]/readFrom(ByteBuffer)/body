{
  reader.setBuffer(buf);
  if (!reader.beforeMessageRead())   return false;
switch (readState) {
case 0:
    byteSrc=reader.readMessage("byteSrc");
  if (!reader.isLastRead())   return false;
readState++;
case 1:
errMsg=reader.readString("errMsg");
if (!reader.isLastRead()) return false;
readState++;
case 2:
success=reader.readBoolean("success");
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
