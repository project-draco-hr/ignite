{
  writer.setBuffer(buf);
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeField("byteSrc",byteSrc,MessageFieldType.MSG))     return false;
  writer.incrementState();
case 1:
if (!writer.writeField("errMsg",errMsg,MessageFieldType.STRING)) return false;
writer.incrementState();
case 2:
if (!writer.writeField("success",success,MessageFieldType.BOOLEAN)) return false;
writer.incrementState();
}
return true;
}
