{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeMessage("byteSrc",byteSrc != null ? byteSrc.clone() : null))     return false;
  state++;
case 1:
if (!writer.writeString("errMsg",errMsg)) return false;
state++;
case 2:
if (!writer.writeBoolean("success",success)) return false;
state++;
}
return true;
}
