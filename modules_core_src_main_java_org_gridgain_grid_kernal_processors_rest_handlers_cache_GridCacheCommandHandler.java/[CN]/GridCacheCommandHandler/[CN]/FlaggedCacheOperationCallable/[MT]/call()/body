{
  GridCacheProjectionEx<?,?> prj=(GridCacheProjectionEx<?,?>)cache(g,cacheName).forSubjectId(clientId).flagsOn(flags);
  if (portable)   prj=prj.forPortables();
  return op.apply((GridCacheProjection<Object,Object>)prj,((GridKernal)g).context()).chain(resultWrapper((GridCacheProjection<Object,Object>)prj,key)).get();
}
