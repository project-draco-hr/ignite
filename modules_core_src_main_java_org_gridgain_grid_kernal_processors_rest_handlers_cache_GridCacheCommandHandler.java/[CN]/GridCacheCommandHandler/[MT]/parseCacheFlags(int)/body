{
  if (cacheFlagsBits == 0)   return EMPTY_FLAGS;
  EnumSet<GridCacheFlag> flagSet=EnumSet.noneOf(GridCacheFlag.class);
  if ((cacheFlagsBits & 1) != 0)   flagSet.add(GridCacheFlag.SKIP_STORE);
  if ((cacheFlagsBits & (1 << 1)) != 0)   flagSet.add(GridCacheFlag.SKIP_SWAP);
  if ((cacheFlagsBits & (1 << 2)) != 0)   flagSet.add(GridCacheFlag.SYNC_COMMIT);
  if ((cacheFlagsBits & (1 << 4)) != 0)   flagSet.add(GridCacheFlag.INVALIDATE);
  return flagSet.toArray(new GridCacheFlag[flagSet.size()]);
}
