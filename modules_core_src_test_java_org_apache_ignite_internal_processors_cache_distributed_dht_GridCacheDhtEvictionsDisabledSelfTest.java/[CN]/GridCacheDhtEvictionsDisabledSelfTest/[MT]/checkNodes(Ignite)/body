{
  Cache<String,String> cache=g.cache("test");
  for (char c='a'; c <= 'z'; c++) {
    String key=Character.toString(c);
    cache.put(key,"val-" + key);
    String v1=cache.get(key);
    String v2=cache.get(key);
    info("v1: " + v1);
    info("v2: " + v2);
    assertNotNull(v1);
    assertNotNull(v2);
    if (cache.affinity().mapKeyToNode(key).isLocal())     assertSame(v1,v2);
 else     assertEquals(v1,v2);
  }
}
