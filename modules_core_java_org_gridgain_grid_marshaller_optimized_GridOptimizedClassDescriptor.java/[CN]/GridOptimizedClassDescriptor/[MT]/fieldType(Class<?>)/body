{
  GridOptimizedFieldType type;
  if (cls == byte.class)   type=BYTE;
 else   if (cls == short.class)   type=SHORT;
 else   if (cls == int.class)   type=INT;
 else   if (cls == long.class)   type=LONG;
 else   if (cls == float.class)   type=FLOAT;
 else   if (cls == double.class)   type=DOUBLE;
 else   if (cls == char.class)   type=CHAR;
 else   if (cls == boolean.class)   type=BOOLEAN;
 else   type=OTHER;
  return type;
}
