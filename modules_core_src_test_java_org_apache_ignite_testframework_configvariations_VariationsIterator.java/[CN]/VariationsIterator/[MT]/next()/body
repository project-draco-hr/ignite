{
  if (position == -1) {
    position=0;
    cntOfVectors++;
    return arraycopy(vector);
  }
  if (!updateVector(vector,position)) {
    if (position + 1 == params.length)     throw new IllegalStateException("[position=" + position + ", vector="+ Arrays.toString(vector)+ ", params="+ Arrays.deepToString(params));
    position++;
    while (position < params.length && params[position].length < 2)     position++;
    if (position == params.length)     throw new IllegalStateException("[position=" + position + ", vector="+ Arrays.toString(vector)+ ", params="+ Arrays.deepToString(params));
    vector[position]=1;
    cntOfVectors++;
    return arraycopy(vector);
  }
  cntOfVectors++;
  return arraycopy(vector);
}
