{
  if (cands != null && !cands.isEmpty())   for (  GridCacheMvccCandidate<K> cand : cands) {
    if (!cand.nodeId().equals(cctx.discovery().localNode().id())) {
      try {
        addRemote(cand);
      }
 catch (      GridDistributedLockCancelledException ignored) {
        if (log.isDebugEnabled())         log.debug("Will not sync up on candidate since lock was canceled: " + cand);
      }
    }
  }
  orderCompleted(baseVer,committedVers,rolledbackVers);
}
