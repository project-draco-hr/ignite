{
  Collection<? extends Cache<?,?>> caches=arg.get1() ? g.cachesx() : F.asList(g.cachex(arg.get2()));
  if (caches != null) {
    Map<String,VisorCacheMetrics> res=U.newHashMap(caches.size());
    for (    Cache<?,?> c : caches)     res.put(c.name(),VisorCacheMetrics.from(c));
    return res;
  }
  return null;
}
