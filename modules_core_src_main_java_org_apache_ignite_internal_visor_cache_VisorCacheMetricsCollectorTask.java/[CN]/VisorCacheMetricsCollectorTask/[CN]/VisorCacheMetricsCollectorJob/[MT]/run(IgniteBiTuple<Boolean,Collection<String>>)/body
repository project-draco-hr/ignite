{
  Collection<? extends GridCache<?,?>> caches=ignite.cachesx(new VisorCachesFilterPredicate(arg.get1(),arg.get2()));
  if (caches != null) {
    Map<String,VisorCacheMetrics> res=U.newHashMap(caches.size());
    for (    GridCache<?,?> c : caches)     res.put(c.name(),VisorCacheMetrics.from(c));
    return res;
  }
  return null;
}
