{
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 0:
    if (!writer.writeIgniteUuid("clsLdrId",clsLdrId))     return false;
  state.increment();
case 1:
if (!writer.writeEnum("depMode",depMode)) return false;
state.increment();
case 2:
if (!writer.writeBoolean("locDepOwner",locDepOwner)) return false;
state.increment();
case 3:
if (!writer.writeMap("participants",participants,UUID.class,IgniteUuid.class)) return false;
state.increment();
case 4:
if (!writer.writeString("userVer",userVer)) return false;
state.increment();
}
return true;
}
