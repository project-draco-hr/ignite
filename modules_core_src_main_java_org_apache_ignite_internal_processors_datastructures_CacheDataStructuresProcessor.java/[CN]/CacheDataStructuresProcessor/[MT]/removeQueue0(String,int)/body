{
  GridCacheQueueHeader hdr=queueHdrView.remove(new GridCacheQueueHeaderKey(name));
  if (hdr == null)   return false;
  if (hdr.empty())   return true;
  GridCacheQueueAdapter.removeKeys(cctx.kernalContext().cache().jcache(cctx.cache().name()),hdr.id(),name,hdr.collocated(),hdr.head(),hdr.tail(),batchSize);
  return true;
}
