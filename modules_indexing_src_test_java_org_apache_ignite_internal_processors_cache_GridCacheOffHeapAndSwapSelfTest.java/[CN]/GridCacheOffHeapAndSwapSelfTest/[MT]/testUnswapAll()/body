{
  Cache<Long,Long> cache=populate();
  Collection<Long> keys=new HashSet<>();
  for (long i=from; i < to; i++)   keys.add(i);
  cache.promoteAll(keys);
  assertEquals(0,swappedCnt.get());
  assertEquals(to - from,unswapedCnt.get() + onheapedCnt.get());
  checkEntries(cache);
  assertEquals(to - from,unswapedCnt.get() + onheapedCnt.get());
}
