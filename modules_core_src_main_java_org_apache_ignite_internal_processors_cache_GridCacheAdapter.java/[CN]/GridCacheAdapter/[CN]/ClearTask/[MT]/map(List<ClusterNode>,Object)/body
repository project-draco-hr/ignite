{
  Map<ComputeJob,ClusterNode> jobs=new HashMap();
  for (  ClusterNode node : subgrid) {
    jobs.put(keys == null ? new GlobalClearAllJob(ctx.name(),ctx.affinity().affinityTopologyVersion()) : new GlobalClearKeySetJob<K>(ctx.name(),ctx.affinity().affinityTopologyVersion(),keys),node);
  }
  return jobs;
}
