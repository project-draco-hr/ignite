{
  A.notNull(key,"key",val,"val");
  if (keyCheck)   validateCacheKey(key);
  validateCacheValue(val);
  return asyncOp(new AsyncOp<Boolean>(){
    @Override public IgniteInternalFuture<Boolean> op(    IgniteTxLocalAdapter tx){
      return tx.putAllAsync(ctx,F.t(key,val),false,ctx.hasValArray()).chain((IgniteClosure<IgniteInternalFuture<GridCacheReturn>,Boolean>)RET2FLAG);
    }
    @Override public String toString(){
      return "replacexAsync [key=" + key + ", val="+ val+ ']';
    }
  }
);
}
