{
  A.notNull(key,"key");
  boolean statsEnabled=ctx.config().isStatisticsEnabled();
  long start=statsEnabled ? System.nanoTime() : 0L;
  if (ctx.keepBinary())   key=(K)ctx.toCacheKeyObject(key);
  T2<V,GridCacheVersion> t=(T2<V,GridCacheVersion>)get(key,!ctx.keepBinary(),true);
  CacheEntry<K,V> val=t != null ? new CacheEntryImplEx<>(ctx.keepBinary() ? (K)ctx.unwrapBinaryIfNeeded(key,true,false) : key,t.get1(),t.get2()) : null;
  if (ctx.config().getInterceptor() != null) {
