{
  Collection<ClusterNode> nodes=ctx.grid().cluster().forCacheNodes(name(),true,true,false).nodes();
  if (!nodes.isEmpty()) {
    ctx.kernalContext().task().setThreadContext(TC_SUBGRID,nodes);
    return ctx.kernalContext().task().execute(new ClearTask(ctx.name(),ctx.affinity().affinityTopologyVersion(),keys),null);
  }
 else   return new GridFinishedFuture<>();
}
