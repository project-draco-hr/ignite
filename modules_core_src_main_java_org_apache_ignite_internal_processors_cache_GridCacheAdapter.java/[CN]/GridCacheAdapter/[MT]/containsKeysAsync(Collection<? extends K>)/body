{
  A.notNull(keys,"keys");
  return getAllAsync(keys,false,false,null,null,null,false,true).chain(new CX1<IgniteInternalFuture<Map<K,V>>,Boolean>(){
    @Override public Boolean applyx(    IgniteInternalFuture<Map<K,V>> fut) throws IgniteCheckedException {
      Map<K,V> kvMap=fut.get();
      Map<K,Boolean> res=U.newHashMap(kvMap.size());
      for (      Map.Entry<K,V> entry : kvMap.entrySet())       if (entry.getValue() == null)       return false;
      return true;
    }
  }
);
}
