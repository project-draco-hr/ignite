{
  CacheOperationContext opCtx=ctx.operationContextPerCall();
  subjId=ctx.subjectIdPerCall(subjId,opCtx);
  return getAllAsync(keys,opCtx == null || !opCtx.skipStore(),entry,!skipTx,subjId,taskName,deserializePortable,forcePrimary,skipVals ? null : expiryPolicy(opCtx != null ? opCtx.expiry() : null),skipVals,canRemap);
}
