{
  return getAllAsync(Collections.singletonList(key),false,null,false,null,null,false,false,null,true).chain(new CX1<IgniteFuture<Map<K,V>>,Boolean>(){
    @Override public Boolean applyx(    IgniteFuture<Map<K,V>> fut) throws IgniteCheckedException {
      return fut.get().get(key) != null;
    }
  }
);
}
