{
  return getAllAsync(Collections.singletonList(key),false,false,null,null,null,false,true).chain(new CX1<IgniteInternalFuture<Map<K,V>>,Boolean>(){
    @Override public Boolean applyx(    IgniteInternalFuture<Map<K,V>> fut) throws IgniteCheckedException {
      return fut.get().get(key) != null;
    }
  }
);
}
