{
  try {
    return getAsync(key,!ctx.config().isReadFromBackup(),false,null,taskName,deserializeBinary,false,true,needVer).get();
  }
 catch (  IgniteException e) {
    if (e.getCause(IgniteCheckedException.class) != null)     throw e.getCause(IgniteCheckedException.class);
 else     throw e;
  }
}
