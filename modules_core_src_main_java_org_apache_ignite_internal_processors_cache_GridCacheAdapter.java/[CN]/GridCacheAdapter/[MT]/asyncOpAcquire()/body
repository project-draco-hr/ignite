{
  try {
    if (asyncOpsSem != null)     asyncOpsSem.acquire();
    return null;
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    return new GridFinishedFutureEx<>(new IgniteInterruptedCheckedException("Failed to wait for asynchronous " + "operation permit (thread got interrupted).",e));
  }
}
