{
  if (F.isEmpty(keys))   return;
  if (keyCheck)   validateCacheKeys(keys);
  GridCacheVersion obsoleteVer=ctx.versions().next();
  for (  K key : keys) {
    KeyCacheObject cacheKey=ctx.toCacheKeyObject(key);
    GridCacheEntryEx e=peekEx(cacheKey);
    try {
      if (e != null)       e.clear(obsoleteVer,readers,null);
    }
 catch (    IgniteCheckedException ex) {
      U.error(log,"Failed to clearLocally entry (will continue to clearLocally other entries): " + e,ex);
    }
  }
}
