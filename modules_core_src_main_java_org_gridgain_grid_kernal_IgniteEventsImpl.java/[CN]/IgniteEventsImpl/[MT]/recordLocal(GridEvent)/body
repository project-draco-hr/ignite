{
  A.notNull(evt,"evt");
  if (evt.type() <= 1000)   throw new IllegalArgumentException("All types in range from 1 to 1000 are reserved for " + "internal GridGain events [evtType=" + evt.type() + ", evt="+ evt+ ']');
  guard();
  try {
    ctx.event().record(evt);
  }
  finally {
    unguard();
  }
}
