{
  MessageWriteState state=MessageWriteState.get();
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 0:
    if (!writer.writeByteArray("errBytes",errBytes))     return false;
  state.increment();
case 1:
if (!writer.writeIgniteUuid("fileId",fileId)) return false;
state.increment();
case 2:
if (!writer.writeLong("id",id)) return false;
state.increment();
}
return true;
}
