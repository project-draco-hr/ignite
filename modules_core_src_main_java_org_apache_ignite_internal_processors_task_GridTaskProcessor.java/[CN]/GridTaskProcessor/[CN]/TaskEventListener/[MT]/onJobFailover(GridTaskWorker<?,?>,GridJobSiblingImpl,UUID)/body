{
  GridIoManager ioMgr=ctx.io();
  if (worker.getSession().isFullSupport()) {
    ioMgr.removeMessageListener(sib.taskTopic(),msgLsnr);
synchronized (worker.getSession()) {
      sib.nodeId(nodeId);
    }
    ioMgr.addMessageListener(sib.taskTopic(),msgLsnr);
  }
 else {
synchronized (worker.getSession()) {
      sib.nodeId(nodeId);
    }
  }
}
