{
  checkObsolete();
  ReaderId<K,V>[] rdrs=this.rdrs;
  int readerIdx=-1;
  for (int i=0; i < rdrs.length; i++) {
    if (rdrs[i].nodeId().equals(nodeId)) {
      readerIdx=i;
      break;
    }
  }
  if (readerIdx == -1 || (rdrs[readerIdx].messageId() > msgId && msgId >= 0))   return false;
  if (rdrs.length == 1)   this.rdrs=ReaderId.EMPTY_ARRAY;
 else {
    ReaderId<K,V>[] newRdrs=Arrays.copyOf(rdrs,rdrs.length - 1);
    System.arraycopy(rdrs,readerIdx + 1,newRdrs,readerIdx,rdrs.length - readerIdx - 1);
    this.rdrs=newRdrs;
  }
  return true;
}
