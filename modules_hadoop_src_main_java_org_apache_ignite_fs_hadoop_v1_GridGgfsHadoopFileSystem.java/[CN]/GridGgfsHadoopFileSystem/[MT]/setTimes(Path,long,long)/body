{
  enterBusy();
  try {
    A.notNull(p,"p");
    if (mode(p) == PROXY) {
      if (secondaryFs == null) {
        assert mgmt;
        return;
      }
      secondaryFs.setTimes(toSecondary(p),mtime,atime);
    }
 else {
      IgniteFsPath path=convert(p);
      rmtClient.setTimes(path,atime,mtime);
    }
  }
  finally {
    leaveBusy();
  }
}
