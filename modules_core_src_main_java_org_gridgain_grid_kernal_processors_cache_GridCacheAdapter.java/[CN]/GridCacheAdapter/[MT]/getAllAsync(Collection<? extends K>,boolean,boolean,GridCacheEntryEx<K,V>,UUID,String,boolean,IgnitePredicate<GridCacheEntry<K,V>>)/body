{
  GridCacheProjectionImpl<K,V> prj=ctx.projectionPerCall();
  subjId=ctx.subjectIdPerCall(subjId,prj);
  ExpiryPolicy expiryPlc=prj != null ? prj.expiry() : null;
  if (expiryPlc == null)   expiryPlc=ctx.expiry();
  return getAllAsync(keys,entry,!skipTx,subjId,taskName,deserializePortable,forcePrimary,GridCacheAccessExpiryPolicy.forPolicy(expiryPlc),filter);
}
