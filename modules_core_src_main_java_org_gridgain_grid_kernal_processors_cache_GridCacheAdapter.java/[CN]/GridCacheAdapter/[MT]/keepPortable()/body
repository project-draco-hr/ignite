{
  if (!ctx.portableEnabled())   throw new IllegalStateException("GridCacheProjection.keepPortable() is called for " + "cache which doesn't work in portable mode. Consider enabling portable mode via " + "GridCacheConfiguration.setPortableEnabled property.");
  GridCacheProjectionImpl<K1,V1> prj=new GridCacheProjectionImpl<>((GridCacheProjection<K1,V1>)this,(GridCacheContext<K1,V1>)ctx,null,null,null,null,true);
  return new GridCacheProxyImpl<>((GridCacheContext<K1,V1>)ctx,prj,prj);
}
