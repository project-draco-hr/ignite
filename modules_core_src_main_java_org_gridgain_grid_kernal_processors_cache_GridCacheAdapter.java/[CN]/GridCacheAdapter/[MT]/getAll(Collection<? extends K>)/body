{
  boolean statsEnabled=ctx.config().isStatisticsEnabled();
  long start=statsEnabled ? System.nanoTime() : 0L;
  Map<K,V> map=getAll(keys,true,null);
  if (ctx.config().getInterceptor() != null)   map=interceptGet(keys,map);
  if (statsEnabled)   ctx.cache().metrics0().addGetTimeNanos(System.nanoTime() - start);
  return map;
}
