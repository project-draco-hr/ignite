{
  if (!ctx.isNear()) {
    ctx.io().addHandler(ctx.cacheId(),GridCacheOptimisticCheckPreparedTxRequest.class,new CI2<UUID,GridCacheOptimisticCheckPreparedTxRequest<K,V>>(){
