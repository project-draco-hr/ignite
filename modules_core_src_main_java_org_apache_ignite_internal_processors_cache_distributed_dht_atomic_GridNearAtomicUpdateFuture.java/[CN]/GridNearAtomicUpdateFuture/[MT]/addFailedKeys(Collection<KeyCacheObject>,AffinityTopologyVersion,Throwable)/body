{
  CachePartialUpdateCheckedException err0=this.err;
  if (err0 == null)   err0=this.err=new CachePartialUpdateCheckedException("Failed to update keys (retry update if possible).");
  Collection<Object> keys=new ArrayList<>(failedKeys.size());
