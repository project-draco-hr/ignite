{
  Map<String,V1> m=null;
  GridDhtCacheEntry<K,V> de=dht().peekExx(key);
  if (de != null)   m=de.allMeta();
  if (ctx.isNear()) {
    GridNearCacheEntry<K,V> ne=near().peekExx(key);
    if (ne != null) {
      Map<String,V1> m1=ne.allMeta();
      if (m == null)       m=m1;
 else       if (!m1.isEmpty()) {
        for (        Map.Entry<String,V1> e1 : m1.entrySet())         if (!m.containsKey(e1.getKey()))         m.put(e1.getKey(),e1.getValue());
      }
    }
  }
  return m;
}
