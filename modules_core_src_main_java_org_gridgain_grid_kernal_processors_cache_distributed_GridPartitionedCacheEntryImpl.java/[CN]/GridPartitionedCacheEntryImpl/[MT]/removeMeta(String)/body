{
  V1 v=null;
  GridDhtCacheEntry<K,V> de=dht().peekExx(key);
  if (de != null)   v=(V1)de.removeMeta(name);
  if (ctx.isNear()) {
    GridNearCacheEntry<K,V> ne=near().peekExx(key);
    if (ne != null) {
      V1 v1=(V1)ne.removeMeta(name);
      if (v == null)       v=v1;
    }
  }
  return v;
}
