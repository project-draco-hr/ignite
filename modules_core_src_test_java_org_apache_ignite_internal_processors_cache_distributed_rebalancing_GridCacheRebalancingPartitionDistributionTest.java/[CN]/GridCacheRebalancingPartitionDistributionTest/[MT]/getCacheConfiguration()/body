{
  return new CacheConfiguration<Integer,Integer>(CACHE_NAME).setAtomicityMode(CacheAtomicityMode.TRANSACTIONAL).setCacheMode(CacheMode.PARTITIONED).setBackups(1).setAffinity(new RendezvousAffinityFunction(true,271)).setAtomicWriteOrderMode(CacheAtomicWriteOrderMode.CLOCK).setRebalanceMode(CacheRebalanceMode.SYNC).setWriteSynchronizationMode(CacheWriteSynchronizationMode.FULL_SYNC);
}
