{
  if (cacheMsg instanceof GridDhtTxPrepareRequest)   return ((GridDhtTxPrepareRequest)cacheMsg).version();
 else   if (cacheMsg instanceof GridDhtTxPrepareResponse)   return ((GridDhtTxPrepareResponse)cacheMsg).version();
 else   if (cacheMsg instanceof GridDhtTxFinishRequest)   return ((GridDhtTxFinishRequest)cacheMsg).version();
 else   if (cacheMsg instanceof GridDhtTxFinishResponse)   return ((GridDhtTxFinishResponse)cacheMsg).xid();
  return null;
}
