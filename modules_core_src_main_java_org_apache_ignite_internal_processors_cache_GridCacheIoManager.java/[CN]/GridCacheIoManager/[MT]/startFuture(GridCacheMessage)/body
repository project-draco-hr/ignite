{
  int cacheId=cacheMsg.cacheId();
  if (cacheId != 0)   return cctx.cacheContext(cacheId).preloader().startFuture();
 else {
    if (F.eq(cacheMsg.topologyVersion(),AffinityTopologyVersion.NONE))     return new GridFinishedFuture<>();
    return cctx.preloadersStartFuture(cacheMsg.topologyVersion());
  }
}
