{
  if (txId(cacheMsg) != null) {
    builder.append("txId=").append(txId(cacheMsg)).append(", dhtTxId=").append(dhtTxId(cacheMsg)).append(", msg=").append(cacheMsg);
  }
 else   if (atomicFututeId(cacheMsg) != null) {
    builder.append("futId=").append(atomicFututeId(cacheMsg)).append(", writeVer=").append(atomicWriteVersion(cacheMsg)).append(", msg=").append(cacheMsg);
  }
 else   builder.append("msg=").append(cacheMsg);
  builder.append(", node=").append(nodeId);
  return builder;
}
