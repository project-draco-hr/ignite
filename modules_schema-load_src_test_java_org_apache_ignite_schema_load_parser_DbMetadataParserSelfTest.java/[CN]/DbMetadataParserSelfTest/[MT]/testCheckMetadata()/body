{
  assertEquals("Metadata should contain 3 element",3,pojoLst.size());
  Iterator<PojoDescriptor> it=pojoLst.iterator();
  PojoDescriptor schema=it.next();
  assertTrue("First element is schema description. Its class name should be empty",schema.valueClassName().isEmpty());
  checkType(it.next());
  checkType(it.next());
}
