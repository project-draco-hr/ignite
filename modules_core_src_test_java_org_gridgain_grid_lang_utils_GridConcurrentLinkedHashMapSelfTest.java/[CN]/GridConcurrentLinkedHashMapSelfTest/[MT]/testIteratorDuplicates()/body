{
  Map<Integer,String> tst=new ConcurrentLinkedHashMap<>();
  for (int i=0; i < 10; i++) {
    tst.put(i,"val" + i);
  }
  Iterator<Integer> it=tst.keySet().iterator();
  for (int i=0; i < 5; i++) {
    assertTrue("Not enough elements",it.hasNext());
    assertEquals(i,it.next().intValue());
  }
  for (int i=0; i < 5; i++) {
    tst.remove(i);
    tst.put(i,"val" + i);
  }
  for (int i=5; i < 10; i++) {
    assertTrue("Not enough elements",it.hasNext());
    assertEquals(i,it.next().intValue());
  }
  assertFalse("Duplicate key",it.hasNext());
}
