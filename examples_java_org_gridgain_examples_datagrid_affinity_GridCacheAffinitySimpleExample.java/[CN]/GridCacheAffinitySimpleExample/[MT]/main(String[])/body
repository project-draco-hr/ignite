{
  try (Grid g=GridGain.start("examples/config/example-cache.xml")){
    GridCache<String,String> cache=g.cache("partitioned");
    if (cache == null) {
      System.err.println("Cache with name 'partitioned' not found (is configuration correct?)");
      return;
    }
    populate(cache);
    visit(g,cache);
  }
 }
