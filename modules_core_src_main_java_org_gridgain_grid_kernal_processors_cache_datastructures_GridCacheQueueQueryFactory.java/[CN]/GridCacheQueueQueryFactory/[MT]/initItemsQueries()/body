{
  itemsQry=qry.createSqlQuery(GridCacheQueueItemImpl.class,"qid=? order by seq asc");
  itemsAtPosQry=qry.createSqlQuery(GridCacheQueueItemImpl.class,"select * from " + "(select *, rownum as r from " + "(select * from GridCacheQueueItemImpl where qid=? "+ "order by seq asc" + ')' + ") where r-1 in (select * from table(x int=?))");
}
