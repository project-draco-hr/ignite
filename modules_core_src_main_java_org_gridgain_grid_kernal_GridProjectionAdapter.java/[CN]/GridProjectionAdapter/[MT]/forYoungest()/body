{
  GridNode youngest=null;
  long maxOrder=Long.MIN_VALUE;
  for (  GridNode n : nodes()) {
    if (n.order() > maxOrder) {
      youngest=n;
      maxOrder=n.order();
    }
  }
  if (maxOrder == Long.MIN_VALUE)   throw new GridRuntimeException("Grid projection is empty.");
  return forNode(youngest);
}
