{
  GridNode oldest=null;
  long minOrder=Long.MAX_VALUE;
  for (  GridNode n : nodes()) {
    if (n.order() < minOrder) {
      oldest=n;
      minOrder=n.order();
    }
  }
  if (minOrder == Long.MAX_VALUE)   throw new GridRuntimeException("Grid projection is empty.");
  return forNode(oldest);
}
