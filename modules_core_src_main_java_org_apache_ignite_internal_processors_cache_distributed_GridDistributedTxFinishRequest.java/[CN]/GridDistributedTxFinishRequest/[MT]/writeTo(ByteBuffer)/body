{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 8:
    if (!writer.writeMessage("baseVer",baseVer != null ? baseVer.clone() : null))     return false;
  state++;
case 9:
if (!writer.writeBoolean("commit",commit)) return false;
state++;
case 10:
if (!writer.writeMessage("commitVer",commitVer != null ? commitVer.clone() : null)) return false;
state++;
case 11:
if (!writer.writeIgniteUuid("futId",futId)) return false;
state++;
case 12:
if (!writer.writeByteArray("grpLockKeyBytes",grpLockKeyBytes)) return false;
state++;
case 13:
if (!writer.writeBoolean("invalidate",invalidate)) return false;
state++;
case 14:
if (!writer.writeCollection("recoveryWritesBytes",recoveryWritesBytes,byte[].class)) return false;
state++;
case 15:
if (!writer.writeBoolean("syncCommit",syncCommit)) return false;
state++;
case 16:
if (!writer.writeBoolean("syncRollback",syncRollback)) return false;
state++;
case 17:
if (!writer.writeBoolean("sys",sys)) return false;
state++;
case 18:
if (!writer.writeLong("threadId",threadId)) return false;
state++;
case 19:
if (!writer.writeInt("txSize",txSize)) return false;
state++;
case 20:
if (!writer.writeCollection("writeEntriesBytes",writeEntriesBytes,byte[].class)) return false;
state++;
}
return true;
}
