{
  System.out.println();
  System.out.println(">>> Starting putAll-getAll example.");
  Ignite g=GridGain.grid();
  final GridCache<Integer,String> cache=g.cache(CACHE_NAME);
  final int keyCnt=20;
  Map<Integer,String> batch=new HashMap<>();
  for (int i=0; i < keyCnt; i++)   batch.put(i,"bulk-" + Integer.toString(i));
  cache.putAll(batch);
  System.out.println(">>> Bulk-stored values in cache.");
  Map<Integer,String> vals=cache.getAll(batch.keySet());
  for (  Map.Entry<Integer,String> e : vals.entrySet())   System.out.println("Got entry [key=" + e.getKey() + ", val="+ e.getValue()+ ']');
}
