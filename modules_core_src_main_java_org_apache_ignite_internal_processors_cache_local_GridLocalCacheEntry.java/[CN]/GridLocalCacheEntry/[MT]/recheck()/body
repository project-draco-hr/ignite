{
  GridCacheMvccCandidate prev=null;
  GridCacheMvccCandidate owner=null;
synchronized (this) {
    GridCacheMvcc mvcc=mvccExtras();
    if (mvcc != null) {
      prev=mvcc.localOwner();
      owner=mvcc.recheck();
      if (mvcc.isEmpty())       mvccExtras(null);
    }
  }
  checkOwnerChanged(prev,owner);
  return owner;
}
