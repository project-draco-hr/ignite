{
  StringBuilder sb=new StringBuilder(getClass().getSimpleName());
  sb.append(" [nodeHash=").append(nodeHash).append(", hashIdRslvr=").append(hashIdRslvr).append(", parts=").append(parts).append(", addedNodes=").append(addedNodes).append(", backupFilter=").append(backupFilter).append("]");
