{
  Collection<String> keys=new ArrayList<>(KEYS_CNT);
  int numKey=0;
  while (keys.size() < 30) {
    String key=String.valueOf(numKey);
    if (nodeType == NEAR_NODE) {
      if (!cache(0).affinity().isPrimaryOrBackup(grid(0).localNode(),key))       keys.add(key);
    }
 else     if (nodeType == PRIMARY_NODE) {
      if (cache(0).affinity().isPrimary(grid(0).localNode(),key))       keys.add(key);
    }
 else     if (nodeType == BACKUP_NODE) {
      if (cache(0).affinity().isBackup(grid(0).localNode(),key))       keys.add(key);
    }
    numKey++;
  }
  return keys;
}
