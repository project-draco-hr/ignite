{
  if (failedKeys == null)   failedKeys=new ConcurrentLinkedQueue<>();
  failedKeys.add(key);
  if (err == null)   err=new GridMultiException("Failed to update keys on primary node.");
  if (e instanceof GridMultiException) {
    for (    Throwable th : ((GridMultiException)e).nestedCauses())     err.add(th);
  }
 else   err.add(e);
}
