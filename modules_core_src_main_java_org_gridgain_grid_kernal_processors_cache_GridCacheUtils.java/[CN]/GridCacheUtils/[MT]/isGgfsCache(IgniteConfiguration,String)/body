{
  IgniteFsConfiguration[] ggfsCfgs=cfg.getGgfsConfiguration();
  if (ggfsCfgs != null) {
    for (    IgniteFsConfiguration ggfsCfg : ggfsCfgs) {
      if (ggfsCfg != null && (F.eq(cacheName,ggfsCfg.getDataCacheName()) || F.eq(cacheName,ggfsCfg.getMetaCacheName())))       return true;
    }
  }
  return false;
}
