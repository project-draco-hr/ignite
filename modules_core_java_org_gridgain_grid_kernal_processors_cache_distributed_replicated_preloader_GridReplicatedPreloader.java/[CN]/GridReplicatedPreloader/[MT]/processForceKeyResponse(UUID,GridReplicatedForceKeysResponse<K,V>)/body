{
  if (!enterBusy())   return;
  try {
    GridReplicatedForceKeysFuture<K,V> f=forceKeyFuts.get(res.futureId());
    if (f == null) {
      if (log.isDebugEnabled())       log.debug("Receive force key response for unknown future (is it duplicate?) [nodeId=" + nodeId + ", res="+ res+ ']');
      return;
    }
    f.onResult(res);
  }
  finally {
    leaveBusy();
  }
}
