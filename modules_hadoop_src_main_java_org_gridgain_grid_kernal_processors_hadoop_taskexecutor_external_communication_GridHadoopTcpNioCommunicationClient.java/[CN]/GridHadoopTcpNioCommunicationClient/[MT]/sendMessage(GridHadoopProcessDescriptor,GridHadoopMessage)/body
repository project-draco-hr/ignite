{
  if (closed())   throw new IgniteCheckedException("Client was closed: " + this);
  GridNioFuture<?> fut=ses.send(msg);
  if (fut.isDone()) {
    try {
      fut.get();
    }
 catch (    IOException e) {
      throw new IgniteCheckedException("Failed to send message [client=" + this + ']',e);
    }
  }
}
