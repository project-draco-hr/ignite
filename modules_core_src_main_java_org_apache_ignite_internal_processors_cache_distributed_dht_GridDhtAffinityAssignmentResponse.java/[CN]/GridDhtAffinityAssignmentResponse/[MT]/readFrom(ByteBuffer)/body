{
  reader.setBuffer(buf);
  if (!super.readFrom(buf))   return false;
switch (state) {
case 3:
    affAssignmentBytes=reader.readByteArray("affAssignmentBytes");
  if (!reader.isLastRead())   return false;
state++;
case 4:
topVer=reader.readLong("topVer");
if (!reader.isLastRead()) return false;
state++;
}
return true;
}
