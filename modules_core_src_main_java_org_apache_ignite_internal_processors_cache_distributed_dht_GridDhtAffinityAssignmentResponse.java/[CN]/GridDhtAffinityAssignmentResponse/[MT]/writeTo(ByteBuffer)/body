{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 3:
    if (!writer.writeByteArray("affAssignmentBytes",affAssignmentBytes))     return false;
  state++;
case 4:
if (!writer.writeLong("topVer",topVer)) return false;
state++;
}
return true;
}
