{
  cacheName=U.readString(in);
  topic=in.readObject();
  boolean b=in.readBoolean();
  if (b)   filterDep=(DeployableObject)in.readObject();
 else   filter=(IgnitePredicate<org.gridgain.grid.cache.query.GridCacheContinuousQueryEntry<K,V>>)in.readObject();
  b=in.readBoolean();
  if (b)   prjPredDep=(DeployableObject)in.readObject();
 else   prjPred=(IgnitePredicate<GridCacheEntry<K,V>>)in.readObject();
  internal=in.readBoolean();
  entryLsnr=in.readBoolean();
  sync=in.readBoolean();
  oldVal=in.readBoolean();
  taskHash=in.readInt();
  keepPortable=in.readBoolean();
}
