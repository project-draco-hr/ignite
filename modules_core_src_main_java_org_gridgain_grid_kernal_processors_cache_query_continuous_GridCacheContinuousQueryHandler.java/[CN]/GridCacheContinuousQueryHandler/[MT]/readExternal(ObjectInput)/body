{
  cacheName=U.readString(in);
  topic=in.readObject();
  boolean b=in.readBoolean();
  if (b)   filterDep=(DeployableObject)in.readObject();
 else   filter=(GridPredicate<org.gridgain.grid.cache.query.GridCacheContinuousQueryEntry<K,V>>)in.readObject();
  b=in.readBoolean();
  if (b)   prjPredDep=(DeployableObject)in.readObject();
 else   prjPred=(GridPredicate<GridCacheEntry<K,V>>)in.readObject();
  internal=in.readBoolean();
}
