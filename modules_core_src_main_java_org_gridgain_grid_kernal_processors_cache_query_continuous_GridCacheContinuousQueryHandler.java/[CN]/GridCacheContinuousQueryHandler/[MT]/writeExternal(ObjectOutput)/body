{
  U.writeString(out,cacheName);
  out.writeObject(topic);
  boolean b=filterDep != null;
  out.writeBoolean(b);
  if (b)   out.writeObject(filterDep);
 else {
    boolean compatibilityMode=GridContinuousProcessor.COMPATIBILITY_MODE.get();
    if (compatibilityMode) {
      GridBiPredicate<K,V> f;
      if (filter instanceof GridCacheContinuousQueryAdapter.FilterWrapper)       f=((GridCacheContinuousQueryAdapter.FilterWrapper<K,V>)filter).filter();
 else       f=new FilterConverter<>(filter);
      out.writeObject(f);
    }
 else     out.writeObject(filter);
  }
  b=prjPredDep != null;
  out.writeBoolean(b);
  if (b)   out.writeObject(prjPredDep);
 else   out.writeObject(prjPred);
  out.writeBoolean(internal);
}
