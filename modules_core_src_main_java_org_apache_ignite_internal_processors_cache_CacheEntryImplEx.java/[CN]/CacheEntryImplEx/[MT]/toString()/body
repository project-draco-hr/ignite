{
  String res="CacheEntry [key=" + getKey() + ", val="+ getValue();
  if (ver != null && ver != GET_ENTRY_INVALID_VER_AFTER_GET && ver != GET_ENTRY_INVALID_VER_UPDATED) {
    res+=", topVer=" + ver.topologyVersion() + ", nodeOrder="+ ver.nodeOrder()+ ", order="+ ver.order()+ ", updateTime="+ ver.globalTime();
  }
 else   res+=", ver=n/a";
  return res + ']';
}
