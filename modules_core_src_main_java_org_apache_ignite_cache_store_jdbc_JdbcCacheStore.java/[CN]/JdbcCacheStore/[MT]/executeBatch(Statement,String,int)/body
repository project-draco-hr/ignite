{
  int[] rowCounts=stmt.executeBatch();
  int numOfRowCnt=rowCounts.length;
  if (numOfRowCnt != batchSz)   log.warning("JDBC driver did not return the expected number of row counts," + " actual row count: " + numOfRowCnt + " expected: "+ batchSz);
  for (  int rowCount : rowCounts)   if (rowCount != 1) {
    log.warning("Batch " + stmtType + " returned unexpected row count from "+ stmtType+ " statement");
  }
}
