{
  String cacheName=session().cacheName();
  EntryMapping em=cacheMappings(cacheName).get(keyTypeId);
  if (em == null)   throw new CacheException("Failed to find mapping description [table = " + em.fullTableName() + ", key="+ key+ ", cache="+ (cacheName != null ? cacheName : "<default>")+ "]");
  return em;
}
