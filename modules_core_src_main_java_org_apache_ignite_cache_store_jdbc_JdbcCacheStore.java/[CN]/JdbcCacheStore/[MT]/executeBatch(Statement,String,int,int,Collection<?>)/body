{
  int[] rowCounts=stmt.executeBatch();
  int numOfRowCnt=rowCounts.length;
  if (numOfRowCnt != prepared)   log.warning("JDBC driver did not return the expected number of updated row counts," + " actual row count: " + numOfRowCnt + " expected: "+ prepared);
  Object[] arr=null;
  for (int i=0; i < numOfRowCnt; i++)   if (rowCounts[i] != 1) {
    if (arr == null)     arr=objects.toArray();
    log.warning("Batch " + stmtType + " returned unexpected updated row count for: "+ arr[fromIdx + i]);
  }
}
