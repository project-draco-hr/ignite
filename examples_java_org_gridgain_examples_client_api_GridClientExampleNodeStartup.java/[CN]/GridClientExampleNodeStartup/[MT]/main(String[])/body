{
  System.setProperty(GridSystemProperties.GG_QUIET,"false");
  String springCfgPath=SSL_ENABLED ? "examples/config/example-client-ssl.xml" : "examples/config/example-client.xml";
  try (Grid g=GridGain.start(springCfgPath)){
    JOptionPane.showMessageDialog(null,new JComponent[]{new JLabel("GridGain started."),new JLabel("Press OK to stop GridGain.")},"GridGain",JOptionPane.INFORMATION_MESSAGE);
  }
 }
