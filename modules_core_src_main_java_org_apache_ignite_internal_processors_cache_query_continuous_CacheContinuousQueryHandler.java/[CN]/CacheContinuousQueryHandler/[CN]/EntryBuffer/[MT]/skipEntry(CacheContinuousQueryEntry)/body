{
  if (lastFiredCntr.get() > e.updateCounter() || e.updateCounter() == 1L) {
    e.markFiltered();
    return e;
  }
 else {
    buf.add(e.updateCounter());
    if (lastFiredCntr.get() > e.updateCounter() && buf.contains(e.updateCounter())) {
      buf.remove(e.updateCounter());
      e.markFiltered();
      return e;
    }
 else     return null;
  }
}
