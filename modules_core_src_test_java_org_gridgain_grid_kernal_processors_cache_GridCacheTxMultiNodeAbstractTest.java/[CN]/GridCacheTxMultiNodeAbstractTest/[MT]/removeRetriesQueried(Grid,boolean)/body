{
  for (int i=0; i < RETRIES; i++) {
    if (DEBUG)     grid.log().info("***");
    if (DEBUG || cntrRmvd.getAndIncrement() % LOG_FREQ == 0)     grid.log().info("*** Iteration #" + i + " ***");
    if (DEBUG)     grid.log().info("***");
    onRemoveItemQueried(putCntr,grid,i);
    if (i % 50 == 0)     ((GridKernal)grid).internalCache().context().tm().printMemoryStats();
  }
}
