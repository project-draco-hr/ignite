{
  inclEvtTypes=null;
  try {
    Ignite g=startGrid();
    IgniteEvents evts=g.events();
    evts.enableLocal(EVT_CACHE_OBJECT_PUT);
    evts.disableLocal(EVT_CACHE_OBJECT_PUT);
    for (    int evtType : evts.enabledEvents())     assertFalse(evtType == EVT_CACHE_OBJECT_PUT);
  }
  finally {
    stopAllGrids();
  }
}
