{
  if (type == TcpCommunicationSpi.NODE_ID_MSG_TYPE)   return new TcpCommunicationSpi.NodeIdMessage();
 else   if (type == TcpCommunicationSpi.RECOVERY_LAST_ID_MSG_TYPE)   return new TcpCommunicationSpi.RecoveryLastReceivedMessage();
 else   if (type == TcpCommunicationSpi.HANDSHAKE_MSG_TYPE)   return new TcpCommunicationSpi.HandshakeMessage();
 else   return create0(type);
}
