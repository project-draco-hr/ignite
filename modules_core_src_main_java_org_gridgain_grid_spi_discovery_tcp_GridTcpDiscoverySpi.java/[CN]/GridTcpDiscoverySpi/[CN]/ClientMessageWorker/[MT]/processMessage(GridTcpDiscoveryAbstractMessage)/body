{
  try {
    if (log.isDebugEnabled())     log.debug("Redirecting message to client [sock=" + sock + ", locNodeId="+ locNodeId+ ", rmtNodeId="+ nodeId+ ", msg="+ msg+ ']');
    writeToSocket(sock,msg);
  }
 catch (  GridException|IOException e) {
    if (log.isDebugEnabled())     U.error(log,"Caught exception on redirecting message to client [sock=" + sock + ", locNodeId="+ locNodeId+ ", rmtNodeId="+ nodeId+ ", msg="+ msg+ ']',e);
  }
}
