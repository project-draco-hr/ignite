{
  log.info("Test cache [mode=" + ccfg.getCacheMode() + ", sync="+ ccfg.getWriteSynchronizationMode()+ ", backups="+ ccfg.getBackups()+ ", memMode="+ ccfg.getMemoryMode()+ ", near="+ (ccfg.getNearConfiguration() != null)+ ", store="+ ccfg.isWriteThrough()+ ", evictPlc="+ (ccfg.getEvictionPolicy() != null)+ ", swap="+ ccfg.isSwapEnabled()+ ", maxOffheap="+ ccfg.getOffHeapMaxMemory()+ ']');
}
