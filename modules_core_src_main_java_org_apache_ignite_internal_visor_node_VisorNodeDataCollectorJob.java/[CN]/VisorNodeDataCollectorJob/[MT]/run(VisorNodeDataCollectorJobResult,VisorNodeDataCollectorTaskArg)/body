{
  res.gridName(ignite.name());
  res.topologyVersion(ignite.cluster().topologyVersion());
  long start0=U.currentTimeMillis();
  events(res,arg);
  if (debug)   start0=log(ignite.log(),"Collected events",getClass(),start0);
  caches(res,arg);
  if (debug)   start0=log(ignite.log(),"Collected caches",getClass(),start0);
  igfs(res);
  if (debug)   log(ignite.log(),"Collected igfs",getClass(),start0);
  res.errorCount(ignite.context().exceptionRegistry().errorCount());
  return res;
}
