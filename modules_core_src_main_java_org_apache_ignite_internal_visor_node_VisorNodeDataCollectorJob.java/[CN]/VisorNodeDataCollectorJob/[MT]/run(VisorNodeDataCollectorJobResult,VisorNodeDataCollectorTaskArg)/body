{
  res.gridName(ignite.name());
  res.topologyVersion(ignite.topologyVersion());
  long start0=U.currentTimeMillis();
  events(res,arg);
  if (debug)   start0=log(ignite.log(),"Collected events",getClass(),start0);
  caches(res,arg);
  if (debug)   start0=log(ignite.log(),"Collected caches",getClass(),start0);
  ggfs(res);
  if (debug)   start0=log(ignite.log(),"Collected igfs",getClass(),start0);
  streamers(res);
  if (debug)   log(ignite.log(),"Collected streamers",getClass(),start0);
  return res;
}
