{
  res.gridName(g.name());
  res.topologyVersion(g.topologyVersion());
  long start0=U.currentTimeMillis();
  events(res,arg);
  if (debug)   start0=log(g.log(),"Collected events",getClass(),start0);
  caches(res,arg);
  if (debug)   start0=log(g.log(),"Collected caches",getClass(),start0);
  ggfs(res);
  if (debug)   start0=log(g.log(),"Collected ggfs",getClass(),start0);
  streamers(res);
  if (debug)   log(g.log(),"Collected streamers",getClass(),start0);
  return res;
}
