{
  commState.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!commState.typeWritten) {
    if (!commState.putByte(directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 5:
    if (!commState.putByteArray(errBytes))     return false;
  commState.idx++;
case 6:
if (!commState.putGridUuid(miniId)) return false;
commState.idx++;
case 7:
if (!commState.putLong(nearThreadId)) return false;
commState.idx++;
}
return true;
}
