{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 5:
    if (!writer.writeByteArray("errBytes",errBytes))     return false;
  state++;
case 6:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
state++;
case 7:
if (!writer.writeLong("nearThreadId",nearThreadId)) return false;
state++;
}
return true;
}
