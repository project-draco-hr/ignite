{
  String name=method.getName();
  if (!names.contains(name) && canAddMethod(method)) {
    if (!Modifier.isPublic(method.getModifiers()))     addTest(warning("Test method isn\'t public: " + method.getName() + "("+ theClass.getCanonicalName()+ ")"));
 else {
      names.add(name);
      addTest(createTest(theClass,name));
      return true;
    }
  }
  return false;
}
