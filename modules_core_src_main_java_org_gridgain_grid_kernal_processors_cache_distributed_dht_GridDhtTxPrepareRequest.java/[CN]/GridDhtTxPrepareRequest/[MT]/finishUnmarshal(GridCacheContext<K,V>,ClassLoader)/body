{
  super.finishUnmarshal(ctx,ldr);
  if (ownedBytes != null && owned == null)   owned=ctx.marshaller().unmarshal(ownedBytes,ldr);
  if (nearWritesBytes != null) {
    nearWrites=new ArrayList<>(nearWritesBytes.size());
    for (    byte[] arr : nearWritesBytes)     nearWrites.add(ctx.marshaller().<GridCacheTxEntry<K,V>>unmarshal(arr,ldr));
    unmarshalTx(nearWrites,ctx,ldr);
  }
}
