{
  super.prepareMarshal(ctx);
  if (ownedBytes == null && owned != null) {
    ownedBytes=CU.marshal(ctx,owned);
    if (ctx.deploymentEnabled()) {
      for (      K k : owned.keySet())       prepareObject(k,ctx);
    }
  }
  if (nearWrites != null) {
    marshalTx(nearWrites,ctx);
    nearWritesBytes=new ArrayList<>(nearWrites.size());
    for (    GridCacheTxEntry<K,V> e : nearWrites)     nearWritesBytes.add(ctx.marshaller().marshal(e));
  }
}
