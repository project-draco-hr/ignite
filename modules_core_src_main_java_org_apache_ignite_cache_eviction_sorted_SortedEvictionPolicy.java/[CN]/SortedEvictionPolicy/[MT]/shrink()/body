{
  int max=this.max;
  int startSize=set.sizex();
  if (startSize > max) {
    for (int i=max; i < startSize && set.sizex() > max; i++) {
      Holder<K,V> h=set.pollFirst();
      if (h == null)       break;
      EvictableEntry<K,V> entry=h.entry;
      if (h.order > 0 && !entry.evict()) {
        entry.removeMeta();
        touch(entry);
      }
    }
  }
}
