{
  fillCaches();
  GridCacheProjection<Integer,FactPurchase> prj=ignite.<Integer,FactPurchase>cache("partitioned").projection(new IgnitePredicate<GridCacheEntry<Integer,FactPurchase>>(){
    @Override public boolean apply(    GridCacheEntry<Integer,FactPurchase> e){
      return e.getKey() > 12;
    }
  }
);
  List<Map.Entry<Integer,FactPurchase>> res=body(prj);
  check(res);
}
