{
  fillCaches();
  GridCacheTwoStepQuery q=new GridCacheTwoStepQuery("select * from _cnts_");
  q.addMapQuery("_cnts_","select count(*) cnt from \"partitioned\".FactPurchase");
  GridCacheQueriesEx<Integer,FactPurchase> qx=(GridCacheQueriesEx<Integer,FactPurchase>)ignite.<Integer,FactPurchase>cache("partitioned").queries();
  for (  List<?> row : qx.execute(q).get())   X.println("__ " + row);
}
