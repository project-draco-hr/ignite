{
  typeName=keyCls.getName();
  for (; keyCls != Object.class && keyCls != null; keyCls=keyCls.getSuperclass()) {
    for (    Field f : keyCls.getDeclaredFields()) {
      if (f.getAnnotation(AffinityKeyMapped.class) != null) {
        affKeyFieldName=f.getName();
        return;
      }
    }
  }
}
