{
  init();
  if (log.isDebugEnabled())   log.debug("Store load [key=" + key + ", tx="+ tx+ ']');
  Session ses=session(tx);
  try {
    GridCacheHibernateBlobStoreEntry entry=(GridCacheHibernateBlobStoreEntry)ses.get(GridCacheHibernateBlobStoreEntry.class,toBytes(key));
    if (entry == null)     return null;
    return fromBytes(entry.getValue());
  }
 catch (  HibernateException e) {
    rollback(ses,tx);
    throw new IgniteCheckedException("Failed to load value from cache store with key: " + key,e);
  }
 finally {
    end(ses,tx);
  }
}
