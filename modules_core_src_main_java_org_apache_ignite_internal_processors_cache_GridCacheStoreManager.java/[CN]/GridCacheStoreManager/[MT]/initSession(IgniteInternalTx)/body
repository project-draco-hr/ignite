{
  if (sesHolder == null)   return;
  SessionData ses;
  if (tx != null) {
    ses=tx.meta(SES_ATTR);
    if (ses == null) {
      ses=new SessionData(tx,cctx.name());
      tx.addMeta(SES_ATTR,ses);
    }
 else     ses.cacheName(cctx.name());
  }
 else   ses=new SessionData(null,cctx.name());
  sesHolder.set(ses);
}
