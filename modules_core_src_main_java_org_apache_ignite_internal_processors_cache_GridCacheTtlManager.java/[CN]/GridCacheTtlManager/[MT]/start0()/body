{
  boolean cleanupDisabled=cctx.kernalContext().isDaemon() || !cctx.config().isEagerTtl() || CU.isAtomicsCache(cctx.name())|| CU.isMarshallerCache(cctx.name())|| CU.isUtilityCache(cctx.name())|| (cctx.kernalContext().clientNode() && cctx.config().getNearConfiguration() == null);
  if (cleanupDisabled)   return;
  cctx.shared().ttl().register(this);
}
