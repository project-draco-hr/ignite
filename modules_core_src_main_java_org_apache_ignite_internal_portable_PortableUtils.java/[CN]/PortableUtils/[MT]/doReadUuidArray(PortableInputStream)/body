{
  int len=in.readInt();
  UUID[] arr=new UUID[len];
  for (int i=0; i < len; i++) {
    byte flag=in.readByte();
    if (flag == NULL)     arr[i]=null;
 else {
      if (flag != UUID)       throw new BinaryObjectException("Invalid flag value: " + flag);
      arr[i]=doReadUuid(in);
    }
  }
  return arr;
}
