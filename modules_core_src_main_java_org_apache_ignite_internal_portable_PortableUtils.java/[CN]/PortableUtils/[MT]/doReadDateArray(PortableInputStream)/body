{
  int len=in.readInt();
  Date[] arr=new Date[len];
  for (int i=0; i < len; i++) {
    byte flag=in.readByte();
    if (flag == NULL)     arr[i]=null;
 else {
      if (flag != DATE)       throw new BinaryObjectException("Invalid flag value: " + flag);
      arr[i]=doReadDate(in);
    }
  }
  return arr;
}
