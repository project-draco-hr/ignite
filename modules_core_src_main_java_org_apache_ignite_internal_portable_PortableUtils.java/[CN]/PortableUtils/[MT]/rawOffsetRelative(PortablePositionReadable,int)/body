{
  short flags=in.readShortPositioned(start + GridPortableMarshaller.FLAGS_POS);
  int len=length(in,start);
  if (hasSchema(flags)) {
    if (hasRaw(flags))     return in.readIntPositioned(start + len - 4);
 else     return in.readIntPositioned(start + GridPortableMarshaller.SCHEMA_OR_RAW_OFF_POS);
  }
 else   return in.readIntPositioned(start + GridPortableMarshaller.SCHEMA_OR_RAW_OFF_POS);
}
