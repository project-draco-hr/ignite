{
  int len=length(in,start);
  short flags=in.readShortPositioned(start + GridPortableMarshaller.FLAGS_POS);
  if (PortableUtils.isRawOnly(flags))   return start + in.readIntPositioned(start + GridPortableMarshaller.SCHEMA_OR_RAW_OFF_POS);
 else {
    int schemaOff=in.readIntPositioned(start + GridPortableMarshaller.SCHEMA_OR_RAW_OFF_POS);
    if ((((len - schemaOff) >> 2) & 0x1) == 0x0)     return start + schemaOff;
 else     return start + in.readIntPositioned(start + len - 4);
  }
}
