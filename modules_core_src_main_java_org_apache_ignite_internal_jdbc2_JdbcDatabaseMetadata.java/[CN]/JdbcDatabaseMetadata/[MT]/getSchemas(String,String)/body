{
  updateMetaData();
  List<List<?>> rows=new ArrayList<>(meta.size());
  for (  String schema : meta.keySet())   if (matches(schema,schemaPtrn))   rows.add(Arrays.<Object>asList(schema,null));
  return new JdbcResultSet(null,conn.createStatement0(),Collections.<String>emptyList(),Arrays.asList("TABLE_SCHEM","TABLE_CATALOG"),Arrays.asList(String.class.getName(),String.class.getName()),rows,true);
}
