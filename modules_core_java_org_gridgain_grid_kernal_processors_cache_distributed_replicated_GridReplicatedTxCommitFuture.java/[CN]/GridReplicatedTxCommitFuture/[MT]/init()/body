{
  if (isSync()) {
    for (Iterator<UUID> it=nodes.iterator(); it.hasNext(); ) {
      UUID id=it.next();
      if (cctx.discovery().node(id) == null)       it.remove();
    }
  }
  if (tx.topologyVersion() != cctx.discovery().topologyVersion())   tx.sendTransformedValues(true);
  checkLocks(null);
}
