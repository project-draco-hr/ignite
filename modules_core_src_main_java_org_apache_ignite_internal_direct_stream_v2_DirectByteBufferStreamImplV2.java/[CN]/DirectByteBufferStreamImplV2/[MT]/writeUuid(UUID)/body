{
switch (uuidState) {
case 0:
    writeBoolean(val == null);
  if (!lastFinished || val == null)   return;
uuidState++;
case 1:
writeLong(val.getMostSignificantBits());
if (!lastFinished) return;
uuidState++;
case 2:
writeLong(val.getLeastSignificantBits());
if (!lastFinished) return;
uuidState=0;
}
}
