{
  GridHadoopDefaultJobInfo jobInfo=(GridHadoopDefaultJobInfo)meta.jobInfo();
  return new GridHadoopJobStatus(meta.jobId(),meta.phase() == PHASE_COMPLETE ? meta.failCause() == null ? STATE_SUCCEEDED : STATE_FAILED : STATE_RUNNING,jobInfo.configuration().getJobName(),jobInfo.configuration().getUser(),meta.pendingSplits() != null ? meta.pendingSplits().size() : 0,meta.pendingReducers() != null ? meta.pendingReducers().size() : 0,meta.totalSplitCount(),meta.totalReducerCount(),meta.phase());
}
