{
  GridSslBasicContextFactory factory=new GridSslBasicContextFactory();
  factory.setKeyStoreFilePath(U.resolveGridGainPath(GridTestProperties.getProperty("ssl.keystore.path")).getAbsolutePath());
  factory.setKeyStorePassword(GridTestProperties.getProperty("ssl.keystore.password").toCharArray());
  factory.setTrustManagers(GridSslBasicContextFactory.getDisabledTrustManager());
  return factory;
}
