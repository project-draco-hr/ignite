{
  if (futs[0] != null && futs[1] != null && futs[2] != null)   return new IgniteBiTuple<>(futs[0].get(),futs[2].get());
  IgniteCache cache=ignite.jcache(cacheName);
  if (futs[0] == null && callAsync(new VisorCacheSizeCallable(cache),0))   return null;
  if (futs[1] == null && callAsync(new VisorCacheClearCallable(cache),1))   return null;
  if (futs[2] == null && callAsync(new VisorCacheSizeCallable(cache),2))   return null;
  return new IgniteBiTuple<>(futs[0].get(),futs[2].get());
}
