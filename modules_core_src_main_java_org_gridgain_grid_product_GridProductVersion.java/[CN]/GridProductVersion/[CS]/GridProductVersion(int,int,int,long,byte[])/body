{
  if (revHash != null && revHash.length != 20)   throw new IllegalArgumentException("Invalid length for SHA1 hash (must be 20): " + revHash.length);
  this.major=(byte)major;
  this.minor=(byte)minor;
  this.maintenance=(byte)maintenance;
  this.revTs=revTs;
  this.revHash=revHash != null ? revHash : new byte[20];
}
