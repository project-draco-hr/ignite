{
  if (verStr == null)   return VERSION_UNKNOWN;
  if (verStr.endsWith("-DEV"))   return VERSION_DEV;
  Matcher match=VER_PATTERN.matcher(verStr);
  if (match.matches()) {
    try {
      byte major=Byte.parseByte(match.group(2));
      byte minor=Byte.parseByte(match.group(3));
      byte maintenance=Byte.parseByte(match.group(4));
      long revTs=0;
      if (match.group(6) != null)       revTs=Long.parseLong(match.group(6));
      byte[] revHash=null;
      if (match.group(8) != null)       revHash=U.decodeHex(match.group(8).toCharArray());
      return new GridProductVersion(major,minor,maintenance,revTs,revHash);
    }
 catch (    IllegalStateException|IndexOutOfBoundsException ignored) {
      return VERSION_UNKNOWN;
    }
catch (    NumberFormatException|GridException ignored) {
      return VERSION_UNKNOWN;
    }
  }
 else   return VERSION_UNKNOWN;
}
