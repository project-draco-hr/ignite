{
  printCacheEntries(cache);
  System.out.println("");
  System.out.println(">> Populating the cache using EntryProcessor.");
  for (int i=0; i < KEY_CNT; i++) {
    cache.invoke(i,new EntryProcessor<Integer,Integer,Object>(){
      @Override public Object process(      MutableEntry<Integer,Integer> entry,      Object... objects) throws EntryProcessorException {
        if (entry.getValue() == null)         entry.setValue((entry.getKey() + 1) * 10);
        return null;
      }
    }
);
  }
  printCacheEntries(cache);
}
