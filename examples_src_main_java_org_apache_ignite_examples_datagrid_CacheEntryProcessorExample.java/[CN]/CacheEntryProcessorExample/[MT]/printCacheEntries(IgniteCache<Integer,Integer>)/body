{
  System.out.println();
  System.out.println(">>> Entries in the cache.");
  Map<Integer,Integer> entries=cache.getAll(KEYS_SET);
  if (entries.isEmpty())   System.out.println("No entries in the cache.");
 else {
    for (    Map.Entry<Integer,Integer> entry : entries.entrySet())     System.out.println("Entry [key=" + entry.getKey() + ", value="+ entry.getValue()+ ']');
  }
}
