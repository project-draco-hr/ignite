{
  reader.setBuffer(buf);
switch (readState) {
case 0:
    topVer=reader.readField("topVer",MessageFieldType.LONG);
  if (!reader.isLastRead())   return false;
readState++;
case 1:
ver=reader.readField("ver",MessageFieldType.LONG);
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
