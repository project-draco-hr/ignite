{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeLong("topVer",topVer))     return false;
  state++;
case 1:
if (!writer.writeLong("ver",ver)) return false;
state++;
}
return true;
}
