{
  if (retVal.get2() != null)   return retVal;
  try {
    try (GridCacheTx tx=CU.txStartInternal(cctx,itemView,PESSIMISTIC,REPEATABLE_READ)){
      itemView.removeAll(keys);
      tx.commit();
    }
     retVal.set1(keys.size());
  }
 catch (  GridException e) {
    retVal.set2(e);
  }
  return retVal;
}
