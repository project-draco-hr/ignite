{
  for (  GridCacheQueueType type : GridCacheQueueType.values()) {
    GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,boolean[],Boolean> containsQry=qry.createReduceQuery(SQL,GridCacheQueueItemImpl.class.getName()," qid=? and id in (select * from table(x int=?)) " + queueOrder(type));
    containsQrys.put(type,containsQry);
  }
}
