{
  for (  GridCacheQueueType type : GridCacheQueueType.values()) {
    GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,GridBiTuple<Integer,GridException>,GridBiTuple<Integer,GridException>> rmvAllKeysQry=qry.createReduceQuery(SQL,GridCacheQueueItemImpl.class.getName(),"qid=? " + queueOrder(type));
    rmvAllKeysQry=rmvAllKeysQry.localReducer(new RemoveAllKeysQueryLocalReducer());
    rmvAllKeysQrys.put(type,rmvAllKeysQry);
  }
}
