{
  GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,GridBiTuple<Integer,GridException>,GridBiTuple<Integer,GridException>> rmvAllKeysQry=qry.createReduceQuery(SQL,GridCacheQueueItemImpl.class.getName(),"qid=? " + "order by seq asc");
  this.rmvAllKeysQry=rmvAllKeysQry.localReducer(new RemoveAllKeysQueryLocalReducer());
}
