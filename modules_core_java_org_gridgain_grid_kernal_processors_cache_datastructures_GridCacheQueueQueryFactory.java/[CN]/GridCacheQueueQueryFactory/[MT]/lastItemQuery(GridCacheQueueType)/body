{
  GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,Map.Entry<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>>,Map.Entry<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>>> lastItemQry=lastItemQrys.get(type);
  Comparator<Map.Entry<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>>> cmp=type == GridCacheQueueType.PRIORITY ? priComp : seqComp;
  return lastItemQry.localReducer(new TerminalItemQueryLocalReducer<>(type,cmp,false));
}
