{
  GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,Map.Entry<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>>,Map.Entry<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>>> firstItemQry=firstItemQrys.get(type);
  Comparator<Map.Entry<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>>> cmp=type == GridCacheQueueType.PRIORITY ? priComp : seqComp;
  return firstItemQry.localReducer(new TerminalItemQueryLocalReducer<>(type,cmp,true));
}
