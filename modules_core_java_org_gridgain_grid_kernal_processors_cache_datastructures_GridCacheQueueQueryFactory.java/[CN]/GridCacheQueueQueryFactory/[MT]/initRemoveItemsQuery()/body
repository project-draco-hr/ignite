{
  for (  GridCacheQueueType type : GridCacheQueueType.values()) {
    GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,GridBiTuple<Integer,GridException>,GridBiTuple<Integer,GridException>> rmvItemsQry=qry.createReduceQuery(SQL,GridCacheQueueItemImpl.class.getName()," qid=? and id in (select * from table(x int=?)) " + queueOrder(type));
    rmvItemsQry=rmvItemsQry.localReducer(new RemoveItemsQueryLocalReducer());
    rmvItemsQrys.put(type,rmvItemsQry);
  }
}
