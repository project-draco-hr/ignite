{
  Long key=entry.getKey();
  Person val=entry.getValue();
  System.out.println(">>> Store write [key=" + key + ", val="+ val+ ']');
  int updated=jdbcTemplate.update("update PERSONS set firstName = ?, lastName = ? where id = ?",val.firstName,val.lastName,val.id);
  if (updated == 0) {
    jdbcTemplate.update("insert into PERSONS (id, firstName, lastName) values (?, ?, ?)",val.id,val.firstName,val.lastName);
  }
}
