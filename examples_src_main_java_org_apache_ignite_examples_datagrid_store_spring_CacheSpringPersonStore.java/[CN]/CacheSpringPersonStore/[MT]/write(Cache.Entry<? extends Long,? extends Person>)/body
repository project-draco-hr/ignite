{
  Long key=entry.getKey();
  Person val=entry.getValue();
  System.out.println(">>> Store write [key=" + key + ", val="+ val+ ']');
  int updated=jdbcTemplate.update("update PERSONS set firstName = ?, lastName = ? where id = ?",val.getFirstName(),val.getLastName(),val.getId());
  if (updated == 0) {
    jdbcTemplate.update("insert into PERSONS (id, firstName, lastName) values (?, ?, ?)",val.getId(),val.getFirstName(),val.getLastName());
  }
}
