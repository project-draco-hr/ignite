{
  int size=map.size();
  writeInt(size);
  writeFloat(getFloat(map,loadFactorFieldOff));
  if (accessOrderFieldOff >= 0)   writeBoolean(getBoolean(map,accessOrderFieldOff));
 else   writeBoolean(false);
  for (  Map.Entry<?,?> e : map.entrySet()) {
    writeObject0(e.getKey());
    if (!set)     writeObject0(e.getValue());
  }
}
