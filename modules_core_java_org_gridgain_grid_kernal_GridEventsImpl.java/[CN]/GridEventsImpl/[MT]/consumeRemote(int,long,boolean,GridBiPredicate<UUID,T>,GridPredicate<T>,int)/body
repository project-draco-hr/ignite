{
  A.ensure(bufSize > 0,"bufSize > 0");
  A.ensure(interval >= 0,"interval >= 0");
  guard();
  try {
    return ctx.continuous().startRoutine(new GridEventConsumeHandler((GridBiPredicate<UUID,GridEvent>)cb,(GridPredicate<GridEvent>)filter,types),bufSize,interval,autoUnsubscribe,prj.predicate());
  }
  finally {
    unguard();
  }
}
