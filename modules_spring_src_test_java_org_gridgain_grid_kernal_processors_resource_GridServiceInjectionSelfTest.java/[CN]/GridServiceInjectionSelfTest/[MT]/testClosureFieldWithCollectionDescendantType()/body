{
  try {
    grid(0).compute().call(new GridCallable<Object>(){
      @GridServiceResource(serviceName="testService") private LinkedList<DummyService> srvcs;
      @Override public Object call() throws Exception {
        return null;
      }
    }
).get();
    fail();
  }
 catch (  GridException e) {
    assertTrue(e.getCause().getMessage().startsWith("Failed to inject resource because target field should have 'Collection' type"));
  }
}
