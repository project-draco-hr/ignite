{
  grid(0).compute(grid(0).cluster().forRemotes()).call(new IgniteCallable<Object>(){
    @IgniteServiceResource(serviceName=SERVICE_NAME1,proxyInterface=DummyService.class) private DummyService svc;
    @Override public Object call() throws Exception {
      assertNotNull(svc);
      assertTrue(svc instanceof GridService);
      svc.noop();
      return null;
    }
  }
);
}
