{
  grid(0).forRemotes().compute().call(new GridCallable<Object>(){
    @GridServiceResource(serviceName=SERVICE_NAME1,proxyInterface=DummyService.class) private DummyService svc;
    @Override public Object call() throws Exception {
      assertNotNull(svc);
      assertTrue(svc instanceof GridService);
      svc.noop();
      return null;
    }
  }
).get();
}
