{
  grid(0).compute(grid(0).cluster().forRemotes()).call(new GridCallable<Object>(){
    @GridServiceResource(serviceName=SERVICE_NAME2,proxyInterface=DummyService.class) private DummyService svc;
    @Override public Object call() throws Exception {
      assertNotNull(svc);
      assertFalse(svc instanceof GridService);
      svc.noop();
      return null;
    }
  }
);
}
