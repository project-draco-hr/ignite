{
  grid(0).forRemotes().compute().call(new GridCallable<Object>(){
    private DummyService svc;
    @GridServiceResource(serviceName=SERVICE_NAME1,proxyInterface=DummyService.class) private void service(    DummyService svc){
      assertNotNull(svc);
      assertTrue(svc instanceof GridService);
      this.svc=svc;
    }
    @Override public Object call() throws Exception {
      svc.noop();
      return null;
    }
  }
).get();
}
