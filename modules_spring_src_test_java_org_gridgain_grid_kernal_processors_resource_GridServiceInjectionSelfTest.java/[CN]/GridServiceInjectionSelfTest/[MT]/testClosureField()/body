{
  grid(0).compute().call(new IgniteCallable<Object>(){
    @IgniteServiceResource(serviceName=SERVICE_NAME1) private DummyService svc;
    @Override public Object call() throws Exception {
      assertNotNull(svc);
      assertTrue(svc instanceof GridService);
      svc.noop();
      return null;
    }
  }
);
}
