{
  directBuf=cfg.isDirectBuffer();
  flushFreq=cfg.getFlushFrequency();
  idleTimeout=cfg.getIdleTimeout();
  locInboundHost=cfg.getLocalInboundHost();
  locInboundPort=cfg.getLocalInboundPort();
