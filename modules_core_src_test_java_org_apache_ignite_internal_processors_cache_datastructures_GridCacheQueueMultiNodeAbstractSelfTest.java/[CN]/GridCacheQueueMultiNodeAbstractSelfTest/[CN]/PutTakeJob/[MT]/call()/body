{
  assertNotNull(ignite);
  ignite.log().info("Running job [node=" + ignite.cluster().localNode().id() + ", job="+ this+ ']');
  IgniteQueue<Integer> queue=ignite.queue(queueName,null,0,false);
  assertNotNull(queue);
  for (int i=0; i < retries; i++) {
    queue.put(i);
    assertNotNull(queue.take());
  }
  return queue.size();
}
