{
  assertNotNull(ignite);
  ignite.log().info("Running job [node=" + ignite.cluster().localNode().id() + ", job="+ this+ "]");
  IgniteQueue<Integer> queue=ignite.queue(queueName,0,null);
  assertNotNull(queue);
  for (int i=0; i < retries; i++)   queue.put(cntr.getAndIncrement());
  return queue.size();
}
