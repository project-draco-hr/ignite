{
  Collection<Class<? extends MessageAdapter>> col=new TreeSet<>(new Comparator<Class<? extends MessageAdapter>>(){
    @Override public int compare(    Class<? extends MessageAdapter> c1,    Class<? extends MessageAdapter> c2){
      return c1.getName().compareTo(c2.getName());
    }
  }
);
  URLClassLoader ldr=(URLClassLoader)getClass().getClassLoader();
  for (  URL url : ldr.getURLs()) {
    File file=new File(url.toURI());
    int prefixLen=file.getPath().length() + 1;
    processFile(file,ldr,prefixLen,col);
  }
  return col;
}
