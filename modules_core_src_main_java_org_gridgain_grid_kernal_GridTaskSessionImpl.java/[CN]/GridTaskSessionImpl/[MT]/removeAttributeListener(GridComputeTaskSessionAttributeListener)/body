{
  A.notNull(lsnr,"lsnr");
  checkFullSupport();
synchronized (mux) {
    if (lsnrs == null)     return false;
    List<GridComputeTaskSessionAttributeListener> lsnrs=new ArrayList<>(this.lsnrs);
    boolean rmv=lsnrs.remove(lsnr);
    this.lsnrs=lsnrs.isEmpty() ? null : lsnrs;
    return rmv;
  }
}
