{
  GridCommunicationListener<GridTcpCommunicationMessageAdapter> lsnr=this.lsnr;
  if (lsnr != null)   lsnr.onMessage(sndId,msg,msgC);
 else   if (log.isDebugEnabled())   log.debug("Received communication message without any registered listeners (will ignore, " + "is node stopping?) [senderNodeId=" + sndId + ", msg="+ msg+ ']');
}
