{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 8:
    if (!writer.writeCollection("keyBytes",keyBytes,byte[].class))     return false;
  state++;
}
return true;
}
