{
  if (retVal || !F.isEmpty(e.entryProcessors()) || !F.isEmpty(e.filters())) {
    if (map == null)     map=new HashMap<>();
    Collection<KeyCacheObject> keys=map.get(e.cacheId());
    if (keys == null) {
      keys=new ArrayList<>();
      map.put(e.cacheId(),keys);
    }
    keys.add(e.key());
  }
  return map;
}
