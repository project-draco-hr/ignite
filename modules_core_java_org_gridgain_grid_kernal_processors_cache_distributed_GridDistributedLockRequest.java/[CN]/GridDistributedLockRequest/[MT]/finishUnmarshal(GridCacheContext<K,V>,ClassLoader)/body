{
  super.finishUnmarshal(ctx,ldr);
  if (keys == null)   keys=unmarshalCollection(keyBytes,ctx,ldr);
  if (grpLockKey == null && grpLockKeyBytes != null)   grpLockKey=ctx.marshaller().unmarshal(grpLockKeyBytes,ldr);
  if (writeEntriesBytes != null) {
    writeEntries=ctx.marshaller().unmarshal(writeEntriesBytes,ldr);
    unmarshalTx(writeEntries,ctx,ldr);
  }
}
