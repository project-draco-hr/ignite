{
  if (col instanceof ArrayList)   return unwrapPortables((ArrayList<Object>)col,keepPortable,cpy);
  if (col instanceof Set)   return unwrapPortables((Set<Object>)col,keepPortable,cpy);
  Collection<Object> col0=new ArrayList<>(col.size());
  for (  Object obj : col)   col0.add(unwrapPortable(obj,keepPortable,cpy));
  return col0;
}
