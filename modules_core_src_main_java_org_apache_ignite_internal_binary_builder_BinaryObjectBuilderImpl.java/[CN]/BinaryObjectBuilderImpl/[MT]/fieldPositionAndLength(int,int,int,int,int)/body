{
  int fieldOffset=PortableUtils.fieldOffsetRelative(reader,footerPos + fieldIdLen,fieldOffsetLen);
  int fieldPos=start + fieldOffset;
  int fieldLen;
  if (footerPos + fieldIdLen + fieldOffsetLen == footerEnd)   fieldLen=rawPos - fieldPos;
 else {
    int nextFieldOffset=PortableUtils.fieldOffsetRelative(reader,footerPos + fieldIdLen + fieldOffsetLen+ fieldIdLen,fieldOffsetLen);
    fieldLen=nextFieldOffset - fieldOffset;
  }
  return F.t(fieldPos,fieldLen);
}
