{
  super(tx.xidVersion(),0);
  commitVer=null;
  threadId=tx.threadId();
  concurrency=tx.concurrency();
  isolation=tx.isolation();
  timeout=tx.timeout();
  invalidate=tx.isInvalidate();
  txSize=tx.size();
  this.reads=reads;
  this.writes=writes;
  this.grpLockKey=grpLockKey;
  this.partLock=partLock;
  this.txNodes=txNodes;
}
