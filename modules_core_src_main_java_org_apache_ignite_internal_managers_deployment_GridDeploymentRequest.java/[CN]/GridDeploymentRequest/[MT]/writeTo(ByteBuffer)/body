{
  MessageWriteState state=MessageWriteState.get();
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 0:
    if (!writer.writeBoolean("isUndeploy",isUndeploy))     return false;
  state.increment();
case 1:
if (!writer.writeIgniteUuid("ldrId",ldrId)) return false;
state.increment();
case 2:
if (!writer.writeCollection("nodeIds",nodeIds,UUID.class)) return false;
state.increment();
case 3:
if (!writer.writeByteArray("resTopicBytes",resTopicBytes)) return false;
state.increment();
case 4:
if (!writer.writeString("rsrcName",rsrcName)) return false;
state.increment();
}
return true;
}
