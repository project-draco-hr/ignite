{
  if (store != null) {
    if (log.isDebugEnabled())     log.debug("Removing value from cache store [key=" + key + ']');
    if (key instanceof GridCacheInternal)     return false;
 else     store.remove(tx,key);
    if (log.isDebugEnabled())     log.debug("Removed value from cache store [key=" + key + ']');
    return true;
  }
  return false;
}
