{
  if (F.isEmpty(keys))   return true;
  if (keys.size() == 1) {
    K key=keys.iterator().next();
    return removeFromStore(tx,key);
  }
  if (store != null) {
    if (log.isDebugEnabled())     log.debug("Removing values from cache store [keys=" + keys + ']');
    try {
      store.removeAll(tx != null && !tx.implicit() ? tx : null,keys);
    }
 catch (    ClassCastException e) {
      handleClassCastException(e);
    }
    if (log.isDebugEnabled())     log.debug("Removed values from cache store [keys=" + keys + ']');
    return true;
  }
  return false;
}
