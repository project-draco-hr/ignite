{
  int max=this.max;
  int batchSize=this.batchSize;
  int startSize=queue.sizex();
  if (startSize >= max + batchSize) {
    for (int i=max; i < startSize && queue.sizex() > max; i++) {
      EvictableEntry<K,V> entry=queue.poll();
      if (entry == null)       break;
      if (!entry.evict()) {
        entry.removeMeta();
        touch(entry);
      }
    }
  }
}
