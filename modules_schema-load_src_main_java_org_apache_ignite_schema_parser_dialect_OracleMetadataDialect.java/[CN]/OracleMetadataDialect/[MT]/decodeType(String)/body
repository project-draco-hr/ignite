{
switch (type) {
case "CHAR":
case "NCHAR":
    return CHAR;
case "VARCHAR2":
case "NVARCHAR2":
  return VARCHAR;
case "LONG":
return LONGVARCHAR;
case "LONG RAW":
return LONGVARBINARY;
case "FLOAT":
return FLOAT;
case "NUMBER":
return NUMERIC;
case "DATE":
return DATE;
case "TIMESTAMP":
return TIMESTAMP;
case "BFILE":
case "BLOB":
return BLOB;
case "CLOB":
case "NCLOB":
case "XMLTYPE":
return CLOB;
}
return OTHER;
}
