{
  GridCacheVersion ver=new GridCacheVersion(UNSAFE.getInt(ptr),UNSAFE.getInt(ptr + 4),UNSAFE.getLong(ptr + 8),UNSAFE.getLong(ptr + 16));
  if (verEx) {
    ptr+=24;
    ver=new GridCacheVersionEx(UNSAFE.getInt(ptr),UNSAFE.getInt(ptr + 4),UNSAFE.getLong(ptr + 8),UNSAFE.getLong(ptr + 16),ver);
  }
  return ver;
}
