{
  commState.setBuffer(buf);
  if (!super.readFrom(buf))   return false;
switch (commState.idx) {
case 3:
    GridDhtPartitionExchangeId exchId0=commState.getDhtPartitionExchangeId("exchId");
  if (exchId0 == DHT_PART_EXCHANGE_ID_NOT_READ)   return false;
exchId=exchId0;
commState.idx++;
case 4:
