{
  if (F.isEmpty(filter)) {
synchronized (this) {
      checkObsolete();
      invalidate(null,nextVersion());
      return true;
    }
  }
 else {
    GridCacheVersion startVer;
synchronized (this) {
      checkObsolete();
      startVer=ver;
    }
    if (!cctx.isAll(this,filter))     return false;
synchronized (this) {
      checkObsolete();
      if (startVer.equals(ver)) {
        invalidate(null,nextVersion());
        return true;
      }
    }
    return invalidate(filter);
  }
}
