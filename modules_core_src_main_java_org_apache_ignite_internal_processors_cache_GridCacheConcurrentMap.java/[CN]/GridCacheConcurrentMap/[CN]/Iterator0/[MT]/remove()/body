{
  if (cur == null)   throw new IllegalStateException();
  GridCacheMapEntry e=cur;
  cur=null;
  curVal=null;
  try {
    CacheProjection prj=ctx.projectionPerCall();
    if (prj != null)     prj.removex(e.key());
 else     ((IgniteKernal)ctx.grid()).getCache(ctx.name()).removex(e.key());
  }
 catch (  IgniteCheckedException ex) {
    throw new IgniteException(ex);
  }
}
