{
  if (res.getStatus() != CLOSED && res.getStatus() != BUFFER_UNDERFLOW && res.getHandshakeStatus() != NOT_HANDSHAKING) {
    handshakeStatus=res.getHandshakeStatus();
    if (log.isDebugEnabled())     log.debug("Renegotiation requested [status=" + res.getStatus() + ", handshakeStatus = "+ handshakeStatus+ "ses="+ ses+ ']');
    handshakeFinished=false;
    handshake();
  }
}
