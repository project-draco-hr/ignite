{
  System.out.println(">>>");
  System.out.println("Find primes in array: " + Arrays.asList(input));
  System.out.println(">>>");
  Collection<Long> res=new ArrayList<>();
  for (  Long val : input) {
    Long divisor=checkPrime(val,2,val);
    if (divisor == null)     res.add(val);
  }
  return res.toArray(new Long[res.size()]);
}
