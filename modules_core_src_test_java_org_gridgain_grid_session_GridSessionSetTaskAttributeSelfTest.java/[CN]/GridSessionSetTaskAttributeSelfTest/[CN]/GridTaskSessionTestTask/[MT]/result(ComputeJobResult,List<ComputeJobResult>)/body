{
  if (result.getException() != null)   throw result.getException();
  if (log.isInfoEnabled()) {
    log.info("Received result from job [res=" + result + ", size="+ received.size()+ ", received="+ received+ ']');
  }
  return received.size() == SPLIT_COUNT ? GridComputeJobResultPolicy.REDUCE : GridComputeJobResultPolicy.WAIT;
}
