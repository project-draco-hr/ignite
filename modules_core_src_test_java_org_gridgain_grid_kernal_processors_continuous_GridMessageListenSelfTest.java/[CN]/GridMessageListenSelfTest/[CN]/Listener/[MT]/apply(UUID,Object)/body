{
  assertNotNull(grid);
  assertNotNull(locNodeId);
  assertNotNull(exec);
  X.println("Received message [nodeId=" + nodeId + ", locNodeId="+ grid.cluster().localNode().id()+ ']');
  assertEquals(prj.grid().cluster().localNode().id(),nodeId);
  assertEquals(MSG,msg);
  nodes.add(locNodeId);
  cnt.incrementAndGet();
  latch.countDown();
  return ret;
}
