{
  assertNotNull(grid());
  UUID locNodeId=grid().localNode().id();
  X.println("Received message [nodeId=" + nodeId + ", locNodeId="+ locNodeId+ ']');
  assertEquals(prj.grid().localNode().id(),nodeId);
  assertEquals(MSG,msg);
  nodes.add(locNodeId);
