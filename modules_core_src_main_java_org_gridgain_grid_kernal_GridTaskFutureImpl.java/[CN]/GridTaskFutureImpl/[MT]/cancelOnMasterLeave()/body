{
  checkValid();
  if (onCancelled()) {
    for (    GridNode node : ctx.discovery().nodes(ses.getTopology())) {
      if (ctx.localNodeId().equals(node.id())) {
        ctx.job().masterLeaveLocal(ses.getId());
        ctx.job().cancelJob(ses.getId(),null,false);
      }
    }
    return true;
  }
  return isCancelled();
}
