{
  if (nonResolvable.contains(name))   throw classNotFoundException(name);
  Class<?> cls;
  try {
    cls=super.loadClass(name,resolve);
    return cls;
  }
 catch (  ClassNotFoundException e) {
  }
  if (resolved.containsKey(name))   return resolved.get(name).loadClass(name);
  cls=sweepContainer(name);
  if (cls == null)   throw classNotFoundException(name);
  return cls;
}
