{
  Map<GridComputeJobAdapter,ClusterNode> map=new HashMap<>(subgrid.size());
  for (  ClusterNode node : subgrid) {
    map.put(new GridComputeJobAdapter(){
      @Override public Serializable execute(){
        log.info("Executing grid job: " + this);
        return null;
      }
    }
,node);
  }
  return map;
}
