{
  depExe.submit(new BusyRunnable(){
    @Override public void run0(){
      for (      Entry<GridServiceDeploymentKey,GridServiceDeployment> e : deps) {
        GridServiceDeployment dep=e.getValue();
        if (dep != null) {
          svcName.set(dep.configuration().getName());
          long topVer=ctx.discovery().topologyVersion();
          GridNode oldest=U.oldest(ctx.discovery().nodes(topVer),null);
          if (oldest.isLocal())           onDeployment(dep,topVer);
        }
 else         if (((GridCacheContinuousQueryEntry)e).keyAssignableFrom(GridServiceDeploymentKey.class)) {
          String name=e.getKey().name();
          svcName.set(name);
          Collection<GridServiceContextImpl> ctxs;
synchronized (locSvcs) {
            ctxs=locSvcs.remove(name);
          }
          if (ctx != null) {
synchronized (ctxs) {
              cancel(ctxs,ctxs.size());
            }
          }
          GridFutureAdapter<?> fut=depFuts.remove(e.getKey().name());
          if (fut != null)           fut.onDone();
          fut=undepFuts.remove(e.getKey().name());
          if (fut != null)           fut.onDone();
        }
      }
    }
  }
);
  return true;
}
