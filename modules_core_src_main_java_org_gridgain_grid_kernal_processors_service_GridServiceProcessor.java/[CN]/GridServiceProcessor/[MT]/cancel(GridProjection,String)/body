{
  return ctx.closure().broadcast(new GridClosure<String,Object>(){
    @Override public Object apply(    String name){
      Collection<GridServiceContextImpl> ctxs=localContexts(name);
      if (ctxs != null) {
synchronized (ctxs) {
          cancel(ctxs,ctxs.size());
        }
      }
      return null;
    }
  }
,name,prj.nodes());
}
