{
  initStore(2);
  try {
    IgniteTx tx=null;
    store.put(tx,1,"v1");
    store.put(tx,2,"v2");
    store.txEnd(tx,true);
    assertEquals("v1",store.load(null,1));
    assertEquals("v2",store.load(null,2));
    assertNull(store.load(null,3));
    store.remove(tx,1);
    store.txEnd(tx,true);
    assertNull(store.load(null,1));
    assertEquals("v2",store.load(null,2));
    assertNull(store.load(null,3));
  }
  finally {
    shutdownStore();
  }
}
