{
  if (cache == null)   U.awaitQuiet(latch);
  if (clsById.putIfAbsent(id,F.t(cls,false)) == null) {
    Thread t=new Thread(new MarshallerCacheUpdater(cache,id,cls.getName()));
    t.start();
    try {
      t.join();
    }
 catch (    InterruptedException e) {
      throw new IgniteException(e);
    }
  }
}
