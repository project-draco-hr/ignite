{
  if (cache == null)   U.awaitQuiet(latch);
  String clsName=cache.get(id);
  if (clsName == null) {
    File file=new File(workDir,id + ".classname");
    try (FileInputStream in=new FileInputStream(file);BufferedReader reader=new BufferedReader(new InputStreamReader(in))){
      FileLock lock=in.getChannel().lock(0,Long.MAX_VALUE,true);
      try {
        clsName=reader.readLine();
      }
  finally {
        lock.release();
      }
    }
 catch (    IOException e) {
      throw new IgniteCheckedException("Failed to read class name from file [id=" + id + ", file="+ file.getAbsolutePath()+ ']',e);
    }
  }
  return clsName;
}
