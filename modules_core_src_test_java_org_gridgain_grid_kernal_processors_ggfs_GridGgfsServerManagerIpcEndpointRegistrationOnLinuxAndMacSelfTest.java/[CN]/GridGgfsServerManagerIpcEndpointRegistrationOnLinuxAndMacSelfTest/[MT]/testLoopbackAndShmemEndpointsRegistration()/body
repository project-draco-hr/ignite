{
  IgniteConfiguration cfg=gridConfiguration();
  cfg.setGgfsConfiguration(gridGgfsConfiguration(null),gridGgfsConfiguration("{type:'tcp', port:" + (DFLT_IPC_PORT + 1) + "}"),gridGgfsConfiguration("{type:'shmem', port:" + (DFLT_IPC_PORT + 2) + "}"));
  G.start(cfg);
  T2<Integer,Integer> res=checkRegisteredIpcEndpoints();
  assertEquals(4,res.get1().intValue());
  assertEquals(2,res.get2().intValue());
}
