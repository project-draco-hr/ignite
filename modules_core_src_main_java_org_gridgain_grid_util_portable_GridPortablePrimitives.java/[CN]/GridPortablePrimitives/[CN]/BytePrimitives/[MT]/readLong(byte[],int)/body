{
  long val=0;
  val|=(arr[off++] & 0xffL);
  val|=(arr[off++] & 0xffL) << 8;
  val|=(arr[off++] & 0xffL) << 16;
  val|=(arr[off++] & 0xffL) << 24;
  val|=(arr[off++] & 0xffL) << 32;
  val|=(arr[off++] & 0xffL) << 40;
  val|=(arr[off++] & 0xffL) << 48;
  val|=(arr[off] & 0xffL) << 56;
  return val;
}
