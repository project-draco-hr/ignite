{
  GridCache<Integer,ObjectValue> cache=ignite.cache(null);
  int cnt=10;
  for (int i=0; i < cnt; i++)   cache.putx(i,new ObjectValue("test" + i,i));
  for (int i=0; i < cnt; i++) {
    assertNotNull(cache.peek(i));
    cache.evict(i);
  }
  CacheQuery<Map.Entry<Integer,ObjectValue>> qry=cache.queries().createSqlQuery(ObjectValue.class,"intVal >= ? order by intVal");
  qry.enableDedup(true);
  assertEquals(0,qry.execute(0).get().size());
}
