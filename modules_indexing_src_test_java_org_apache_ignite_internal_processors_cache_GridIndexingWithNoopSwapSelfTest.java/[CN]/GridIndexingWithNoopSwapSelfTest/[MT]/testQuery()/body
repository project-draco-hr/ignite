{
  GridCacheAdapter<Integer,ObjectValue> cache=((IgniteKernal)ignite).internalCache(null);
  int cnt=10;
  for (int i=0; i < cnt; i++)   cache.getAndPut(i,new ObjectValue("test" + i,i));
  for (int i=0; i < cnt; i++) {
    assertNotNull(localPeek(cache,i));
    cache.evictAll(Collections.singleton(i));
  }
  CacheQuery<Map.Entry<Integer,ObjectValue>> qry=cache.queries().createSqlQuery(ObjectValue.class,"intVal >= ? order by intVal");
  qry.enableDedup(true);
  assertEquals(0,qry.execute(0).get().size());
}
