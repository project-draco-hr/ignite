{
  GridCache<GridCacheAffinityKey<UUID>,Person> cache=GridGain.grid().cache(CACHE_NAME);
  GridCacheQuery<GridCacheAffinityKey<UUID>,Person> qry=cache.queries().createQuery(SQL,Person.class,"from Person, Organization " + "where Person.orgId = Organization.id and lower(Organization.name) = lower(?)");
  print("Following people are 'GridGain' employees: ",qry.queryArguments("GridGain").execute());
  print("Following people are 'Other' employees: ",qry.queryArguments("Other").execute());
}
