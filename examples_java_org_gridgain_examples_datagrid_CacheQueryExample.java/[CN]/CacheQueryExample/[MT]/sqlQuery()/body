{
  GridCache<GridCacheAffinityKey<UUID>,Person> cache=GridGain.grid().cache(CACHE_NAME);
  GridCacheQuery<GridCacheAffinityKey<UUID>,Person> qry=cache.queries().createQuery(SQL,Person.class,"salary > ? and salary <= ?");
  print("People with salaries between 0 and 1000: ",qry.queryArguments(0,1000).execute());
  print("People with salaries between 1000 and 2000: ",qry.queryArguments(1000,2000).execute());
  print("People with salaries greater than 2000: ",qry.queryArguments(2000,Integer.MAX_VALUE).execute());
}
