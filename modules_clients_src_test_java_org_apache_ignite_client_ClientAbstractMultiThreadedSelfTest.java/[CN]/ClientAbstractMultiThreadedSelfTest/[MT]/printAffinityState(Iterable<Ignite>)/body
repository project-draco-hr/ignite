{
  for (  Ignite g : grids) {
    GridAffinityAssignmentCache affCache=getFieldValue(((GridKernal)g).internalCache(PARTITIONED_CACHE_NAME).context().affinity(),"aff");
    CacheAffinityFunction aff=getFieldValue(affCache,"aff");
    info("Affinity [nodeId=" + g.cluster().localNode().id() + ", affinity="+ aff+ "]");
  }
}
