{
  reader.setBuffer(buf);
switch (readState) {
case 0:
    data=reader.readField("data",MessageFieldType.BYTE_ARR);
  if (!reader.isLastRead())   return false;
readState++;
case 1:
size=reader.readField("size",MessageFieldType.INT);
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
