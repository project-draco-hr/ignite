{
  if (!msgTypeDone) {
    if (!buf.hasRemaining())     return null;
    byte type=readByte();
    msg=type == Byte.MIN_VALUE ? null : msgFactory.create(type);
    msgTypeDone=true;
  }
  if (msg == null || msg.readFrom(buf)) {
    GridTcpCommunicationMessageAdapter msg0=msg;
    msgTypeDone=false;
    msg=null;
    return msg0;
  }
 else   return null;
}
