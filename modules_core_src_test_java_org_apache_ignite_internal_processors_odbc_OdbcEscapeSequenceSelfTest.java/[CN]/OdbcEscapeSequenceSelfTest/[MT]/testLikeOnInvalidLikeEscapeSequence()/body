{
  checkFail("LIKE 'AAA's'");
  checkFail("LIKE 'AAA\'s'");
  checkFail("LIKE '\\%AAA%' {escape'\\' }");
  checkFail("LIKE '\\%AAA%' {'\\' ORDER BY id");
  checkFail("LIKE '\\%AAA%' {escape '\\' ORDER BY id;");
  checkFail("LIKE '\\%AAA%' '\\'} ORDER BY id");
  checkFail("LIKE '\\%AAA%' escape '\\'} ORDER BY id;");
}
