{
  check("'12345678-9abc-def0-1234-123456789abc'","{guid '12345678-9abc-def0-1234-123456789abc'}");
  check("select '12345678-9abc-def0-1234-123456789abc' from SomeTable;","select {guid '12345678-9abc-def0-1234-123456789abc'} from SomeTable;");
  check("select '12345678-9abc-def0-1234-123456789abc'","select {guid '12345678-9abc-def0-1234-123456789abc'}");
  checkFail("select {guid '1234567-1234-1234-1234-123456789abc'}");
  checkFail("select {guid '1234567-8123-4123-4123-4123456789abc'}");
  checkFail("select {guid '12345678-9abc-defg-1234-123456789abc'}");
  checkFail("select {guid '12345678-12345678-1234-1234-1234-123456789abc'}");
  checkFail("select {guid '12345678-1234-1234-1234-123456789abcdef'}");
}
