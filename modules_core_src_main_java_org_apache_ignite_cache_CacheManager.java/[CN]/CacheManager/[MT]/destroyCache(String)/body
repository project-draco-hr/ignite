{
  kernalGateway.readLock();
  try {
    IgniteCache<?,?> cache=getCache0(cacheName);
    if (cache != null) {
      unregisterCacheObject(cacheName,CACHE_CONFIGURATION);
      unregisterCacheObject(cacheName,CACHE_STATISTICS);
      cache.close();
    }
  }
  finally {
    kernalGateway.readUnlock();
  }
}
