{
  if (closed.compareAndSet(false,true)) {
    Ignite[] ignites;
synchronized (igniteMap) {
      ignites=igniteMap.values().toArray(new Ignite[igniteMap.values().size()]);
      igniteMap.clear();
    }
    for (    Ignite ignite : ignites) {
      try {
        ignite.close();
      }
 catch (      Exception ignored) {
      }
    }
  }
}
