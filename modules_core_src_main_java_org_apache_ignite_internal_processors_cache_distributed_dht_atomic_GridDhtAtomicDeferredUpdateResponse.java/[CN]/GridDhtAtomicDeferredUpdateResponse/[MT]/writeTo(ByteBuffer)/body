{
  commState.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!commState.typeWritten) {
    if (!commState.putByte(directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 3:
    if (futVers != null) {
      if (commState.it == null) {
        if (!commState.putInt(futVers.size()))         return false;
        commState.it=futVers.iterator();
      }
      while (commState.it.hasNext() || commState.cur != NULL) {
        if (commState.cur == NULL)         commState.cur=commState.it.next();
        if (!commState.putCacheVersion((GridCacheVersion)commState.cur))         return false;
        commState.cur=NULL;
      }
      commState.it=null;
    }
 else {
      if (!commState.putInt(-1))       return false;
    }
  commState.idx++;
}
return true;
}
