{
  CacheOperationContext prev=gate.enter(opCtx);
  try {
    checkTx();
    boolean res=delegate.lockAll(keys,-1);
    if (res)     incrementLockCounter();
    return res;
  }
 catch (  IgniteCheckedException e) {
    throw new CacheException(e.getMessage(),e);
  }
 finally {
    gate.leave(prev);
  }
}
