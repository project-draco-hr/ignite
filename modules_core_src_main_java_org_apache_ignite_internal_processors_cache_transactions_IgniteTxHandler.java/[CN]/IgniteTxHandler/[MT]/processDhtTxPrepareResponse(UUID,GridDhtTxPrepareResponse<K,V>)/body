{
  GridDhtTxPrepareFuture<K,V> fut=(GridDhtTxPrepareFuture<K,V>)ctx.mvcc().<IgniteInternalTx<K,V>>future(res.version(),res.futureId());
  if (fut == null) {
    if (log.isDebugEnabled())     log.debug("Received response for unknown future (will ignore): " + res);
    return;
  }
  fut.onResult(nodeId,res);
}
