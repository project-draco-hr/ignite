{
  if (log.isDebugEnabled())   log.debug("Processing check prepared transaction response [nodeId=" + nodeId + ", res="+ res+ ']');
  GridCacheTxRecoveryFuture fut=(GridCacheTxRecoveryFuture)ctx.mvcc().<Boolean>future(res.version(),res.futureId());
  if (fut == null) {
    if (log.isDebugEnabled())     log.debug("Received response for unknown future (will ignore): " + res);
    return;
  }
  fut.onResult(nodeId,res);
}
