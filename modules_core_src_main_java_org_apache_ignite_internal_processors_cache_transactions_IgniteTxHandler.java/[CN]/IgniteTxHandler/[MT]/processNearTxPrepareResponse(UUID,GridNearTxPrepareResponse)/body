{
  if (txPrepareMsgLog.isDebugEnabled())   txPrepareMsgLog.debug("Received near prepare response [txId=" + res.version() + ", node="+ nodeId+ ']');
  GridNearTxPrepareFutureAdapter fut=(GridNearTxPrepareFutureAdapter)ctx.mvcc().<IgniteInternalTx>mvccFuture(res.version(),res.futureId());
  if (fut == null) {
    U.warn(log,"Failed to find future for near prepare response [txId=" + res.version() + ", node="+ nodeId+ ", res="+ res+ ']');
    return;
  }
  fut.onResult(nodeId,res);
}
