{
  List<Ignite> clientIgnites=new ArrayList<>(clientNodes);
  try {
    loadTestConfiguration();
    for (int i=0; i < srvNodes; i++)     startNode("server",SERVER_NODE_CONFIGURATION);
    for (int i=0; i < clientNodes; i++)     clientIgnites.add(startNode("client",CLIENT_NODE_CONFIGURATION));
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return clientIgnites;
}
