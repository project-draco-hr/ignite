{
  GridKernal kernal=((GridKernal)grid());
  GridRestProcessorAdapter proc=kernal.context().rest();
  Field hndField=proc.getClass().getSuperclass().getDeclaredField("handlers");
  hndField.setAccessible(true);
  Map handlers=(Map)hndField.get(proc);
  Map cp=new HashMap(handlers);
  handlers.clear();
  try {
    GridTestUtils.assertThrows(log,new Callable<Object>(){
      @Override public Object call() throws Exception {
        return client.cacheGet(null,"key");
      }
    }
,GridException.class,"Failed to process client request: Failed to find registered handler for command: CACHE_GET");
  }
  finally {
    handlers.putAll(cp);
  }
}
