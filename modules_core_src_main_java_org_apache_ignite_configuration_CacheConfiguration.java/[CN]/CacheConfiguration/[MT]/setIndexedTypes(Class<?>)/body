{
  A.ensure(indexedTypes == null || (indexedTypes.length & 1) == 0,"Number of indexed types is expected to be even. Refer to method javadoc for details.");
  if (indexedTypes != null) {
    int len=indexedTypes.length;
    Class<?>[] newIndexedTypes=new Class<?>[len];
    for (int i=0; i < len; i++)     newIndexedTypes[i]=U.box(indexedTypes[i]);
    this.indexedTypes=newIndexedTypes;
  }
 else   this.indexedTypes=null;
  return this;
}
