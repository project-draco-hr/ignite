{
  if (this.qryEntities == null)   this.qryEntities=new ArrayList<>(qryEntities);
  for (  QueryEntity entity : qryEntities) {
    boolean found=false;
    for (    QueryEntity existing : this.qryEntities) {
      if (F.eq(entity.getValueType(),existing.getValueType())) {
        found=true;
        break;
      }
    }
    if (!found)     this.qryEntities.add(entity);
  }
  return this;
}
