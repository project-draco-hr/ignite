{
synchronized (mux) {
    if (lsnrs == null)     return;
    if (lsnr == null || lsnr.length == 0)     lsnrs.clear();
 else {
      for (Iterator<GridInClosure<? super GridFuture<R>>> it=lsnrs.iterator(); it.hasNext(); ) {
        GridInClosure<? super GridFuture<R>> l1=it.next();
        for (        GridInClosure<? super GridFuture<R>> l2 : lsnr)         if (l1.equals(l2))         it.remove();
      }
    }
  }
}
