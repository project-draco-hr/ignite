{
  IgniteConfiguration cfg=super.getConfiguration(gridName);
  cfg.setCacheConfiguration(cacheCfg("tx",TRANSACTIONAL,false),cacheCfg("atomic",ATOMIC,false),cacheCfg("tx_near",TRANSACTIONAL,true),cacheCfg("atomic_near",ATOMIC,true));
  TcpDiscoverySpi disco=new TcpDiscoverySpi();
  disco.setIpFinder(IP_FINDER);
  cfg.setDiscoverySpi(disco);
  return cfg;
}
