{
  List<PropertyDescriptor> primitivePropDescriptors=PropertyMappingHelper.getPojoPropertyDescriptors(getJavaClass(),AffinityKeyMapped.class,true);
  return primitivePropDescriptors != null && !primitivePropDescriptors.isEmpty() ? primitivePropDescriptors : PropertyMappingHelper.getPojoPropertyDescriptors(getJavaClass(),true);
}
