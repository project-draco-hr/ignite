{
  try {
    GridAsyncSupportAdapter<T> clone;
    if (this instanceof Cloneable) {
      clone=(T)clone();
    }
 else {
      byte[] b=m.marshal(this);
      clone=m.unmarshal(b,this.getClass().getClassLoader());
    }
    clone.async=true;
    return (T)clone;
  }
 catch (  CloneNotSupportedException e) {
    throw new GridRuntimeException(e);
  }
}
