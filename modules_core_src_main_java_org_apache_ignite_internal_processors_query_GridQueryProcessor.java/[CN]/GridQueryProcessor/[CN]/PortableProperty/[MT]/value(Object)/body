{
  if (parent != null)   obj=parent.value(obj);
  if (obj == null)   return null;
  if (!ctx.portable().isPortableObject(obj))   throw new IgniteCheckedException("Non-portable object received as a result of property extraction " + "[parent=" + parent + ", propName="+ propName+ ", obj="+ obj+ ']');
  return ctx.portable().field(obj,propName);
}
