{
  Object x=this.key ? key : val;
  if (parent != null)   x=parent.value(key,val);
  if (x == null)   return null;
  try {
    if (field) {
      Field field=(Field)member;
      return field.get(x);
    }
 else {
      Method mtd=(Method)member;
      return mtd.invoke(x);
    }
  }
 catch (  Exception e) {
    throw new IgniteCheckedException(e);
  }
}
