{
  this.member=member;
  this.key=key;
  this.name=!F.isEmpty(name) ? name : member instanceof Method && member.getName().startsWith("get") && member.getName().length() > 3 ? member.getName().substring(3) : member.getName();
  ((AccessibleObject)member).setAccessible(true);
  field=member instanceof Field;
  this.coCtx=coCtx;
}
