{
  Map<Integer,String> portableIds=this.portableIds;
  if (portableIds == null) {
    portableIds=new HashMap<>();
    for (    CacheConfiguration ccfg : ctx.config().getCacheConfiguration()) {
      QueryConfiguration qryCfg=ccfg.getQueryConfiguration();
      if (qryCfg != null) {
        for (        QueryTypeMetadata meta : qryCfg.getTypeMetadata())         portableIds.put(ctx.portable().typeId(meta.getType()),meta.getType());
      }
    }
    this.portableIds=portableIds;
  }
  return portableIds.get(typeId);
}
