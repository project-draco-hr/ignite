{
  if (log.isDebugEnabled())   log.debug("Unswap [space=" + spaceName + ", key="+ key+ ", val="+ val+ "]");
  ctx.indexing().onUnswap(spaceName,key,val);
  if (idx == null)   return;
  if (!busyLock.enterBusy())   throw new IllegalStateException("Failed to process swap event (grid is stopping).");
  try {
    idx.onUnswap(spaceName,key,val,valBytes);
  }
  finally {
    busyLock.leaveBusy();
  }
}
