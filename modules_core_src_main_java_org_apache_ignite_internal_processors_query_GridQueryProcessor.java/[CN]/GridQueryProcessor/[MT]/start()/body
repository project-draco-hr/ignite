{
  super.start();
  if (idx != null) {
    ctx.resource().injectGeneric(idx);
    idx.start(ctx);
    for (    CacheConfiguration<?,?> ccfg : ctx.config().getCacheConfiguration()) {
      CacheQueryConfiguration qryCfg=ccfg.getQueryConfiguration();
      if (qryCfg != null) {
        if (!F.isEmpty(ccfg.getTypeMetadata())) {
          for (          CacheTypeMetadata meta : ccfg.getTypeMetadata())           declaredTypesByName.put(new TypeName(ccfg.getName(),meta.getValueType()),meta);
        }
        if (qryCfg.getTypeResolver() != null)         typeResolvers.put(ccfg.getName(),qryCfg.getTypeResolver());
      }
    }
    execSvc=ctx.getExecutorService();
  }
}
