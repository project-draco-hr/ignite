{
  for (Iterator<StartFuture> itr=startFuts.values().iterator(); itr.hasNext(); ) {
    StartFuture fut=itr.next();
    itr.remove();
    fut.onDone(e);
  }
  for (Iterator<StopFuture> itr=stopFuts.values().iterator(); itr.hasNext(); ) {
    StopFuture fut=itr.next();
    itr.remove();
    fut.onDone(e);
  }
}
