{
  GridTestUtils.assertThrows(log,new Callable<Object>(){
    @Override public Object call() throws Exception {
      return CommandLineTransformer.transform("-J-Xmx1g","-J-XX:OnError=\"dir c:\\\"");
    }
  }
,RuntimeException.class,CommandLineTransformer.JVM_OPTION_PREFIX + " JVM parameters for GridGain batch scripts " + "with double quotes are not supported. "+ "Use JVM_OPTS environment variable to pass any custom JVM option.");
  GridTestUtils.assertThrows(log,new Callable<Object>(){
    @Override public Object call() throws Exception {
      return CommandLineTransformer.transform("-J-Xmx1g","-J-XX:OnOutOfMemoryError=\"dir c:\\\"");
    }
  }
,RuntimeException.class,CommandLineTransformer.JVM_OPTION_PREFIX + " JVM parameters for GridGain batch scripts " + "with double quotes are not supported. "+ "Use JVM_OPTS environment variable to pass any custom JVM option.");
}
