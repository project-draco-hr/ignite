{
  writer.setBuffer(buf);
  if (!writer.isHeaderWritten()) {
    if (!writer.writeHeader(new MessageHeader(directType(),(byte)1)))     return false;
    writer.onHeaderWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeIgniteUuid("cancelledFutId",cancelledFutId))     return false;
  writer.incrementState();
}
return true;
}
