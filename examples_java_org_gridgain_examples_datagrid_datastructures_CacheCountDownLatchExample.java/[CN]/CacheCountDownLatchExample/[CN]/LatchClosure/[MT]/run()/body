{
  try {
    GridCacheCountDownLatch latch=GridGain.grid().cache(cacheName).dataStructures().countDownLatch(latchName,1,false,true);
    int newCnt=latch.countDown();
    System.out.println("Counted down [newCnt=" + newCnt + ", nodeId="+ GridGain.grid().localNode().id()+ ']');
  }
 catch (  GridException e) {
    throw new RuntimeException(e);
  }
}
