{
  reader.setBuffer(buf);
switch (readState) {
case 0:
    globalTime=reader.readLong("globalTime");
  if (!reader.isLastRead())   return false;
readState++;
case 1:
nodeOrderDrId=reader.readInt("nodeOrderDrId");
if (!reader.isLastRead()) return false;
readState++;
case 2:
order=reader.readLong("order");
if (!reader.isLastRead()) return false;
readState++;
case 3:
topVer=reader.readInt("topVer");
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
