{
  if (topologyVersion() == other.topologyVersion()) {
    if (order == other.order)     return nodeOrder() == other.nodeOrder() ? 0 : nodeOrder() < other.nodeOrder() ? -1 : 1;
 else     return order < other.order ? -1 : 1;
  }
 else   return topologyVersion() < other.topologyVersion() ? -1 : 1;
}
