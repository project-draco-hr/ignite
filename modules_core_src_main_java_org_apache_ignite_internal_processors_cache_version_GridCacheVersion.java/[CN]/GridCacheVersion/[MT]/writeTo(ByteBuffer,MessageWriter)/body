{
  writer.setBuffer(buf);
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeField("globalTime",globalTime,MessageFieldType.LONG))     return false;
  writer.incrementState();
case 1:
if (!writer.writeField("nodeOrderDrId",nodeOrderDrId,MessageFieldType.INT)) return false;
writer.incrementState();
case 2:
if (!writer.writeField("order",order,MessageFieldType.LONG)) return false;
writer.incrementState();
case 3:
if (!writer.writeField("topVer",topVer,MessageFieldType.INT)) return false;
writer.incrementState();
}
return true;
}
