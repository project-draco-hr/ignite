{
  depEnabled=in.readBoolean();
  if (depEnabled) {
    topicBytes=U.readByteArray(in);
    predBytes=U.readByteArray(in);
    clsName=U.readString(in);
    depInfo=(GridDeploymentInfoBean)in.readObject();
  }
 else {
    topic=in.readObject();
    pred=(GridBiPredicate<UUID,Object>)in.readObject();
  }
}
