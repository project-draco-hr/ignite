{
  for (int i=0; i < gridCnt; i++) {
    if (plcMaxMemSize > 0) {
      int size=0;
      for (      EvictableEntry entry : proxy(policy(i)).queue())       size+=entry.size();
      assertEquals(size,proxy(policy(i)).getCurrentMemSize());
    }
 else     assertTrue(proxy(policy(i)).queue().size() <= plcMax + plcBatchSize);
  }
}
