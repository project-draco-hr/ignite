{
  if (cctx.offheapTiered() && ((flags & IS_OFFHEAP_PTR_MASK) != 0)) {
    flags&=~IS_OFFHEAP_PTR_MASK;
    if (prevVal != null) {
      cctx.swap().removeOffheap(key());
      value(prevVal);
      GridCacheQueryManager qryMgr=cctx.queries();
      if (qryMgr.enabled())       qryMgr.onUnswap(key,prevVal);
    }
  }
}
