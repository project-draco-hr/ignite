{
  checkObsolete();
  if (!serReadVer.equals(ver)) {
    boolean empty=isStartVersion() || deletedUnlocked();
    if (serReadVer.equals(IgniteTxEntry.SER_READ_EMPTY_ENTRY_VER))     return empty;
 else     if (serReadVer.equals(IgniteTxEntry.SER_READ_NOT_EMPTY_VER))     return !empty;
    return false;
  }
  return true;
}
