{
  return F.asList(new ComputeJobAdapter(){
    @Override public Object execute(){
      CacheAffinity<Object> aff=ignite.affinity(null);
      ClusterNode primary=aff.mapKeyToNode(affKey);
      if (log.isInfoEnabled())       log.info("Primary node for the job key [affKey=" + affKey + ", primary="+ primary.id()+ "]");
      return F.eqNodes(ignite.cluster().localNode(),primary);
    }
  }
);
}
