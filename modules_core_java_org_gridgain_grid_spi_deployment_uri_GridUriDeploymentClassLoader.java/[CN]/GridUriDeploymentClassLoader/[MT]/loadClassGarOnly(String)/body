{
  Class<?> cls=findLoadedClass(name);
  if (cls == null) {
    try {
      cls=findClass(name);
    }
 catch (    ClassNotFoundException e) {
      throw e;
    }
catch (    Throwable e) {
      throw new ClassNotFoundException("Failed to load class due to unexpected error: " + name,e);
    }
  }
  return cls;
}
