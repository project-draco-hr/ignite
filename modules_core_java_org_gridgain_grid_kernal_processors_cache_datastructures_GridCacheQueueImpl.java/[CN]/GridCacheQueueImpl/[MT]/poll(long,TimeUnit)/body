{
  A.ensure(timeout >= 0,"Timeout cannot be negative: " + timeout);
  try {
    checkRemovedx();
    boolean peek=false;
    return blockReadOp(queryCallable(qryFactory.firstItemQuery(),new TerminalItemQueryLocalReducer<T>(true),peek),TAKE_TIMEOUT,timeout,unit,peek);
  }
 catch (  GridException e) {
    throw new GridRuntimeException(e);
  }
}
