{
  boolean retVal;
  GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,boolean[],Boolean> qry=qryFactory.containsQuery(type).queryArguments(qid,hashes).closureArguments(cctx.cache().name(),items);
  if (collocated) {
    GridNode node=CU.primaryNode(cctx,key);
    retVal=qry.reduce(cctx.grid().forNode(node)).get();
  }
 else   retVal=qry.reduce(cctx.grid()).get();
  return retVal;
}
