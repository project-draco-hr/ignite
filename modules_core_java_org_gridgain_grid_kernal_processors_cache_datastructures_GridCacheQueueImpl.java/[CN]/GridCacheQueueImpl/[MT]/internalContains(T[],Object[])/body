{
  GridCacheReduceQuery<GridCacheQueueItemKey,GridCacheQueueItemImpl<T>,boolean[],Boolean> qry=qryFactory.containsQuery(type,items).queryArguments(qid,hashes);
  if (collocated) {
    GridNode node=CU.primaryNode(cctx,key);
    qry.projection(cctx.grid().forNode(node));
  }
  return qry.reduce().get();
}
