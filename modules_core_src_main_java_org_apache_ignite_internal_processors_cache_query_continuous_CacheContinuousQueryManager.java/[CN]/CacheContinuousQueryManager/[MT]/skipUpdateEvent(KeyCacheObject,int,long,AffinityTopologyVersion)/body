{
  if (lsnrCnt.get() > 0) {
    for (    CacheContinuousQueryListener lsnr : lsnrs.values()) {
      CacheContinuousQueryEntry e0=new CacheContinuousQueryEntry(cctx.cacheId(),UPDATED,key,null,null,lsnr.keepBinary(),partId,updCntr,topVer);
      CacheContinuousQueryEvent evt=new CacheContinuousQueryEvent<>(cctx.kernalContext().cache().jcache(cctx.name()),cctx,e0);
      lsnr.skipUpdateEvent(evt,topVer);
    }
  }
}
