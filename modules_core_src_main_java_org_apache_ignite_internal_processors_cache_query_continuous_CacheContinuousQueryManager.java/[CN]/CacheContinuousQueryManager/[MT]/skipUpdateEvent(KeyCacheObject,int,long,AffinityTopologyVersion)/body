{
  if (lsnrCnt.get() > 0) {
    CacheContinuousQueryEntry e0=new CacheContinuousQueryEntry(cctx.cacheId(),UPDATED,key,null,null,partId,updCntr,topVer);
    CacheContinuousQueryEvent evt=new CacheContinuousQueryEvent<>(cctx.kernalContext().cache().jcache(cctx.name()),cctx,e0);
    for (    CacheContinuousQueryListener lsnr : lsnrs.values())     lsnr.skipUpdateEvent(evt,topVer);
  }
}
