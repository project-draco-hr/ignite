{
  CacheContinuousQueryListener<K,V> lsnr;
  if (internal) {
    if ((lsnr=intLsnrs.remove(id)) != null) {
      intLsnrCnt.decrementAndGet();
      lsnr.onUnregister();
    }
  }
 else {
    if ((lsnr=lsnrs.remove(id)) != null) {
      lsnrCnt.decrementAndGet();
      lsnr.onUnregister();
    }
  }
}
