{
  byte flag=readByte();
switch (flag) {
case NULL:
    return null;
case HANDLE:
  return null;
case OBJ:
int typeId=readInt();
Class<?> cls=null;
if (cls == null) throw new GridPortableInvalidClassException("");
GridPortableClassDescriptor desc=GridPortableClassDescriptor.get(cls);
rawOff+=8;
int rawOff0=readInt();
return desc.read(new GridPortableReaderImpl(arr,rawOff,rawOff0));
default :
throw new GridPortableException("Invalid flag value: " + flag);
}
}
