{
  multithreaded(new Runnable(){
    @Override public void run(){
      for (      GridCacheMvccCandidate<String> cand : cands) {
        boolean b=grid.<String,String>internalCache().context().mvcc().addNext(ctx,cand);
        assert b;
      }
      info("Link thread finished.");
    }
  }
,1);
}
