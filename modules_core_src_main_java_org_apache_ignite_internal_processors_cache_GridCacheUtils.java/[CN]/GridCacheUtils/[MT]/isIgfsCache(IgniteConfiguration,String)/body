{
  IgfsConfiguration[] igfsCfgs=cfg.getIgfsConfiguration();
  if (igfsCfgs != null) {
    for (    IgfsConfiguration igfsCfg : igfsCfgs) {
      if (igfsCfg != null && (F.eq(cacheName,igfsCfg.getDataCacheName()) || F.eq(cacheName,igfsCfg.getMetaCacheName())))       return true;
    }
  }
  return false;
}
