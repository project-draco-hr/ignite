{
  FileSystemConfiguration[] igfsCfgs=cfg.getFileSystemConfiguration();
  if (igfsCfgs != null) {
    for (    FileSystemConfiguration igfsCfg : igfsCfgs) {
      if (igfsCfg != null && (F.eq(cacheName,igfsCfg.getDataCacheName()) || F.eq(cacheName,igfsCfg.getMetaCacheName())))       return true;
    }
  }
  return false;
}
