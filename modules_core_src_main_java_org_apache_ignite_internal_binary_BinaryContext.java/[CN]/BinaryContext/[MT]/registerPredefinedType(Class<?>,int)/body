{
  String typeName=typeName(cls.getName());
  BinaryClassDescriptor desc=new BinaryClassDescriptor(this,cls,false,id,typeName,null,BinaryInternalIdMapper.defaultInstance(),null,false,true,true);
  predefinedTypeNames.put(typeName,id);
  predefinedTypes.put(id,desc);
  descByCls.put(cls,desc);
  return desc;
}
