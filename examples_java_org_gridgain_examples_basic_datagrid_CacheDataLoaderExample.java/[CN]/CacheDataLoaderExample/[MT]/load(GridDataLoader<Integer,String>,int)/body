{
  long start=System.currentTimeMillis();
  for (int i=0; i < cnt; i++) {
    ldr.addData(i,Integer.toString(i));
    if (i > 0 && i % 10000 == 0)     System.out.println("Loaded " + i + " keys.");
  }
  long end=System.currentTimeMillis();
  System.out.println(">>> Loaded " + cnt + " keys in "+ (end - start)+ "ms.");
}
