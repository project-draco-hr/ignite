{
  Collection<GridCollisionJobContext> activeJobs=ctx.activeJobs();
  Collection<GridCollisionJobContext> waitJobs=ctx.waitingJobs();
synchronized (mux) {
    colResolutionCnt++;
  }
  for (  GridCollisionJobContext job : waitJobs)   job.activate();
  for (  GridCollisionJobContext job : activeJobs)   job.cancel();
}
