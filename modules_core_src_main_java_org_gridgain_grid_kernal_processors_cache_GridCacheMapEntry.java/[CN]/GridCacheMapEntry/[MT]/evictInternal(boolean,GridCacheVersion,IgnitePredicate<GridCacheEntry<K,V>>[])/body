{
  boolean marked=false;
  try {
    if (F.isEmptyOrNulls(filter)) {
synchronized (this) {
        V prev=saveValueForIndexUnlocked();
        if (!hasReaders() && markObsolete0(obsoleteVer,false)) {
          if (swap) {
            if (!isStartVersion()) {
              try {
                swap();
              }
 catch (              GridException e) {
                U.error(log,"Failed to write entry to swap storage: " + this,e);
              }
            }
          }
 else           clearIndex(prev);
          value(null,null);
          marked=true;
          return true;
        }
      }
    }
 else {
      while (true) {
        GridCacheVersion v;
synchronized (this) {
          v=ver;
        }
        if (!cctx.isAll(this,filter))         return false;
synchronized (this) {
          if (!v.equals(ver))           continue;
          V prevVal=saveValueForIndexUnlocked();
          if (!hasReaders() && markObsolete0(obsoleteVer,false)) {
            if (swap) {
              if (!isStartVersion()) {
                try {
                  swap();
                }
 catch (                GridException e) {
                  U.error(log,"Failed to write entry to swap storage: " + this,e);
                }
              }
            }
 else             clearIndex(prevVal);
            value(null,null);
            marked=true;
            return true;
          }
 else           return false;
        }
      }
    }
  }
 catch (  GridCacheEntryRemovedException ignore) {
    if (log.isDebugEnabled())     log.debug("Got removed entry when evicting (will simply return): " + this);
    return true;
  }
 finally {
    if (marked)     onMarkedObsolete();
  }
  return false;
}
