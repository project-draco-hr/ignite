{
  checkObsolete();
  if (isNew()) {
    V val=unswapped.value();
    if (cctx.portableEnabled())     val=(V)cctx.kernalContext().portable().detachPortable(val);
    update(val,unswapped.valueBytes(),unswapped.expireTime(),unswapped.ttl(),unswapped.version());
    return true;
  }
  return false;
}
