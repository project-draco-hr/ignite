{
  log=U.logger(cctx.kernalContext(),logRef,this);
  this.key=key;
  this.hash=hash;
  this.cctx=cctx;
  ttlAndExpireTimeExtras(ttl,toExpireTime(ttl));
  if (cctx.portableEnabled())   val=(V)cctx.kernalContext().portable().detachPortable(val);
synchronized (this) {
    value(val,null);
  }
  next(hdrId,next);
  ver=cctx.versions().next();
  startVer=ver.order();
}
