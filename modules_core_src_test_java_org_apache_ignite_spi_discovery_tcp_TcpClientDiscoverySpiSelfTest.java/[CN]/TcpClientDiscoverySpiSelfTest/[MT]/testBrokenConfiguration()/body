{
  startServerNodes(1);
  cfgClientMode=false;
  try {
    startClientNodes(1);
    fail("Configuration is boken, node cannot be started with DiscoverySpi.clientMode = true and " + "IgniteConfiguration.clientMode = false");
  }
 catch (  Exception e) {
    assertTrue(e.getMessage(),e.getMessage().contains("DiscoverySpi is in client mode"));
  }
}
