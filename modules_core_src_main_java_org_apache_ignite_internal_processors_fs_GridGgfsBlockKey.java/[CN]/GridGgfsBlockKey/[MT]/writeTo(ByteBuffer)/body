{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeIgniteUuid("affKey",affKey))     return false;
  state++;
case 1:
if (!writer.writeLong("blockId",blockId)) return false;
state++;
case 2:
if (!writer.writeBoolean("evictExclude",evictExclude)) return false;
state++;
case 3:
if (!writer.writeIgniteUuid("fileId",fileId)) return false;
state++;
}
return true;
}
