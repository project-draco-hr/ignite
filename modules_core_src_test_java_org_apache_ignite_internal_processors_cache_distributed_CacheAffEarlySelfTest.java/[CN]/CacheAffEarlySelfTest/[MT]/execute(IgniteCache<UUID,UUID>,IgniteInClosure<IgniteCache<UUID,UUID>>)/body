{
  c.apply(cache);
  IgniteFuture<Object> fut=cache.future();
  boolean success=false;
  int iter=0;
  while (!success) {
    try {
      fut.get(OP_TIMEOUT);
      success=true;
    }
 catch (    IgniteFutureTimeoutException e) {
      debug(iter == 0 || ALWAYS_DUMP_THREADS);
    }
    iter++;
  }
}
