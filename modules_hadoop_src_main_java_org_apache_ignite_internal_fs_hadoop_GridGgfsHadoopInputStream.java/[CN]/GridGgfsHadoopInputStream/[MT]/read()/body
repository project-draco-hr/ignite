{
  checkClosed();
  readStart();
  try {
    if (eof())     return -1;
    buf.refreshAhead(pos);
    int res=buf.atPosition(pos);
    pos++;
    total++;
    buf.refreshAhead(pos);
    return res;
  }
 catch (  IgniteCheckedException e) {
    throw GridGgfsHadoopUtils.cast(e);
  }
 finally {
    readEnd();
  }
}
