{
  for (  Grid g : grids) {
    GridAffinityAssignmentCache affCache=getFieldValue(((GridKernal)g).internalCache(PARTITIONED_CACHE_NAME).context().affinity(),"aff");
    GridCacheAffinityFunction aff=getFieldValue(affCache,"aff");
    info("Affinity [nodeId=" + g.localNode().id() + ", affinity="+ aff+ "]");
  }
}
