{
  if (dual)   return;
  if (appendSupported()) {
    create(igfs,paths(DIR,SUBDIR),null);
    createFile(igfs.asSecondary(),FILE,false);
    IgfsOutputStream os=null;
    try {
      os=igfs.append(FILE,false);
      igfs.rename(FILE,FILE2);
      os.close();
    }
  finally {
      U.closeQuiet(os);
    }
  }
}
