{
  for (int i=0; i < REPEAT_CNT; i++) {
    try {
      info(">>>>>> Start deadlock test.");
      checkDeadlocks(5,2,2,2,OPS_CNT,0,0,0,0);
      info(">>>>>> End deadlock test.");
    }
  finally {
      info(">>>>>> Start cleanup.");
      clear(igfs,igfsSecondary);
      info(">>>>>> End cleanup.");
    }
  }
}
