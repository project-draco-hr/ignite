{
  for (int i=0; i < 1000; i++) {
    String key=String.valueOf(i);
switch (type) {
case 0:
{
        if (!cache.affinity().isPrimaryOrBackup(node,key))         return key;
        break;
      }
case 1:
{
      if (cache.affinity().isPrimary(node,key))       return key;
      break;
    }
case 2:
{
    if (cache.affinity().isBackup(node,key))     return key;
    break;
  }
}
}
throw new IllegalStateException("Failed to find key.");
}
