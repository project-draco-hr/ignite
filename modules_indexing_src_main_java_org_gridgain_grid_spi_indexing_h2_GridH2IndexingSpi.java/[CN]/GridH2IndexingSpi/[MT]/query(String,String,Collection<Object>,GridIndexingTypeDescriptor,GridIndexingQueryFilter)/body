{
  final TableDescriptor tbl=tableDescriptor(spaceName,type);
  if (tbl == null)   return new GridEmptyCloseableIterator<>();
  setFilters(filters);
  localSpi.set(this);
  try {
    ResultSet rs=executeQuery(qry,params,tbl);
    return new KeyValIterator(rs);
  }
  finally {
    setFilters(null);
    localSpi.remove();
  }
}
