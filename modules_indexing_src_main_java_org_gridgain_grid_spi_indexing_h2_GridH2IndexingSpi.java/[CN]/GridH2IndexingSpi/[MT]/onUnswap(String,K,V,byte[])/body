{
  localSpi.set(this);
  try {
    for (    TableDescriptor tbl : tables(schema(spaceName))) {
      if (tbl.type().keyClass().equals(key.getClass()) || !isIndexFixedTyping(spaceName)) {
        try {
          if (tbl.tbl.onUnswap(key,val))           return;
        }
 catch (        GridException e) {
          throw new GridSpiException(e);
        }
      }
    }
  }
  finally {
    localSpi.remove();
  }
}
