{
  ResultSetMetaData m=rs.getMetaData();
  int cnt=m.getColumnCount();
  List<GridIndexingEntity<?>> row=new ArrayList<>(cnt);
  for (int i=1; i <= cnt; i++) {
    Object val=rs.getObject(i);
    row.add(val instanceof GridIndexingEntity ? (GridIndexingEntity<?>)val : new GridIndexingEntityAdapter<>(val,null));
  }
  return row;
}
