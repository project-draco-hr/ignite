{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    if (!commState.putCacheVersion(null))     return false;
  commState.idx++;
case 1:
if (!commState.putInt(-1)) return false;
commState.idx++;
case 2:
if (!commState.putLong(0)) return false;
commState.idx++;
case 3:
if (!commState.putInt(-1)) return false;
commState.idx++;
case 4:
if (!commState.putInt(-1)) return false;
commState.idx++;
}
return true;
}
