{
  assertLinksAppearOnce(new GridException("test",new GridException("test nested",new GridException("last"))),DFLT_HELP_LINKS);
  assertLinksAppearOnce(new GridRuntimeException("test",new GridRuntimeException("test nested",new GridRuntimeException("last"))),DFLT_HELP_LINKS);
  assertLinksAppearOnce(new GridException("test",new GridRuntimeException("test nested",new GridException("last"))),DFLT_HELP_LINKS);
  assertLinksAppearOnce(new GridRuntimeException("test",new GridException("test nested",new GridRuntimeException("last"))),DFLT_HELP_LINKS);
}
