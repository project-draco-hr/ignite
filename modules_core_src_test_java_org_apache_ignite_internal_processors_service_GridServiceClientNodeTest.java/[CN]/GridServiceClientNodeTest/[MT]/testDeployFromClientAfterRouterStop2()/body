{
  startGrid(0);
  client=true;
  Ignite ignite=startGrid(1);
  client=false;
  startGrid(2);
  client=true;
  startGrid(3);
  client=false;
  startGrid(4);
  U.sleep(1000);
  stopGrid(0);
  awaitPartitionMapExchange();
  checkDeploy(ignite,"service1");
  startGrid(5);
  for (int i=0; i < 10; i++)   checkDeploy(ignite,"service2-" + i);
}
