{
  GridCacheTx tx=new DummyTx();
  store.put(tx,"k1","v1");
  store.put(tx,"k2","v2");
  store.txEnd(tx,true);
  assertEquals("v1",store.load(null,"k1"));
  assertEquals("v2",store.load(null,"k2"));
  assertNull(store.load(null,"k3"));
  store.remove(tx,"k1");
  store.txEnd(tx,true);
  assertNull(store.load(null,"k1"));
  assertEquals("v2",store.load(null,"k2"));
  assertNull(store.load(null,"k3"));
}
