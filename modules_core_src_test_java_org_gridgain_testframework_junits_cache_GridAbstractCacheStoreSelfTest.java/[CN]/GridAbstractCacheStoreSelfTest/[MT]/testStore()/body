{
  IgniteTx tx=new DummyTx();
  ses.newSession(tx);
  store.put("k1","v1");
  store.put("k2","v2");
  store.txEnd(true);
  ses.newSession(null);
  assertEquals("v1",store.load("k1"));
  assertEquals("v2",store.load("k2"));
  assertNull(store.load("k3"));
  ses.newSession(tx);
  store.remove("k1");
  store.txEnd(true);
  ses.newSession(null);
  assertNull(store.load("k1"));
  assertEquals("v2",store.load("k2"));
  assertNull(store.load("k3"));
}
