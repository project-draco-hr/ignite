{
  if (mode == CacheMode.PARTITIONED)   cc.setIndexedTypes(Integer.class,Organization.class,AffinityKey.class,Person.class,AffinityKey.class,Purchase.class);
 else   if (mode == CacheMode.REPLICATED)   cc.setIndexedTypes(Integer.class,Product.class,Integer.class,Address.class);
 else   throw new IllegalStateException("mode: " + mode);
}
