{
  if (disabled)   return;
  if (!F.isEmpty(perfs) && !suppressed.containsAll(perfs)) {
    U.quietAndInfo(log,"Performance suggestions for grid '" + (gridName == null ? "<default>" : '\'' + gridName + '\'') + "' (fix if possible)");
    U.quietAndInfo(log,"To disable, set -D" + GG_PERFORMANCE_SUGGESTIONS_DISABLED + "=true");
    for (    String s : perfs)     if (!suppressed.contains(s))     U.quietAndInfo(log,"  ^-- " + s);
    U.quietAndInfo(log,"");
    perfs.clear();
  }
}
