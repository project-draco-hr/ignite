{
  long ptr0=ptr;
  if (ptr0 == 0)   deploy();
  lock.readLock().lock();
  try {
    if (closed)     throw new CacheEntryListenerException("Failed to evaluate the filter because it has been closed.");
    PlatformContext platformCtx=PlatformUtils.platformContext(grid);
    return PlatformUtils.evaluateContinuousQueryEvent(platformCtx,ptr,evt);
  }
  finally {
    lock.readLock().unlock();
  }
}
