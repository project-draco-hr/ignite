{
  if (!(o instanceof GridCacheEntryImpl))   return false;
  GridCacheEntry<K,V> e=(GridCacheEntry<K,V>)o;
  if (F.isAll(e,filter) && set.remove(e)) {
    try {
      e.removex();
    }
 catch (    GridException ex) {
      throw new GridRuntimeException(ex);
    }
    return true;
  }
  return false;
}
