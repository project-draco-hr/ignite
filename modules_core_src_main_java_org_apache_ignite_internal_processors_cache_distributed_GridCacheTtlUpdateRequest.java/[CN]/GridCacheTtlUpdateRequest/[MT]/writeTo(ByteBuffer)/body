{
  MessageWriteState state=MessageWriteState.get();
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 3:
    if (!writer.writeCollection("keysBytes",keysBytes,byte[].class))     return false;
  state.increment();
case 4:
if (!writer.writeCollection("nearKeysBytes",nearKeysBytes,byte[].class)) return false;
state.increment();
case 5:
if (!writer.writeCollection("nearVers",nearVers,GridCacheVersion.class)) return false;
state.increment();
case 6:
if (!writer.writeLong("topVer",topVer)) return false;
state.increment();
case 7:
if (!writer.writeLong("ttl",ttl)) return false;
state.increment();
case 8:
if (!writer.writeCollection("vers",vers,GridCacheVersion.class)) return false;
state.increment();
}
return true;
}
