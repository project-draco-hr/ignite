{
  if (el instanceof GridSqlAlias)   el=el.child();
  if (el instanceof GridSqlOperation || el instanceof GridSqlFunction || el instanceof GridSqlAggregateFunction) {
    for (    GridSqlElement child : el)     collectAllSpacesInSubqueries(child,spaces);
  }
 else   if (el instanceof GridSqlSubquery)   collectAllSpaces(((GridSqlSubquery)el).select(),spaces);
}
