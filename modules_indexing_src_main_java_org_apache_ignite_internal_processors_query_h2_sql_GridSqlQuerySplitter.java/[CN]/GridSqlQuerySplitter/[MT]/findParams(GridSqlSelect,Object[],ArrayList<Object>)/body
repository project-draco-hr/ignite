{
  if (params.length == 0)   return target;
  for (  GridSqlElement el : qry.select())   findParams(el,params,target);
  findParams(qry.from(),params,target);
  findParams(qry.where(),params,target);
  for (  GridSqlElement el : qry.groups())   findParams(el,params,target);
  findParams(qry.having(),params,target);
  for (  GridSqlElement el : qry.sort().keySet())   findParams(el,params,target);
  findParams(qry.limit(),params,target);
  findParams(qry.offset(),params,target);
  return target;
}
