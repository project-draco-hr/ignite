{
  if (el == null)   return;
  if (el instanceof GridSqlParameter) {
    int idx=((GridSqlParameter)el).index();
    while (target.size() < idx)     target.add(null);
    Object param=params[idx];
    if (idx == target.size())     target.add(param);
 else     target.set(idx,param);
  }
 else   if (el instanceof GridSqlSubquery)   findParams(((GridSqlSubquery)el).select(),params,target);
 else   for (  GridSqlElement child : el)   findParams(child,params,target);
}
