{
  int hash=key.hashCode();
  byte[] keyBytes=marsh.marshal(key);
  byte[] val=null;
  Lock l=filesLock.readLock();
  l.lock();
  try {
    DataEntry e=findEntry(hash,keyBytes);
    if (e == null)     return null;
    val=e.value();
  }
  finally {
    l.unlock();
  }
  return marsh.unmarshal(val,LOADER);
}
