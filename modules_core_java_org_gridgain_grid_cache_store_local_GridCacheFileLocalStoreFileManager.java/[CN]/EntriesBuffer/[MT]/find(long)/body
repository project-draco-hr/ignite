{
  for (AbstractFileEntry e=head.get(); e != null; e=e.prev) {
    if (e.type() == EntryType.COMPOUND) {
      AbstractFileEntry e0=((CompoundEntry)e).find(pos);
      if (e0 != null)       return e0;
    }
 else     if (e.pos == pos)     return e;
    if (e.pos < pos)     break;
  }
  return null;
}
