{
  X.println(">>>");
  X.println(">>> Printing affinity for node: " + g.name());
  CacheAffinity<Object> aff=affinity(g);
  for (int i=0; i < keyCnt; i++) {
    Collection<? extends ClusterNode> affNodes=nodes(aff,i);
    X.println(">>> Affinity nodes [key=" + i + ", partition="+ aff.partition(i)+ ", nodes="+ U.nodes2names(affNodes)+ ", ids="+ U.nodeIds(affNodes)+ ']');
  }
  partitionMap(g);
}
