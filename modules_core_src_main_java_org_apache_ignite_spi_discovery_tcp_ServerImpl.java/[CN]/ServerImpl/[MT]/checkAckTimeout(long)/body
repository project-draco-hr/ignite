{
  if (ackTimeout > adapter.maxAckTimeout) {
    LT.warn(log,null,"Acknowledgement timeout is greater than maximum acknowledgement timeout " + "(consider increasing 'maxAckTimeout' configuration property) " + "[ackTimeout=" + ackTimeout + ", maxAckTimeout="+ adapter.maxAckTimeout+ ']');
    return false;
  }
  return true;
}
