{
  if (ackTimeout > spi.maxAckTimeout) {
    LT.warn(log,null,"Acknowledgement timeout is greater than maximum acknowledgement timeout " + "(consider increasing 'maxAckTimeout' configuration property) " + "[ackTimeout=" + ackTimeout + ", maxAckTimeout="+ spi.maxAckTimeout+ ']');
    return false;
  }
  return true;
}
