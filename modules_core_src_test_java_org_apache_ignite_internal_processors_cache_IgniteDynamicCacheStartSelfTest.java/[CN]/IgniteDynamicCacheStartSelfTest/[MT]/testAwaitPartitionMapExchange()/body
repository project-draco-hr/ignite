{
  try (IgniteCache ignored=grid(0).getOrCreateCache(new CacheConfiguration(DYNAMIC_CACHE_NAME))){
    awaitPartitionMapExchange();
    startGrid(nodeCount());
    awaitPartitionMapExchange();
    startGrid(nodeCount() + 1);
    awaitPartitionMapExchange();
    stopGrid(nodeCount() + 1);
    awaitPartitionMapExchange();
    stopGrid(nodeCount());
  }
 }
