{
  while (!finish.get()) {
    try {
      long t0=System.currentTimeMillis();
      long submitTime1=t0;
      GridComputeTaskFuture<T3<Long,Integer,Integer>> f1=g.forPredicate(serverNode()).compute().execute(GridDsiRequestTask.class,new GridDsiMessage(terminalId,nodeId));
      submitTime.setIfGreater(System.currentTimeMillis() - submitTime1);
      T3<Long,Integer,Integer> res1=f1.get();
      submitTime1=System.currentTimeMillis();
      GridComputeTaskFuture<T3<Long,Integer,Integer>> f2=g.forPredicate(serverNode()).compute().execute(GridDsiResponseTask.class,new GridDsiMessage(terminalId,nodeId));
      submitTime.setIfGreater(System.currentTimeMillis() - submitTime1);
      T3<Long,Integer,Integer> res2=f2.get();
      long t1=System.currentTimeMillis();
      txCnt.incrementAndGet();
      latency.addAndGet(t1 - t0);
      if (res1 != null)       srvStats=res1;
      if (res2 != null)       srvStats=res2;
    }
 catch (    GridException e) {
      e.printStackTrace();
    }
  }
  return null;
}
