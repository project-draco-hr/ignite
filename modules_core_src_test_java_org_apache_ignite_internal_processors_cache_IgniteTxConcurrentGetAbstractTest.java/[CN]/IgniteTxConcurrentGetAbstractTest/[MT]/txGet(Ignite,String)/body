{
  try (Transaction tx=ignite.transactions().txStart(PESSIMISTIC,REPEATABLE_READ)){
    GridCacheEntryEx<String,Integer> dhtEntry=dht(ignite).peekEx(key);
    if (DEBUG)     info("DHT entry [hash=" + System.identityHashCode(dhtEntry) + ", xid="+ tx.xid()+ ", entry="+ dhtEntry+ ']');
    String val=ignite.<String,String>jcache(null).get(key);
    assertNotNull(val);
    assertEquals("val",val);
    tx.commit();
    return val;
  }
 }
