{
  Collection<PojoDescriptor> res=new ArrayList<>();
  for (  TreeItem<String> schemeItem : rootItem.getChildren())   for (  TreeItem<String> tblItem : schemeItem.getChildren())   if (((CheckBoxTreeItem)tblItem).isSelected())   res.add(schemas.get(schemeItem.getValue()).get(tblItem.getValue()));
  return res;
}
