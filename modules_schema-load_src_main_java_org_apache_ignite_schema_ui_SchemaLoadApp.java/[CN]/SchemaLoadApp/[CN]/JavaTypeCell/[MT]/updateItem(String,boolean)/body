{
  super.updateItem(item,empty);
  setGraphic(null);
  if (!empty) {
    setText(item);
    TableRow row=getTableRow();
    if (row != null) {
      PojoField pojo=(PojoField)row.getItem();
      if (pojo != null) {
        comboBox.setItems(pojo.conversions());
        comboBox.getSelectionModel().select(pojo.javaTypeName());
      }
    }
  }
}
