{
  commState.setBuffer(buf);
  if (!super.readFrom(buf))   return false;
switch (commState.idx) {
case 0:
    IgniteUuid fileId0=commState.getGridUuid();
  if (fileId0 == GRID_UUID_NOT_READ)   return false;
