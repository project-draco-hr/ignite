{
  if (ver > 0) {
    while (true) {
      long order=this.order.get();
      if (ver > order) {
        if (!this.order.compareAndSet(order,ver))         continue;
 else         if (log.isDebugEnabled())         log.debug("Updated version from node [nodeId=" + nodeId + ", ver="+ ver+ ']');
      }
 else       if (log.isDebugEnabled()) {
        log.debug("Did not update version from node (version has lower order) [nodeId=" + nodeId + ", ver="+ ver+ ", curOrder="+ this.order+ ']');
      }
      break;
    }
  }
}
