{
  this.name=desc.getName();
  Method rdMthd=desc.getReadMethod();
  QuerySqlField sqlField=rdMthd != null && rdMthd.getAnnotation(QuerySqlField.class) != null ? rdMthd.getAnnotation(QuerySqlField.class) : desc.getWriteMethod() == null ? null : desc.getWriteMethod().getAnnotation(QuerySqlField.class);
  col=sqlField != null && sqlField.name() != null && !sqlField.name().trim().isEmpty() ? sqlField.name() : name.toLowerCase();
  init(desc);
  if (sqlField != null)   init(sqlField);
}
