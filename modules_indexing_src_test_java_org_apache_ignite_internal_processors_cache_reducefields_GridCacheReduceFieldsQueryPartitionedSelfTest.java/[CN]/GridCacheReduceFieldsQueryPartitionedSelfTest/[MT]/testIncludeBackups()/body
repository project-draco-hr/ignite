{
  CacheQuery<List<?>> qry=((IgniteKernal)grid(0)).getCache(null).context().queries().createSqlFieldsQuery("select age from Person",false);
  qry.includeBackups(true);
  int sum=0;
  for (  IgniteBiTuple<Integer,Integer> tuple : qry.execute(new AverageRemoteReducer()).get())   sum+=tuple.get1();
  assertEquals("Sum",200,sum);
}
