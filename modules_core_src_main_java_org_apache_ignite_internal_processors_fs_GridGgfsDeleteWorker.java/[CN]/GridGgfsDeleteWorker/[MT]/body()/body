{
  if (log.isDebugEnabled())   log.debug("Delete worker started.");
  while (!cancelled) {
    lock.lock();
    try {
      if (!cancelled && !force)       cond.await(FREQUENCY,TimeUnit.MILLISECONDS);
      force=false;
    }
  finally {
      lock.unlock();
    }
    if (!cancelled)     delete();
  }
}
