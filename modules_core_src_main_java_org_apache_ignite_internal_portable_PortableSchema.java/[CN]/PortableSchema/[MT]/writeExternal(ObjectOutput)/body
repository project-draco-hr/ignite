{
  out.writeInt(schemaId);
  if (inline) {
    out.writeBoolean(true);
    out.writeInt(id0);
    out.writeInt(id1);
    out.writeInt(id2);
    out.writeInt(id3);
    out.writeInt(id4);
    out.writeInt(id5);
    out.writeInt(id6);
    out.writeInt(id7);
  }
 else {
    out.writeBoolean(false);
    out.writeInt(ids.size());
    for (    Integer id : ids)     out.writeInt(id);
  }
}
