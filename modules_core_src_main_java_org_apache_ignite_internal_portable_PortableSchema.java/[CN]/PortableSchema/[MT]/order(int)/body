{
  if (inline) {
    if (id == id0)     return 0;
    if (id == id1)     return 1;
    if (id == id2)     return 2;
    if (id == id3)     return 3;
    if (id == id4)     return 4;
    if (id == id5)     return 5;
    if (id == id6)     return 6;
    if (id == id7)     return 7;
    return ORDER_NOT_FOUND;
  }
 else {
    Integer order=map.get(id);
    return order != null ? order : ORDER_NOT_FOUND;
  }
}
