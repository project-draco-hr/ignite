{
  if (inline) {
    if (id == id1)     return offset1;
    if (id == id2)     return offset2;
    if (id == id3)     return offset3;
    if (id == id4)     return offset4;
    if (id == id5)     return offset5;
    if (id == id6)     return offset6;
    if (id == id7)     return offset7;
    if (id == id8)     return offset8;
    return 0;
  }
 else {
    Integer off=map.get(id);
    return off != null ? off : 0;
  }
}
