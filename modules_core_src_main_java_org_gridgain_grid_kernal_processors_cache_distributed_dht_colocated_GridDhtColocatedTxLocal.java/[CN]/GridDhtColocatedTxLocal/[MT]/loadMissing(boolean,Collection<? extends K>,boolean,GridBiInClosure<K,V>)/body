{
  return cctx.colocated().loadAsync(keys,false,false,topologyVersion(),CU.subjectId(this,cctx),deserializePortable,null).chain(new C1<GridFuture<Map<K,V>>,Boolean>(){
    @Override public Boolean apply(    GridFuture<Map<K,V>> f){
      try {
        Map<K,V> map=f.get();
        for (        K key : keys)         c.apply(key,map.get(key));
        return true;
      }
 catch (      Exception e) {
        setRollbackOnly();
        throw new GridClosureException(e);
      }
    }
  }
);
}
