{
  if (nearKeys == null) {
    nearKeys=new ArrayList<>();
    nearKeyBytes=new ArrayList<>();
    nearVals=new ArrayList<>();
    nearValBytes=new ArrayList<>();
  }
  nearKeys.add(key);
  nearKeyBytes.add(keyBytes);
  nearVals.add(val);
  nearValBytes.add(valBytes != null ? GridCacheValueBytes.marshaled(valBytes) : null);
}
