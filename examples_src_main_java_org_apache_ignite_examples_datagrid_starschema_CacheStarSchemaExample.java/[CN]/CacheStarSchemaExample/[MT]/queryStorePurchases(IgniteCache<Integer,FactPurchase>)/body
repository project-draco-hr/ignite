{
  QueryCursor<Cache.Entry<Integer,FactPurchase>> storePurchases=factCache.query(sql(FactPurchase.class,"from \"" + REPLICATED_CACHE_NAME + "\".DimStore, \""+ PARTITIONED_CACHE_NAME+ "\".FactPurchase "+ "where DimStore.id=FactPurchase.storeId and DimStore.name=?").setArgs("Store1"));
