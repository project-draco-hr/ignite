{
  IgniteCache<Integer,FactPurchase> factCache=Ignition.ignite().jcache(PARTITIONED_CACHE_NAME);
  QueryCursor<Cache.Entry<Integer,FactPurchase>> storePurchases=factCache.query(new SqlQuery(FactPurchase.class,"from \"replicated\".DimStore, \"partitioned\".FactPurchase " + "where DimStore.id=FactPurchase.storeId and DimStore.name=?").setArgs("Store1"));
  printQueryResults("All purchases made at store1:",storePurchases.getAll());
}
