{
  PojoMethodsCache t=mtdsCache.get(typeName);
  Object obj=t.newInstance();
  try {
    for (    GridCacheQueryTypeDescriptor field : fields)     t.setters.get(field.getJavaName()).invoke(obj,rs.getObject(field.getDbName()));
    return (R)obj;
  }
 catch (  Exception e) {
    throw new IgniteCheckedException("Failed to read object of class: " + typeName,e);
  }
}
