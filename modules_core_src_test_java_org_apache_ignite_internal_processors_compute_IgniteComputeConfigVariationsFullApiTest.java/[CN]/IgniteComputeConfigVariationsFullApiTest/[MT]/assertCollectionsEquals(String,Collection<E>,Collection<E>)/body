{
  assertEquals(msg + "; Size are different",exp.size(),act.size());
  for (  Object o : exp) {
    if (!act.contains(o)) {
      error("Expected: " + exp.toString());
      error("Actual: " + act.toString());
      assertTrue(msg + String.format("; actual collection doesn't contain the object [%s]",o),false);
    }
  }
}
