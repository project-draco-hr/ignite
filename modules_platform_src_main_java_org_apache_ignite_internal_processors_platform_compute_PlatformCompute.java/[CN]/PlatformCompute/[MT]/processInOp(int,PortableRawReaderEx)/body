{
switch (type) {
case OP_UNICAST:
    processClosures(reader.readLong(),reader,false,false);
  return TRUE;
case OP_BROADCAST:
processClosures(reader.readLong(),reader,true,false);
return TRUE;
case OP_AFFINITY:
processClosures(reader.readLong(),reader,false,true);
return TRUE;
default :
return throwUnsupported(type);
}
}
