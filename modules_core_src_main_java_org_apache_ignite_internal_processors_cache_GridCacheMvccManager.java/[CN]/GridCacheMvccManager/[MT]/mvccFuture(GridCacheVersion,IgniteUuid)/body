{
  Collection<GridCacheMvccFuture<?>> futs=this.mvccFuts.get(ver);
  if (futs != null) {
synchronized (futs) {
      for (      GridCacheMvccFuture<?> fut : futs) {
        if (fut.futureId().equals(futId)) {
          if (log.isDebugEnabled())           log.debug("Found future in futures map: " + fut);
          return fut;
        }
      }
    }
  }
  if (log.isDebugEnabled())   log.debug("Failed to find future in futures map [ver=" + ver + ", futId="+ futId+ ']');
  return null;
}
