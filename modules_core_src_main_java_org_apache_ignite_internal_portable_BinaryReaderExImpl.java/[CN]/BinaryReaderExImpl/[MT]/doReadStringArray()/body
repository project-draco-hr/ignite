{
  int hPos=in.position() - 1;
  int len=in.readInt();
  String[] arr=new String[len];
  setHandler(arr,hPos);
  for (int i=0; i < len; i++) {
    byte flag=in.readByte();
    if (flag == NULL)     arr[i]=null;
 else {
      if (flag != STRING)       throw new BinaryObjectException("Invalid flag value: " + flag);
      arr[i]=doReadString();
    }
  }
  return arr;
}
