{
  int hPos=positionForHandle();
  int len=in.readInt();
  BigDecimal[] arr=new BigDecimal[len];
  setHandler(arr,hPos);
  for (int i=0; i < len; i++) {
    byte flag=in.readByte();
    if (flag == NULL)     arr[i]=null;
 else {
      if (flag != DECIMAL)       throw new BinaryObjectException("Invalid flag value: " + flag);
      arr[i]=doReadDecimal();
    }
  }
  return arr;
}
