{
  JmhIdeBenchmarkRunner.create().forks(1).warmupIterations(10).measurementIterations(2000).classes(JmhCachePutBenchmark.class).jvmArguments("-Xms4g","-Xmx4g","-XX:+UnlockCommercialFeatures","-XX:+FlightRecorder","-XX:StartFlightRecording=delay=20s,duration=60s,dumponexit=true,settings=alloc,filename=ignite-put_bench.jfr",JmhIdeBenchmarkRunner.createProperty(PROP_BACKUPS,1),JmhIdeBenchmarkRunner.createProperty(PROP_ATOMICITY_MODE,CacheAtomicityMode.ATOMIC),JmhIdeBenchmarkRunner.createProperty(PROP_WRITE_SYNC_MODE,CacheWriteSynchronizationMode.PRIMARY_SYNC)).run();
}
