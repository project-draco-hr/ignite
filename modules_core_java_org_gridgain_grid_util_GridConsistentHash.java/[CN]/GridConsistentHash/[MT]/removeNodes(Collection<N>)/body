{
  if (F.isEmpty(nodes))   return;
  rw.writeLock().lock();
  try {
    if (!this.nodes.removeAll(nodes))     return;
    for (Iterator<SortedSet<N>> it=circle.values().iterator(); it.hasNext(); ) {
      SortedSet<N> set=it.next();
      if (!set.removeAll(nodes))       continue;
      if (set.isEmpty())       it.remove();
    }
  }
  finally {
    rw.writeLock().unlock();
  }
}
