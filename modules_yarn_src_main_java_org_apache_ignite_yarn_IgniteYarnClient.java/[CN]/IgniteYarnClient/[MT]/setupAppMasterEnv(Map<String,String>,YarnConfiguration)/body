{
  for (  String c : conf.getStrings(YarnConfiguration.YARN_APPLICATION_CLASSPATH,YarnConfiguration.DEFAULT_YARN_APPLICATION_CLASSPATH))   Apps.addToEnvironment(envs,Environment.CLASSPATH.name(),c.trim(),File.pathSeparator);
  Apps.addToEnvironment(envs,Environment.CLASSPATH.name(),Environment.PWD.$() + File.separator + "*",File.pathSeparator);
}
