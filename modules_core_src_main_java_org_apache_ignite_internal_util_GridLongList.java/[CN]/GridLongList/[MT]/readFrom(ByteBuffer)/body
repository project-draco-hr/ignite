{
  reader.setBuffer(buf);
  if (!reader.beforeMessageRead())   return false;
switch (readState) {
case 0:
    arr=reader.readLongArray("arr");
  if (!reader.isLastRead())   return false;
readState++;
case 1:
idx=reader.readInt("idx");
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
