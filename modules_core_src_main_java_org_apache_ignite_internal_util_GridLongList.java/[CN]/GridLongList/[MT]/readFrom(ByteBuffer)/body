{
  reader.setBuffer(buf);
switch (readState) {
case 0:
    arr=reader.readField("arr",MessageFieldType.LONG_ARR);
  if (!reader.isLastRead())   return false;
readState++;
case 1:
idx=reader.readField("idx",MessageFieldType.INT);
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
