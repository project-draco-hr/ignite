{
  if (this == o)   return true;
  if (!(o instanceof GridLongList))   return false;
  GridLongList that=(GridLongList)o;
  if (idx != that.idx)   return false;
  if (idx == 0 || arr == that.arr)   return true;
  for (int i=0; i < idx; i++) {
    if (arr[i] != that.arr[i])     return false;
  }
  return true;
}
