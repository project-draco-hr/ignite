{
  writer.setBuffer(buf);
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeLongArray("arr",arr))     return false;
  state++;
case 1:
if (!writer.writeInt("idx",idx)) return false;
state++;
}
return true;
}
