{
  for (  CacheConfiguration ccfg : ctx.cache().dataStructuresCacheNames()) {
    if (ccfg == null)     continue;
    if (ccfg.getAtomicityMode() == cfg.atomicityMode() && ccfg.getMemoryMode() == cfg.memoryMode() && ccfg.getCacheMode() == cfg.cacheMode() && ccfg.getBackups() == cfg.backups() && ccfg.getOffHeapMaxMemory() == cfg.offHeapMaxMem())     return ccfg.getName();
  }
  return null;
}
