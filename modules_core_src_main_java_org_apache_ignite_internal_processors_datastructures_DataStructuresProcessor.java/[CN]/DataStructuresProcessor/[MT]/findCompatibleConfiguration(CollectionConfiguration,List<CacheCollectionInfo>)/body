{
  for (  CacheCollectionInfo col : infos) {
    if (col.cfg.getAtomicityMode() == cfg.getAtomicityMode() && col.cfg.getMemoryMode() == cfg.getMemoryMode() && col.cfg.getCacheMode() == cfg.getCacheMode() && col.cfg.getBackups() == cfg.getBackups() && col.cfg.getOffHeapMaxMemory() == cfg.getOffHeapMaxMemory())     return col.cacheName;
  }
  return null;
}
