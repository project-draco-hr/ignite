{
  GridConfiguration c=super.getConfiguration(gridName);
  c.setPeerClassLoadingLocalClassPathExclude(GridUserExternalResourceTask1.class.getName(),GridUserExternalResourceTask2.class.getName(),GridUserExternalResourceTask1.GridUserExternalResourceJob1.class.getName(),GridUserExternalResourceTask2.GridUserExternalResourceJob2.class.getName(),GridAbstractUserExternalResource.class.getName(),GridUserExternalResource1.class.getName(),GridUserExternalResource2.class.getName());
