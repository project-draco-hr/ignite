{
  if (log.isDebugEnabled())   log.debug("Processing node leave event [fut=" + this + ", nodeId="+ nodeId+ ']');
  GridDhtAtomicUpdateRequest req=mappings.get(nodeId);
  if (req != null) {
    updateRes.addFailedKeys(req.keys(),new ClusterTopologyCheckedException("Failed to write keys on backup " + "(node left grid before response is received): " + nodeId));
    mappings.remove(nodeId);
    checkComplete();
    return true;
  }
  return false;
}
