{
  if (plc instanceof CacheLruEvictionPolicyMBean)   return ((CacheLruEvictionPolicyMBean)plc).getMaxSize();
  if (plc instanceof CacheRandomEvictionPolicyMBean)   return ((CacheRandomEvictionPolicyMBean)plc).getMaxSize();
  if (plc instanceof CacheFifoEvictionPolicyMBean)   return ((CacheFifoEvictionPolicyMBean)plc).getMaxSize();
  return null;
}
