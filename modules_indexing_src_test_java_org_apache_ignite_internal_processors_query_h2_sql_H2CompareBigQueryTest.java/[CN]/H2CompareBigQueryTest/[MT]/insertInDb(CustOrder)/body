{
  try (PreparedStatement st=conn.prepareStatement("insert into \"part\".CustOrder (_key, _val, orderId, rootOrderId, date, alias, archSeq, origOrderId) " + "values(?, ?, ?, ?, ?, ?, ?, ?)")){
    int i=0;
    st.setObject(++i,o.orderId);
    st.setObject(++i,o);
    st.setObject(++i,o.orderId);
    st.setObject(++i,o.rootOrderId);
    st.setObject(++i,o.date);
    st.setObject(++i,o.alias);
    st.setObject(++i,o.archSeq);
    st.setObject(++i,o.origOrderId);
    st.executeUpdate();
  }
 }
