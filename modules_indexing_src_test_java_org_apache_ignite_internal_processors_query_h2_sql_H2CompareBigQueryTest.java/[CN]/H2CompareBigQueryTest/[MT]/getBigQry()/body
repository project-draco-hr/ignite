{
  String res="";
  Reader isr=new InputStreamReader(getClass().getResourceAsStream("bigQuery.sql"));
  try (BufferedReader reader=new BufferedReader(isr)){
    for (String line; (line=reader.readLine()) != null; )     if (!line.startsWith("--"))     res+=line + '\n';
  }
 catch (  Throwable e) {
    e.printStackTrace();
    fail();
  }
  return res;
}
