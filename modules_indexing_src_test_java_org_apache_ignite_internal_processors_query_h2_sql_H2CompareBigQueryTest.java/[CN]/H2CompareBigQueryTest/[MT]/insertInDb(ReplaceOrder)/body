{
  try (PreparedStatement st=conn.prepareStatement("insert into \"part\".ReplaceOrder (_key, _val, id, orderId, rootOrderId, date, alias, archSeq, refOrderId) " + "values(?, ?, ?, ?, ?, ?, ?, ?, ?)")){
    int i=0;
    st.setObject(++i,o.key(useColocatedData()));
    st.setObject(++i,o);
    st.setObject(++i,o.id);
    st.setObject(++i,o.orderId);
    st.setObject(++i,o.rootOrderId);
    st.setObject(++i,o.date);
    st.setObject(++i,o.alias);
    st.setObject(++i,o.archSeq);
    st.setObject(++i,o.refOrderId);
    st.executeUpdate();
  }
 }
