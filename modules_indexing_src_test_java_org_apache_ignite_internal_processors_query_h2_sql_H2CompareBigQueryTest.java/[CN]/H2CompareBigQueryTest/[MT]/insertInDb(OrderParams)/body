{
  try (PreparedStatement st=conn.prepareStatement("insert into \"part\".OrderParams (_key, _val, id, date, orderId, parentAlgo) values(?, ?, ?, ?, ?, ?)")){
    int i=0;
    st.setObject(++i,o.key(useColocatedData()));
    st.setObject(++i,o);
    st.setObject(++i,o.id);
    st.setObject(++i,o.date);
    st.setObject(++i,o.orderId);
    st.setObject(++i,o.parentAlgo);
    st.executeUpdate();
  }
 }
