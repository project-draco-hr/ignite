{
  for (int i=0; i < srvCnt; i++) {
    Grid g=G.grid("server-" + i);
    for (    GridNode n : g.nodes()) {
      if (n.metrics().getTotalExecutedJobs() != execJobsCnt)       return false;
    }
  }
  for (int i=0; i < clientCnt; i++) {
    Grid g=G.grid("client-" + i);
    for (    GridNode n : g.nodes()) {
      if (n.metrics().getTotalExecutedJobs() != execJobsCnt)       return false;
    }
  }
  return true;
}
