{
  String word=words.poll();
  if (word != null) {
    mapper.send(new GridComputeJobAdapter(word){
      @Override public Object execute(){
        String word=argument(0);
        int cnt=word.length();
        try {
          Thread.sleep(1000);
        }
 catch (        InterruptedException ignored) {
        }
        return cnt;
      }
    }
);
  }
}
