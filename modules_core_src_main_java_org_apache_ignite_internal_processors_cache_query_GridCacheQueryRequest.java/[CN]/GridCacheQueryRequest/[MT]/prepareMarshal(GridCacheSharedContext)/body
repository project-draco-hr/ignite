{
  super.prepareMarshal(ctx);
  if (keyValFilter != null) {
    if (ctx.deploymentEnabled())     prepareObject(keyValFilter,ctx);
    keyValFilterBytes=CU.marshal(ctx,keyValFilter);
  }
  if (rdc != null) {
    if (ctx.deploymentEnabled())     prepareObject(rdc,ctx);
    rdcBytes=CU.marshal(ctx,rdc);
  }
  if (trans != null) {
    if (ctx.deploymentEnabled())     prepareObject(trans,ctx);
    transBytes=CU.marshal(ctx,trans);
  }
  if (!F.isEmpty(args)) {
    if (ctx.deploymentEnabled()) {
      for (      Object arg : args)       prepareObject(arg,ctx);
    }
    argsBytes=CU.marshal(ctx,args);
  }
}
