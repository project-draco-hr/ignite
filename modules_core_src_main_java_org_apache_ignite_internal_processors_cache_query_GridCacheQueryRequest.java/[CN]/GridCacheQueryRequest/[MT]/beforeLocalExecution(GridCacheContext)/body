{
  Marshaller marsh=ctx.marshaller();
  rdc=rdc != null ? marsh.<IgniteReducer<Object,Object>>unmarshal(marsh.marshal(rdc),U.resolveClassLoader(ctx.gridConfig())) : null;
  trans=trans != null ? marsh.<IgniteClosure<Object,Object>>unmarshal(marsh.marshal(trans),U.resolveClassLoader(ctx.gridConfig())) : null;
}
