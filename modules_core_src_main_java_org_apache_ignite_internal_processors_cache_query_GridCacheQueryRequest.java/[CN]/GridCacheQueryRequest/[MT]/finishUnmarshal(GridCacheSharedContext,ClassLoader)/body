{
  super.finishUnmarshal(ctx,ldr);
  Marshaller mrsh=ctx.marshaller();
  if (keyValFilterBytes != null && keyValFilter == null)   keyValFilter=mrsh.unmarshal(keyValFilterBytes,ldr);
  if (rdcBytes != null && rdc == null)   rdc=mrsh.unmarshal(rdcBytes,ldr);
  if (transBytes != null && trans == null)   trans=mrsh.unmarshal(transBytes,ldr);
  if (argsBytes != null && args == null)   args=mrsh.unmarshal(argsBytes,ldr);
}
