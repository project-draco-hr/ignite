{
  A.notNull(o,"o");
  boolean rmv=false;
  for (Iterator<Cache.Entry<K,V>> it=map.values().iterator(); it.hasNext(); ) {
    Cache.Entry<K,V> e=it.next();
    if (F.isAll(e,filter) && F.eq(o,e.getValue())) {
      it.remove();
      ctx.grid().jcache(ctx.name()).remove(e.getKey(),e.getValue());
      rmv=true;
    }
  }
  return rmv;
}
