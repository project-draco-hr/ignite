{
  NodeResults nodeRess=qryRess.get(node.id());
  QueryResults qr=nodeRess == null ? null : nodeRess.results().get(req.queryRequestId());
  if (qr == null || qr.canceled)   sendError(node,req.queryRequestId(),new CacheException("No query result found for request: " + req));
 else   sendNextPage(nodeRess,node,qr,req.query(),req.pageSize());
}
