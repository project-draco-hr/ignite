{
  grid(0).cache(null).put("key0","val0");
  String ret=content(F.asMap("cmd",GridRestCommand.CACHE_GET_AND_PUT_IF_ABSENT.key(),"key","key0","val","val1"));
  assertNotNull(ret);
  assertTrue(!ret.isEmpty());
  jsonEquals(ret,cachePattern("val0",true));
  assertEquals("val0",grid(0).cache(null).get("key0"));
}
