{
  put("p" + idx++,Map.class.getName());
  put("p" + idx++,keyCls.getName());
  put("p" + idx++,valCls.getName());
  SB sb=new SB();
  boolean first=true;
  for (  Map.Entry<?,?> entry : map.entrySet()) {
    if (!first)     sb.a(";");
    sb.a(entry.getKey());
    if (entry.getValue() != null)     sb.a("=").a(entry.getValue());
    first=false;
  }
  put("p" + idx++,URLEncoder.encode(sb.toString()));
  return this;
}
