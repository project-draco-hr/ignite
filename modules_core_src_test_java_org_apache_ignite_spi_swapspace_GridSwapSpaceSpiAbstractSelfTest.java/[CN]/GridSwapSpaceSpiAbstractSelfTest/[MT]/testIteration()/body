{
  spi.clear(SPACE1);
  int cnt=10;
  for (int i=0; i < cnt; i++)   spi.store(SPACE1,new SwapKey("key" + i,i),str2ByteArray("value" + i),context());
  for (int i=0; i < cnt; i++)   assertArrayEquals(str2ByteArray("value" + i),spi.read(SPACE1,new SwapKey("key" + i,i),context()));
  try (IgniteSpiCloseableIterator<Map.Entry<byte[],byte[]>> iter=spi.rawIterator(SPACE1)){
    assertNotNull(iter);
    int i=0;
    while (iter.hasNext()) {
      Map.Entry<byte[],byte[]> next=iter.next();
      String key=getTestResources().getMarshaller().unmarshal(next.getKey(),null);
      info("Got from iterator [key=" + key + ", val="+ new String(next.getValue()));
      i++;
      iter.remove();
    }
    assertEquals(10,i);
  }
   assertEquals(0,spi.count(SPACE1));
}
