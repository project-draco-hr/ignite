{
  try (Grid g=GridGain.start(SSL_ENABLED ? "examples/config/example-cache-ssl.xml" : "examples/config/example-cache-authentication-passcode.xml")){
    JOptionPane.showMessageDialog(null,new JComponent[]{new JLabel("GridGain started."),new JLabel("Press OK to stop GridGain.")},"GridGain",INFORMATION_MESSAGE);
  }
 catch (  GridException e) {
    System.out.println("Failed to create grid: " + e.getMessage());
  }
}
