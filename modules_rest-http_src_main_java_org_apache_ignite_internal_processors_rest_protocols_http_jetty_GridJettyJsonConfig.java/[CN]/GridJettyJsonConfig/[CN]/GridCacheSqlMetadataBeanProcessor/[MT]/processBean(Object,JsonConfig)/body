{
  if (bean == null)   return new JSONObject(true);
  if (bean instanceof GridCacheSqlMetadata) {
    GridCacheSqlMetadata r=(GridCacheSqlMetadata)bean;
    return new JSONObject().element("cacheName",r.cacheName(),jsonCfg).element("types",r.types(),jsonCfg).element("keyClasses",r.keyClasses(),jsonCfg).element("valClasses",r.valClasses(),jsonCfg).element("fields",r.fields(),jsonCfg).element("indexes",r.indexes(),jsonCfg);
  }
  throw new UnsupportedOperationException("Serialize bean to json is not supported: " + bean);
}
