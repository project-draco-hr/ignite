{
  T res=pluginProc.createComponent(cls);
  if (res != null)   return res;
  if (cls.equals(GridCacheDrManager.class))   return (T)new GridOsCacheDrManager();
 else   if (cls.equals(GridPortableProcessor.class))   return (T)new GridOsPortableProcessor(this);
 else   if (cls.equals(IgniteCacheSerializationManager.class))   return (T)new IgniteCacheOsSerializationManager();
  throw new IgniteException("Unsupported component type: " + cls);
}
