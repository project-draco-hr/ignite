{
  IgniteTx tx=cache.txStart();
  try {
    assertNull(cache.get("key1"));
    tx.commit();
  }
  finally {
    tx.close();
  }
  tx=cache.txStart();
  try {
    assertNull(cache.getAsync("key2").get());
    tx.commit();
  }
  finally {
    tx.close();
  }
  tx=cache.txStart();
  try {
    assertTrue(cache.getAll(F.asList("key3","key4")).isEmpty());
    tx.commit();
  }
  finally {
    tx.close();
  }
  tx=cache.txStart();
  try {
    assertTrue(cache.getAllAsync(F.asList("key5","key6")).get().isEmpty());
    tx.commit();
  }
  finally {
    tx.close();
  }
  tx=cache.txStart();
  try {
    cache.put("key7","key7");
    cache.remove("key7");
    assertNull(cache.get("key7"));
    tx.commit();
  }
  finally {
    tx.close();
  }
  checkEmpty(cache);
}
