{
  if (cancelled)   throw new HadoopTaskCancelledException("Task cancelled.");
  try (HadoopTaskOutput out=createOutputInternal(ctx);HadoopTaskInput in=createInputInternal(ctx)){
    ctx.input(in);
    ctx.output(out);
    perfCntr.onTaskStart(ctx.taskInfo(),U.currentTimeMillis());
    ctx.run();
  }
 }
