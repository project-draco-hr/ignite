{
  if (topHistSize < DFLT_TOP_HISTORY_SIZE) {
    U.warn(log,"Topology history size should be greater than or equal to default size. " + "Specified size will not be set [curSize=" + this.topHistSize + ", specifiedSize="+ topHistSize+ ", defaultSize="+ DFLT_TOP_HISTORY_SIZE+ ']');
    return this;
  }
  this.topHistSize=topHistSize;
  return this;
}
