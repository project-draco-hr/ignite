{
  if (ctxInitLatch.getCount() > 0)   ctxInitLatch.countDown();
  U.interrupt(sockTimeoutWorker);
  U.join(sockTimeoutWorker,log);
  if (ipFinder != null) {
    try {
      ipFinder.close();
    }
 catch (    Exception e) {
      log.error("Failed to close ipFinder",e);
    }
  }
  unregisterMBean();
  if (impl != null)   impl.spiStop();
}
