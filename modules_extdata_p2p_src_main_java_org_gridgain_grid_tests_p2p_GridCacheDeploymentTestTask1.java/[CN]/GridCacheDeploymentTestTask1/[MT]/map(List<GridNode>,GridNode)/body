{
  return F.asMap(new GridComputeJobAdapter(){
    @GridInstanceResource private Grid grid;
    @Override public Object execute() throws GridException {
      X.println("Executing GridCacheDeploymentTestTask1 job on node " + grid.cluster().localNode().id());
      GridCache<String,GridCacheDeploymentTestValue> cache=grid.cache(null);
      for (int i=0; i < PUT_CNT; i++)       cache.putx("1" + i,new GridCacheDeploymentTestValue());
      return null;
    }
  }
,node);
}
