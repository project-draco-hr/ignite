{
  A.ensure(batchSize >= 0,"Batch size cannot be negative: " + batchSize);
  try {
    IgniteBiTuple<Long,Long> t=(IgniteBiTuple<Long,Long>)cache.transformAndCompute(queueKey,new ClearClosure(id));
    if (t == null)     return;
    checkRemoved(t.get1());
    removeKeys(cache,id,queueName,collocated,t.get1(),t.get2(),batchSize);
  }
 catch (  GridException e) {
    throw new GridRuntimeException(e);
  }
}
