{
  boolean rmvd=queueRemoved(hdr,id);
  if (rmvd)   return new GridBiTuple<>(hdr,new GridBiTuple<>(QUEUE_REMOVED_IDX,QUEUE_REMOVED_IDX));
 else   if (hdr.empty())   return new GridBiTuple<>(hdr,null);
  GridCacheQueueHeader newHdr=new GridCacheQueueHeader(hdr.id(),hdr.capacity(),hdr.collocated(),hdr.tail(),hdr.tail(),null);
  return new GridBiTuple<>(newHdr,new GridBiTuple<>(hdr.head(),hdr.tail()));
}
