{
  boolean rmvd=queueRemoved(hdr,id);
  if (rmvd)   return new IgniteBiTuple<>(hdr,new IgniteBiTuple<>(QUEUE_REMOVED_IDX,QUEUE_REMOVED_IDX));
 else   if (hdr.empty())   return new IgniteBiTuple<>(hdr,null);
  GridCacheQueueHeader newHdr=new GridCacheQueueHeader(hdr.id(),hdr.capacity(),hdr.collocated(),hdr.tail(),hdr.tail(),null);
  return new IgniteBiTuple<>(newHdr,new IgniteBiTuple<>(hdr.head(),hdr.tail()));
}
