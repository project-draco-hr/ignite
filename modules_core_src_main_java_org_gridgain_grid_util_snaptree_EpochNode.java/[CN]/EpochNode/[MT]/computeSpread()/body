{
  final long state=get();
  if (isAnyChildPresent(state)) {
    int sum=0;
    for (int which=0; which < BF; ++which) {
      final EpochNode child=getChild(state,which);
      if (child != null) {
        sum+=child.computeSpread();
      }
 else {
        sum+=1;
      }
    }
    return sum;
  }
 else {
    return 1;
  }
}
