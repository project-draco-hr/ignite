{
  GridCacheEvictionPolicy evictPlc=cfg.getEvictionPolicy();
  if (evictPlc != null && evictPlc instanceof GridCacheGgfsPerBlockLruEvictionPolicy) {
    GridCacheEvictionFilter evictFilter=cfg.getEvictionFilter();
    if (evictFilter != null && !(evictFilter instanceof GridCacheGgfsEvictionFilter))     throw new GridException("Eviction filter cannot be set explicitly when using " + "GridCacheGgfsPerBlockLruEvictionPolicy:" + cfg.getName());
  }
}
