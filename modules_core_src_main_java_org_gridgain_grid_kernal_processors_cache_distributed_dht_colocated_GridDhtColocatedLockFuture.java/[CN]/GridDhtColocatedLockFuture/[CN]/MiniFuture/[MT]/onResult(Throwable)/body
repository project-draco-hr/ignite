{
  if (rcvRes.compareAndSet(false,true)) {
    if (log.isDebugEnabled())     log.debug("Failed to get future result [fut=" + this + ", err="+ e+ ']');
    onDone(e);
  }
 else   U.warn(log,"Received error after another result has been processed [fut=" + GridDhtColocatedLockFuture.this + ", mini="+ this+ ']',e);
}
