{
  if (super.onDone(res,err)) {
    if (log.isDebugEnabled())     log.debug("Completed stage execution future [fut=" + this + ", err="+ err+ ']');
    if (rootExecution() && metricsHolder != null) {
      if (err != null)       metricsHolder.onSessionFinished();
 else       metricsHolder.onSessionFailed();
    }
    streamer.onFutureCompleted(this);
    return true;
  }
  return false;
}
