{
  key=U.hash(key);
  GridLongList res=null;
  if (key == 0) {
synchronized (zeroKeyVals) {
      if (!zeroKeyVals.isEmpty()) {
        res=new GridLongList();
        res.addAll(zeroKeyVals);
      }
    }
  }
 else {
    GridCacheFileLocalStoreMap t=segment(key);
synchronized (t) {
      res=t.get(key,res);
    }
  }
  return res;
}
