{
  for (  GridFuture<Object> f : futures())   if (isMini(f)) {
    MiniFuture mini=(MiniFuture)f;
    if (mini.miniId().equals(res.miniId())) {
      mini.onResult(res);
      return;
    }
  }
  if (log.isDebugEnabled())   log.debug("Failed to find mini future for response [cacheName=" + cctx.name() + ", res="+ res+ ']');
}
