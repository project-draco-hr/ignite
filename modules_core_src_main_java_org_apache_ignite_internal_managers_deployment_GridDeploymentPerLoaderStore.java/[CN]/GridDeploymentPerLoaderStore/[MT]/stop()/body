{
  Collection<IsolatedDeployment> cp=new HashSet<>();
synchronized (mux) {
    for (    IsolatedDeployment dep : cache.values()) {
      dep.undeploy();
      cp.add(dep);
    }
    cache.clear();
  }
  for (  IsolatedDeployment dep : cp)   dep.recordUndeployed();
  if (log.isDebugEnabled())   log.debug(stopInfo());
}
