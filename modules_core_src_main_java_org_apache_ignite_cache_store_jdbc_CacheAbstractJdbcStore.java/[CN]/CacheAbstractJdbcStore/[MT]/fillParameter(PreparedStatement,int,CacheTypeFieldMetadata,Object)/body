{
  try {
    if (fieldVal != null) {
      if (field.getJavaType() == UUID.class) {
        if (field.getDatabaseType() == Types.BINARY)         fieldVal=U.uuidToBytes((UUID)fieldVal);
 else         if (F.contains(new int[]{Types.CHAR,Types.VARCHAR},field.getDatabaseType()))         fieldVal=fieldVal.toString();
      }
      stmt.setObject(i,fieldVal);
    }
 else     stmt.setNull(i,field.getDatabaseType());
  }
 catch (  SQLException e) {
    throw new CacheException("Failed to set statement parameter name: " + field.getDatabaseName(),e);
  }
}
