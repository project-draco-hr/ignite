{
  EntryMapping em=cacheMappings(cacheName).get(keyTypeId);
  if (em == null) {
    String maskedCacheName=U.maskName(cacheName);
    throw new CacheException("Failed to find mapping description [key=" + key + ", cache="+ maskedCacheName+ "]. Please configure CacheTypeMetadata to associate '"+ maskedCacheName+ "' with JdbcPojoStore.");
  }
  return em;
}
