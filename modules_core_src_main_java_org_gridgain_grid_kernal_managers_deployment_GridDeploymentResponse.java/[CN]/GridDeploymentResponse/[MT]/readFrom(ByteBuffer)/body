{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    byteSrc=commState.getByteArrayList("byteSrc");
  if (!commState.lastRead())   return false;
commState.idx++;
case 1:
errMsg=commState.getString("errMsg");
if (!commState.lastRead()) return false;
commState.idx++;
case 2:
success=commState.getBoolean("success");
if (!commState.lastRead()) return false;
commState.idx++;
}
return true;
}
