{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    GridByteArrayList byteSrc0=commState.getByteArrayList(null);
  if (byteSrc0 == BYTE_ARR_LIST_NOT_READ)   return false;
byteSrc=byteSrc0;
commState.idx++;
case 1:
String errMsg0=commState.getString(null);
if (errMsg0 == STR_NOT_READ) return false;
errMsg=errMsg0;
commState.idx++;
case 2:
if (buf.remaining() < 1) return false;
success=commState.getBoolean(null);
commState.idx++;
}
return true;
}
