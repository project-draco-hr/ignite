{
  Collection<PluginProvider> plugins=ctx.plugins().allProviders();
  if (plugins.size() == 0)   return U.nl() + ">>>    " + PLUGIN_INFO+ "none";
  String info=U.nl() + ">>>    " + PLUGIN_INFO+ U.nl();
  for (  PluginProvider plugin : plugins)   info+=">>>    " + plugin.name() + " "+ plugin.version()+ U.nl()+ ">>>    "+ plugin.copyright();
  return info;
}
