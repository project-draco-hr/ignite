{
  Map<String,Object> discData=null;
  for (  Map.Entry<String,PluginProvider> e : plugins.entrySet()) {
    Object data=e.getValue().provideDiscoveryData(nodeId);
    if (data != null) {
      if (discData == null)       discData=new HashMap<>();
      discData.put(e.getKey(),data);
    }
  }
  return discData;
}
