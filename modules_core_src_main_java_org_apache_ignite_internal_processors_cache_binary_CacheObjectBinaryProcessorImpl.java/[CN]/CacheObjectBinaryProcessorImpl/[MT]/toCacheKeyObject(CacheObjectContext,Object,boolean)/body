{
  if (!((CacheObjectBinaryContext)ctx).portableEnabled())   return super.toCacheKeyObject(ctx,obj,userObj);
  if (obj instanceof KeyCacheObject)   return (KeyCacheObject)obj;
  if (((CacheObjectBinaryContext)ctx).portableEnabled()) {
    obj=toPortable(obj);
    if (obj instanceof BinaryObject)     return (BinaryObjectImpl)obj;
  }
  return toCacheKeyObject0(obj,userObj);
}
