{
  if (!((CacheObjectPortableContext)ctx).portableEnabled())   return super.toCacheObject(ctx,obj,userObj);
  if (obj == null || obj instanceof CacheObject)   return (CacheObject)obj;
  obj=toPortable(obj);
  if (obj instanceof BinaryObject)   return (BinaryObjectImpl)obj;
  return toCacheObject0(obj,userObj);
}
