{
  for (int i=from; i < from + TEST_SIZE; i++) {
    if ((i + 1) % (TEST_SIZE / 10) == 0)     log.info("<" + name + "> Checked "+ (i + 1) * 100 / (TEST_SIZE) + "% entries. [count=" + TEST_SIZE + ", iteration=" + iter + ", cache=" + name + "]");
    assertTrue(i + " value " + (i + name.hashCode() + iter)+ " does not match ("+ ignite.cache(name).get(i)+ ")",ignite.cache(name).get(i) != null && ignite.cache(name).get(i).equals(i + name.hashCode() + iter));
  }
}
