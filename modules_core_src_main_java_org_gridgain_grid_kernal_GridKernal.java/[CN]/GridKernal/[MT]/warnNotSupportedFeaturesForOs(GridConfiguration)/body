{
  Collection<String> msgs=new ArrayList<>();
  if (!F.isEmpty(cfg.getSegmentationResolvers()))   msgs.add("Segmentation resolvers");
  if (cfg.getDrReceiverHubConfiguration() != null || cfg.getDrSenderHubConfiguration() != null)   msgs.add("Data replication");
  if (cfg.getSecureSessionSpi() != null && !(cfg.getSecureSessionSpi() instanceof GridNoopSecureSessionSpi))   msgs.add("Secure session SPI");
  if (cfg.getAuthenticationSpi() != null && !(cfg.getAuthenticationSpi() instanceof GridNoopAuthenticationSpi))   msgs.add("Authentication SPI");
  if (!F.isEmpty(msgs)) {
    U.quietAndInfo(log,"The following configuration settings are not supported in open source edition " + "and will be ignored (consider downloading enterprise edition from http://www.gridgain.com):");
    for (    String s : msgs)     U.quietAndInfo(log,"  ^-- " + s);
    U.quietAndInfo(log,"");
  }
}
