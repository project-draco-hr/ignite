{
  for (int i=0; i < ITERATIONS; i++) {
    info("Performing iteration: " + i);
    latch=null;
    startTempNodes();
    awaitDiscovery(PERM_NODES_CNT + TMP_NODES_CNT);
    latch=new CountDownLatch(PERM_NODES_CNT * TMP_NODES_CNT);
    stopTempNodes();
    latch.await();
    validateAlives();
  }
}
