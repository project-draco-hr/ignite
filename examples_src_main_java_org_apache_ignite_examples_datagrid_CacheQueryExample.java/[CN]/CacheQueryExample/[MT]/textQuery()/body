{
  IgniteCache<AffinityKey<UUID>,Person> cache=Ignition.ignite().cache(PERSONS_CACHE_NAME);
  QueryCursor<Cache.Entry<AffinityKey<UUID>,Person>> masters=cache.query(new TextQuery<AffinityKey<UUID>,Person>(Person.class,"Master"));
  QueryCursor<Cache.Entry<AffinityKey<UUID>,Person>> bachelors=cache.query(new TextQuery<AffinityKey<UUID>,Person>(Person.class,"Bachelor"));
  print("Following people have 'Master Degree' in their resumes: ",masters.getAll());
  print("Following people have 'Bachelor Degree' in their resumes: ",bachelors.getAll());
}
