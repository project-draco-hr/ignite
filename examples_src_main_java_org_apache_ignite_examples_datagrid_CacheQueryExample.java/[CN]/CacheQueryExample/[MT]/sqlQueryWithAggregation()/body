{
  IgniteCache<CacheAffinityKey<UUID>,Person> cache=Ignition.ignite().jcache(CACHE_NAME);
  QueryCursor<List<?>> cursor=cache.query(new SqlFieldsQuery("select avg(salary) from Person, Organization where Person.orgId = Organization.id and " + "lower(Organization.name) = lower(?)").setArgs("GridGain"));
  print("Average salary for 'GridGain' employees: " + cursor.getAll());
}
