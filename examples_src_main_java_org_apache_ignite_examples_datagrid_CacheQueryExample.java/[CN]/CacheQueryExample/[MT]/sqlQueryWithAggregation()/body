{
  IgniteCache<AffinityKey<UUID>,Person> cache=Ignition.ignite().cache(PERSONS_CACHE_NAME);
  String sql="select avg(salary) " + "from Person, \"" + ORGANIZATIONS_CACHE_NAME + "\".Organization as org "+ "where Person.orgId = org.id "+ "and lower(org.name) = lower(?)";
  QueryCursor<List<?>> cursor=cache.query(new SqlFieldsQuery(sql).setArgs("GridGain"));
  print("Average salary for 'GridGain' employees: ",cursor.getAll());
}
