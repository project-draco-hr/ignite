{
  IgniteCache<CacheAffinityKey<UUID>,Person> cache=Ignition.ignite().jcache(CACHE_NAME);
  QueryCursor<List<?>> qry=cache.queryFields(new QuerySqlPredicate("select avg(salary) from Person, Organization where Person.orgId = Organization.id and " + "lower(Organization.name) = lower(?)","Ignite"));
  print("Average salary for 'Ignite' employees: " + qry.getAll());
}
