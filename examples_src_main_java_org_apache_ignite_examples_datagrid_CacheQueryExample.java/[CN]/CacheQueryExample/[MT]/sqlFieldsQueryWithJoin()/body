{
  IgniteCache<?,?> cache=Ignition.ignite().jcache(CACHE_NAME);
  QueryCursor<List<?>> cursor=cache.queryFields(sql("select concat(firstName, ' ', lastName), " + "Organization.name from Person, Organization where " + "Person.orgId = Organization.id"));
  List<List<?>> res=cursor.getAll();
  print("Names of all employees and organizations they belong to:",res);
}
