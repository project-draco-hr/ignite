{
  IgniteCache<?,?> cache=Ignition.ignite().jcache(CACHE_NAME);
  QueryCursor<List<?>> qry=cache.queryFields(new QuerySqlPredicate("select concat(firstName, ' ', lastName), " + "Organization.name from Person, Organization where " + "Person.orgId = Organization.id"));
  Collection<List<?>> res=qry.getAll();
  print("Names of all employees and organizations they belong to:",res);
}
