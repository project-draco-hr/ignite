{
  IgniteCache<?,?> cache=Ignition.ignite().cache(PERSONS_CACHE_NAME);
  String sql="select concat(firstName, ' ', lastName), org.name " + "from Person, \"" + ORGANIZATIONS_CACHE_NAME + "\".Organization as org "+ "where Person.orgId = org.id";
  QueryCursor<List<?>> cursor=cache.query(new SqlFieldsQuery(sql));
  List<List<?>> res=cursor.getAll();
  print("Names of all employees and organizations they belong to:",res);
}
