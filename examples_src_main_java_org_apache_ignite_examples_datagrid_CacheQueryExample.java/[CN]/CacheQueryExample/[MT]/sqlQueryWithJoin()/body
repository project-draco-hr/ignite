{
  IgniteCache<CacheAffinityKey<UUID>,Person> cache=Ignition.ignite().cache(CACHE_NAME);
  String joinSql="from Person, Organization " + "where Person.orgId = Organization.id " + "and lower(Organization.name) = lower(?)";
  print("Following people are 'GridGain' employees: ",cache.query(new SqlQuery<CacheAffinityKey<UUID>,Person>(Person.class,joinSql).setArgs("GridGain")).getAll());
  print("Following people are 'Other' employees: ",cache.query(new SqlQuery<CacheAffinityKey<UUID>,Person>(Person.class,joinSql).setArgs("Other")).getAll());
}
