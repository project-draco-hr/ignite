{
  IgniteCache<CacheAffinityKey<UUID>,Person> cache=Ignition.ignite().jcache(CACHE_NAME);
  String sql="salary > ? and salary <= ?";
  print("People with salaries between 0 and 1000: ",cache.query(new QuerySqlPredicate(Person.class,sql,0,1000)).getAll());
  print("People with salaries between 1000 and 2000: ",cache.query(new QuerySqlPredicate(Person.class,sql,1000,2000)).getAll());
  print("People with salaries greater than 2000: ",cache.query(new QuerySqlPredicate(Person.class,sql,2000,Integer.MAX_VALUE)).getAll());
}
