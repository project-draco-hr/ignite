{
  IgniteCache<AffinityKey<UUID>,Person> cache=Ignition.ignite().cache(PERSON_CACHE);
  String sql="salary > ? and salary <= ?";
  print("People with salaries between 0 and 1000 (queried with SQL query): ",cache.query(new SqlQuery<AffinityKey<UUID>,Person>(Person.class,sql).setArgs(0,1000)).getAll());
  print("People with salaries between 1000 and 2000 (queried with SQL query): ",cache.query(new SqlQuery<AffinityKey<UUID>,Person>(Person.class,sql).setArgs(1000,2000)).getAll());
}
