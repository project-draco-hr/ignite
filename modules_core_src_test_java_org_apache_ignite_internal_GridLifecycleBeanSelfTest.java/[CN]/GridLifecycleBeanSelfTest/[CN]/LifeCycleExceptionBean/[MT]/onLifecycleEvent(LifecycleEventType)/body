{
  if (evt == errType) {
    if (gridErr)     throw new IgniteCheckedException("Expected exception for event: " + evt){
      @Override public void printStackTrace(      PrintStream s){
      }
      @Override public void printStackTrace(      PrintWriter s){
      }
    }
;
 else     throw new RuntimeException("Expected exception for event: " + evt){
      @Override public void printStackTrace(      PrintStream s){
      }
      @Override public void printStackTrace(      PrintWriter s){
      }
    }
;
  }
  super.onLifecycleEvent(evt);
}
