{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    jobId=commState.getGridUuid("jobId");
  if (!commState.lastRead())   return false;
commState.idx++;
case 1:
sesId=commState.getGridUuid("sesId");
if (!commState.lastRead()) return false;
commState.idx++;
case 2:
sys=commState.getBoolean("sys");
if (!commState.lastRead()) return false;
commState.idx++;
}
return true;
}
