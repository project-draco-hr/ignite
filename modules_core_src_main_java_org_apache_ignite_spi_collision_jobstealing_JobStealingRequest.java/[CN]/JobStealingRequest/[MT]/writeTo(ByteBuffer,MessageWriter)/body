{
  writer.setBuffer(buf);
  if (!writer.isHeaderWritten()) {
    if (!writer.writeHeader(directType(),(byte)1))     return false;
    writer.onHeaderWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeInt("delta",delta))     return false;
  writer.incrementState();
}
return true;
}
