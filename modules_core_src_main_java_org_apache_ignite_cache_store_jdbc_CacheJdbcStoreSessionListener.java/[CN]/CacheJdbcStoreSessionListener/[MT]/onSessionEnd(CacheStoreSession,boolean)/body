{
  Connection conn=ses.attachment();
  if (conn != null) {
    ses.attach(null);
    try {
      if (commit)       conn.commit();
 else       conn.rollback();
    }
 catch (    SQLException e) {
      throw new CacheWriterException("Failed to start store session [tx=" + ses.transaction() + ']',e);
    }
 finally {
      U.closeQuiet(conn);
    }
  }
}
