{
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!super.writeTo(buf,state))   return false;
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 3:
    if (!writer.writeCollection("parts",parts,int.class))     return false;
  state.increment();
case 4:
if (!writer.writeLong("timeout",timeout)) return false;
state.increment();
case 5:
if (!writer.writeLong("topVer",topVer)) return false;
state.increment();
case 6:
if (!writer.writeByteArray("topicBytes",topicBytes)) return false;
state.increment();
case 7:
if (!writer.writeLong("updateSeq",updateSeq)) return false;
state.increment();
case 8:
if (!writer.writeInt("workerId",workerId)) return false;
state.increment();
}
return true;
}
