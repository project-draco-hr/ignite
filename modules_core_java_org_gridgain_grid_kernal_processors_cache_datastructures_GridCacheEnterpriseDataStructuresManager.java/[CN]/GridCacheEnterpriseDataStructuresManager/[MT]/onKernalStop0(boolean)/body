{
  super.onKernalStop0(cancel);
  if (queueQry != null) {
    try {
      queueQry.close();
    }
 catch (    GridException e) {
      U.warn(log,"Failed to cancel queue header query.",e);
    }
  }
  if (queueCleanupWorker != null) {
    U.cancel(queueCleanupWorker);
    U.join(queueCleanupWorker,log);
  }
}
