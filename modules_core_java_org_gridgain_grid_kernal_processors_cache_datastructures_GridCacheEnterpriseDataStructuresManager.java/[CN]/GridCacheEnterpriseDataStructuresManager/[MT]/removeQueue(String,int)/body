{
  waitInitialization();
  GridCacheQueueKey key=new GridCacheQueueKey(name);
  GridCacheQueueHeader2 header=queueView.remove(key);
  if (header == null)   return false;
  if (header.empty())   return true;
  GridCacheQueueAdapter.removeKeys(cctx.cache(),header.uuid(),header.collocated(),header.head(),header.tail(),batchSize);
  return true;
}
