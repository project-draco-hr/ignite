{
  GridCacheQueueKey key=new GridCacheQueueKey(name);
  GridCacheQueueHeader header=queueHdrView.remove(key);
  if (header == null)   return false;
  if (!header.empty())   GridCacheQueueAdapter.removeKeys(cctx.cache(),header.uuid(),name,header.collocated(),header.head(),header.tail(),batchSize);
  return true;
}
