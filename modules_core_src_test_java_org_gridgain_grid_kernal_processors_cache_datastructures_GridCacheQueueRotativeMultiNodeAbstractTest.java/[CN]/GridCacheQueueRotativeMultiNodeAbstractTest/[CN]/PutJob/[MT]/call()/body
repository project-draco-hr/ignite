{
  assertNotNull(grid);
  grid.log().info("Running job [node=" + grid.cluster().localNode().id() + ", job="+ this+ "]");
  GridCacheQueue<Integer> queue=grid.cache(null).dataStructures().queue(queueName,QUEUE_CAPACITY,true,true);
  assertNotNull(queue);
  for (int i=0; i < retries; i++)   queue.put(i);
  return queue.size();
}
