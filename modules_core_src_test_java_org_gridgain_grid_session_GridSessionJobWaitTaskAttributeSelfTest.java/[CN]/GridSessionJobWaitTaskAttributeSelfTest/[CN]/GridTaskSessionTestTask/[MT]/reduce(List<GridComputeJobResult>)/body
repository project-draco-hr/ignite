{
  if (log.isInfoEnabled())   log.info("Reducing job [job=" + this + ", results="+ results+ ']');
  if (results.size() < SPLIT_COUNT)   fail("Results size is less than split count: " + results.size());
  int sum=0;
  for (  GridComputeJobResult res : results) {
    if (res.getData() == null)     fail("Got null result data: " + res);
 else     log.info("Reducing result: " + res.getData());
    sum+=(Integer)res.getData();
  }
  return sum;
}
