{
  int fieldOffset=PortableUtils.fieldOffsetRelative(reader,footerPos + 4,fieldOffsetSize);
  int fieldPos=start + fieldOffset;
  int fieldLen;
  if (footerPos + 4 + fieldOffsetSize == footerEnd)   fieldLen=rawPos - fieldPos;
 else {
    int nextFieldOffset=PortableUtils.fieldOffsetRelative(reader,footerPos + 4 + fieldOffsetSize+ 4,fieldOffsetSize);
    fieldLen=nextFieldOffset - fieldOffset;
  }
  return F.t(fieldPos,fieldLen);
}
