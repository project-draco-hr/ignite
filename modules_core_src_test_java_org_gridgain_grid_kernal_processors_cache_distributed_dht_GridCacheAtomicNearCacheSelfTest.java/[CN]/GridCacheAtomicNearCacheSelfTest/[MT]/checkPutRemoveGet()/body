{
  Grid grid0=grid(0);
  GridCache<Integer,Integer> cache0=grid0.cache(null);
  Integer key=key(grid0,NOT_PRIMARY_AND_BACKUP);
  cache0.put(key,key);
  for (int i=0; i < GRID_CNT; i++)   grid(i).cache(null).get(key);
  cache0.remove(key);
  cache0.put(key,key);
  for (int i=0; i < GRID_CNT; i++)   grid(i).cache(null).get(key);
  cache0.remove(key);
}
