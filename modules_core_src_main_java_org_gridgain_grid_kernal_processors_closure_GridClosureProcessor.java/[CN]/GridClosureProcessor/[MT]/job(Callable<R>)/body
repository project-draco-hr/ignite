{
  A.notNull(c,"job");
  if (c instanceof ComputeJobMasterLeaveAware)   return new C2<>(c);
 else {
    return new ComputeJobAdapter(){
      @Override public Object execute(){
        try {
          return c.call();
        }
 catch (        Exception e) {
          throw new IgniteException(e);
        }
      }
    }
;
  }
}
