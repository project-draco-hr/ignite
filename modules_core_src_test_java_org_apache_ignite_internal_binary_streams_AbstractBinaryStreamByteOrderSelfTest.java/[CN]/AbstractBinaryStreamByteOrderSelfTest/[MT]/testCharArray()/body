{
  char[] arr=new char[ARR_LEN];
  for (int i=0; i < ARR_LEN; i++)   arr[i]=(char)RND.nextLong();
  out.writeCharArray(arr);
  byte[] outArr=array();
  for (int i=0; i < ARR_LEN; i++)   assertEquals(arr[i],getCharByByteLE(outArr,i * 2));
  Assert.assertArrayEquals(arr,in.readCharArray(ARR_LEN));
}
