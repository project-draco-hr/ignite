{
  char val=(char)RND.nextLong();
  reset();
  out.unsafeWriteChar(val);
  checkValueLittleEndian(val);
  assertEquals(val,in.readChar());
  reset();
  out.writeCharFast(val);
  out.shift(2);
  checkValueLittleEndian(val);
  assertEquals(val,in.readCharFast());
  reset();
  out.writeChar(val);
  checkValueLittleEndian(val);
  assertEquals(val,in.readChar());
}
