{
  float val=RND.nextFloat();
  reset();
  out.unsafeWriteFloat(val);
  checkValueLittleEndian(val);
  assertEquals(val,in.readFloat(),0);
  reset();
  out.writeFloat(val);
  checkValueLittleEndian(val);
  assertEquals(val,in.readFloat(),0);
  reset();
  out.writeFloatArray(new float[]{val});
  checkValueLittleEndian(val);
  assertEquals(val,in.readFloatArray(1)[0],0);
}
