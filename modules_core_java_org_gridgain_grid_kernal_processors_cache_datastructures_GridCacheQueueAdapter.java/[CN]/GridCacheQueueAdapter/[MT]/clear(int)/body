{
  try {
    GridBiTuple<Long,Long> t=(GridBiTuple<Long,Long>)cache.transformCompute(queueKey,new ClearClosure(uuid));
    if (t == null)     return;
    checkRemoved(t.get1());
    removeKeys(cache,uuid,collocated(),t.get1(),t.get2(),batchSize);
  }
 catch (  GridException e) {
    throw new GridRuntimeException(e);
  }
}
