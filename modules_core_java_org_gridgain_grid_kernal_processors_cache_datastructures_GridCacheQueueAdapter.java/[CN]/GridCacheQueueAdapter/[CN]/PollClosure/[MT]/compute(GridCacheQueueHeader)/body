{
  if (queueRemoved(header,uuid))   return QUEUE_REMOVED_IDX;
  if (header.empty())   return null;
  Set<Long> rmvIdx=header.removedIndexes();
  if (rmvIdx == null)   return header.head();
  long next=header.head();
  do {
    if (!rmvIdx.contains(next))     return next;
    next++;
  }
 while (next != header.tail());
  return null;
}
