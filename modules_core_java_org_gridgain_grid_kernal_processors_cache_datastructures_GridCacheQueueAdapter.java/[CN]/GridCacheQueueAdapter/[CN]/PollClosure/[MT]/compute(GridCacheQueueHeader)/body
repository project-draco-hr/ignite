{
  if (queueRemoved(hdr,uuid))   return QUEUE_REMOVED_IDX;
  if (hdr.empty())   return null;
  Set<Long> rmvIdx=hdr.removedIndexes();
  if (rmvIdx == null)   return hdr.head();
  long next=hdr.head();
  do {
    if (!rmvIdx.contains(next))     return next;
    next++;
  }
 while (next != hdr.tail());
  return null;
}
