{
  cache=true;
  startGrids(3);
  try {
    checkAffinity();
    client=true;
    startGrid(3);
    checkAffinity();
    startGrid(4);
    checkAffinity();
    cache=false;
    startGrid(5);
    checkAffinity();
    stopGrid(5);
    checkAffinity();
    stopGrid(4);
    checkAffinity();
    stopGrid(3);
    checkAffinity();
  }
  finally {
    stopAllGrids();
  }
}
