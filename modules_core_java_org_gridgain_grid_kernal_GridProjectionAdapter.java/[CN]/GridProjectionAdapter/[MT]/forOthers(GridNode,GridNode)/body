{
  A.notNull(node,"node");
  A.notNull(node,"nodes");
  Collection<UUID> ids;
  if (!F.isEmpty(nodes)) {
    ids=new HashSet<>(nodes.length + 1);
    ids.add(node.id());
    for (    GridNode n : nodes)     ids.add(n.id());
  }
 else   ids=Collections.singleton(node.id());
  return forOthers(ids);
}
