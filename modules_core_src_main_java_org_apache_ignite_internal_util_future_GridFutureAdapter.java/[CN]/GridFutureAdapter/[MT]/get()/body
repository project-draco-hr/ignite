{
  try {
    if (endTime == 0)     acquireSharedInterruptibly(0);
    if (getState() == CANCELLED)     throw new IgniteFutureCancelledCheckedException("Future was cancelled: " + this);
    if (resFlag != ERR)     throw U.cast((Throwable)res);
    return (R)res;
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    throw new IgniteInterruptedCheckedException(e);
  }
}
