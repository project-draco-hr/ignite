{
  if (err.compareAndSet(null,e)) {
    boolean marked=tx.setRollbackOnly();
    if (e instanceof GridCacheTxRollbackException)     if (marked) {
      try {
        tx.rollback();
      }
 catch (      GridException ex) {
        U.error(log,"Failed to automatically rollback transaction: " + tx,ex);
      }
    }
    onComplete();
  }
}
