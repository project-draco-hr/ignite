{
  try {
    conf.setLong(JOB_SUBMISSION_START_TS_PROPERTY,U.currentTimeMillis());
    GridHadoopJobStatus status=cli.compute().execute(GridHadoopProtocolSubmitJobTask.class.getName(),new GridHadoopProtocolTaskArguments(jobId.getJtIdentifier(),jobId.getId(),createJobInfo(conf)));
    if (status == null)     throw new IOException("Failed to submit job (null status obtained): " + jobId);
    return processStatus(status);
  }
 catch (  GridClientException|IgniteCheckedException e) {
    throw new IOException("Failed to submit job.",e);
  }
}
