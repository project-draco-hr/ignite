{
  super.prepareMarshal(ctx);
  if (owned != null) {
    ownedKeys=owned.keySet();
    ownedVals=owned.values();
    for (    IgniteTxKey key : ownedKeys)     key.prepareMarshal(ctx.cacheContext(key.cacheId()));
    if (ctx.deploymentEnabled()) {
      for (      IgniteTxKey k : owned.keySet())       prepareObject(k,ctx);
    }
  }
  if (nearWrites != null)   marshalTx(nearWrites,ctx);
}
