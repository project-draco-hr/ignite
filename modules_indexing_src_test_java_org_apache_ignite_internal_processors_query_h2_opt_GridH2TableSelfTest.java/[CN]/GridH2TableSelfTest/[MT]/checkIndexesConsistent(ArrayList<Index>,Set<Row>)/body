{
  for (  Index idx : idxs) {
    if (!(idx instanceof GridH2TreeIndex))     continue;
    Set<Row> set=new HashSet<>();
    Iterator<GridH2Row> iter=((GridH2TreeIndex)idx).rows();
    while (iter.hasNext())     assertTrue(set.add(iter.next()));
    if (rowSet == null)     rowSet=set;
 else     assertEquals(rowSet,set);
  }
  return rowSet;
}
