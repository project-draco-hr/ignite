{
  GridCacheProjection<GridCacheAffinityKey<UUID>,AffinityPerson> cache=cache(g);
  GridCacheQuery<GridCacheAffinityKey<UUID>,AffinityPerson> masters=cache.queries().createQuery(TEXT,AffinityPerson.class,"Master");
  GridCacheQuery<GridCacheAffinityKey<UUID>,AffinityPerson> bachelors=cache.queries().createQuery(TEXT,AffinityPerson.class,"Bachelor");
  print("Following people have 'Master Degree' in their resumes: ",masters.execute(p));
  print("Following people have 'Bachelor Degree' in their resumes: ",bachelors.execute(p));
}
