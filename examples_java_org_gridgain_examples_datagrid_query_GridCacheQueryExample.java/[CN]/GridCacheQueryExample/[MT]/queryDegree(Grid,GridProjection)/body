{
  GridCacheProjection<GridCacheAffinityKey<UUID>,AffinityPerson> cache=cache(g);
  GridCacheQuery<GridCacheAffinityKey<UUID>,AffinityPerson> masters=cache.queries().createQuery(TEXT,AffinityPerson.class,"Master").projection(p);
  GridCacheQuery<GridCacheAffinityKey<UUID>,AffinityPerson> bachelors=cache.queries().createQuery(TEXT,AffinityPerson.class,"Bachelor").projection(p);
  print("Following people have 'Master Degree' in their resumes: ",masters.execute());
  print("Following people have 'Bachelor Degree' in their resumes: ",bachelors.execute());
}
