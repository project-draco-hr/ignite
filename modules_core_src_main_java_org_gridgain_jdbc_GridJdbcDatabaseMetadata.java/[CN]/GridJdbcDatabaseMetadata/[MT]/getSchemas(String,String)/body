{
  updateMetaData();
  List<List<Object>> rows=new ArrayList<>(meta.size());
  for (  String schema : meta.keySet())   if (matches(schema,schemaPtrn))   rows.add(Arrays.<Object>asList(schema,(String)null));
  return new GridJdbcResultSet(conn.createStatement0(),Collections.<String>emptyList(),Arrays.asList("TABLE_SCHEM","TABLE_CATALOG"),Arrays.<String>asList(String.class.getName(),String.class.getName()),rows);
}
