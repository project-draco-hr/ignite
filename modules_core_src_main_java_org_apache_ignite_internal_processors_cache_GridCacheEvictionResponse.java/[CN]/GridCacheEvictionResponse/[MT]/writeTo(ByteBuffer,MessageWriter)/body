{
  writer.setBuffer(buf);
  if (!super.writeTo(buf,writer))   return false;
  if (!writer.isHeaderWritten()) {
    if (!writer.writeHeader(directType(),(byte)6))     return false;
    writer.onHeaderWritten();
  }
switch (writer.state()) {
case 3:
    if (!writer.writeBoolean("err",err))     return false;
  writer.incrementState();
case 4:
if (!writer.writeLong("futId",futId)) return false;
writer.incrementState();
case 5:
if (!writer.writeCollection("rejectedKeyBytes",rejectedKeyBytes,Type.BYTE_ARR)) return false;
writer.incrementState();
}
return true;
}
