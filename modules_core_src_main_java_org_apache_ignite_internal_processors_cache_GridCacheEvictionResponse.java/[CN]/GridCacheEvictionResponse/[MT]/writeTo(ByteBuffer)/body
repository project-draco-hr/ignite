{
  MessageWriteState state=MessageWriteState.get();
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 3:
    if (!writer.writeBoolean("err",err))     return false;
  state.increment();
case 4:
if (!writer.writeLong("futId",futId)) return false;
state.increment();
case 5:
if (!writer.writeCollection("rejectedKeyBytes",rejectedKeyBytes,byte[].class)) return false;
state.increment();
}
return true;
}
