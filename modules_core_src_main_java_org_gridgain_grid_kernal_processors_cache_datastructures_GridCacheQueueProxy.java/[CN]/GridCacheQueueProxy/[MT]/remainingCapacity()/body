{
  gate.enter();
  try {
    if (cctx.transactional())     return CU.outTx(new Callable<Integer>(){
      @Override public Integer call() throws Exception {
        return delegate.remainingCapacity();
      }
    }
,cctx);
    return delegate.remainingCapacity();
  }
 catch (  IgniteCheckedException e) {
    throw new IgniteException(e);
  }
 finally {
    gate.leave();
  }
}
