{
  gate.enter();
  try {
    if (cctx.transactional())     return CU.outTx(new Callable<Integer>(){
      @Override public Integer call() throws Exception {
        return delegate.size();
      }
    }
,cctx);
    return delegate.size();
  }
 catch (  GridException e) {
    throw new GridRuntimeException(e);
  }
 finally {
    gate.leave();
  }
}
