{
  gate.enter();
  try {
    if (cctx.transactional())     return CU.outTx(new Callable<Boolean>(){
      @Override public Boolean call() throws Exception {
        return delegate.remove(item);
      }
    }
,cctx);
    return delegate.remove(item);
  }
 catch (  GridException e) {
    throw new GridRuntimeException(e);
  }
 finally {
    gate.leave();
  }
}
