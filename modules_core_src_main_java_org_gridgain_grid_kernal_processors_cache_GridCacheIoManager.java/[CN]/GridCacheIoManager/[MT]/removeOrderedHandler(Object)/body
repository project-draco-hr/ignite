{
  if (orderedHandlers.remove(topic) != null) {
    cctx.gridIO().removeMessageId(topic);
    cctx.gridIO().removeMessageListener(topic);
    if (log != null && log.isDebugEnabled())     log.debug("Unregistered ordered cache communication handler for topic:" + topic);
  }
 else   if (log != null)   U.warn(log,"Failed to unregister ordered cache communication handler because it was not found " + "for topic: " + topic);
}
