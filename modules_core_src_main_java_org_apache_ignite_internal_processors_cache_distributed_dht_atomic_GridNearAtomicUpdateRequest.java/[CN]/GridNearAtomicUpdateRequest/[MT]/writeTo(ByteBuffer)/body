{
  MessageWriteState state=MessageWriteState.get();
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 3:
    if (!writer.writeMessage("drExpireTimes",drExpireTimes))     return false;
  state.increment();
case 4:
if (!writer.writeMessage("drTtls",drTtls)) return false;
state.increment();
case 5:
if (!writer.writeCollection("drVers",drVers,GridCacheVersion.class)) return false;
state.increment();
case 6:
if (!writer.writeByteArray("expiryPlcBytes",expiryPlcBytes)) return false;
state.increment();
case 7:
if (!writer.writeBoolean("fastMap",fastMap)) return false;
state.increment();
case 8:
if (!writer.writeObjectArray("filterBytes",filterBytes,byte[].class)) return false;
state.increment();
case 9:
if (!writer.writeBoolean("forceTransformBackups",forceTransformBackups)) return false;
state.increment();
case 10:
if (!writer.writeMessage("futVer",futVer)) return false;
state.increment();
case 11:
if (!writer.writeBoolean("hasPrimary",hasPrimary)) return false;
state.increment();
case 12:
if (!writer.writeObjectArray("invokeArgsBytes",invokeArgsBytes,byte[].class)) return false;
state.increment();
case 13:
if (!writer.writeCollection("keyBytes",keyBytes,byte[].class)) return false;
state.increment();
case 14:
if (!writer.writeEnum("op",op)) return false;
state.increment();
case 15:
if (!writer.writeBoolean("retval",retval)) return false;
state.increment();
case 16:
if (!writer.writeUuid("subjId",subjId)) return false;
state.increment();
case 17:
if (!writer.writeEnum("syncMode",syncMode)) return false;
state.increment();
case 18:
if (!writer.writeInt("taskNameHash",taskNameHash)) return false;
state.increment();
case 19:
if (!writer.writeLong("topVer",topVer)) return false;
state.increment();
case 20:
if (!writer.writeMessage("updateVer",updateVer)) return false;
state.increment();
case 21:
if (!writer.writeCollection("valBytes",valBytes,GridCacheValueBytes.class)) return false;
state.increment();
}
return true;
}
