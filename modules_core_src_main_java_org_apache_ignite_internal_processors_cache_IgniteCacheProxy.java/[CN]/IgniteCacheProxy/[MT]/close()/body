{
  if (!gate.enterIfNotClosed())   return;
  IgniteInternalFuture<?> fut;
  try {
    fut=context().kernalContext().cache().dynamicStopCache(context().name());
  }
  finally {
    gate.leave();
  }
  try {
    fut.get();
  }
 catch (  IgniteCheckedException e) {
    throw CU.convertToCacheException(e);
  }
}
