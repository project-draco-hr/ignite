{
  GridCacheGateway<K,V> gate=this.gate;
  if (!onEnterIfNoStop(gate))   return;
  IgniteInternalFuture<?> fut;
  try {
    fut=ctx.kernalContext().cache().dynamicCloseCache(ctx.name());
  }
  finally {
    onLeave(gate);
  }
  try {
    fut.get();
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
