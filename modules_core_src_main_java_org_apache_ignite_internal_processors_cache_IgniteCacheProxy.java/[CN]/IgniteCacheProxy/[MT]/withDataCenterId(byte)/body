{
  CacheOperationContext prev=onEnter(gate,opCtx);
  try {
    Byte prevDataCenterId=opCtx != null ? opCtx.dataCenterId() : null;
    if (prevDataCenterId != null && dataCenterId == prevDataCenterId)     return this;
    CacheOperationContext opCtx0=new CacheOperationContext(opCtx != null && opCtx.skipStore(),opCtx != null ? opCtx.subjectId() : null,opCtx != null && opCtx.isKeepBinary(),opCtx != null ? opCtx.expiry() : null,opCtx != null && opCtx.noRetries(),dataCenterId);
    return new IgniteCacheProxy<>(ctx,delegate,opCtx0,isAsync(),lock);
  }
  finally {
    onLeave(gate,prev);
  }
}
