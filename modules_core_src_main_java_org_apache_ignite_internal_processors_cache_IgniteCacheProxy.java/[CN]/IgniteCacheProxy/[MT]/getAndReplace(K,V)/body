{
  try {
    CacheOperationContext prev=onEnter(opCtx);
    try {
      if (isAsync()) {
        setFuture(delegate.getAndReplaceAsync(key,val));
        return null;
      }
 else       return delegate.getAndReplace(key,val);
    }
  finally {
      onLeave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
