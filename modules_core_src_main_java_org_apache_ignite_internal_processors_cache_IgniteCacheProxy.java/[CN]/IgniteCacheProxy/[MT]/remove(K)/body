{
  try {
    CacheOperationContext prev=gate.enter(prjCtx);
    try {
      if (isAsync()) {
        setFuture(delegate.removeAsync(key));
        return false;
      }
 else       return delegate.remove(key);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
