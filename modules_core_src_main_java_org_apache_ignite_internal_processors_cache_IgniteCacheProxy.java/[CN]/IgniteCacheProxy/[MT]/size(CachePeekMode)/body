{
  CacheOperationContext prev=gate.enter(opCtx);
  try {
    if (isAsync()) {
      setFuture(delegate.sizeAsync(peekModes));
      return 0;
    }
 else     return delegate.size(peekModes);
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
 finally {
    gate.leave(prev);
  }
}
