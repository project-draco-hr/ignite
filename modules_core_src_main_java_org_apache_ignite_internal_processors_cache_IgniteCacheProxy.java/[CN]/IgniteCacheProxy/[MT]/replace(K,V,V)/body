{
  try {
    CacheOperationContext prev=onEnter(opCtx);
    try {
      if (isAsync()) {
        setFuture(delegate.replaceAsync(key,oldVal,newVal));
        return false;
      }
 else       return delegate.replace(key,oldVal,newVal);
    }
  finally {
      onLeave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
