{
  try {
    GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
    try {
      if (isAsync()) {
        setFuture(delegate.getAsync(key));
        return null;
      }
 else       return delegate.get(key);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
