{
  try {
    CacheOperationContext prev=onEnter(opCtx);
    try {
      if (isAsync()) {
        setFuture(delegate.invokeAllAsync(map,args));
        return null;
      }
 else       return delegate.invokeAll(map,args);
    }
  finally {
      onLeave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
