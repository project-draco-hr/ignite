{
  try {
    GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
    try {
      if (isAsync()) {
        curFut.set(delegate.putAsync(key,val));
        return null;
      }
 else       return delegate.put(key,val);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
