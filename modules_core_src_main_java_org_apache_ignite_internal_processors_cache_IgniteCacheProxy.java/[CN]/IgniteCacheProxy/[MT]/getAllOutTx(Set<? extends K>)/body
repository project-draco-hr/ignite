{
  try {
    CacheOperationContext prev=onEnter(opCtx);
    try {
      if (isAsync()) {
        setFuture(delegate.getAllOutTxAsync(keys));
        return null;
      }
 else       return delegate.getAllOutTx(keys);
    }
  finally {
      onLeave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
