{
  GridCacheGateway<K,V> gate=this.gate;
  CacheOperationContext prev=onEnter(gate,opCtx);
  try {
    if (isAsync())     setFuture(delegate.clearAsync(key));
 else     delegate.clear(key);
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
 finally {
    onLeave(gate,prev);
  }
}
