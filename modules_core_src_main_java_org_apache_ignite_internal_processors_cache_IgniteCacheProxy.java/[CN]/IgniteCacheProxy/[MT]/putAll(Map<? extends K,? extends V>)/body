{
  try {
    CacheProjectionContext<K,V> prev=gate.enter(prjCtx);
    try {
      if (isAsync())       setFuture(delegate.putAllAsync(map));
 else       delegate.putAll(map);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
