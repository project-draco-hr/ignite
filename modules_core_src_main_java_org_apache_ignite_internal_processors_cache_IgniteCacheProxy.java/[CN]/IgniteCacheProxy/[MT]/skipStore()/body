{
  CacheProjectionContext prev=gate.enter(prjCtx);
  try {
    boolean skip=prjCtx != null && prjCtx.skipStore();
    if (skip)     return this;
    CacheProjectionContext prj0=new CacheProjectionContext(true,prjCtx != null ? prjCtx.subjectId() : null,prjCtx != null && prjCtx.isKeepPortable(),prjCtx != null ? prjCtx.expiry() : null);
    return new IgniteCacheProxy<>(ctx,delegate,prj0,isAsync());
  }
  finally {
    gate.leave(prev);
  }
}
