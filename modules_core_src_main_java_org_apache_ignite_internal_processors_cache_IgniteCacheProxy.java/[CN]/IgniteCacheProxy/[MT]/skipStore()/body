{
  GridCacheGateway<K,V> gate=this.gate;
  CacheOperationContext prev=onEnter(gate,opCtx);
  try {
    boolean skip=opCtx != null && opCtx.skipStore();
    if (skip)     return this;
    CacheOperationContext opCtx0=new CacheOperationContext(true,opCtx != null ? opCtx.subjectId() : null,opCtx != null && opCtx.isKeepBinary(),opCtx != null ? opCtx.expiry() : null,opCtx != null && opCtx.noRetries());
    return new IgniteCacheProxy<>(ctx,delegate,opCtx0,isAsync(),lock);
  }
  finally {
    onLeave(gate,prev);
  }
}
