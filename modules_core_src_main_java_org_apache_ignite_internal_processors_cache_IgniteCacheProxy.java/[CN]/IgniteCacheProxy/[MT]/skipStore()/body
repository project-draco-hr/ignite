{
  CacheOperationContext prev=onEnter(opCtx);
  try {
    boolean skip=opCtx != null && opCtx.skipStore();
    if (skip)     return this;
    CacheOperationContext opCtx0=new CacheOperationContext(true,opCtx.subjectId(),opCtx.isKeepPortable(),opCtx.expiry());
    return new IgniteCacheProxy<>(ctx,delegate,opCtx0,isAsync(),lock);
  }
  finally {
    onLeave(prev);
  }
}
