{
  GridCacheProjectionImpl<K,V> prev=onEnter(prj);
  try {
    boolean skip=prj != null && prj.skipStore();
    if (skip)     return this;
    GridCacheProjectionImpl<K,V> prj0=new GridCacheProjectionImpl<>((prj != null ? prj : delegate),ctx,true,prj != null ? prj.subjectId() : null,prj != null && prj.isKeepPortable(),prj != null ? prj.expiry() : null);
    return new IgniteCacheProxy<>(ctx,prj0,prj0,isAsync());
  }
  finally {
    onLeave(prev);
  }
}
