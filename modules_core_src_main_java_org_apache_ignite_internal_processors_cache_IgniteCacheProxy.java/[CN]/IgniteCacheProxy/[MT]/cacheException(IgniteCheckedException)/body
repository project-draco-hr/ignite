{
  if (e instanceof GridCachePartialUpdateException)   return new CachePartialUpdateException((GridCachePartialUpdateException)e);
 else   if (e instanceof IgniteNullArgumentCheckedException)   throw new NullPointerException(e.getMessage());
  if (e.getCause() instanceof CacheException)   return (CacheException)e.getCause();
  return new CacheException(e);
}
