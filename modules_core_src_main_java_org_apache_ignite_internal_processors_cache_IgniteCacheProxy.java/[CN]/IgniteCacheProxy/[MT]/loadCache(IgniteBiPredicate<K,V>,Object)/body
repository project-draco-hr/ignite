{
  try {
    CacheOperationContext prev=gate.enter(opCtx);
    try {
      if (isAsync())       setFuture(ctx.cache().globalLoadCacheAsync(p,args));
 else       ctx.cache().globalLoadCache(p,args);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
