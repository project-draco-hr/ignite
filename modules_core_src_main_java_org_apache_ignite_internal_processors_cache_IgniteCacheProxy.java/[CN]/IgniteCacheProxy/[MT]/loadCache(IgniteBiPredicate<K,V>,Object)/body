{
  try {
    GridCacheGateway<K,V> gate=this.gate;
    CacheOperationContext prev=onEnter(gate,opCtx);
    try {
      if (isAsync()) {
        if (ctx.cache().isLocal())         setFuture(ctx.cache().localLoadCacheAsync(p,args));
 else         setFuture(ctx.cache().globalLoadCacheAsync(p,args));
      }
 else {
        if (ctx.cache().isLocal())         ctx.cache().localLoadCache(p,args);
 else         ctx.cache().globalLoadCache(p,args);
      }
    }
  finally {
      onLeave(gate,prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
