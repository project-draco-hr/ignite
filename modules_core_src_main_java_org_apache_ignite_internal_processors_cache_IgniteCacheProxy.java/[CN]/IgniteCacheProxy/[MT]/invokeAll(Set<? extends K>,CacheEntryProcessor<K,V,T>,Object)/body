{
  try {
    CacheProjectionContext prev=gate.enter(prjCtx);
    try {
      if (isAsync()) {
        setFuture(delegate.invokeAllAsync(keys,entryProcessor,args));
        return null;
      }
 else       return delegate.invokeAll(keys,entryProcessor,args);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
