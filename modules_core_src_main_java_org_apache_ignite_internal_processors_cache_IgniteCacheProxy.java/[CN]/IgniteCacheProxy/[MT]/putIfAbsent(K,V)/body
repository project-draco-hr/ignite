{
  try {
    CacheProjectionContext<K,V> prev=gate.enter(prjCtx);
    try {
      if (isAsync()) {
        setFuture(delegate.putIfAbsentAsync(key,val));
        return false;
      }
 else       return delegate.putIfAbsent(key,val);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
