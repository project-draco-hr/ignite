{
  try {
    CacheProjectionContext<K,V> prev=gate.enter(prjCtx);
    try {
      if (isAsync())       setFuture(delegate.removeAllAsync(keys));
 else       delegate.removeAll(keys);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
