{
  try {
    GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
    try {
      if (isAsync())       curFut.set(delegate.removeAllAsync(keys));
 else       delegate.removeAll(keys);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
