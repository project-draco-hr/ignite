{
  CacheOperationContext prev=onEnter(opCtx);
  try {
    CacheOperationContext opCtx0=new CacheOperationContext(opCtx.skipStore(),opCtx.subjectId(),true,opCtx.expiry());
    return new IgniteCacheProxy<>((GridCacheContext<K1,V1>)ctx,(GridCacheAdapter<K1,V1>)delegate,opCtx0,isAsync(),lock);
  }
  finally {
    onLeave(prev);
  }
}
