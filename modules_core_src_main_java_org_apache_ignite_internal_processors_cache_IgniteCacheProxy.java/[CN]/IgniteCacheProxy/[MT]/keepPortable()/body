{
  CacheProjectionContext prev=gate.enter(prjCtx);
  try {
    CacheProjectionContext prj0=new CacheProjectionContext(prjCtx != null ? prjCtx.skipStore() : false,prjCtx != null ? prjCtx.subjectId() : null,true,prjCtx != null ? prjCtx.expiry() : null);
    return new IgniteCacheProxy<>((GridCacheContext<K1,V1>)ctx,(GridCacheAdapter<K1,V1>)delegate,prj0,isAsync());
  }
  finally {
    gate.leave(prev);
  }
}
