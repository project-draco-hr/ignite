{
  GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
  try {
    GridCacheProjectionImpl<K1,V1> prj0=new GridCacheProjectionImpl<>((CacheProjection<K1,V1>)(prj != null ? prj : lockFreeCache),(GridCacheContext<K1,V1>)context(),prj != null && prj.skipStore(),prj != null ? prj.subjectId() : null,true,prj != null ? prj.expiry() : null);
    return new IgniteCacheProxy<>((GridCacheContext<K1,V1>)context(),prj0,prj0,isAsync());
  }
  finally {
    gate.leave(prev);
  }
}
