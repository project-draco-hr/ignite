{
  GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
  try {
    GridCacheProjectionImpl<K1,V1> prj0=new GridCacheProjectionImpl<>((GridCacheProjection<K1,V1>)(prj != null ? prj : delegate),(GridCacheContext<K1,V1>)ctx,null,null,prj != null ? prj.flags() : null,prj != null ? prj.subjectId() : null,true,prj != null ? prj.expiry() : null);
    return new IgniteCacheProxy<>((GridCacheContext<K1,V1>)ctx,prj0,prj0,isAsync());
  }
  finally {
    gate.leave(prev);
  }
}
