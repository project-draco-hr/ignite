{
  CacheOperationContext prev=gate.enter(opCtx);
  try {
    CacheOperationContext prj=new CacheOperationContext(opCtx.skipStore(),opCtx.subjectId(),true,opCtx.expiry());
    return new IgniteCacheProxy<>((GridCacheContext<K1,V1>)ctx,(GridCacheAdapter<K1,V1>)delegate,prj,isAsync());
  }
  finally {
    gate.leave(prev);
  }
}
