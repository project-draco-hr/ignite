{
  CacheProjectionContext prev=gate.enter(prjCtx);
  try {
    CacheProjectionContext prj=new CacheProjectionContext(prjCtx != null ? prjCtx.skipStore() : false,prjCtx != null ? prjCtx.subjectId() : null,true,prjCtx != null ? prjCtx.expiry() : null);
    return new IgniteCacheProxy<>((GridCacheContext<K1,V1>)ctx,(GridCacheAdapter<K1,V1>)delegate,prj,isAsync());
  }
  finally {
    gate.leave(prev);
  }
}
