{
  GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
  try {
    Set<CacheFlag> res;
    Set<CacheFlag> flags0=prj != null ? prj.flags() : null;
    if (flags0 != null) {
      if (flags0.contains(flag))       return this;
      res=EnumSet.copyOf(flags0);
    }
