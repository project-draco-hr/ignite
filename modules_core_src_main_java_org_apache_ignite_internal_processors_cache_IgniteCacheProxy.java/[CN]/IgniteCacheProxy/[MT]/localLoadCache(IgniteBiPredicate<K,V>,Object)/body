{
  try {
    GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
    try {
      if (isAsync())       setFuture(delegate.<K,V>cache().loadCacheAsync(p,0,args));
 else       delegate.<K,V>cache().loadCache(p,0,args);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
