{
  try {
    CacheProjectionContext prev=gate.enter(prjCtx);
    try {
      if (isAsync())       setFuture(delegate.localLoadCacheAsync(p,args));
 else       delegate.localLoadCache(p,args);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
