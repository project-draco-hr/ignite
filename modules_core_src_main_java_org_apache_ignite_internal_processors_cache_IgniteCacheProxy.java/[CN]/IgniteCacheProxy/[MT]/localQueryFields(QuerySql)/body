{
  A.notNull(filter,"filter");
  GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
  try {
    return new QueryCursorImpl<>(ctx.kernalContext().query().queryLocalFields(ctx.name(),filter.getSql(),filter.getArgs()));
  }
 catch (  Exception e) {
    if (e instanceof CacheException)     throw e;
    throw new CacheException(e);
  }
 finally {
    gate.leave(prev);
  }
}
