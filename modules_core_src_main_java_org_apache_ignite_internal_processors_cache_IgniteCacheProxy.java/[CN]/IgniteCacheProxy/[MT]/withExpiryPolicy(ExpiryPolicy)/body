{
  GridCacheProjectionImpl<K,V> prev=onEnter(prj);
  try {
    GridCacheProjectionEx<K,V> prj0=prj != null ? prj.withExpiryPolicy(plc) : delegate.withExpiryPolicy(plc);
    return new IgniteCacheProxy<>(ctx,prj0,(GridCacheProjectionImpl<K,V>)prj0,isAsync(),lock);
  }
  finally {
    onLeave(prev);
  }
}
