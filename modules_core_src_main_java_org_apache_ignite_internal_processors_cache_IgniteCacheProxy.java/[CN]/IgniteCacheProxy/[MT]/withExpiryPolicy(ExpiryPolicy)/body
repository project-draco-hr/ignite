{
  GridCacheGateway<K,V> gate=this.gate;
  CacheOperationContext prev=onEnter(gate,opCtx);
  try {
    CacheOperationContext prj0=opCtx != null ? opCtx.withExpiryPolicy(plc) : new CacheOperationContext(false,null,false,plc,false,null);
    return new IgniteCacheProxy<>(ctx,delegate,prj0,isAsync(),lock);
  }
  finally {
    onLeave(gate,prev);
  }
}
