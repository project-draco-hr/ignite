{
  try {
    GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
    try {
      if (isAsync())       setFuture(delegate.putxAsync(key,val));
 else       delegate.putx(key,val);
    }
  finally {
      gate.leave(prev);
    }
  }
 catch (  IgniteCheckedException e) {
    throw cacheException(e);
  }
}
