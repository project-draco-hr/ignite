{
  GridCacheProjectionImpl<K,V> prev=gate.enter(prj);
  try {
    GridCacheQuery<Map.Entry<K,V>> qry=delegate.queries().createScanQuery(null);
    qry.includeBackups(ctx.config().getCacheMode() == GridCacheMode.REPLICATED);
    qry.keepAll(false);
    GridCacheQueryFuture<Map.Entry<K,V>> fut=qry.execute();
    return ctx.itHolder().iterator(fut,new CacheIteratorConverter<Entry<K,V>,Map.Entry<K,V>>(){
      @Override protected Entry<K,V> convert(      Map.Entry<K,V> e){
        return new CacheEntryImpl<>(e.getKey(),e.getValue());
      }
      @Override protected void remove(      Entry<K,V> item){
        IgniteCacheProxy.this.remove(item.getKey());
      }
    }
);
  }
  finally {
    gate.leave(prev);
  }
}
