{
  Path file=new Path(ggfsHome,"out-0");
  benchmarkWrite(fs,file,fileLen,1024 * 1024,(short)1,null);
  for (int i=0; i < 5; i++) {
    if (op == OP_WRITE)     benchmarkWrite(fs,file,fileLen,1024 * 1024,(short)1,null);
 else     benchmarkRead(fs,file,1024 * 1024,null);
  }
  fs.delete(file,true);
}
