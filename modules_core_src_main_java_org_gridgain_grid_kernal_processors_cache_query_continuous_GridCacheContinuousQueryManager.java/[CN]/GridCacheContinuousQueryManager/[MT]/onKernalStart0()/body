{
  if (intLsnrCnt.get() > 0 || lsnrCnt.get() > 0) {
    Collection<ClusterNode> nodes=cctx.discovery().cacheNodes(cctx.name(),-1);
    for (    ClusterNode n : nodes) {
      if (!n.version().greaterThanEqual(6,2,0))       throw new GridException("Rolling update is not supported for continuous queries " + "for versions below 6.2.0");
    }
  }
}
