{
switch (edition) {
case "dev-ent":
case "dev-os":
    return PLATFORM;
case "datagrid-ent":
case "datagrid-os":
  return DATA_GRID;
case "hadoop-ent":
case "hadoop-os":
return HADOOP;
case "streaming-ent":
case "streaming-os":
return STREAMING;
case "mongo-ent":
case "mongo-os":
return MONGO;
case "platform-ent":
case "platform-os":
return PLATFORM;
}
throw new GridRuntimeException("Failed to determine GridGain edition: " + edition);
}
