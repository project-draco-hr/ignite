{
  lock.writeLock().lock();
  try {
    node2part=null;
    part2node=new HashMap<>();
    lastExchangeId=null;
    updateSeq.set(1);
    topReadyFut=null;
    rebalancedTopVer=AffinityTopologyVersion.NONE;
    topVer=AffinityTopologyVersion.NONE;
  }
  finally {
    lock.writeLock().unlock();
  }
}
