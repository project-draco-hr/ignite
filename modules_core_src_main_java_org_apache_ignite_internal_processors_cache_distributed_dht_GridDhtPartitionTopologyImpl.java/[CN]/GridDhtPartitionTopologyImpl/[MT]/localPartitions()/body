{
  LinkedList<GridDhtLocalPartition> list=new LinkedList<>();
  lock.readLock().lock();
  try {
    for (int i=0; i < locParts.length; i++) {
      GridDhtLocalPartition part=locParts[i];
      if (part != null)       list.add(part);
    }
    return list;
  }
  finally {
    lock.readLock().unlock();
  }
}
