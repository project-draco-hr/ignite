{
  IgniteConfiguration cfg=super.getConfiguration(gridName);
  CacheConfiguration cache0=cacheConfiguration(null);
  CacheConfiguration cache1=cacheConfiguration(null);
  cache1.setName(CACHE1);
  if (gridName.contains("0"))   cfg.setCacheConfiguration();
 else   if (gridName.contains("1"))   cfg.setCacheConfiguration(cache0);
 else   cfg.setCacheConfiguration(cache0,cache1);
  return cfg;
}
