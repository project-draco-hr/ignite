{
  for (int i=0; i < gridCount(); ++i) {
    if (grid(i).cachex(null) != null)     checkAffinity(grid(i).<String,Integer>jcache(null),grid(i).<String,Integer>cache(null));
    if (grid(i).cachex(CACHE1) != null)     checkAffinity(grid(i).<String,Integer>jcache(CACHE1),grid(i).<String,Integer>cache(CACHE1));
  }
}
