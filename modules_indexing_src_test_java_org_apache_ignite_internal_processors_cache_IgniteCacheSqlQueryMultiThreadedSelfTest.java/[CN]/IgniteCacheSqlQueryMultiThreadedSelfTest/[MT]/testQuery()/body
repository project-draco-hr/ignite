{
  final IgniteCache<Integer,Person> cache=grid(0).jcache(null);
  for (int i=0; i < 2000; i++)   cache.put(i,new Person(i));
  GridTestUtils.runMultiThreaded(new Callable<Void>(){
    @Override public Void call() throws Exception {
      for (int i=0; i < 100; i++) {
        QueryCursor<Cache.Entry<Integer,Person>> qry=cache.query(new SqlQuery("Person","age >= 0"));
        int cnt=0;
        for (        Cache.Entry<Integer,Person> e : qry)         cnt++;
        assertEquals(2000,cnt);
      }
      return null;
    }
  }
,16,"test");
}
