{
  try {
    info("Removing values form cache [0," + cnt + ')');
    CU.inTx(cache,concurrency,isolation,new CIX1<CacheProjection<String,Integer>>(){
      @Override public void applyx(      CacheProjection<String,Integer> cache) throws IgniteCheckedException {
        for (int i=0; i < cnt; i++)         cache.removex("key" + i);
      }
    }
);
  }
 catch (  IgniteCheckedException e) {
    if (!X.hasCause(e,ClusterTopologyException.class))     throw e;
 else     info("Failed to remove values from cache due to topology exception [0," + cnt + ')');
  }
}
