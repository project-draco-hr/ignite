{
  mode=PROXY;
  setSecondaryFsUri=true;
  String errMsg=null;
  try {
    startUp();
  }
 catch (  GridException e) {
    errMsg=e.getCause().getMessage();
  }
  assertTrue(errMsg.startsWith("Grid configuration parameter invalid: secondaryHadoopFileSystemConfigPath cannot be null " + "when mode is SECONDARY"));
}
