{
  setSecondaryFs=true;
  mode=DUAL_ASYNC;
  pathModes(F.t("/gridgain/primary",PROXY),F.t("/gridgain/proxy",DUAL_SYNC),F.t("/gridgain/sync",DUAL_ASYNC),F.t("/gridgain/async",PRIMARY));
  startUp();
  checkMode("/gridgain/primary",PRIMARY);
  checkMode("/gridgain/proxy",PROXY);
  checkMode("/gridgain/sync",DUAL_SYNC);
  checkMode("/gridgain/async",DUAL_ASYNC);
}
