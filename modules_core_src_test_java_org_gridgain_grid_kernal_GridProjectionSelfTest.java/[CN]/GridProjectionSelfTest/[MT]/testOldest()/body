{
  GridProjection oldest=grid.cluster().forOldest();
  GridNode node=null;
  long minOrder=Long.MAX_VALUE;
  for (  GridNode n : grid.cluster().nodes()) {
    if (n.order() < minOrder) {
      node=n;
      minOrder=n.order();
    }
  }
  assertEquals(oldest.node(),grid.cluster().forNode(node).node());
}
