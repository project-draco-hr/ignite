{
  GridProjection youngest=grid.cluster().forYoungest();
  GridNode node=null;
  long maxOrder=Long.MIN_VALUE;
  for (  GridNode n : grid.cluster().nodes()) {
    if (n.order() > maxOrder) {
      node=n;
      maxOrder=n.order();
    }
  }
  assertEquals(youngest.node(),grid.cluster().forNode(node).node());
}
