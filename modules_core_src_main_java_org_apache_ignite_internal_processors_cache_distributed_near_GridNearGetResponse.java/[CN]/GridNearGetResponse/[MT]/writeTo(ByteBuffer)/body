{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 3:
    if (!writer.writeByteArray("entriesBytes",entriesBytes))     return false;
  state++;
case 4:
if (!writer.writeByteArray("errBytes",errBytes)) return false;
state++;
case 5:
if (!writer.writeIgniteUuid("futId",futId)) return false;
state++;
case 6:
if (!writer.writeCollection("invalidParts",invalidParts,int.class)) return false;
state++;
case 7:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
state++;
case 8:
if (!writer.writeLong("topVer",topVer)) return false;
state++;
case 9:
if (!writer.writeMessage("ver",ver != null ? ver.clone() : null)) return false;
state++;
}
return true;
}
