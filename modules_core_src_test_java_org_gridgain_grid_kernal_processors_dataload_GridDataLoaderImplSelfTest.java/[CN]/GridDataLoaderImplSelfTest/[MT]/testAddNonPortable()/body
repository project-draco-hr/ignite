{
  try {
    portables=true;
    startGrids(2);
    Grid g0=grid(0);
    GridDataLoader<Integer,TestObjectBad> dataLdr=g0.dataLoader(null);
    Map<Integer,TestObjectBad> map=U.newHashMap(KEYS_COUNT);
    for (int i=0; i < KEYS_COUNT; i++)     map.put(i,new TestObjectBad(i));
    dataLdr.addData(map);
    dataLdr.close(false);
  }
  finally {
    G.stopAll(true);
  }
}
