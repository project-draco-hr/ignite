{
  try {
    startGrid(1);
    nodeId=UUID.randomUUID();
    startGrid(2);
    GridTestUtils.assertThrows(log,new Callable<Object>(){
      @Nullable @Override public Object call() throws Exception {
        startGrid(3);
        return null;
      }
    }
,GridException.class,null);
  }
  finally {
    stopAllGrids();
  }
}
