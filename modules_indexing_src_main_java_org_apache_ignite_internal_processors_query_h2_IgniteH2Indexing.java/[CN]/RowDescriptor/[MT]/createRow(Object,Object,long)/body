{
  try {
    return schema.offheap == null ? new GridH2KeyValueRowOnheap(this,key,keyType,val,valType,expirationTime) : new GridH2KeyValueRowOffheap(this,key,keyType,val,valType,expirationTime);
  }
 catch (  ClassCastException e) {
    throw new IgniteCheckedException("Failed to convert key to SQL type. " + "Please make sure that you always store each value type with the same key type or disable " + "'defaultIndexFixedTyping' property.",e);
  }
}
