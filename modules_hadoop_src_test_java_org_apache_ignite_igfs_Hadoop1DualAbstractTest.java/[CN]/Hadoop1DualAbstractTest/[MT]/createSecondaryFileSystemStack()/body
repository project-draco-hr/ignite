{
  startUnderlying();
  prepareConfiguration();
  IgniteHadoopIgfsSecondaryFileSystem second=new IgniteHadoopIgfsSecondaryFileSystem(secondaryUri,secondaryConfFullPath);
  FileSystem fileSystem=second.fileSystem();
  igfsSecondary=new HadoopFileSystemUniversalFileSystemAdapter(fileSystem);
  return second;
}
