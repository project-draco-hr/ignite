{
  commState.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!commState.typeWritten) {
    if (!commState.putByte(null,directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 8:
    if (!commState.putCacheVersion("baseVer",baseVer))     return false;
  commState.idx++;
case 9:
if (!commState.putBoolean("commit",commit)) return false;
commState.idx++;
case 10:
if (!commState.putCacheVersion("commitVer",commitVer)) return false;
commState.idx++;
case 11:
if (!commState.putGridUuid("futId",futId)) return false;
commState.idx++;
case 12:
if (!commState.putByteArray("grpLockKeyBytes",grpLockKeyBytes)) return false;
commState.idx++;
case 13:
if (!commState.putBoolean("invalidate",invalidate)) return false;
commState.idx++;
case 14:
if (recoveryWritesBytes != null) {
if (commState.it == null) {
if (!commState.putInt(null,recoveryWritesBytes.size())) return false;
commState.it=recoveryWritesBytes.iterator();
}
while (commState.it.hasNext() || commState.cur != NULL) {
if (commState.cur == NULL) commState.cur=commState.it.next();
if (!commState.putByteArray(null,(byte[])commState.cur)) return false;
commState.cur=NULL;
}
commState.it=null;
}
 else {
if (!commState.putInt(null,-1)) return false;
}
commState.idx++;
case 15:
if (!commState.putBoolean("syncCommit",syncCommit)) return false;
commState.idx++;
case 16:
if (!commState.putBoolean("syncRollback",syncRollback)) return false;
commState.idx++;
case 17:
if (!commState.putLong("threadId",threadId)) return false;
commState.idx++;
case 18:
if (!commState.putInt("txSize",txSize)) return false;
commState.idx++;
case 19:
if (writeEntriesBytes != null) {
if (commState.it == null) {
if (!commState.putInt(null,writeEntriesBytes.size())) return false;
commState.it=writeEntriesBytes.iterator();
}
while (commState.it.hasNext() || commState.cur != NULL) {
if (commState.cur == NULL) commState.cur=commState.it.next();
if (!commState.putByteArray(null,(byte[])commState.cur)) return false;
commState.cur=NULL;
}
commState.it=null;
}
 else {
if (!commState.putInt(null,-1)) return false;
}
commState.idx++;
case 20:
if (!commState.putBoolean("sys",sys)) return false;
commState.idx++;
}
return true;
}
