{
  GridCacheAttributes[] caches=n.attribute(ATTR_CACHE);
  if (caches == null)   return false;
  if (!hasCache(caches,cacheName,distributionMode))   return false;
  if (!F.isEmpty(cacheNames))   for (  String cn : cacheNames)   if (!hasCache(caches,cn,distributionMode))   return false;
  return true;
}
