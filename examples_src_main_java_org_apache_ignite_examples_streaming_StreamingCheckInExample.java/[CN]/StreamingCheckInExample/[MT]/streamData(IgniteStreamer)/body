{
  try {
    for (int i=0; i < CNT; i++) {
      CheckInEvent evt=new CheckInEvent(USER_NAMES[ThreadLocalRandom.current().nextInt(USER_NAMES.length)],new Location(RAND.nextDouble() + RAND.nextInt(MAX_X - 1),RAND.nextDouble() + RAND.nextInt(MAX_Y)));
      System.out.println(">>> Generating event: " + evt);
      streamer.addEvent(evt);
      Thread.sleep(1000);
    }
  }
 catch (  InterruptedException ignored) {
  }
}
