{
  ctx.denyOnFlag(LOCAL);
  String taskName=ctx.kernalContext().job().currentTaskName();
  Map<K,V> m=getAllInternal(Collections.singleton(key),filter != null ? new GridPredicate[]{filter} : null,ctx.isSwapOrOffheapEnabled(),ctx.isStoreEnabled(),ctx.hasFlag(CLONE),taskName,deserializePortable);
  return m.get(key);
}
