{
  map.setEntryFactory(new GridCacheMapEntryFactory(){
    /** 
 * {@inheritDoc} 
 */
    @Override public GridCacheMapEntry create(    GridCacheContext ctx,    long topVer,    KeyCacheObject key,    int hash,    CacheObject val,    GridCacheMapEntry next,    long ttl,    int hdrId){
      return new GridNearCacheEntry(ctx,key,hash,val,next,ttl,hdrId);
    }
  }
);
}
