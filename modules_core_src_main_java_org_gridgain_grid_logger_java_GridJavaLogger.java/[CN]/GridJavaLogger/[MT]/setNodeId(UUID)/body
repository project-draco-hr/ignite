{
  A.notNull(nodeId,"nodeId");
  if (this.nodeId == null) {
    this.nodeId=nodeId;
synchronized (mux) {
      if (nodeId != null) {
        try {
          addDefaultFileHandler(impl);
        }
 catch (        GridException e) {
          throw new RuntimeException("Failed to create default file handler.",e);
        }
      }
    }
  }
}
