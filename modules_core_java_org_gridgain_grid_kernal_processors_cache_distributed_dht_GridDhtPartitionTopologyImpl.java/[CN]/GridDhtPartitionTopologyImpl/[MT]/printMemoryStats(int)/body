{
  X.println(">>>  Cache partition topology stats [grid=" + cctx.gridName() + ", cache="+ cctx.name()+ ']');
  for (  GridDhtLocalPartition part : locParts.values()) {
    int size=part.size();
    if (size >= threshold)     X.println(">>>   Local partition [part=" + part.id() + ", size="+ size+ ']');
  }
}
