{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 3:
    if (!writer.writeBoolean("ack",ack))     return false;
  state++;
case 4:
if (!writer.writeByteArray("infoBytes",infoBytes)) return false;
state++;
case 5:
if (!writer.writeCollection("last",last,int.class)) return false;
state++;
case 6:
if (!writer.writeCollection("missed",missed,int.class)) return false;
state++;
case 7:
if (!writer.writeLong("updateSeq",updateSeq)) return false;
state++;
case 8:
if (!writer.writeInt("workerId",workerId)) return false;
state++;
}
return true;
}
