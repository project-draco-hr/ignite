{
  boolean rollback=true;
  try {
    if (e != null)     throw new GridClosureException(e);
    IgniteFuture<T> fut=postMiss(t);
    rollback=false;
    return fut;
  }
 catch (  IgniteCheckedException ex) {
    throw new GridClosureException(ex);
  }
 finally {
    if (rollback)     setRollbackOnly();
  }
}
