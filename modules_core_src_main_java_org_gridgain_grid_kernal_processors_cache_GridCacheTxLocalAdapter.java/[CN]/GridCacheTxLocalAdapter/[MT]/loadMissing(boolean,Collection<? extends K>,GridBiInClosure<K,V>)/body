{
  if (!async) {
    try {
      return new GridFinishedFuture<>(cctx.kernalContext(),cctx.store().loadAllFromStore(this,keys,c));
    }
 catch (    GridException e) {
      return new GridFinishedFuture<>(cctx.kernalContext(),e);
    }
  }
 else   return cctx.closures().callLocalSafe(new GPC<Boolean>(){
    @Override public Boolean call() throws Exception {
      return cctx.store().loadAllFromStore(GridCacheTxLocalAdapter.this,keys,c);
    }
  }
,true);
}
