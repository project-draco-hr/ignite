{
  GridConcurrentHashSet<GridCacheSetItemKey> set=setDataMap.get(key.setId());
  if (set == null) {
    if (rmv)     return;
    GridConcurrentHashSet<GridCacheSetItemKey> old=setDataMap.putIfAbsent(key.setId(),set=new GridConcurrentHashSet<>());
    if (old != null)     set=old;
  }
  if (rmv)   set.remove(key);
 else   set.add(key);
}
