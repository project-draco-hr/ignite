{
  GridCacheAffinityManager aff=cctx.affinity();
  for (  GridConcurrentHashSet<SetItemKey> set : setDataMap.values()) {
    Iterator<SetItemKey> iter=set.iterator();
    while (iter.hasNext()) {
      SetItemKey key=iter.next();
      if (aff.partition(key) == part)       iter.remove();
    }
  }
}
