{
  boolean wrongVer=X.hasCause(e,RemoteException.class) || (e.getMessage() != null && e.getMessage().contains("Failed on local"));
  IgfsException igfsErr=!wrongVer ? cast(detailMsg,e) : new IgfsInvalidHdfsVersionException("HDFS version you are connecting to differs from local " + "version.",e);
  return igfsErr;
}
