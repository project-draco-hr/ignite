{
  String locSeqName=UUID.randomUUID().toString();
  IgniteAtomicSequence seq=grid().atomicSequence(locSeqName,0,true);
  seq.addAndGet(153);
  seq.close();
  try {
    seq.addAndGet(153);
    fail("Exception expected.");
  }
 catch (  IgniteCheckedException e) {
    info("Caught expected exception: " + e);
  }
}
