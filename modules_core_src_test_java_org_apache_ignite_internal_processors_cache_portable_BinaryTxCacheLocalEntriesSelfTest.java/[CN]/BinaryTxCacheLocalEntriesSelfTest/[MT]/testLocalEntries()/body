{
  IgniteCache<Integer,BinaryObject> cache=grid(0).cache(null).withKeepBinary();
  final int ENTRY_CNT=10;
  for (int i=0; i < ENTRY_CNT; i++)   cache.put(i,userObject("user-" + i));
  assertEquals(ENTRY_CNT,cache.localSize(CachePeekMode.ALL));
  for (int i=0; i < gridCount(); i++)   jcache(i).withKeepBinary().localEntries();
  cache.removeAll();
}
