{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 21:
    if (!writer.writeEnum("isolation",isolation))     return false;
  state++;
case 22:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
state++;
case 23:
if (!writer.writeUuid("nearNodeId",nearNodeId)) return false;
state++;
case 24:
if (!writer.writeMessage("nearTtls",nearTtls != null ? nearTtls.clone() : null)) return false;
state++;
case 25:
if (!writer.writeCollection("nearWritesBytes",nearWritesBytes,byte[].class)) return false;
state++;
case 26:
if (!writer.writeBoolean("onePhaseCommit",onePhaseCommit)) return false;
state++;
case 27:
if (!writer.writeCollection("pendingVers",pendingVers,GridCacheVersion.class)) return false;
state++;
case 28:
if (!writer.writeUuid("subjId",subjId)) return false;
state++;
case 29:
if (!writer.writeBoolean("sysInvalidate",sysInvalidate)) return false;
state++;
case 30:
if (!writer.writeInt("taskNameHash",taskNameHash)) return false;
state++;
case 31:
if (!writer.writeLong("topVer",topVer)) return false;
state++;
case 32:
if (!writer.writeMessage("ttls",ttls != null ? ttls.clone() : null)) return false;
state++;
case 33:
if (!writer.writeMessage("writeVer",writeVer != null ? writeVer.clone() : null)) return false;
state++;
}
return true;
}
