{
  if (rmvd)   throw removedError();
  if (rmvCheck) {
    try {
      rmvd=seqView.get(key) == null;
    }
 catch (    IgniteCheckedException e) {
      throw U.convertException(e);
    }
    rmvCheck=false;
    if (rmvd) {
      ctx.kernalContext().dataStructures().onRemoved(key,this);
      throw removedError();
    }
  }
}
