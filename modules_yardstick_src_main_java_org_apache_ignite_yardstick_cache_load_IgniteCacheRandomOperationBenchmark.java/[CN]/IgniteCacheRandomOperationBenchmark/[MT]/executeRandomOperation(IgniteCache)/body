{
switch (Operation.valueOf(nextRandom(operations))) {
case PUT:
    doPut(cache);
  break;
case PUT_ALL:
doPutAll(cache);
break;
case GET:
doGet(cache);
break;
case GET_ALL:
doGetAll(cache);
break;
case INVOKE:
doInvoke(cache);
break;
case INVOKE_ALL:
doInvokeAll(cache);
break;
case REMOVE:
doRemove(cache);
break;
case REMOVE_ALL:
doRemoveAll(cache);
break;
case PUT_IF_ABSENT:
doPutIfAbsent(cache);
break;
case REPLACE:
doReplace(cache);
break;
case SCAN_QUERY:
doScanQuery(cache);
break;
case SQL_QUERY:
doSqlQuery(cache);
}
}
