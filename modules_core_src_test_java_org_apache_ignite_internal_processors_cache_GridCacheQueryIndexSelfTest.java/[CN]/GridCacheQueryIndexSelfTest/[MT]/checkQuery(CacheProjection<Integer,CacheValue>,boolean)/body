{
  CacheQuery<Map.Entry<Integer,CacheValue>> qry=cache.queries().createSqlQuery(CacheValue.class,"val >= 5");
  if (backups)   qry.includeBackups(true);
  Collection<Map.Entry<Integer,CacheValue>> queried=qry.execute().get();
  assertEquals("Unexpected query result: " + queried,5,queried.size());
}
