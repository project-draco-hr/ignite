{
  if (cls.isAssignableFrom(getClass()))   return cls.cast(this);
 else   if (cls.isAssignableFrom(CacheEntry.class) && e instanceof GridCacheVersionAware)   return (T)new CacheEntryImplEx<>(e.getKey(),e.getValue(),((GridCacheVersionAware)e).version());
  throw new IllegalArgumentException("Unwrapping to class is not supported: " + cls);
}
