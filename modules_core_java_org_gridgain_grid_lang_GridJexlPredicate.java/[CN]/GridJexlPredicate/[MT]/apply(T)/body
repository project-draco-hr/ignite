{
  lazyCompile();
  if (expr != null) {
    JexlContext ctx=new MapContext();
    ctx.set(var,t);
    for (    Map.Entry<String,Object> e : map.entrySet()) {
      ctx.set(e.getKey(),e.getValue());
    }
    try {
      Object obj=expr.evaluate(ctx);
      if (obj instanceof Boolean) {
        return (Boolean)obj;
      }
    }
 catch (    Exception ex) {
      F.wrap(ex);
    }
  }
  return false;
}
