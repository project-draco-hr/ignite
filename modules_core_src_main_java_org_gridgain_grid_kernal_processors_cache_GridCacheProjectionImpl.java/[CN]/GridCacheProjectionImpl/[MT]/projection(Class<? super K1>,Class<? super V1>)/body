{
  A.notNull(keyType,"keyType",valType,"valType");
  if (cctx.deploymentEnabled()) {
    try {
      cctx.deploy().registerClasses(keyType,valType);
    }
 catch (    GridException e) {
      throw new GridRuntimeException(e);
    }
  }
  GridCacheProjectionImpl<K1,V1> prj=new GridCacheProjectionImpl<>((GridCacheProjection<K1,V1>)this,(GridCacheContext<K1,V1>)cctx,CU.<K1,V1>typeFilter(keyType,valType),(GridPredicate<GridCacheEntry>)noNullEntryFilter.entryFilter,flags,subjId,GridPortableObject.class.isAssignableFrom(keyType),GridPortableObject.class.isAssignableFrom(valType));
  return new GridCacheProxyImpl((GridCacheContext<K1,V1>)cctx,prj,prj);
}
