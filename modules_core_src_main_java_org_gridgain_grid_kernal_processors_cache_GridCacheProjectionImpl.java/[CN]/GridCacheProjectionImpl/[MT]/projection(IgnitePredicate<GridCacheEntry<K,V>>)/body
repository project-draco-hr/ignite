{
  if (filter == null)   return new GridCacheProxyImpl<>(cctx,this,this);
  if (noNullEntryFilter.entryFilter != null)   filter=and(filter,true);
  if (cctx.deploymentEnabled()) {
    try {
      cctx.deploy().registerClasses(filter);
    }
 catch (    GridException e) {
      throw new GridRuntimeException(e);
    }
  }
  GridCacheProjectionImpl<K,V> prj=new GridCacheProjectionImpl<>(this,cctx,noNullKvFilter.kvFilter,filter,flags,subjId,keepPortable);
  return new GridCacheProxyImpl<>(cctx,prj,prj);
}
