{
  if (isDone() || this.evtType != evtType)   return;
  if (TEST_INFO)   X.println("EventTypeFuture.onEvent() [evtName=" + U.gridEventName(evtType) + ", evtType="+ evtType+ ", cnt="+ cnt+ ", expCnt="+ expCnt+ ']');
  this.cnt=cnt;
  if (expCnt < cnt && (!partitioned || evtType != EVT_CACHE_OBJECT_READ || expCnt + 1 < cnt))   onDone(new IgniteCheckedException("Wrong event count [evtName=" + U.gridEventName(evtType) + ", evtType="+ evtType+ ", expCnt="+ expCnt+ ", actCnt="+ cnt+ ", partitioned="+ partitioned+ "]"));
  if (expCnt == cnt || (partitioned && expCnt + 1 == cnt))   onDone();
}
