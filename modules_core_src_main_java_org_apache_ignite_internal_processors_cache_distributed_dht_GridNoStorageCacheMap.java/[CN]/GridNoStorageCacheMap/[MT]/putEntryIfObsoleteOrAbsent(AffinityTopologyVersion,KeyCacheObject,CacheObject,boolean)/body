{
  if (create) {
    GridCacheMapEntry entry=ctx.useOffheapEntry() ? new GridDhtOffHeapCacheEntry(ctx,topVer,key,hash(key.hashCode()),val,null,0) : new GridDhtCacheEntry(ctx,topVer,key,hash(key.hashCode()),val,null,0);
    return new GridTriple<>(entry,null,null);
  }
 else   return emptyTriple;
}
