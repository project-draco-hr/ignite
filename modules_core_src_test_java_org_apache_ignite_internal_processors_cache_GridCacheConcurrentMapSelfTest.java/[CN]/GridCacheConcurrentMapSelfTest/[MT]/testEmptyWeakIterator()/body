{
  final IgniteCache<Integer,String> c=grid().jcache(null);
  for (int i=0; i < 10; i++) {
    multithreaded(new Callable<Object>(){
      @SuppressWarnings("UnusedAssignment") @Override public Object call() throws Exception {
        Iterator<Cache.Entry<Integer,String>> it=c.iterator();
        for (int i=0; i < 1000; i++) {
          c.put(i,String.valueOf(i));
          if (i == 0)           it.hasNext();
        }
        it=null;
        System.gc();
        return null;
      }
    }
,Runtime.getRuntime().availableProcessors());
    for (int r=0; r < 10; r++) {
      System.gc();
      c.get(100);
      if (local().map.iteratorMapSize() == 0)       break;
 else       U.sleep(500);
    }
    assertEquals(0,local().map.iteratorMapSize());
  }
}
