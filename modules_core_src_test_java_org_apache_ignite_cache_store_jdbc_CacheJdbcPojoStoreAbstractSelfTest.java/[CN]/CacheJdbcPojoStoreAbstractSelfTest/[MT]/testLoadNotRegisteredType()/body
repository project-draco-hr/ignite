{
  startTestGrid(false,false,false,false);
  IgniteCache<Object,Object> c1=grid().cache(CACHE_NAME);
  try {
    c1.loadCache(null,"PersonKeyWrong","SELECT * FROM Person");
  }
 catch (  CacheLoaderException e) {
    String msg=e.getMessage();
    assertTrue("Unexpected exception: " + msg,("Provided key type is not found in store or cache configuration " + "[cache=" + CACHE_NAME + ", key=PersonKeyWrong]").equals(msg));
  }
}
