{
  try {
    PortableMetadata oldMeta=entry.getValue();
    Map<String,String> fields=new HashMap<>();
    if (checkMeta(typeId,oldMeta,newMeta,fields)) {
      PortableMetadata res=new PortableMetaDataImpl(newMeta.typeName(),fields,newMeta.affinityKeyFieldName());
      entry.setValue(res);
      return null;
    }
 else     return null;
  }
 catch (  PortableException e) {
    return e;
  }
}
