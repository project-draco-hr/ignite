{
  if (clientNode)   return new ArrayList<>(clientMetaDataCache.values());
  return F.viewReadOnly(metaDataCache.entrySetx(metaPred),new C1<Cache.Entry<PortableMetaDataKey,PortableMetadata>,PortableMetadata>(){
    private static final long serialVersionUID=0L;
    @Override public PortableMetadata apply(    Cache.Entry<PortableMetaDataKey,PortableMetadata> e){
      return e.getValue();
    }
  }
);
}
