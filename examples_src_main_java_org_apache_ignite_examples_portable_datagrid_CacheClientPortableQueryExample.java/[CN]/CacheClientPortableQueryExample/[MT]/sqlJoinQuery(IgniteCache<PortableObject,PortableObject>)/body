{
  SqlQuery<PortableObject,PortableObject> query=new SqlQuery<>(Employee.class,"from Employee, \"" + ORGANIZATION_CACHE_NAME + "\".Organization as org "+ "where Employee.organizationId = org._key and org.name = ?");
  String organizationName="GridGain";
  QueryCursor<Cache.Entry<PortableObject,PortableObject>> employees=cache.query(query.setArgs(organizationName));
  System.out.println();
  System.out.println(">>> Employees working for " + organizationName + ':');
  for (  Cache.Entry<PortableObject,PortableObject> e : employees.getAll())   System.out.println(">>>     " + e.getValue());
}
