{
  IgniteConfiguration cfg=super.getConfiguration(gridName);
  cfg.setPeerClassLoadingLocalClassPathExclude(GridP2PTestTask.class.getName(),GridP2PTestTask1.class.getName(),GridP2PTestJob.class.getName(),GridP2PRemoteClassLoadersSelfTest.class.getName());
  cfg.setDeploymentMode(depMode);
  return cfg;
}
