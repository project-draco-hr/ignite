{
  if (initialized() && !isDone() && lsnrCalls == futuresCount()) {
    try {
      onDone(rdc != null ? rdc.reduce() : null);
    }
 catch (    RuntimeException e) {
      U.error(null,"Failed to execute compound future reducer: " + this,e);
      onDone(e);
    }
catch (    AssertionError e) {
      U.error(null,"Failed to execute compound future reducer: " + this,e);
      onDone(e);
      throw e;
    }
  }
}
