{
  if (System.currentTimeMillis() - statReportedTime < 30000)   return;
  statReportedTime=System.currentTimeMillis();
  int completed=warmup ? (int)(statReportedTime - warmupStartTime) * 100 / TestsHelper.getLoadTestsWarmupPeriod() : (int)(statReportedTime - startTime) * 100 / TestsHelper.getLoadTestsExecutionTime();
  if (completed > 100)   completed=100;
  if (warmup) {
    log.info("Warm up messages processed " + warmupMsgProcessed + ", "+ "speed "+ getWarmUpSpeed()+ " msg/sec, "+ completed+ "% completed");
  }
 else {
    log.info("Messages processed " + msgProcessed + ", "+ "speed "+ getSpeed()+ " msg/sec, "+ completed+ "% completed, "+ "errors "+ msgFailed+ " / "+ String.format("%.2f",getErrorsPercent()).replace(",",".")+ "%");
  }
}
