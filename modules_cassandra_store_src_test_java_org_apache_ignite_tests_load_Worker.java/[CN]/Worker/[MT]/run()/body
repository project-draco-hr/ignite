{
  try {
    if (ignite != null)     igniteCache=ignite.getOrCreateCache(new CacheConfiguration(TestsHelper.getLoadTestsCacheName()));
    execute();
  }
 catch (  Throwable e) {
    executionError=e;
    throw new RuntimeException("Test execution abnormally terminated",e);
  }
 finally {
    reportTestCompletion();
  }
}
