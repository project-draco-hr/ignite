{
  commState.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!commState.typeWritten) {
    if (!commState.putByte(directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 2:
    if (!commState.putInt(cnt))     return false;
  commState.idx++;
case 3:
if (!commState.putInt(mod)) return false;
commState.idx++;
case 4:
if (!commState.putInt(part)) return false;
commState.idx++;
case 5:
if (!commState.putLong(timeout)) return false;
commState.idx++;
case 6:
if (!commState.putByteArray(topicBytes)) return false;
commState.idx++;
case 7:
if (!commState.putInt(workerId)) return false;
commState.idx++;
}
return true;
}
