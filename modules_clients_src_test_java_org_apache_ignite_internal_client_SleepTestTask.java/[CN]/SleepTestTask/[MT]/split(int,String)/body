{
  return Collections.singleton(new ComputeJobAdapter(arg){
    @Override public Object execute(){
      try {
        Thread.sleep(10000);
        String val=argument(0);
        return val == null ? 0 : val.length();
      }
 catch (      InterruptedException e) {
        throw new RuntimeException(e);
      }
    }
  }
);
}
