{
  UUID res;
  int cnt=0;
  UUID primaryId=primary.cluster().localNode().id();
  do {
    res=UUID.randomUUID();
    cnt++;
    if (cnt > 10000)     throw new IllegalStateException("Cannot find key for primary node: " + primaryId);
  }
 while (!primary.cluster().mapKeyToNode(null,res).id().equals(primaryId));
  return res;
}
