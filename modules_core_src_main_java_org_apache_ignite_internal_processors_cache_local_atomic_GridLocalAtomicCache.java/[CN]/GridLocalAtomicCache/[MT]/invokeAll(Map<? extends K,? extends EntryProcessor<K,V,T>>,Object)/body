{
  A.notNull(map,"map");
  if (keyCheck)   validateCacheKeys(map.keySet());
  CacheOperationContext opCtx=ctx.operationContextPerCall();
  return (Map<K,EntryProcessorResult<T>>)updateAllInternal(TRANSFORM,map.keySet(),map.values(),args,expiryPerCall(),false,false,null,ctx.writeThrough(),ctx.readThrough(),opCtx != null && opCtx.isKeepBinary());
}
