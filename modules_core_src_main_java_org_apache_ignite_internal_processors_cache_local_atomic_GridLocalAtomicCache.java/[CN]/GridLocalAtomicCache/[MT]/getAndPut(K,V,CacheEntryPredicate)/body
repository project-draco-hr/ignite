{
  A.notNull(key,"key",val,"val");
  CacheOperationContext opCtx=ctx.operationContextPerCall();
  return (V)updateAllInternal(UPDATE,Collections.singleton(key),Collections.singleton(val),null,expiryPerCall(),true,false,filter,ctx.writeThrough(),ctx.readThrough(),opCtx != null && opCtx.isKeepBinary());
}
