{
  boolean statsEnabled=ctx.config().isStatisticsEnabled();
  long start=statsEnabled ? System.nanoTime() : 0L;
  CacheOperationContext opCtx=ctx.operationContextPerCall();
  updateAllInternal(UPDATE,m.keySet(),m.values(),null,expiryPerCall(),false,false,CU.empty0(),ctx.writeThrough(),ctx.readThrough(),opCtx != null && opCtx.isKeepBinary());
  if (statsEnabled)   metrics0().addPutTimeNanos(System.nanoTime() - start);
}
