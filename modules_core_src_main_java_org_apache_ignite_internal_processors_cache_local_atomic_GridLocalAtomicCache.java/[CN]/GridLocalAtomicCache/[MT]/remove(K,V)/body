{
  A.notNull(key,"key",val,"val");
  CacheOperationContext opCtx=ctx.operationContextPerCall();
  return (Boolean)updateAllInternal(DELETE,Collections.singleton(key),null,null,expiryPerCall(),false,false,ctx.equalsValArray(val),ctx.writeThrough(),ctx.readThrough(),opCtx != null && opCtx.isKeepBinary());
