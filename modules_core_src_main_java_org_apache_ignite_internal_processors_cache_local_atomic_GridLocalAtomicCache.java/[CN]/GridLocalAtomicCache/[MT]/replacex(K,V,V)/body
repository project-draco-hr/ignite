{
  A.notNull(key,"key",oldVal,"oldVal",newVal,"newVal");
  return (GridCacheReturn)updateAllInternal(UPDATE,Collections.singleton(key),Collections.singleton(newVal),null,expiryPerCall(),true,true,ctx.equalsValArray(oldVal),ctx.writeThrough(),ctx.readThrough(),ctx.operationContextPerCall().isKeepBinary());
}
