{
  ctx.denyOnFlag(LOCAL);
  A.notNull(keys,"keys");
  String taskName=ctx.kernalContext().job().currentTaskName();
  if (ctx.portableEnabled() && !F.isEmpty(keys)) {
    keys=F.viewReadOnly(keys,new C1<K,K>(){
      @Override public K apply(      K k){
        return (K)ctx.marshalToPortable(k);
      }
    }
);
  }
  return getAllInternal(keys,ctx.isSwapOrOffheapEnabled(),ctx.readThrough(),ctx.hasFlag(CLONE),taskName,deserializePortable,false);
}
