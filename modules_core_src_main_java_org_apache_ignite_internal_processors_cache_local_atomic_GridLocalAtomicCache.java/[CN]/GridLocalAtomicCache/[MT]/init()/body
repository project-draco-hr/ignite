{
  map.setEntryFactory(new GridCacheMapEntryFactory(){
    @Override public GridCacheMapEntry create(    GridCacheContext ctx,    long topVer,    KeyCacheObject key,    int hash,    CacheObject val,    @Nullable GridCacheMapEntry next,    long ttl,    int hdrId){
      return new GridLocalCacheEntry(ctx,key,hash,val,next,ttl,hdrId);
    }
  }
);
}
