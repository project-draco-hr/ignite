{
  map.setEntryFactory(new GridCacheMapEntryFactory(){
    @Override public GridCacheMapEntry create(    GridCacheContext ctx,    AffinityTopologyVersion topVer,    KeyCacheObject key,    int hash,    CacheObject val,    @Nullable GridCacheMapEntry next,    int hdrId){
      return new GridLocalCacheEntry(ctx,key,hash,val,next,hdrId);
    }
  }
);
}
