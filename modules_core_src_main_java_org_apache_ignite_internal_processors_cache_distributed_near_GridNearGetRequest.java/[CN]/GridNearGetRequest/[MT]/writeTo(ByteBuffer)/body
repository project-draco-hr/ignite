{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 3:
    if (!writer.writeLong("accessTtl",accessTtl))     return false;
  state++;
case 4:
if (!writer.writeObjectArray("filterBytes",filterBytes,byte[].class)) return false;
state++;
case 5:
if (!writer.writeIgniteUuid("futId",futId)) return false;
state++;
case 6:
if (!writer.writeMap("keyBytes",keyBytes,byte[].class,boolean.class)) return false;
state++;
case 7:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
state++;
case 8:
if (!writer.writeBoolean("readThrough",readThrough)) return false;
state++;
case 9:
if (!writer.writeBoolean("reload",reload)) return false;
state++;
case 10:
if (!writer.writeUuid("subjId",subjId)) return false;
state++;
case 11:
if (!writer.writeInt("taskNameHash",taskNameHash)) return false;
state++;
case 12:
if (!writer.writeLong("topVer",topVer)) return false;
state++;
case 13:
if (!writer.writeMessage("ver",ver != null ? ver.clone() : null)) return false;
state++;
}
return true;
}
