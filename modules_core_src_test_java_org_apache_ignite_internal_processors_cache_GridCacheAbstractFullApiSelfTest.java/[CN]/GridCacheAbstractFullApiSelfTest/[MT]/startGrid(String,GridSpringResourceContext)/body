{
  if (cacheCfgMap == null)   return super.startGrid(gridName,ctx);
  IgniteConfiguration cfg=getConfiguration(gridName);
  cacheCfgMap.put(gridName,cfg.getCacheConfiguration());
  cfg.setCacheConfiguration();
  if (!isRemoteJvm(gridName))   return IgnitionEx.start(optimize(cfg),ctx);
 else   return startRemoteGrid(gridName,optimize(cfg),ctx);
}
