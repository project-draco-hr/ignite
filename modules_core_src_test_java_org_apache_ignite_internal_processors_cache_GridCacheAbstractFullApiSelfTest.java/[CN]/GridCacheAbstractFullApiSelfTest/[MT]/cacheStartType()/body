{
  String mode=System.getProperty("cache.start.mode");
  if (CacheStartMode.NODES_THEN_CACHES.name().equalsIgnoreCase(mode))   return CacheStartMode.NODES_THEN_CACHES;
  if (CacheStartMode.ONE_BY_ONE.name().equalsIgnoreCase(mode))   return CacheStartMode.ONE_BY_ONE;
  return CacheStartMode.STATIC;
}
