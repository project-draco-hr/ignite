{
  GridCache<?,?> cache=GridGain.grid().cache(CACHE_NAME);
  GridCacheQuery<List<?>> qry1=cache.queries().createSqlFieldsQuery("select concat(firstName, ' ', lastName), Organization.name from Person, Organization where " + "Person.orgId = Organization.id");
  Collection<List<?>> res=qry1.execute().get();
  print("Names of all employees and organizations they belong to:",res);
}
