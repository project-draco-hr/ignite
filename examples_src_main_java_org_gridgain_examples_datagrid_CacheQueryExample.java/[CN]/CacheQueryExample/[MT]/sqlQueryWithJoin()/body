{
  GridCache<GridCacheAffinityKey<UUID>,Person> cache=Ignition.grid().cache(CACHE_NAME);
  GridCacheQuery<Map.Entry<GridCacheAffinityKey<UUID>,Person>> qry=cache.queries().createSqlQuery(Person.class,"from Person, Organization " + "where Person.orgId = Organization.id " + "and lower(Organization.name) = lower(?)");
  print("Following people are 'GridGain' employees: ",qry.execute("GridGain").get());
  print("Following people are 'Other' employees: ",qry.execute("Other").get());
}
