{
  GridCacheQueue<SampleItem> queue=g.cache(CACHE_NAME).dataStructures().queue(queueName,PRIORITY,0,false,true);
  for (int i=0; i < g.nodes().size() * RETRIES * 2; i++)   queue.put(new SampleItem(i,g.localNode().id().toString()));
  print("Queue size after initializing: " + queue.size());
  return queue;
}
