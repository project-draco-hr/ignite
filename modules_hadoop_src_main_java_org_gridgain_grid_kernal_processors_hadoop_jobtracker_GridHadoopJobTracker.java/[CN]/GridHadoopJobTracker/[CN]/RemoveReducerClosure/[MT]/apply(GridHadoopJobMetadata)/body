{
  U.debug("REMOVE REDUCER: " + meta);
  GridHadoopJobMetadata cp=new GridHadoopJobMetadata(meta);
  Collection<Integer> rdcCp=new HashSet<>(cp.pendingReducers());
  rdcCp.remove(rdc);
  cp.pendingReducers(rdcCp);
  if (err != null) {
    cp.phase(PHASE_CANCELLING);
    cp.failCause(err);
  }
  return cp;
}
