{
  GridHadoopJobId jobId=taskInfo.jobId();
  if (status.state() == FAILED || status.state() == CRASHED)   jobMetaPrj.transformAsync(jobId,new RemoveReducerClosure(taskInfo.taskNumber(),status.failCause()));
 else   jobMetaPrj.transformAsync(jobId,new RemoveReducerClosure(taskInfo.taskNumber()));
}
