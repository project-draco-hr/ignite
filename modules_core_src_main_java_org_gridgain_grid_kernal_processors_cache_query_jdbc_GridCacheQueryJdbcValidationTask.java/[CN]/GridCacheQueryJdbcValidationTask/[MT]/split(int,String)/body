{
  GridLicenseUseRegistry.onUsage(DATA_GRID,getClass());
  return F.asSet(new GridComputeJobAdapter(){
    @GridInstanceResource private Ignite ignite;
    @Override public Object execute(){
      for (      GridNode n : ignite.cluster().nodes())       if (U.hasCache(n,cacheName))       return true;
      return false;
    }
  }
);
}
