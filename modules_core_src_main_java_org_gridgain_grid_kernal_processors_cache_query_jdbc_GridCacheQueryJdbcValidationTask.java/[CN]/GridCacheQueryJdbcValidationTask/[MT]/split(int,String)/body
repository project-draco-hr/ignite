{
  GridLicenseUseRegistry.onUsage(DATA_GRID,getClass());
  return F.asSet(new ComputeJobAdapter(){
    @IgniteInstanceResource private Ignite ignite;
    @Override public Object execute(){
      for (      ClusterNode n : ignite.cluster().nodes())       if (U.hasCache(n,cacheName))       return true;
      return false;
    }
  }
);
}
