{
  GridLicenseUseRegistry.onUsage(DATA_GRID,getClass());
  return F.asSet(new GridComputeJobAdapter(){
    @GridInstanceResource private Grid grid;
    @Override public Object execute(){
      for (      GridNode n : grid.cluster().nodes())       if (U.hasCache(n,cacheName))       return true;
      return false;
    }
  }
);
}
