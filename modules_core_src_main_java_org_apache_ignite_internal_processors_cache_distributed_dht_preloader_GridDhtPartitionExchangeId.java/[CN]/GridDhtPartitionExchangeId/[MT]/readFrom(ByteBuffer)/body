{
  reader.setBuffer(buf);
switch (state) {
case 0:
    evt=reader.readInt("evt");
  if (!reader.isLastRead())   return false;
state++;
case 1:
nodeId=reader.readUuid("nodeId");
if (!reader.isLastRead()) return false;
state++;
case 2:
topVer=reader.readLong("topVer");
if (!reader.isLastRead()) return false;
state++;
}
return true;
}
