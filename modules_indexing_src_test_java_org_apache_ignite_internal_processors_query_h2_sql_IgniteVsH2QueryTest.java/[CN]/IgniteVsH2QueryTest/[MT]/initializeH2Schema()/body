{
  Statement st=conn.createStatement();
  st.execute("CREATE SCHEMA \"part\"");
  st.execute("CREATE SCHEMA \"repl\"");
  st.execute("create table \"part\".ORGANIZATION" + "  (id int unique," + "  name varchar(255))");
  st.execute("create table \"part\".PERSON" + "  (id int unique, " + "  firstName varchar(255), "+ "  lastName varchar(255),"+ "  orgId int not null,"+ "  salary double )");
  st.execute("create table \"repl\".PRODUCT" + "  (id int unique, " + "  name varchar(255), "+ "  price int)");
  st.execute("create table \"part\".PURCHASE" + "  (id int unique, " + "  personId int, "+ "  productId int)");
  conn.commit();
}
