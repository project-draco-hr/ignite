{
  Statement st=conn.createStatement();
  st.execute("CREATE SCHEMA \"part\"");
  st.execute("CREATE SCHEMA \"repl\"");
  st.execute("create table \"part\".ORGANIZATION" + "  (_key int not null," + "  _val other not null,"+ "  id int unique,"+ "  name varchar(255))");
  st.execute("create table \"part\".PERSON" + "  (_key other not null ," + "   _val other not null ,"+ "  id int unique, "+ "  firstName varchar(255), "+ "  lastName varchar(255),"+ "  orgId int not null,"+ "  salary double )");
  st.execute("create table \"repl\".PRODUCT" + "  (_key int not null ," + "   _val other not null ,"+ "  id int unique, "+ "  name varchar(255), "+ "  price int)");
  st.execute("create table \"part\".PURCHASE" + "  (_key other not null ," + "   _val other not null ,"+ "  id int unique, "+ "  personId int, "+ "  productId int)");
  conn.commit();
}
