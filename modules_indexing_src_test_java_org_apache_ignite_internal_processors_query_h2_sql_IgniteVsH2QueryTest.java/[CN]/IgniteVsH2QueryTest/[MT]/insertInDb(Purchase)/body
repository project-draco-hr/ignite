{
  try (PreparedStatement st=conn.prepareStatement("insert into \"part\".PURCHASE (_key, _val, id, personId, productId) values(?, ?, ?, ?, ?)")){
    st.setObject(1,p.key());
    st.setObject(2,p);
    st.setObject(3,p.id);
    st.setObject(4,p.personId);
    st.setObject(5,p.productId);
    st.executeUpdate();
  }
 }
