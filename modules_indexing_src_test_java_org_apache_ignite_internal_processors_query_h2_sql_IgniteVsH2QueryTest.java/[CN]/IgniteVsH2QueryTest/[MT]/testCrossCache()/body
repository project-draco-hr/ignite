{
  testQuery("select firstName, lastName" + "  from Person, Purchase" + "  where Person.id = Purchase.personId");
  testQuery("select concat(firstName, ' ', lastName), Product.name " + "  from Person, Purchase, \"replicated\".Product " + "  where Person.id = Purchase.personId and Purchase.productId = Product.id"+ "  group by Product.id");
  testQuery("select concat(firstName, ' ', lastName), count (Product.id) " + "  from Person, Purchase, \"replicated\".Product " + "  where Person.id = Purchase.personId and Purchase.productId = Product.id"+ "  group by Product.id");
}
