{
  int len=in.readInt();
  BigDecimal[] arr=new BigDecimal[len];
  for (int i=0; i < len; i++) {
    byte flag=in.readByte();
    if (flag == GridBinaryMarshaller.NULL)     arr[i]=null;
 else {
      if (flag != GridBinaryMarshaller.DECIMAL)       throw new BinaryObjectException("Invalid flag value: " + flag);
      arr[i]=doReadDecimal(in);
    }
  }
  return arr;
}
