{
  if (evt instanceof TaskEvent)   return taskEvent((TaskEvent)evt,type,id,name,nid,ts,msg,shortDisplay);
  if (evt instanceof JobEvent)   return jobEvent((JobEvent)evt,type,id,name,nid,ts,msg,shortDisplay);
  if (evt instanceof DeploymentEvent)   return deploymentEvent((DeploymentEvent)evt,type,id,name,nid,ts,msg,shortDisplay);
  if (evt instanceof DiscoveryEvent)   return discoveryEvent((DiscoveryEvent)evt,type,id,name,nid,ts,msg,shortDisplay);
  return null;
}
