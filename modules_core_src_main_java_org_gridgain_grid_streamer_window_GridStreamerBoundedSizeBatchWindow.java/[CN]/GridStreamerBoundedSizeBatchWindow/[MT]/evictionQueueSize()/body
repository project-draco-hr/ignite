{
  QueueHolder win=holder;
  int oversizeCnt=Math.max(0,win.batchQueueSize().get() - maxBatches);
  Iterator<Batch> it=win.batchQueue().iterator();
  int size=0;
  int idx=0;
  while (it.hasNext()) {
    Batch batch=it.next();
    if (idx++ < oversizeCnt)     size+=batch.size();
  }
  return size;
}
