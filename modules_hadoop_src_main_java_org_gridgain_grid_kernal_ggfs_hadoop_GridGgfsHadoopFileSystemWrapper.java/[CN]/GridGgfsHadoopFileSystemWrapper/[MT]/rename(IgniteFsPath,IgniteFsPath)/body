{
  try {
    if (!fileSys.rename(convert(src),convert(dest)))     throw new IgniteFsException("Failed to rename (secondary file system returned false) " + "[src=" + src + ", dest="+ dest+ ']');
  }
 catch (  IOException e) {
    throw handleSecondaryFsError(e,"Failed to rename file [src=" + src + ", dest="+ dest+ ']');
  }
}
