{
  if (log.isDebugEnabled())   log.debug("Received onHeld() callback [worker=" + worker + ']');
  boolean res=false;
  if (activeJobs.containsKey(worker.getJobId())) {
    res=heldJobs.add(worker.getJobId());
    if (!activeJobs.containsKey(worker.getJobId())) {
      heldJobs.remove(worker.getJobId());
      res=false;
    }
  }
  return res;
}
