{
  if (log.isDebugEnabled())   log.debug("Received onJobStarted() callback: " + worker);
  if (metricsUpdateFreq > -1L)   updateJobMetrics();
  if (worker.endTime() < Long.MAX_VALUE)   ctx.timeout().addTimeoutObject(worker);
  if (worker.getSession().isFullSupport())   ctx.io().addMessageListener(worker.getJobTopic(),sesLsnr);
}
