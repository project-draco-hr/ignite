{
  return gridNames.isEmpty() && topVersions.isEmpty() && unhandledEx.isEmpty()&& taskMonitoringEnabled.isEmpty()&& evts.isEmpty()&& evtsEx.isEmpty()&& caches.isEmpty()&& cachesEx.isEmpty()&& igfss.isEmpty()&& igfsEndpoints.isEmpty()&& igfssEx.isEmpty()&& streamers.isEmpty()&& streamersEx.isEmpty();
}
