{
  URI uri=path.toUri();
  if (!path.isAbsoluteAndSchemeAuthorityNull()) {
    try {
      path=new Path(GridGgfsHadoopEndpoint.normalize(uri));
    }
 catch (    IllegalArgumentException e) {
      throw new InvalidPathException(path.toString(),e.getMessage());
    }
  }
  super.checkPath(path);
}
