{
  A.notNull(o,"o");
  boolean rmv=false;
  for (Iterator<GridCacheEntry<K,V>> it=map.values().iterator(); it.hasNext(); ) {
    GridCacheEntry<K,V> e=it.next();
    if (F.isAll(e,filter) && F.eq(o,e.getValue())) {
      it.remove();
      try {
        e.removex();
      }
 catch (      GridException ex) {
        throw new GridRuntimeException(ex);
      }
      rmv=true;
    }
  }
  return rmv;
}
