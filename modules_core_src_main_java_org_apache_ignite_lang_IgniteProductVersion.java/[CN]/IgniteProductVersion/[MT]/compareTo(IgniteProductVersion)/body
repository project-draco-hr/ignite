{
  if (major == o.major) {
    if (minor == o.minor) {
      if (maintenance == o.maintenance)       return revTs != o.revTs ? revTs < o.revTs ? -1 : 1 : 0;
 else       return maintenance < o.maintenance ? -1 : 1;
    }
 else     return minor < o.minor ? -1 : 1;
  }
 else   return major < o.major ? -1 : 1;
}
