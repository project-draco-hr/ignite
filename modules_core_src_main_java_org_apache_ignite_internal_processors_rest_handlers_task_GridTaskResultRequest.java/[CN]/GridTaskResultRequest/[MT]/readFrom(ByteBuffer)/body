{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    taskId=commState.getGridUuid("taskId");
  if (!commState.lastRead())   return false;
commState.idx++;
case 1:
topicBytes=commState.getByteArray("topicBytes");
if (!commState.lastRead()) return false;
commState.idx++;
}
return true;
}
