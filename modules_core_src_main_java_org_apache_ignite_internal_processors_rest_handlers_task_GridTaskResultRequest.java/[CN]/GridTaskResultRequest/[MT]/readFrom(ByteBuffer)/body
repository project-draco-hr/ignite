{
  reader.setBuffer(buf);
  if (!reader.beforeMessageRead())   return false;
switch (readState) {
case 0:
    taskId=reader.readIgniteUuid("taskId");
  if (!reader.isLastRead())   return false;
readState++;
case 1:
topicBytes=reader.readByteArray("topicBytes");
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
