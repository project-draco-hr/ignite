{
  reader.setBuffer(buf);
switch (state) {
case 0:
    err=reader.readString("err");
  if (!reader.isLastRead())   return false;
state++;
case 1:
finished=reader.readBoolean("finished");
if (!reader.isLastRead()) return false;
state++;
case 2:
found=reader.readBoolean("found");
if (!reader.isLastRead()) return false;
state++;
case 3:
resBytes=reader.readByteArray("resBytes");
if (!reader.isLastRead()) return false;
state++;
}
return true;
}
