{
  writer.setBuffer(buf);
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeField("err",err,MessageFieldType.STRING))     return false;
  writer.incrementState();
case 1:
if (!writer.writeField("finished",finished,MessageFieldType.BOOLEAN)) return false;
writer.incrementState();
case 2:
if (!writer.writeField("found",found,MessageFieldType.BOOLEAN)) return false;
writer.incrementState();
case 3:
if (!writer.writeField("resBytes",resBytes,MessageFieldType.BYTE_ARR)) return false;
writer.incrementState();
}
return true;
}
