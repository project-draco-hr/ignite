{
  if (this == obj) {
    return true;
  }
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  GridDeploymentResourceAdapter rsrc=(GridDeploymentResourceAdapter)obj;
  return clsLdr.equals(rsrc.clsLdr) == true && name.equals(rsrc.name) == true && rsrcCls.equals(rsrc.rsrcCls) == true;
}
