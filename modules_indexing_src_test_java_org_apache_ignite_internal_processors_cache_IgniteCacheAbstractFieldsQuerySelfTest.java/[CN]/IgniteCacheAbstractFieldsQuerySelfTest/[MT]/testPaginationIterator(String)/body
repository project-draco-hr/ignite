{
  QueryCursor<List<?>> qry=grid(0).jcache(cacheName).queryFields(sql("select _key, _val from Integer").setPageSize(10));
  int cnt=0;
  for (  List<?> row : qry) {
    assertEquals(2,row.size());
    assertEquals(row.get(0),row.get(1));
    assertTrue((Integer)row.get(0) >= 0 && (Integer)row.get(0) < 200);
    cnt++;
  }
  int size=200;
  assertEquals(size,cnt);
}
