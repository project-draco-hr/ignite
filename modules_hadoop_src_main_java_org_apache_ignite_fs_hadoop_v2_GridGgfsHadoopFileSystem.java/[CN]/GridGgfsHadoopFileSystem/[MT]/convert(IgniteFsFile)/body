{
  return new FileStatus(file.length(),file.isDirectory(),dfltReplication,file.groupBlockSize(),file.modificationTime(),file.accessTime(),permission(file),file.property(PROP_USER_NAME,DFLT_USER_NAME),file.property(PROP_GROUP_NAME,"users"),convert(file.path())){
    @Override public String toString(){
      return "FileStatus [path=" + getPath() + ", isDir="+ isDirectory()+ ", len="+ getLen()+ "]";
    }
  }
;
}
