{
  try {
    Long idx=(Long)cache.transformCompute(queueKey,new AddClosure(uuid,1));
    if (idx == null)     return false;
    checkRemoved(idx);
    cache.putx(new ItemKey(uuid,idx,collocated()),item,null);
    return true;
  }
 catch (  GridException e) {
    throw new GridRuntimeException(e);
  }
}
