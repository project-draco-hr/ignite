{
  init();
  T res=next;
  try {
    next=rs != null && !rs.isClosed() && rs.next() ? loadRow() : null;
  }
 catch (  Exception e) {
    if (e instanceof SQLException)     onSqlException((SQLException)e);
    throw new GridException("Failed to load row.",e);
  }
  if (res == null)   throw new NoSuchElementException();
  return res;
}
