{
  if (lockingEnabled()) {
    GridCache<String,Integer> cache=cache();
    cache.put("a",1);
    cache.lockAll(F.asList("a","b","c","d"),0);
    try {
      cache.remove("a");
      cache.putAll(F.asMap("b",2,"c",3,"d",4));
    }
  finally {
      cache.unlockAll(F.asList("a","b","c","d"));
    }
  }
}
