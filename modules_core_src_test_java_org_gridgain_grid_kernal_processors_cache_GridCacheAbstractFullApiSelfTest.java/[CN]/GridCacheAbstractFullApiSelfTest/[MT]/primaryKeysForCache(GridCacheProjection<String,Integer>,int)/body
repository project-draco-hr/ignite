{
  List<String> found=new ArrayList<>(cnt);
  for (int i=1; i < 10000; i++) {
    String key="key" + i;
    if (cache.entry(key).primary()) {
      found.add(key);
      if (found.size() == cnt)       return found;
    }
  }
  throw new GridException("Unable to find " + cnt + " keys as primary for cache.");
}
