{
  sender.cancel();
  try {
    sender.join();
  }
 catch (  InterruptedException e) {
    throw new GridInterruptedException(e);
  }
  collectUpdatesAndSend(true);
  GridCompoundFuture fut=new GridCompoundFuture<>();
  for (  GridFuture<?> f : ioFuts)   fut.add(f);
  fut.markInitialized();
  return fut;
}
