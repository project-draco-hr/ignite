{
  return new CacheEntryUpdatedListener<Integer,Integer>(){
    @Override public void onUpdated(    Iterable<CacheEntryEvent<? extends Integer,? extends Integer>> events) throws CacheEntryListenerException {
      int size=0;
      for (      CacheEntryEvent<? extends Integer,? extends Integer> e : events)       ++size;
      cntr.addAndGet(size);
    }
  }
;
}
