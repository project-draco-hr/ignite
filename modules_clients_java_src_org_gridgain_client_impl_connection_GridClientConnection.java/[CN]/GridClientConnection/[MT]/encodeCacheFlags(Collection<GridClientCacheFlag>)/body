{
  int bits=0;
  if (flagSet.contains(GridClientCacheFlag.SKIP_STORE))   bits|=1;
  if (flagSet.contains(GridClientCacheFlag.SKIP_SWAP))   bits|=1 << 1;
  if (flagSet.contains(GridClientCacheFlag.SYNC_COMMIT))   bits|=1 << 2;
  if (flagSet.contains(GridClientCacheFlag.SYNC_ROLLBACK))   bits|=1 << 3;
  if (flagSet.contains(GridClientCacheFlag.INVALIDATE))   bits|=1 << 4;
  return bits;
}
