{
  if (ctx.deploymentEnabled())   prepareObject(key,ctx.shared());
  if (keyBytes != null) {
    if (this.keyBytes == null)     this.keyBytes=new ArrayList<>(keysCount());
    this.keyBytes.add(keyBytes);
  }
  if (keys == null)   keys=new ArrayList<>(keysCount());
  keys.add(key);
  candidatesByIndex(idx,cands);
  retVals[idx]=retVal;
  idx++;
}
