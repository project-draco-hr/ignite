{
  MessageWriter writer=state.writer();
  writer.setBuffer(buf);
  if (!super.writeTo(buf,state))   return false;
  if (!state.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    state.setTypeWritten();
  }
switch (state.index()) {
case 8:
    if (!writer.writeObjectArray("drVersByIdx",drVersByIdx,GridCacheVersion.class))     return false;
  state.increment();
case 9:
if (!writer.writeIgniteUuid("futId",futId)) return false;
state.increment();
case 10:
if (!writer.writeByteArray("grpLockKeyBytes",grpLockKeyBytes)) return false;
state.increment();
case 11:
if (!writer.writeBoolean("isInTx",isInTx)) return false;
state.increment();
case 12:
