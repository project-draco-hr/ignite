{
  commState.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!commState.typeWritten) {
    if (!commState.putByte(directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 8:
    if (!commState.putGridUuid(futId))     return false;
  commState.idx++;
case 9:
if (!commState.putByteArray(grpLockKeyBytes)) return false;
commState.idx++;
case 10:
if (!commState.putBoolean(isInTx)) return false;
commState.idx++;
case 11:
if (!commState.putBoolean(isInvalidate)) return false;
commState.idx++;
case 12:
if (!commState.putBoolean(isRead)) return false;
commState.idx++;
case 13:
if (!commState.putEnum(isolation)) return false;
commState.idx++;
case 14:
if (keyBytes != null) {
if (commState.it == null) {
if (!commState.putInt(keyBytes.size())) return false;
commState.it=keyBytes.iterator();
}
while (commState.it.hasNext() || commState.cur != NULL) {
if (commState.cur == NULL) commState.cur=commState.it.next();
if (!commState.putByteArray((byte[])commState.cur)) return false;
commState.cur=NULL;
}
commState.it=null;
}
 else {
if (!commState.putInt(-1)) return false;
}
commState.idx++;
case 15:
if (!commState.putCacheVersion(nearXidVer)) return false;
commState.idx++;
case 16:
if (!commState.putUuid(nodeId)) return false;
commState.idx++;
case 17:
if (!commState.putBoolean(partLock)) return false;
commState.idx++;
case 18:
if (!commState.putBooleanArray(retVals)) return false;
commState.idx++;
case 19:
if (!commState.putLong(threadId)) return false;
commState.idx++;
case 20:
if (!commState.putLong(timeout)) return false;
commState.idx++;
case 21:
if (!commState.putInt(txSize)) return false;
commState.idx++;
}
return true;
}
