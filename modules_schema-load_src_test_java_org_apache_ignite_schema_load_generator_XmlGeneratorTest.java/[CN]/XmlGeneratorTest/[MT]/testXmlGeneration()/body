{
  Collection<PojoDescriptor> all=new ArrayList<>();
  for (  PojoDescriptor pojo : pojos)   if (pojo.parent() != null)   all.add(pojo);
  String fileName="Ignite.xml";
  XmlGenerator.generate("org.apache.ignite.schema.load.model",all,true,new File(OUT_DIR_PATH,fileName),askOverwrite);
  assertTrue("Generated XML file content is differ from expected one",compareFilesInt(getClass().getResourceAsStream("/org/apache/ignite/schema/load/model/" + fileName),new File(OUT_DIR_PATH + "/" + fileName),"XML generated by Apache Ignite Schema Load utility"));
}
