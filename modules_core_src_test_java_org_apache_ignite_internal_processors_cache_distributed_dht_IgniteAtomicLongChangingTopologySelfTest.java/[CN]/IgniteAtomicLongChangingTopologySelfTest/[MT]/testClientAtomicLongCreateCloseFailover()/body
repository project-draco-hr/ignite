{
  fail("https://issues.apache.org/jira/browse/IGNITE-1732");
  testFailoverWithClient(new IgniteInClosure<Ignite>(){
    @Override public void apply(    Ignite ignite){
      for (int i=0; i < 100; i++) {
        IgniteAtomicLong l=ignite.atomicLong("long-" + 1,0,true);
        l.close();
      }
    }
  }
);
