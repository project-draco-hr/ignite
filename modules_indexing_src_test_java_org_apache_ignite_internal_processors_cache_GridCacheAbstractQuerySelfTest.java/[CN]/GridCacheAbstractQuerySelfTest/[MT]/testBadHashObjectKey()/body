{
  Cache<BadHashKeyObject,Integer> cache=ignite.cache(null);
  cache.put(new BadHashKeyObject("test_key1"),9);
  cache.put(new BadHashKeyObject("test_key0"),1005001);
  cache.put(new BadHashKeyObject("test_key1"),7);
  assertEquals(1005001,cache.queries().createSqlQuery(Integer.class,"_key = ?").execute(new BadHashKeyObject("test_key0")).get().iterator().next().getValue().intValue());
}
