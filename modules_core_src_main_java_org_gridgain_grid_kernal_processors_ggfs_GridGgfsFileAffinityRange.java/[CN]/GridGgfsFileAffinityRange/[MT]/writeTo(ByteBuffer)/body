{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putGridUuid(affKey))     return false;
  commState.idx++;
case 1:
if (!commState.putBoolean(done)) return false;
commState.idx++;
case 2:
if (!commState.putLong(endOff)) return false;
commState.idx++;
case 3:
if (!commState.putLong(startOff)) return false;
commState.idx++;
case 4:
if (!commState.putInt(status)) return false;
commState.idx++;
}
return true;
}
