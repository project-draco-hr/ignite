{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(null,directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putGridUuid(null,affKey))     return false;
  commState.idx++;
case 1:
if (!commState.putBoolean(null,done)) return false;
commState.idx++;
case 2:
if (!commState.putLong(null,endOff)) return false;
commState.idx++;
case 3:
if (!commState.putLong(null,startOff)) return false;
commState.idx++;
case 4:
if (!commState.putInt(null,status)) return false;
commState.idx++;
}
return true;
}
