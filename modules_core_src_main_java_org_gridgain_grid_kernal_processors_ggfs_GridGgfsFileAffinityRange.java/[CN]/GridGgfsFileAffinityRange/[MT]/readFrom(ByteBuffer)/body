{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    IgniteUuid affKey0=commState.getGridUuid(null);
  if (affKey0 == GRID_UUID_NOT_READ)   return false;
affKey=affKey0;
commState.idx++;
case 1:
if (buf.remaining() < 1) return false;
done=commState.getBoolean(null);
commState.idx++;
case 2:
if (buf.remaining() < 8) return false;
endOff=commState.getLong(null);
commState.idx++;
case 3:
if (buf.remaining() < 8) return false;
startOff=commState.getLong(null);
commState.idx++;
case 4:
if (buf.remaining() < 4) return false;
status=commState.getInt(null);
commState.idx++;
}
return true;
}
