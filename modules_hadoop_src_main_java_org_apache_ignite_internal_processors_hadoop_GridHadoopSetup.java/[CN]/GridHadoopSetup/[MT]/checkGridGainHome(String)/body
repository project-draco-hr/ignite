{
  URL jarUrl=U.class.getProtectionDomain().getCodeSource().getLocation();
  try {
    Path jar=Paths.get(jarUrl.toURI());
    Path gg=Paths.get(ggHome);
    if (!jar.startsWith(gg))     exit("GridGain JAR files are not under IGNITE_HOME.",null);
  }
 catch (  Exception e) {
    exit(e.getMessage(),e);
  }
}
