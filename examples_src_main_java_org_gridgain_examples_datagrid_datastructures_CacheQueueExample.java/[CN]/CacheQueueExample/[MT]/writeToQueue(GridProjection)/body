{
  final String queueName=queue.name();
  g.compute().run(new QueueClosure(CACHE_NAME,queueName,true));
  System.out.println("Queue size after writing [expected=" + g.nodes().size() * RETRIES + ", actual=" + queue.size() + ']');
  System.out.println("Iterate over queue.");
  for (  String item : queue)   System.out.println("Queue item: " + item);
}
