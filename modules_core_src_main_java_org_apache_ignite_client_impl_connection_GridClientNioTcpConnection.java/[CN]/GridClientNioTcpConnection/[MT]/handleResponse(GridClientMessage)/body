{
  lastMsgRcvTime=U.currentTimeMillis();
  TcpClientFuture fut=pendingReqs.get(res.requestId());
  if (fut == null) {
    log.warning("Response for an unknown request is received, ignoring. " + "[res=" + res + ", ses="+ ses+ ']');
    return;
  }
  if (fut.forward()) {
  }
 else {
    if (res instanceof GridClientResponse)     handleClientResponse(fut,(GridClientResponse)res);
 else     log.warning("Unsupported response type received: " + res);
  }
}
