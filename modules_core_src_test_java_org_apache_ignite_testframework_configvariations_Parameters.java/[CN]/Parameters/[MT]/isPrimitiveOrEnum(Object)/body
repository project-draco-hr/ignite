{
  return val.getClass().isPrimitive() || val.getClass().equals(Boolean.class) || val.getClass().equals(Byte.class)|| val.getClass().equals(Short.class)|| val.getClass().equals(Character.class)|| val.getClass().equals(Integer.class)|| val.getClass().equals(Long.class)|| val.getClass().equals(Float.class)|| val.getClass().equals(Double.class)|| val.getClass().isEnum();
}
