{
  int starvationInc=this.starvationInc;
  for (int i=0; i < jobs.size(); i++) {
    GridCollisionJobContextWrapper wrapper=jobs.get(i);
    if (i > wrapper.originalIndex())     wrapper.getContext().getJobContext().setAttribute(jobPriAttrKey,getJobPriority(wrapper.getContext()) + starvationInc);
  }
}
