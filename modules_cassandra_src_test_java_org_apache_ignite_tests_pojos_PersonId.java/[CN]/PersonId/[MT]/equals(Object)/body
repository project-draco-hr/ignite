{
  if (obj == null || !(obj instanceof PersonId))   return false;
  PersonId id=(PersonId)obj;
  if ((companyCode != null && !companyCode.equals(id.companyCode)) || (id.companyCode != null && !id.companyCode.equals(companyCode)))   return false;
  if ((companyCode != null && !companyCode.equals(id.companyCode)) || (id.companyCode != null && !id.companyCode.equals(companyCode)))   return false;
  return personNum == id.personNum;
}
