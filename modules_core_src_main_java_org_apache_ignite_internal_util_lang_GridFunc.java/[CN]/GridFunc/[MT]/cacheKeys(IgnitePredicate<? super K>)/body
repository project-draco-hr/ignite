{
  return isEmpty(ps) || isAlwaysTrue(ps) ? F.<GridCacheEntry<K,V>>alwaysTrue() : isAlwaysFalse(ps) ? F.<GridCacheEntry<K,V>>alwaysFalse() : new IgnitePredicate<GridCacheEntry<K,V>>(){
    @Override public boolean apply(    GridCacheEntry<K,V> e){
      return F.isAll(e.getKey(),ps);
    }
  }
;
}
