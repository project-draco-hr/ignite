{
  return isEmpty(ps) || isAlwaysTrue(ps) ? F.<CacheEntry<K,V>>alwaysTrue() : isAlwaysFalse(ps) ? F.<CacheEntry<K,V>>alwaysFalse() : new IgnitePredicate<CacheEntry<K,V>>(){
    @Override public boolean apply(    CacheEntry<K,V> e){
      V v=e.peek();
      return v != null && F.isAll(v,ps);
    }
  }
;
}
