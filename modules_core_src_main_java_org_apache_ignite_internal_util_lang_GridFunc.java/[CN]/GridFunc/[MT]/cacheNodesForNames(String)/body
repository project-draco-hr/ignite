{
  if (cacheNames == null)   return alwaysFalse();
  return new P1<ClusterNode>(){
    @Override public boolean apply(    ClusterNode n){
      Collection<String> names=U.cacheNames(n);
      for (      String name : names) {
        if (name == null && cacheNames.length == 0)         return true;
        if (U.containsStringArray(cacheNames,name,false))         return true;
      }
      return false;
    }
  }
;
}
