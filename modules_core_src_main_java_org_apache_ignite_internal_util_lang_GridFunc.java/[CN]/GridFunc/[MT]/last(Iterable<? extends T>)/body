{
  if (c == null)   return null;
  if (c instanceof RandomAccess && c instanceof List) {
    List<T> l=(List<T>)c;
    return l.get(l.size() - 1);
  }
 else   if (c instanceof NavigableSet) {
    NavigableSet<T> s=(NavigableSet<T>)c;
    return s.last();
  }
  T last=null;
  for (  T t : c)   last=t;
  return last;
}
