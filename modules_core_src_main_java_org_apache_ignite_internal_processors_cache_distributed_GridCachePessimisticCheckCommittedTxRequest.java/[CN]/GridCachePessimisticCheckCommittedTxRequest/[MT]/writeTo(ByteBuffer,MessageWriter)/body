{
  writer.setBuffer(buf);
  if (!super.writeTo(buf,writer))   return false;
  if (!writer.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 8:
    if (!writer.writeIgniteUuid("futId",futId))     return false;
  writer.incrementState();
case 9:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
writer.incrementState();
case 10:
if (!writer.writeBoolean("nearOnlyCheck",nearOnlyCheck)) return false;
writer.incrementState();
case 11:
if (!writer.writeMessage("nearXidVer",nearXidVer)) return false;
writer.incrementState();
case 12:
if (!writer.writeUuid("originatingNodeId",originatingNodeId)) return false;
writer.incrementState();
case 13:
if (!writer.writeLong("originatingThreadId",originatingThreadId)) return false;
writer.incrementState();
case 14:
if (!writer.writeBoolean("sys",sys)) return false;
writer.incrementState();
}
return true;
}
