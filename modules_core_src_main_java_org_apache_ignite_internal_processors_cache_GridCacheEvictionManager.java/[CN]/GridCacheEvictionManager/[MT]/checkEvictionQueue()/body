{
  int maxSize=maxQueueSize();
  int bufSize=bufEvictQ.sizex();
  if (bufSize >= maxSize) {
    if (log.isDebugEnabled())     log.debug("Processing eviction queue: " + bufSize);
    Collection<EvictionInfo> evictInfos=new ArrayList<>(bufSize);
    for (int i=0; i < bufSize; i++) {
      EvictionInfo info=bufEvictQ.poll();
      if (info == null)       break;
      evictInfos.add(info);
    }
    if (!evictInfos.isEmpty())     addToCurrentFuture(evictInfos);
  }
}
