{
  this.path=path;
  this.timestamp=timestamp;
  this.mode=mode;
  this.size=size;
  this.bytesRead=bytesRead;
  this.readTime=readTime;
  this.userReadTime=userReadTime;
  this.bytesWritten=bytesWritten;
