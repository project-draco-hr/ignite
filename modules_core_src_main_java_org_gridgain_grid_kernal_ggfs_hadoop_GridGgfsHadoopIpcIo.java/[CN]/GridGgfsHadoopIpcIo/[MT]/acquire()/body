{
  while (true) {
    int cnt=activeCnt.get();
    if (cnt == 0) {
      if (log.isDebugEnabled())       log.debug("IPC IO not acquired (count was 0): " + this);
      return false;
    }
    if (activeCnt.compareAndSet(cnt,cnt + 1)) {
      if (log.isDebugEnabled())       log.debug("IPC IO acquired: " + this);
      return true;
    }
  }
}
