{
  if (fail) {
    fails++;
    return;
  }
synchronized (mux) {
    lastTime=startTime;
    if (firstTime == 0) {
      firstTime=lastTime;
      minTime=duration;
      maxTime=duration;
    }
    if (minTime > duration)     minTime=duration;
    if (maxTime < duration)     maxTime=duration;
    execs++;
    avgTime=(avgTime * (execs - 1) + duration) / execs;
  }
}
