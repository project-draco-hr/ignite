{
  reader.setBuffer(buf);
switch (state) {
case 0:
    errBytes=reader.readByteArray("errBytes");
  if (!reader.isLastRead())   return false;
state++;
case 1:
forceLocDep=reader.readBoolean("forceLocDep");
if (!reader.isLastRead()) return false;
state++;
case 2:
reqId=reader.readLong("reqId");
if (!reader.isLastRead()) return false;
state++;
}
return true;
}
