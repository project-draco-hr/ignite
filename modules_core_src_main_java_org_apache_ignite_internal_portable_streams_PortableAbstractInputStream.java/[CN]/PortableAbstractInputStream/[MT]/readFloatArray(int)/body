{
  int len=cnt << 2;
  ensureEnoughData(len);
  float[] res=new float[cnt];
  if (LITTLE_ENDIAN)   copyAndShift(res,FLOAT_ARR_OFF,len);
 else {
    for (int i=0; i < res.length; i++) {
      int x=readIntFast();
      shift(4);
      res[i]=Float.intBitsToFloat(Integer.reverseBytes(x));
    }
  }
  return res;
}
