{
  Set<Long> reqIds=pendingAcks.get(nodeId);
  if (reqIds != null && !reqIds.isEmpty()) {
    if (e.hasCause(IgniteFsOutOfSpaceException.class))     onDone(new IgniteCheckedException("Failed to write data (not enough space on node): " + nodeId,e));
 else     onDone(new IgniteCheckedException("Failed to wait for write completion (write failed on node): " + nodeId,e));
  }
}
