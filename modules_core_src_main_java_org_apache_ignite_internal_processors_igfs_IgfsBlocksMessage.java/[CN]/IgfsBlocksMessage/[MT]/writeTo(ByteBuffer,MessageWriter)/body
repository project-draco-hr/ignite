{
  writer.setBuffer(buf);
  if (!super.writeTo(buf,writer))   return false;
  if (!writer.isHeaderWritten()) {
    if (!writer.writeHeader(new MessageHeader(directType(),(byte)3)))     return false;
    writer.onHeaderWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeMap("blocks",blocks,Type.MSG,Type.BYTE_ARR))     return false;
  writer.incrementState();
case 1:
if (!writer.writeIgniteUuid("fileId",fileId)) return false;
writer.incrementState();
case 2:
if (!writer.writeLong("id",id)) return false;
writer.incrementState();
}
return true;
}
