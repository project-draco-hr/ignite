{
  if (log.isDebugEnabled())   log.debug("Received set data response [req=" + res + ", nodeId="+ nodeId+ ']');
  res.nodeId(nodeId);
  SetDataResponseHandler hnd=setHndMap.get(res.id());
  if (hnd == null) {
    if (log.isDebugEnabled())     log.debug("Received response for unknown handler [res=" + res + ']');
    return;
  }
  if (hnd.onResponse(res))   setHndMap.remove(res.id());
}
