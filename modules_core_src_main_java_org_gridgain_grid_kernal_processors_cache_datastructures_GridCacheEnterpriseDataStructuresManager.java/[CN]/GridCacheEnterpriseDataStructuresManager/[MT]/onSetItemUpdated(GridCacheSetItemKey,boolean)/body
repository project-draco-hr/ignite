{
  GridConcurrentHashSet<GridCacheSetItemKey> set=setData.get(key.setId());
  if (set == null) {
    GridConcurrentHashSet<GridCacheSetItemKey> old=setData.putIfAbsent(key.setId(),set=new GridConcurrentHashSet<>());
    if (old != null)     set=old;
  }
  if (rmv)   set.remove(key);
 else   set.add(key);
}
