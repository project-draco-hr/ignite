{
  GridH2CollocationModel mdl=buildCollocationModel(null,-1,qry,null,true);
  Type type=mdl.type(true);
  if (!type.isCollocated() && mdl.multiplier == MULTIPLIER_REPLICATED_NOT_LAST)   throw new CacheException("Failed to execute query: for distributed join " + "all REPLICATED caches must be at the end of the joined tables list.");
  return type.isCollocated();
}
