{
  next=null;
  do {
    if (curIt == null) {
      if (partIt.hasNext()) {
        int part=partIt.next();
        try {
          curIt=cacheEntryIterator(lazyIterator(partitionIterator(part)));
        }
 catch (        IgniteCheckedException e) {
          throw new IgniteException(e);
        }
      }
    }
    if (curIt != null) {
      if (curIt.hasNext()) {
        next=curIt.next();
        break;
      }
 else       curIt=null;
    }
  }
 while (partIt.hasNext());
}
