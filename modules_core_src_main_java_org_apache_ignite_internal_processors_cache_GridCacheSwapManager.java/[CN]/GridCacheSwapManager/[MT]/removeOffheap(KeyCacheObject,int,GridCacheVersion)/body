{
  assert offheapEnabled;
  checkIteratorQueue();
  boolean rmv=offheap.removex(spaceName,part,key,key.valueBytes(cctx.cacheObjectContext()),new IgniteBiPredicate<Long,Integer>(){
    @Override public boolean apply(    Long ptr,    Integer len){
      GridCacheVersion ver0=GridCacheOffheapSwapEntry.version(ptr);
      return ver.equals(ver0);
    }
  }
);
  if (rmv && cctx.config().isStatisticsEnabled())   cctx.cache().metrics0().onOffHeapRemove();
  return rmv;
}
