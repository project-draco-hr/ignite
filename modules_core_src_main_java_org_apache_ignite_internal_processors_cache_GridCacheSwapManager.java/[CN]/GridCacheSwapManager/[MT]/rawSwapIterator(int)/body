{
  if (!swapEnabled)   return new GridEmptyCloseableIterator<>();
  checkIteratorQueue();
  return new CloseablePartitionsIterator<Map.Entry<byte[],byte[]>,Map.Entry<byte[],byte[]>>(Collections.singleton(part)){
    @Override protected GridCloseableIterator<Map.Entry<byte[],byte[]>> partitionIterator(    int part) throws IgniteCheckedException {
      return swapMgr.rawIterator(spaceName,part);
    }
  }
;
}
