{
  assert swapEnabled;
  checkIteratorQueue();
  swapMgr.write(spaceName,new SwapKey(key.value(cctx.cacheObjectContext(),false),part,key.valueBytes(cctx.cacheObjectContext())),entry,cctx.deploy().globalLoader());
  if (cctx.config().isStatisticsEnabled())   cctx.cache().metrics0().onSwapWrite();
  if (cctx.events().isRecordable(EVT_CACHE_OBJECT_SWAPPED))   cctx.events().addEvent(part,key,cctx.nodeId(),(IgniteUuid)null,null,EVT_CACHE_OBJECT_SWAPPED,null,false,null,true,null,null,null);
}
