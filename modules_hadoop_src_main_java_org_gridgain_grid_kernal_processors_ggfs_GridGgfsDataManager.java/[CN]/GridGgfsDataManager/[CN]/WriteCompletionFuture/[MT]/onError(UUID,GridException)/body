{
  Set<Long> reqIds=pendingAcks.get(nodeId);
  if (reqIds != null && !reqIds.isEmpty()) {
    if (e instanceof GridGgfsOutOfSpaceException)     onDone(new GridException("Failed to write data (not enough space on node): " + nodeId,e));
 else     onDone(new GridException("Failed to wait for write completion (write failed on node): " + nodeId,e));
  }
}
