{
  Collection<Integer> args=Arrays.asList(1,2,3);
  Collection<Integer> res=grid().compute().apply(new C1<Integer,Integer>(){
    @Override public Integer apply(    Integer arg){
      ids.add(this);
      return 10 + arg;
    }
  }
,args);
  assertEquals(36,F.sumInt(res));
  assertEquals(3,ids.size());
}
