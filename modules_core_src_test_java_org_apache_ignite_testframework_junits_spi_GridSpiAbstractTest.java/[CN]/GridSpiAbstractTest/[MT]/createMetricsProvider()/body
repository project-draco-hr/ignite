{
  return new DiscoveryMetricsProvider(){
    /** 
 * {@inheritDoc} 
 */
    @Override public ClusterMetrics metrics(){
      return new ClusterMetricsSnapshot();
    }
    /** 
 * {@inheritDoc} 
 */
    @Override public Map<Integer,CacheMetrics> cacheMetrics(){
      return Collections.emptyMap();
    }
  }
;
}
