{
  List<String> p1Comps=path.components();
  List<String> p2Comps=prefix.components();
  if (p2Comps.size() > p1Comps.size())   return false;
  for (int i=0; i < p1Comps.size(); i++) {
    if (i >= p2Comps.size() || p2Comps.get(i) == null)     return true;
    if (!p1Comps.get(i).equals(p2Comps.get(i)))     return false;
  }
  return true;
}
