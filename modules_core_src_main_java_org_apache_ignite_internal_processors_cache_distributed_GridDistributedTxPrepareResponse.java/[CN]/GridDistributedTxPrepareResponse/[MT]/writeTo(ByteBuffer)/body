{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 8:
    if (!writer.writeByteArray("candsBytes",candsBytes))     return false;
  state++;
case 9:
if (!writer.writeByteArray("errBytes",errBytes)) return false;
state++;
}
return true;
}
