{
  commState.setBuffer(buf);
  if (!super.readFrom(buf))   return false;
switch (commState.idx) {
case 8:
    candsBytes=commState.getByteArray("candsBytes");
  if (!commState.lastRead())   return false;
commState.idx++;
case 9:
errBytes=commState.getByteArray("errBytes");
if (!commState.lastRead()) return false;
commState.idx++;
}
return true;
}
