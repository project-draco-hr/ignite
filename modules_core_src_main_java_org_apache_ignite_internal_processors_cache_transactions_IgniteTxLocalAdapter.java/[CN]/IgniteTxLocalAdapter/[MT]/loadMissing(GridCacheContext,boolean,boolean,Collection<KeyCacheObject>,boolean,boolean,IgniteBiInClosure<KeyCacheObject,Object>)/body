{
  if (!async) {
    try {
      if (!readThrough || !cacheCtx.readThrough()) {
        for (        KeyCacheObject key : keys)         c.apply(key,null);
        return new GridFinishedFuture<>(false);
      }
      return new GridFinishedFuture<>(cacheCtx.store().loadAllFromStore(this,keys,c));
    }
 catch (    IgniteCheckedException e) {
      return new GridFinishedFuture<>(e);
    }
  }
 else {
    return cctx.kernalContext().closure().callLocalSafe(new GPC<Boolean>(){
      @Override public Boolean call() throws Exception {
        if (!readThrough || !cacheCtx.readThrough()) {
          for (          KeyCacheObject key : keys)           c.apply(key,null);
          return false;
        }
        return cacheCtx.store().loadAllFromStore(IgniteTxLocalAdapter.this,keys,c);
      }
    }
,true);
  }
}
