{
  double startBalance=100 + nextRandom(100) / 1.123;
  return ignite.binary().builder(DEPOSIT).setField(ID,id).setField(TRADER_ID,traderId).setField(TRADER_LINK,num).setField(BALANCE,new BigDecimal(startBalance)).setField(MARGIN_RATE,new BigDecimal(0.1)).setField(BALANCE_ON_DAY_OPEN,new BigDecimal(startBalance)).build();
}
