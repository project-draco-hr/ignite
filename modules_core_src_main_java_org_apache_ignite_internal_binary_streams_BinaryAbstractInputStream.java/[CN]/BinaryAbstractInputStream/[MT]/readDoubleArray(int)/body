{
  int len=cnt << 3;
  ensureEnoughData(len);
  double[] res=new double[cnt];
  if (LITTLE_ENDIAN)   copyAndShift(res,DOUBLE_ARR_OFF,len);
 else {
    for (int i=0; i < res.length; i++) {
      long x=readLongFast();
      shift(8);
      res[i]=Double.longBitsToDouble(Long.reverseBytes(x));
    }
  }
  return res;
}
