{
  if (writer != null)   System.out.println("WRITTEN TO GG HADOOP CTX [key=" + key + ", val="+ val+ ", writer="+ writer+ ']');
  if (writer != null)   writer.write(key,val);
 else {
    try {
      output.write(key,val);
    }
 catch (    GridException e) {
      throw new IOException(e);
    }
  }
}
