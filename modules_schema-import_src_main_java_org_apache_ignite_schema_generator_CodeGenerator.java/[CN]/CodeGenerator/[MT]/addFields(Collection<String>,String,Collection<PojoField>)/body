{
  for (  PojoField field : fields) {
    String javaTypeName=field.javaTypeName();
    if (javaTypeName.startsWith(JAVA_LANG_PKG))     javaTypeName=javaTypeName.substring(JAVA_LANG_PKG.length());
 else     if (javaTypeName.startsWith(JAVA_UTIL_PKG))     javaTypeName=javaTypeName.substring(JAVA_UTIL_PKG.length());
    add2(src,owner + ".add(new JdbcTypeField(Types." + field.dbTypeName()+ ", \""+ field.dbName()+ "\", "+ javaTypeName+ ".class, \""+ field.javaName()+ "\"));");
  }
}
