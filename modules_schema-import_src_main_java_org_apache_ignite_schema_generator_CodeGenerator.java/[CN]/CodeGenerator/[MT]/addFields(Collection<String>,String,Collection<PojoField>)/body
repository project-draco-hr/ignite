{
  for (  PojoField field : fields) {
    String javaTypeName=field.javaTypeName();
    if (javaTypeName.startsWith("java.lang."))     javaTypeName=javaTypeName.substring(10);
    add2(src,owner + ".add(new CacheTypeFieldMetadata(\"" + field.dbName()+ "\", "+ "Types."+ field.dbTypeName()+ ", \""+ field.javaName()+ "\", "+ javaTypeName+ ".class));");
  }
}
