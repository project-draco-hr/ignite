{
  GridEmbeddedHttpServer srv=null;
  try {
    srv=GridEmbeddedHttpServer.startHttpServer().withFileDownloadingHandler(null,GridTestUtils.resolveIgnitePath("/modules/core/src/test/config/cache.xml"));
    IgniteCache cache=ignite(0).createCache(srv.getBaseUrl());
    assertEquals("TestDynamicCache",cache.getName());
  }
  finally {
    if (srv != null)     srv.stop(1);
  }
}
