{
  final int size=size();
  if (start < 0 || end > size || start > end)   throw new IndexOutOfBoundsException();
  if (start == end)   return empty();
  if (start == 0) {
    if (end == size)     return this;
    return this.minus(size - 1).subList(start,end);
  }
  return this.minus(0).subList(start - 1,end - 1);
}
