{
  if (!finished) {
synchronized (mux) {
      if (!finished) {
        try {
          res=c.call();
        }
 catch (        Exception e) {
          this.e=e;
        }
 finally {
          finished=true;
          mux.notifyAll();
        }
      }
    }
  }
  if (e != null)   throw e instanceof GridException ? (GridException)e : new GridException(e);
  return res;
}
