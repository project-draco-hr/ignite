{
  writer.setBuffer(buf);
  if (!super.writeTo(buf,writer))   return false;
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeField("fileId",fileId,MessageFieldType.IGNITE_UUID))     return false;
  writer.incrementState();
case 1:
if (!writer.writeCollectionField("fragmentRanges",fragmentRanges,MessageFieldType.MSG)) return false;
writer.incrementState();
}
return true;
}
