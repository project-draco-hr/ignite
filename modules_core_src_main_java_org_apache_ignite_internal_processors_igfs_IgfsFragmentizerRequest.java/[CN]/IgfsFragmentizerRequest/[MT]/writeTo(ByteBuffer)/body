{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 0:
    if (!writer.writeIgniteUuid("fileId",fileId))     return false;
  state++;
case 1:
if (!writer.writeCollection("fragmentRanges",fragmentRanges,IgfsFileAffinityRange.class)) return false;
state++;
}
return true;
}
