{
  if (F.isEmpty(p))   return true;
  CacheFlag[] oldFlags=forceFlags(FLAG_READ);
  try {
    boolean pass=F.isAll(e,p);
    if (log.isDebugEnabled())     log.debug("Evaluated filters for entry [pass=" + pass + ", entry="+ e+ ", filters="+ Arrays.toString(p)+ ']');
    return pass;
  }
 catch (  RuntimeException ex) {
    throw U.cast(ex);
  }
 finally {
    forceFlags(oldFlags);
  }
}
