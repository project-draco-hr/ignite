{
  if (valIsByteArr)   return new CacheObjectImpl(bytes,null);
  ClassLoader ldr=clsLdrId != null ? deploy().getClassLoader(clsLdrId) : deploy().localLoader();
  if (ldr == null)   return null;
  return ctx.portable().toCacheObject(cacheObjCtx,ctx.portable().unmarshal(cacheObjCtx,bytes,ldr),bytes);
}
