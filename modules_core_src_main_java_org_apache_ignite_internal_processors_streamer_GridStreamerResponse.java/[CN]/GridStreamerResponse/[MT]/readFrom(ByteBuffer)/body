{
  reader.setBuffer(buf);
switch (readState) {
case 0:
    errBytes=reader.readField("errBytes",MessageFieldType.BYTE_ARR);
  if (!reader.isLastRead())   return false;
readState++;
case 1:
futId=reader.readField("futId",MessageFieldType.IGNITE_UUID);
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
