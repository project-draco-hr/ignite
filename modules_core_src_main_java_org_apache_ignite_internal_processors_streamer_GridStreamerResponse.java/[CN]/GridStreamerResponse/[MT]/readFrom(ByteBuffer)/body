{
  reader.setBuffer(buf);
  if (!reader.beforeMessageRead())   return false;
switch (readState) {
case 0:
    errBytes=reader.readByteArray("errBytes");
  if (!reader.isLastRead())   return false;
readState++;
case 1:
futId=reader.readIgniteUuid("futId");
if (!reader.isLastRead()) return false;
readState++;
}
return true;
}
