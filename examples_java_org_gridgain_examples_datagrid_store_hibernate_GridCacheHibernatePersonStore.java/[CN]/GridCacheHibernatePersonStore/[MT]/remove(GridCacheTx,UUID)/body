{
  System.out.println("Store remove [key=" + key + ", tx="+ tx+ ']');
  Session ses=session(tx);
  try {
    ses.createQuery("delete " + Person.class.getSimpleName() + " where key = :key").setParameter("key",key).setFlushMode(FlushMode.ALWAYS).executeUpdate();
  }
 catch (  HibernateException e) {
    rollback(ses,tx);
    throw new GridException("Failed to remove value from cache store with key: " + key,e);
  }
 finally {
    end(ses,tx);
  }
}
