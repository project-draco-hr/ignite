{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putBoolean(isUndeploy))     return false;
  commState.idx++;
case 1:
if (!commState.putGridUuid(ldrId)) return false;
commState.idx++;
case 2:
if (nodeIds != null) {
if (commState.it == null) {
if (!commState.putInt(nodeIds.size())) return false;
commState.it=nodeIds.iterator();
}
while (commState.it.hasNext() || commState.cur != NULL) {
if (commState.cur == NULL) commState.cur=commState.it.next();
if (!commState.putUuid((UUID)commState.cur)) return false;
commState.cur=NULL;
}
commState.it=null;
}
 else {
if (!commState.putInt(-1)) return false;
}
commState.idx++;
case 3:
if (!commState.putByteArray(resTopicBytes)) return false;
commState.idx++;
case 4:
if (!commState.putString(rsrcName)) return false;
commState.idx++;
}
return true;
}
