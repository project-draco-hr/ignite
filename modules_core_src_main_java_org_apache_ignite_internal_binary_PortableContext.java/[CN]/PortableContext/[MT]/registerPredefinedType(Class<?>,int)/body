{
  String typeName=typeName(cls.getName());
  PortableClassDescriptor desc=new PortableClassDescriptor(this,cls,false,id,typeName,null,BinaryInternalIdMapper.defaultInstance(),null,false,true,true);
  predefinedTypeNames.put(typeName,id);
  predefinedTypes.put(id,desc);
  descByCls.put(cls,desc);
  return desc;
}
