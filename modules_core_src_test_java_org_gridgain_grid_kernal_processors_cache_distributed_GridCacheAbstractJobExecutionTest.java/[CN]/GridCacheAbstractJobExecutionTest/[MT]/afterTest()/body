{
  GridCacheProjection<String,int[]> cache=grid(0).cache(null).flagsOn(SYNC_COMMIT).projection(String.class,int[].class);
  cache.removeAll();
  for (int i=0; i < GRID_CNT; i++) {
    Grid g=grid(i);
    GridCache<String,int[]> c=g.cache(null);
    assertEquals(0,c.size());
    assertEquals(0,c.size());
  }
}
