{
  GridDhtLocalPartition part=topVer > 0 ? cache().topology().localPartition(cctx.affinity().partition(key),topVer,true) : cache().topology().localPartition(key,false);
  if (part == null)   return false;
  if (!parts.contains(part)) {
    if (part.reserve()) {
      parts.add(part);
      return true;
    }
 else     return false;
  }
 else   return true;
}
