{
  Grid grid=startGrid();
  try {
    GridClosure<String,Integer> cInvoke=GridFunc.cInvoke("length");
    Integer res=grid.compute().apply(cInvoke,Arrays.asList("Hello","World","!"),GridFunc.sumIntReducer()).get();
    assertEquals("Hello".length() + "World".length() + "!".length(),res.intValue());
  }
  finally {
    stopGrid();
  }
}
