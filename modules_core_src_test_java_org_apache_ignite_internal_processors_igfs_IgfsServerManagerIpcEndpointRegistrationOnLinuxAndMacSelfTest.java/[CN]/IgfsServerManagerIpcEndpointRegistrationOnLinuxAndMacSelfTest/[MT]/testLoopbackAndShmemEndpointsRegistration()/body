{
  IgniteConfiguration cfg=gridConfiguration();
  cfg.setIgfsConfiguration(igfsConfiguration(null,null,null),igfsConfiguration("tcp",DFLT_IPC_PORT + 1,null),igfsConfiguration("shmem",DFLT_IPC_PORT + 2,null));
  G.start(cfg);
  T2<Integer,Integer> res=checkRegisteredIpcEndpoints();
  assertEquals(4,res.get1().intValue());
  assertEquals(2,res.get2().intValue());
}
