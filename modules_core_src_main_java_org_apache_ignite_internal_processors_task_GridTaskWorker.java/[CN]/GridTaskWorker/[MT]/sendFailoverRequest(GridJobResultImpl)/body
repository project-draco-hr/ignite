{
  evtLsnr.onJobFailover(this,jobRes.getSibling(),jobRes.getNode().id());
  long timeout=ses.getEndTime() - U.currentTimeMillis();
  if (timeout > 0) {
    recordJobEvent(EVT_JOB_FAILED_OVER,jobRes.getJobContext().getJobId(),jobRes.getNode(),"Job failed over.");
    sendRequest(jobRes);
  }
 else   U.warn(log,"Failed to fail-over job due to task timeout: " + jobRes);
}
