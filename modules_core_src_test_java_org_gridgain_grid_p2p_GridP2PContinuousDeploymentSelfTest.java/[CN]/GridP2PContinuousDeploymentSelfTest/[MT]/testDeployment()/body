{
  Grid grid=startGrid(GRID_NAME);
  Class cls=getExternalClassLoader().loadClass(TEST_TASK_1);
  grid.forRemotes().compute().execute(cls,null);
  stopGrid(GRID_NAME);
  grid=startGrid(GRID_NAME);
  cls=getExternalClassLoader().loadClass(TEST_TASK_2);
  grid.forRemotes().compute().execute(cls,null);
  stopGrid(GRID_NAME);
}
