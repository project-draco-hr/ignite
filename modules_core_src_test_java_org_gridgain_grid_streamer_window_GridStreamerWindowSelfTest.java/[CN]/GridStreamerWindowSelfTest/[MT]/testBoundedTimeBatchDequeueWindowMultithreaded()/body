{
  StreamerBoundedTimeBatchWindow<Integer> win=new StreamerBoundedTimeBatchWindow<>();
  win.setMaximumBatches(10);
  win.setBatchTimeInterval(100);
  win.setBatchSize(50);
  win.start();
  checkWindowMultithreaded(win,100000,10,1000);
  win.consistencyCheck();
  finalChecks(win,500);
  U.sleep(1000);
  finalChecks(win,0);
}
