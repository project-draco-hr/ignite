{
  if (str == null) {
    StringBuilder res=new StringBuilder("FilterChain[filters=[");
    GridNioFilter ref=tail.nextFilter();
    while (ref != head) {
      res.append(ref);
      ref=ref.nextFilter();
      if (ref != head)       res.append(", ");
    }
    res.append(']');
    str=res.toString();
  }
  return str;
}
