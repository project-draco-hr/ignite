{
  super("FilterChain");
  this.log=log;
  this.lsnr=lsnr;
  this.head=head;
  GridNioFilter prev;
  tail=prev=new TailFilter();
  for (  GridNioFilter filter : filters) {
    prev.nextFilter(filter);
    filter.previousFilter(prev);
    prev=filter;
  }
  prev.nextFilter(head);
  head.previousFilter(prev);
}
