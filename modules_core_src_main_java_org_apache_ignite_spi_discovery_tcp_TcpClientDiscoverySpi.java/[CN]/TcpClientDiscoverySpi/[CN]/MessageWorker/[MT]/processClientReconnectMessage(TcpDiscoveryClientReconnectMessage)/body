{
  if (getSpiContext().isStopping())   return;
  if (getLocalNodeId().equals(msg.creatorNodeId())) {
  }
  if (getLocalNodeId().equals(msg.creatorNodeId())) {
    if (msg.success()) {
      pending=true;
      try {
        for (        TcpDiscoveryAbstractMessage pendingMsg : msg.pendingMessages())         processDiscoveryMessage(pendingMsg);
      }
  finally {
        pending=false;
      }
    }
 else {
      getSpiContext().recordEvent(new DiscoveryEvent(locNode,"Client node disconnected: " + locNode,EVT_NODE_SEGMENTED,locNode));
    }
  }
 else   if (log.isDebugEnabled())   log.debug("Discarding reconnect message for another client: " + msg);
}
