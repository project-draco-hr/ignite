{
  writer.setBuffer(buf);
  if (!super.writeTo(buf))   return false;
  if (!typeWritten) {
    if (!writer.writeByte(null,directType()))     return false;
    typeWritten=true;
  }
switch (state) {
case 11:
    if (!writer.writeObjectArray("dhtVers",dhtVers,GridCacheVersion.class))     return false;
  state++;
case 12:
if (!writer.writeBooleanArray("filterRes",filterRes)) return false;
state++;
case 13:
if (!writer.writeObjectArray("mappedVers",mappedVers,GridCacheVersion.class)) return false;
state++;
case 14:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
state++;
case 15:
if (!writer.writeCollection("pending",pending,GridCacheVersion.class)) return false;
state++;
}
return true;
}
