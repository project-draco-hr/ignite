{
  writer.setBuffer(buf);
  if (!super.writeTo(buf,writer))   return false;
  if (!writer.isTypeWritten()) {
    if (!writer.writeByte(null,directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 11:
    if (!writer.writeObjectArray("dhtVers",dhtVers,MessageFieldType.MSG))     return false;
  writer.incrementState();
case 12:
if (!writer.writeBooleanArray("filterRes",filterRes)) return false;
writer.incrementState();
case 13:
if (!writer.writeObjectArray("mappedVers",mappedVers,MessageFieldType.MSG)) return false;
writer.incrementState();
case 14:
if (!writer.writeIgniteUuid("miniId",miniId)) return false;
writer.incrementState();
case 15:
if (!writer.writeCollection("pending",pending,MessageFieldType.MSG)) return false;
writer.incrementState();
}
return true;
}
