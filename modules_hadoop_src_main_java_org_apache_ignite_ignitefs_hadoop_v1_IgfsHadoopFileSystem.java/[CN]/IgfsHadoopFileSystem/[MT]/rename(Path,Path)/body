{
  A.notNull(src,"src");
  A.notNull(dst,"dst");
  enterBusy();
  try {
    IgfsPath srcPath=convert(src);
    IgfsPath dstPath=convert(dst);
    IgniteFsMode mode=mode(srcPath);
    if (mode == PROXY) {
      if (secondaryFs == null) {
        assert mgmt;
        return false;
      }
      if (clientLog.isLogEnabled())       clientLog.logRename(srcPath,PROXY,dstPath);
      return secondaryFs.rename(toSecondary(src),toSecondary(dst));
    }
 else {
      rmtClient.rename(srcPath,dstPath);
      if (clientLog.isLogEnabled())       clientLog.logRename(srcPath,mode,dstPath);
      return true;
    }
  }
 catch (  IOException e) {
    if (F.eq(IOException.class,e.getClass()) && (e.getCause() == null || !X.hasCause(e.getCause(),IgniteFsException.class)))     throw e;
 else     return false;
  }
 finally {
    leaveBusy();
  }
}
