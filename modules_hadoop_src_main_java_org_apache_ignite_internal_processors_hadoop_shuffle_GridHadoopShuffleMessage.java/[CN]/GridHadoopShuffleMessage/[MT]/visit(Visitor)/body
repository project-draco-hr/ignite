{
  for (int i=0; i < off; ) {
    byte marker=buf[i++];
    int size=UNSAFE.getInt(buf,BYTE_ARR_OFF + i);
    i+=4;
    if (marker == MARKER_VALUE)     v.onValue(buf,i,size);
 else     if (marker == MARKER_KEY)     v.onKey(buf,i,size);
 else     throw new IllegalStateException();
    i+=size;
  }
}
