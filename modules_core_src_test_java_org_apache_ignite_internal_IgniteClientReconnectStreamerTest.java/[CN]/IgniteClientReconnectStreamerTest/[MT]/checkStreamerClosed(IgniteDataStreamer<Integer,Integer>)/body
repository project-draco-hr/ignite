{
  try {
    streamer.addData(100,100);
    fail();
  }
 catch (  CacheException e) {
    checkAndWait(e);
  }
  try {
    streamer.flush();
    fail();
  }
 catch (  CacheException e) {
    checkAndWait(e);
  }
  try {
    streamer.future().get();
    fail();
  }
 catch (  CacheException e) {
    checkAndWait(e);
  }
  streamer.tryFlush();
  streamer.close();
}
