{
  UUID subjId=null;
  int taskNameHash=cctx.kernalContext().job().currentTaskNameHash();
  return new GridNearTxLocal<>(cctx,implicit,implicitSingle,concurrency,isolation,timeout,invalidate,syncCommit,syncRollback,swapOrOffheapEnabled,storeEnabled,txSize,grpLockKey,partLock,subjId,taskNameHash);
}
