{
  X.println("BeforeFailover start TestJob [this=" + this + ", identity="+ System.identityHashCode(this)+ ", flag="+ flag+ "]");
  beforeFailoverCnt.incrementAndGet();
  flag=true;
  X.println("BeforeFailover end TestJob [this=" + this + ", identity="+ System.identityHashCode(this)+ ", flag="+ flag+ "]");
}
