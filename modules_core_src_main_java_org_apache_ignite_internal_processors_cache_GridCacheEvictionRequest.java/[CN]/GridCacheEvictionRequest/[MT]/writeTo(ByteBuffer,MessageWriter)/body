{
  writer.setBuffer(buf);
  if (!super.writeTo(buf,writer))   return false;
  if (!writer.isHeaderWritten()) {
    if (!writer.writeHeader(new MessageHeader(directType(),(byte)6)))     return false;
    writer.onHeaderWritten();
  }
switch (writer.state()) {
case 3:
    if (!writer.writeByteArray("entriesBytes",entriesBytes))     return false;
  writer.incrementState();
case 4:
if (!writer.writeLong("futId",futId)) return false;
writer.incrementState();
case 5:
if (!writer.writeLong("topVer",topVer)) return false;
writer.incrementState();
}
return true;
}
