{
  if (ctx.isStoreEnabled() && req.operation() == TRANSFORM) {
    List<K> keys=req.keys();
    for (int i=0; i < keys.size(); i++) {
      GridDhtCacheEntry<K,V> entry=entryExx(keys.get(i),req.topologyVersion());
      if (!entry.hasValue()) {
        req.forceTransformBackups(false);
        return;
      }
    }
  }
}
