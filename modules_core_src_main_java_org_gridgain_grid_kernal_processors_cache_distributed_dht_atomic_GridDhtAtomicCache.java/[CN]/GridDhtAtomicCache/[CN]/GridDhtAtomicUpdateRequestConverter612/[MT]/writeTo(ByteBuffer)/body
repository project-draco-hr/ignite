{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
{
      if (!commState.putInt(-1))       return false;
      commState.idx++;
    }
case 1:
{
    if (!commState.putBoolean(false))     return false;
    commState.idx++;
  }
case 2:
{
  if (!commState.putInt(-1))   return false;
  commState.idx++;
}
}
return true;
}
