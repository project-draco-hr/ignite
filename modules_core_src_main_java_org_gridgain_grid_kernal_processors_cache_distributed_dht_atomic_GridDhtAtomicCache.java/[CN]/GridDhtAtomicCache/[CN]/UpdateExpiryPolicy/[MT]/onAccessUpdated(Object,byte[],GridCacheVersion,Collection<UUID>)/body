{
  if (entries == null)   entries=new HashMap<>();
  IgniteBiTuple t=new IgniteBiTuple<>(keyBytes,ver);
  entries.put(key,t);
  if (!F.isEmpty(rdrs)) {
    if (rdrs == null)     rdrsMap=new HashMap<>();
    for (    UUID nodeId : rdrs) {
      Collection<IgniteBiTuple<byte[],GridCacheVersion>> col=rdrsMap.get(nodeId);
      if (col == null)       rdrsMap.put(nodeId,col=new ArrayList<>());
      col.add(t);
    }
  }
}
