{
  if (cancel)   return;
  long startTime=U.currentTimeMillis();
  try {
    task.run();
  }
  finally {
    long executionTime=U.currentTimeMillis() - startTime;
    if (executionTime > 10) {
      U.warn(log,"Timer task take a lot of time, tasks submitted to GridTimeoutProcessor must work " + "quickly [executionTime=" + executionTime + ']');
    }
    if (!cancel && period > 0) {
      endTime=U.currentTimeMillis() + period;
      addTimeoutObject(this);
    }
  }
}
