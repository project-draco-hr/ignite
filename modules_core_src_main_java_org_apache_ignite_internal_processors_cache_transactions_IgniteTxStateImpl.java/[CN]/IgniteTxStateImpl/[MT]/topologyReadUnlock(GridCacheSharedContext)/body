{
  if (!activeCacheIds.isEmpty()) {
    GridCacheContext<?,?> nonLocCtx=null;
    for (    int cacheId : activeCacheIds) {
      GridCacheContext<?,?> cacheCtx=cctx.cacheContext(cacheId);
      if (!cacheCtx.isLocal()) {
        nonLocCtx=cacheCtx;
        break;
      }
    }
    if (nonLocCtx != null)     nonLocCtx.topology().readUnlock();
  }
}
