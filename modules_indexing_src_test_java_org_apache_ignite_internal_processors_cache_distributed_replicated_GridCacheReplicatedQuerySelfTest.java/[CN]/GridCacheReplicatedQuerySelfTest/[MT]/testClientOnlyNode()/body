{
  try {
    Ignite g=startGrid("client");
    GridCache<Integer,Integer> c=g.cache(null);
    for (int i=0; i < 10; i++)     c.putx(i,i);
    assertEquals(0,c.size());
    Collection<Map.Entry<Integer,Integer>> res=c.queries().createSqlQuery(Integer.class,"_key >= 5 order by _key").execute().get();
    assertEquals(5,res.size());
    Iterator<Map.Entry<Integer,Integer>> it=res.iterator();
    int i=5;
    while (it.hasNext()) {
      Map.Entry<Integer,Integer> e=it.next();
      assertEquals(i,e.getKey().intValue());
      assertEquals(i,e.getValue().intValue());
      i++;
    }
  }
  finally {
    stopGrid("client");
  }
}
