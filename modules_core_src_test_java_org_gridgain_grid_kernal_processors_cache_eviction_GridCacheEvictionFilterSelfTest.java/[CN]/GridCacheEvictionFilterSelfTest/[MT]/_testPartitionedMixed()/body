{
  mode=PARTITIONED;
  nearEnabled=false;
  filter=new EvictionFilter();
  Ignite g=startGrid();
  GridCache<Object,Object> cache=g.cache(null);
  try {
    int id=1;
    cache.putx(id++,1);
    cache.putx(id++,2);
    for (int i=id + 1; i < 10; i++) {
      cache.putx(id,id);
      cache.putx(i,String.valueOf(i));
    }
    info(">>>> " + cache.get(1));
    info(">>>> " + cache.get(2));
    info(">>>> " + cache.get(3));
  }
  finally {
    stopGrid();
  }
}
