{
  CacheConfiguration ccfg=null;
  for (  CacheConfiguration ccfg0 : ctx.config().getCacheConfiguration()) {
    if (F.eq(cacheName,ccfg0.getName())) {
      ccfg=ccfg0;
      break;
    }
  }
  return new CacheObjectContext(ctx,new GridCacheDefaultAffinityKeyMapper(),ccfg != null && ccfg.isCopyOnGet(),ctx.config().isPeerClassLoadingEnabled() || (ccfg != null && ccfg.isQueryIndexEnabled()));
}
