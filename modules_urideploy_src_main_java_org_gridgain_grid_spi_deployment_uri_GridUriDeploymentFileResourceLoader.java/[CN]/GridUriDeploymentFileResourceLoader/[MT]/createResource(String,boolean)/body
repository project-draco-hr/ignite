{
  if (scanPathDir.isDirectory())   fileName=fileName.substring(scanPathDir.getAbsolutePath().length() + 1);
  if (fileName.endsWith(".class")) {
    String str=fileName;
    str=str.replaceAll("\\/|\\\\",".");
    str=str.substring(0,str.indexOf(".class"));
    try {
      return (Class<? extends GridComputeTask<?,?>>)clsLdr.loadClass(str);
    }
 catch (    ClassNotFoundException e) {
      if (ignoreUnknownRsrc) {
      }
 else       throw new GridSpiException("Failed to load class: " + str,e);
    }
  }
  return null;
}
