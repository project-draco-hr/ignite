{
  IgniteCache<Integer,Integer> cache=jcache();
  invoke(cache,null);
  if (atomicityMode() == TRANSACTIONAL) {
    invoke(cache,PESSIMISTIC);
    invoke(cache,OPTIMISTIC);
  }
 else   if (gridCount() > 1) {
    cache=((IgniteCacheProxy<Integer,Integer>)cache).flagsOn(FORCE_TRANSFORM_BACKUP);
    invoke(cache,null);
  }
}
