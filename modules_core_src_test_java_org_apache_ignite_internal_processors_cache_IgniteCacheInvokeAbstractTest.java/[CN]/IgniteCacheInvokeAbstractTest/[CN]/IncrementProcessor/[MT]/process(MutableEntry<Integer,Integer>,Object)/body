{
  Ignite ignite=e.unwrap(Ignite.class);
  assertNotNull(ignite);
  if (e.exists()) {
    Integer val=e.getValue();
    assertNotNull(val);
    e.setValue(val + 1);
    assertTrue(e.exists());
    assertEquals(val + 1,(int)e.getValue());
    return val;
  }
 else {
    e.setValue(1);
    return -1;
  }
}
