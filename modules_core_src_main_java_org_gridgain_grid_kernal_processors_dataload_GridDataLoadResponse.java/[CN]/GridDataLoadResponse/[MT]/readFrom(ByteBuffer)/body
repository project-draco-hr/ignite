{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    errBytes=commState.getByteArray("errBytes");
  if (!commState.lastRead())   return false;
commState.idx++;
case 1:
forceLocDep=commState.getBoolean("forceLocDep");
if (!commState.lastRead()) return false;
commState.idx++;
case 2:
reqId=commState.getLong("reqId");
if (!commState.lastRead()) return false;
commState.idx++;
}
return true;
}
