{
  Collection<Object> objs=new ArrayList<>();
  if (!F.isEmpty(cfg.getLifecycleBeans()))   F.copy(objs,cfg.getLifecycleBeans());
  if (!F.isEmpty(cfg.getSegmentationResolvers()))   F.copy(objs,cfg.getSegmentationResolvers());
  if (cfg.getConnectorConfiguration() != null)   F.copy(objs,cfg.getConnectorConfiguration().getMessageInterceptor(),cfg.getConnectorConfiguration().getSslContextFactory());
  F.copy(objs,cfg.getMarshaller(),cfg.getGridLogger(),cfg.getMBeanServer());
  return objs;
}
