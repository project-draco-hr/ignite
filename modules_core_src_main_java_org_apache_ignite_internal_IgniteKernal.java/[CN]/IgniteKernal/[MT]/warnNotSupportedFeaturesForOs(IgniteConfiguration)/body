{
  Collection<String> msgs=new ArrayList<>();
  if (!F.isEmpty(cfg.getSegmentationResolvers()))   msgs.add("Network segmentation detection.");
  if (!F.isEmpty(msgs)) {
    U.quietAndInfo(log,"The following features are not supported in open source edition, " + "related configuration settings will be ignored " + "(consider downloading enterprise edition from http://www.gridgain.com):");
    for (    String s : msgs)     U.quietAndInfo(log,"  ^-- " + s);
    U.quietAndInfo(log,"");
  }
}
