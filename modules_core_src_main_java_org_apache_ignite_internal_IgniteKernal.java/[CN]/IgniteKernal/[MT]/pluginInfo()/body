{
  Collection<PluginProvider> pluginProviders=ctx.plugins().allProviders();
  if (pluginProviders.size() == 0)   return PLUGIN_INFO + "none";
  String info=PLUGIN_INFO + NL;
  for (  PluginProvider provider : pluginProviders)   info+=provider.plugin().name() + " " + provider.plugin().version()+ NL;
  return info;
}
