{
  return new GridHadoopTaskOutput(){
    /** 
 * {@inheritDoc} 
 */
    @Override public void write(    Object key,    Object val){
    }
    /** 
 * {@inheritDoc} 
 */
    @Override public GridFuture<?> finish(){
      return new GridFinishedFutureEx<>();
    }
    /** 
 * {@inheritDoc} 
 */
    @Override public void close() throws Exception {
    }
  }
;
}
