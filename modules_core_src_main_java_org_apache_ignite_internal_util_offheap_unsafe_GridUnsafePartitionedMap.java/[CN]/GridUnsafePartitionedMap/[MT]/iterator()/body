{
  return new PartitionedMapCloseableIterator<IgniteBiTuple<byte[],byte[]>>(){
    protected void advance() throws IgniteCheckedException {
      curIt=null;
      while (p < parts) {
        curIt=mapFor(p++).iterator();
        if (curIt.hasNext())         return;
 else         curIt.close();
      }
      curIt=null;
    }
  }
;
}
