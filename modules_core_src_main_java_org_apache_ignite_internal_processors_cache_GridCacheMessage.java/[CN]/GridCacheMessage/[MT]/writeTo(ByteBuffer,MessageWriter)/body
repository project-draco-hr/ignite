{
  writer.setBuffer(buf);
  if (!writer.isTypeWritten()) {
    if (!writer.writeMessageType(directType()))     return false;
    writer.onTypeWritten();
  }
switch (writer.state()) {
case 0:
    if (!writer.writeField("cacheId",cacheId,MessageFieldType.INT))     return false;
  writer.incrementState();
case 1:
if (!writer.writeField("depInfo",depInfo,MessageFieldType.MSG)) return false;
writer.incrementState();
case 2:
if (!writer.writeField("msgId",msgId,MessageFieldType.LONG)) return false;
writer.incrementState();
}
return true;
}
