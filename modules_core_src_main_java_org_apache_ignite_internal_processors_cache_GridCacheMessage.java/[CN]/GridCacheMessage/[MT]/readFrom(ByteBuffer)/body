{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    cacheId=commState.getInt("cacheId");
  if (!commState.lastRead())   return false;
commState.idx++;
case 1:
depInfo=(GridDeploymentInfoBean)commState.getMessage("depInfo");
if (!commState.lastRead()) return false;
commState.idx++;
case 2:
msgId=commState.getLong("msgId");
if (!commState.lastRead()) return false;
commState.idx++;
}
return true;
}
