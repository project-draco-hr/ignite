{
  if (col == null)   return;
  int size=col.size();
  boolean depEnabled=ctx.deploymentEnabled();
  for (int i=0; i < size; i++) {
    CacheObject obj=col.get(i);
    if (obj != null) {
      obj.prepareMarshal(ctx.cacheObjectContext());
      if (depEnabled)       prepareObject(obj.value(ctx,false),ctx.shared());
    }
  }
}
