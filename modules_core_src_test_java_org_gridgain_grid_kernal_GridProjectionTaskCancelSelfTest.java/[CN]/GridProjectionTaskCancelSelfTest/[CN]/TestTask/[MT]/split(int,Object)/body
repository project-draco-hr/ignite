{
  Collection<GridComputeJob> jobs=new ArrayList<>(NODES_CNT);
  for (int i=0; i < NODES_CNT; i++)   jobs.add(new GridComputeJobAdapter(){
    @Override public Object execute(){
      try {
        finishJobs.await();
      }
 catch (      InterruptedException ignored) {
        return null;
      }
      return SUCCESS;
    }
  }
);
  return jobs;
}
