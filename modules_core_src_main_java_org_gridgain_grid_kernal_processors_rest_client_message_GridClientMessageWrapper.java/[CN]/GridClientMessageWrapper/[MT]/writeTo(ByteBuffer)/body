{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(null,directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putInt(null,msgSize))     return false;
  commState.idx++;
case 1:
if (!commState.putLong(null,reqId)) return false;
commState.idx++;
case 2:
if (!commState.putUuid(null,clientId)) return false;
commState.idx++;
case 3:
if (!commState.putUuid(null,destId)) return false;
commState.idx++;
case 4:
commState.idx++;
}
return true;
}
