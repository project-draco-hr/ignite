{
  commState.setBuffer(buf);
  if (!commState.typeWritten) {
    if (!commState.putByte(null,directType()))     return false;
    commState.typeWritten=true;
  }
switch (commState.idx) {
case 0:
    if (!commState.putUuid("clientId",clientId))     return false;
  commState.idx++;
case 1:
if (!commState.putUuid("destId",destId)) return false;
commState.idx++;
case 2:
if (!commState.putByteBuffer("msg",msg)) return false;
commState.idx++;
case 3:
if (!commState.putInt("msgSize",msgSize)) return false;
commState.idx++;
case 4:
if (!commState.putLong("reqId",reqId)) return false;
commState.idx++;
}
return true;
}
