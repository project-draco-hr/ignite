{
  commState.setBuffer(buf);
switch (commState.idx) {
case 0:
    if (buf.remaining() < 4)     return false;
  msgSize=commState.getIntClient();
if (msgSize == 0) return true;
commState.idx++;
case 1:
if (buf.remaining() < 8) return false;
reqId=commState.getLongClient();
commState.idx++;
case 2:
UUID clientId0=commState.getUuidClient();
if (clientId0 == UUID_NOT_READ) return false;
clientId=clientId0;
commState.idx++;
case 3:
UUID destId0=commState.getUuidClient();
if (destId0 == UUID_NOT_READ) return false;
destId=destId0;
commState.idx++;
case 4:
byte[] msg0=commState.getByteArrayClient(msgSize - 40);
if (msg0 == BYTE_ARR_NOT_READ) return false;
msg=msg0;
commState.idx++;
}
return true;
}
