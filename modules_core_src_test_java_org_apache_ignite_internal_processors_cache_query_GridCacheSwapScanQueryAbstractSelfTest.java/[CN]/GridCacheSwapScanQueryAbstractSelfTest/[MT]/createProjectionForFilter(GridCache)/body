{
  return cache.projection(new IgnitePredicate<CacheEntry<Key,Person>>(){
    @Override public boolean apply(    CacheEntry<Key,Person> e){
      Key key=e.getKey();
      Person val=e.peek();
      assertNotNull(e.version());
      assertEquals(key.id,(Integer)val.salary);
      return key.id % 100 != 0;
    }
  }
);
}
